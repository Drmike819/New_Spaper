[32m+[m[32mvar uncurryThis = require('../internals/function-uncurry-this');[m
[32m+[m[32mvar toIntegerOrInfinity = require('../internals/to-integer-or-infinity');[m
[32m+[m[32mvar toString = require('../internals/to-string');[m
[32m+[m[32mvar requireObjectCoercible = require('../internals/require-object-coercible');[m
[32m+[m
[32m+[m[32mvar charAt = uncurryThis(''.charAt);[m
[32m+[m[32mvar charCodeAt = uncurryThis(''.charCodeAt);[m
[32m+[m[32mvar stringSlice = uncurryThis(''.slice);[m
[32m+[m
[32m+[m[32mvar createMethod = function (CONVERT_TO_STRING) {[m
[32m+[m[32m  return function ($this, pos) {[m
[32m+[m[32m    var S = toString(requireObjectCoercible($this));[m
[32m+[m[32m    var position = toIntegerOrInfinity(pos);[m
[32m+[m[32m    var size = S.length;[m
[32m+[m[32m    var first, second;[m
[32m+[m[32m    if (position < 0 || position >= size) return CONVERT_TO_STRING ? '' : undefined;[m
[32m+[m[32m    first = charCodeAt(S, position);[m
[32m+[m[32m    return first < 0xD800 || first > 0xDBFF || position + 1 === size[m
[32m+[m[32m      || (second = charCodeAt(S, position + 1)) < 0xDC00 || second > 0xDFFF[m
[32m+[m[32m        ? CONVERT_TO_STRING[m
[32m+[m[32m          ? charAt(S, position)[m
[32m+[m[32m          : first[m
[32m+[m[32m        : CONVERT_TO_STRING[m
[32m+[m[32m          ? stringSlice(S, position, position + 2)[m
[32m+[m[32m          : (first - 0xD800 << 10) + (second - 0xDC00) + 0x10000;[m
[32m+[m[32m  };[m
