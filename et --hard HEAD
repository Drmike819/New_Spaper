[32m+[m
[32m+[m[32m},{}],93:[function(require,module,exports){[m
[32m+[m[32m'use strict';[m
[32m+[m[32mvar global = require('../internals/global');[m
[32m+[m[32mvar apply = require('../internals/function-apply');[m
[32m+[m[32mvar uncurryThis = require('../internals/function-uncurry-this');[m
[32m+[m[32mvar isCallable = require('../internals/is-callable');[m
[32m+[m[32mvar getOwnPropertyDescriptor = require('../internals/object-get-own-property-descriptor').f;[m
[32m+[m[32mvar isForced = require('../internals/is-forced');[m
[32m+[m[32mvar path = require('../internals/path');[m
[32m+[m[32mvar bind = require('../internals/function-bind-context');[m
[32m+[m[32mvar createNonEnumerableProperty = require('../internals/create-non-enumerable-property');[m
[32m+[m[32mvar hasOwn = require('../internals/has-own-property');[m
[32m+[m
[32m+[m[32mvar wrapConstructor = function (NativeConstructor) {[m
[32m+[m[32m  var Wrapper = function (a, b, c) {[m
[32m+[m[32m    if (this instanceof Wrapper) {[m
[32m+[m[32m      switch (arguments.length) {[m
[32m+[m[32m        case 0: return new NativeConstructor();[m
[32m+[m[32m        case 1: return new NativeConstructor(a);[m
[32m+[m[32m        case 2: return new NativeConstructor(a, b);[m
[32m+[m[32m      } return new NativeConstructor(a, b, c);[m
[32m+[m[32m    } return apply(NativeConstructor, this, arguments);[m
[32m+[m[32m  };[m
[32m+[m[32m  Wrapper.prototype = NativeConstructor.prototype;[m
[32m+[m[32m  return Wrapper;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m  options.target      - name of the target object[m
[32m+[m[32m  options.global      - target is the global object[m
[32m+[m[32m  options.stat        - export as static methods of target[m
[32m+[m[32m  options.proto       - export as prototype methods of target[m
[32m+[m[32m  options.real        - real prototype method for the `pure` version[m
[32m+[m[32m  options.forced      - export even if the native feature is available[m
[32m+[m[32m  options.bind        - bind methods to the target, required for the `pure` version[m
[32m+[m[32m  options.wrap        - wrap constructors to preventing global pollution, required for the `pure` version[m
[32m+[m[32m  options.unsafe      - use the simple assignment of property instead of delete + defineProperty[m
[32m+[m[32m  options.sham        - add a flag to not completely full polyfills[m
[32m+[m[32m  options.enumerable  - export as enumerable property[m
[32m+[m[32m  options.noTargetGet - prevent calling a getter on target[m
[32m+[m[32m  options.name        - the .name of the function if it does not match the key[m
[32m+[m[32m*/[m
[32m+[m[32mmodule.exports = function (options, source) {[m
[32m+[m[32m  var TARGET = options.target;[m
[32m+[m[32m  var GLOBAL = options.global;[m
[32m+[m[32m  var STATIC = options.stat;[m
[32m+[m[32m  var PROTO = options.proto;[m
[32m+[m
[32m+[m[32m  var nativeSource = GLOBAL ? global : STATIC ? global[TARGET] : (global[TARGET] || {}).prototype;[m
[32m+[m
[32m+[m[32m  var target = GLOBAL ? path : path[TARGET] || createNonEnumerableProperty(path, TARGET, {})[TARGET];[m
[32m+[m[32m  var targetPrototype = target.prototype;[m
[32m+[m
[32m+[m[32m  var FORCED, USE_NATIVE, VIRTUAL_PROTOTYPE;[m
[32m+[m[32m  var key, sourceProperty, targetProperty, nativeProperty, resultProperty, descriptor;[m
[32m+[m
[32m+[m[32m  for (key in source) {[m
[32m+[m[32m    FORCED = isForced(GLOBAL ? key : TARGET + (STATIC ? '.' : '#') + key, options.forced);[m
[32m+[m[32m    // contains in native[m
[32m+[m[32m    USE_NATIVE = !FORCED && nativeSource && hasOwn(nativeSource, key);[m
[32m+[m
[32m+[m[32m    targetProperty = target[key];[m
[32m+[m
[32m+[m[32m    if (USE_NATIVE) if (options.noTargetGet) {[m
[32m+[m[32m      descriptor = getOwnPropertyDescriptor(nativeSource, key);[m
[32m+[m[32m      nativeProperty = descriptor && descriptor.value;[m
[32m+[m[32m    } else nativeProperty = nativeSource[key];[m
[32m+[m
[32m+[m[32m    // export native or implementation[m
[32m+[m[32m    sourceProperty = (USE_NATIVE && nativeProperty) ? nativeProperty : source[key];[m
[32m+[m
[32m+[m[32m    if (USE_NATIVE && typeof targetProperty == typeof sourceProperty) continue;[m
[32m+[m
[32m+[m[32m    // bind timers to global for call from export context[m
[32m+[m[32m    if (options.bind && USE_NATIVE) resultProperty = bind(sourceProperty, global);[m
[32m+[m[32m    // wrap global constructors for prevent changs in this version[m
[32m+[m[32m    else if (options.wrap && USE_NATIVE) resultProperty = wrapConstructor(sourceProperty);[m
[32m+[m[32m    // make static versions for prototype methods[m
[32m+[m[32m    else if (PROTO && isCallable(sourceProperty)) resultProperty = uncurryThis(sourceProperty);[m
[32m+[m[32m    // default case[m
[32m+[m[32m    else resultProperty = sourceProperty;[m
[32m+[m
[32m+[m[32m    // add a flag to not completely full polyfills[m
[32m+[m[32m    if (options.sham || (sourceProperty && sourceProperty.sham) || (targetProperty && targetProperty.sham)) {[m
[32m+[m[32m      createNonEnumerableProperty(resultProperty, 'sham', true);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    createNonEnumerableProperty(target, key, resultProperty);[m
[32m+[m
[32m+[m[32m    if (PROTO) {[m
[32m+[m[32m      VIRTUAL_PROTOTYPE = TARGET + 'Prototype';[m
[32m+[m[32m      if (!hasOwn(path, VIRTUAL_PROTOTYPE)) {[m
[32m+[m[32m        createNonEnumerableProperty(path, VIRTUAL_PROTOTYPE, {});[m
[32m+[m[32m      }[m
[32m+[m[32m      // export virtual prototype methods[m
[32m+[m[32m      createNonEnumerableProperty(path[VIRTUAL_PROTOTYPE], key, sourceProperty);[m
[32m+[m[32m      // export real prototype methods[m
[32m+[m[32m      if (options.real && targetPrototype && !targetPrototype[key]) {[m
[32m+[m[32m        createNonEnumerableProperty(targetPrototype, key, sourceProperty);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m},{"../internals/create-non-enumerable-property":78,"../internals/function-apply":95,"../internals/function-bind-context":96,"../internals/function-uncurry-this":99,"../internals/global":104,"../internals/has-own-property":105,"../internals/is-callable":114,"../internals/is-forced":116,"../internals/object-get-own-property-descriptor":130,"../internals/path":142}],94:[function(require,module,exports){[m
