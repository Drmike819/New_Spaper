[32m+[m[41m      [m
[32m+[m[32m      <!-- Bot√≥n de actualizar -->[m
[32m+[m[32m      <button class="btn btn-info ml-2" type="submit">Update</button>[m
[32m+[m[32m  </form>[m
[32m+[m[41m  [m
[32m+[m[32m  <!-- JavaScript para a√±adir formularios de imagen din√°micamente -->[m
[32m+[m[32m  <script>[m
[32m+[m[32m      function addImageForm() {[m
[32m+[m[32m          var newForm = document.querySelector('.image-form').cloneNode(true);[m
[32m+[m[32m          newForm.querySelector('input[type="file"]').value = '';[m
[32m+[m[32m          newForm.querySelector('input[type="checkbox"]').checked = false;[m
[32m+[m[32m          document.querySelector('form').insertBefore(newForm, document.querySelector('button[onclick="addImageForm()"]'));[m
[32m+[m[32m      }[m
[32m+[m[32m  </script>[m
[32m+[m[32m{% endblock content %}[m
