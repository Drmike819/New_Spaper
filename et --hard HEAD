[1mdiff --git a/staticfiles/admin/js/theme.js b/staticfiles/admin/js/theme.js[m
[1mnew file mode 100644[m
[1mindex 0000000..794cd15[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/theme.js[m
[36m@@ -0,0 +1,56 @@[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    window.addEventListener('load', function(e) {[m
[32m+[m
[32m+[m[32m        function setTheme(mode) {[m
[32m+[m[32m            if (mode !== "light" && mode !== "dark" && mode !== "auto") {[m
[32m+[m[32m                console.error(`Got invalid theme mode: ${mode}. Resetting to auto.`);[m
[32m+[m[32m                mode = "auto";[m
[32m+[m[32m            }[m
[32m+[m[32m            document.documentElement.dataset.theme = mode;[m
[32m+[m[32m            localStorage.setItem("theme", mode);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        function cycleTheme() {[m
[32m+[m[32m            const currentTheme = localStorage.getItem("theme") || "auto";[m
[32m+[m[32m            const prefersDark = window.matchMedia("(prefers-color-scheme: dark)").matches;[m
[32m+[m
[32m+[m[32m            if (prefersDark) {[m
[32m+[m[32m                // Auto (dark) -> Light -> Dark[m
[32m+[m[32m                if (currentTheme === "auto") {[m
[32m+[m[32m                    setTheme("light");[m
[32m+[m[32m                } else if (currentTheme === "light") {[m
[32m+[m[32m                    setTheme("dark");[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    setTheme("auto");[m
[32m+[m[32m                }[m
[32m+[m[32m            } else {[m
[32m+[m[32m                // Auto (light) -> Dark -> Light[m
[32m+[m[32m                if (currentTheme === "auto") {[m
[32m+[m[32m                    setTheme("dark");[m
[32m+[m[32m                } else if (currentTheme === "dark") {[m
[32m+[m[32m                    setTheme("light");[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    setTheme("auto");[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        function initTheme() {[m
[32m+[m[32m            // set theme defined in localStorage if there is one, or fallback to auto mode[m
[32m+[m[32m            const currentTheme = localStorage.getItem("theme");[m
[32m+[m[32m            currentTheme ? setTheme(currentTheme) : setTheme("auto");[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        function setupTheme() {[m
[32m+[m[32m            // Attach event handlers for toggling themes[m
[32m+[m[32m            const buttons = document.getElementsByClassName("theme-toggle");[m
[32m+[m[32m            Array.from(buttons).forEach((btn) => {[m
[32m+[m[32m                btn.addEventListener("click", cycleTheme);[m
[32m+[m[32m            });[m
[32m+[m[32m            initTheme();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        setupTheme();[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
