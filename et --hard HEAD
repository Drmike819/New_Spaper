[1mnew file mode 100644[m
[1mindex 0000000..aa1cae9[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/admin/DateTimeShortcuts.js[m
[36m@@ -0,0 +1,408 @@[m
[32m+[m[32m/*global Calendar, findPosX, findPosY, get_format, gettext, gettext_noop, interpolate, ngettext, quickElement*/[m
[32m+[m[32m// Inserts shortcut buttons after all of the following:[m
[32m+[m[32m//     <input type="text" class="vDateField">[m
[32m+[m[32m//     <input type="text" class="vTimeField">[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    const DateTimeShortcuts = {[m
[32m+[m[32m        calendars: [],[m
[32m+[m[32m        calendarInputs: [],[m
[32m+[m[32m        clockInputs: [],[m
[32m+[m[32m        clockHours: {[m
[32m+[m[32m            default_: [[m
[32m+[m[32m                [gettext_noop('Now'), -1],[m
[32m+[m[32m                [gettext_noop('Midnight'), 0],[m
[32m+[m[32m                [gettext_noop('6 a.m.'), 6],[m
[32m+[m[32m                [gettext_noop('Noon'), 12],[m
[32m+[m[32m                [gettext_noop('6 p.m.'), 18][m
[32m+[m[32m            ][m
[32m+[m[32m        },[m
[32m+[m[32m        dismissClockFunc: [],[m
[32m+[m[32m        dismissCalendarFunc: [],[m
[32m+[m[32m        calendarDivName1: 'calendarbox', // name of calendar <div> that gets toggled[m
[32m+[m[32m        calendarDivName2: 'calendarin', // name of <div> that contains calendar[m
[32m+[m[32m        calendarLinkName: 'calendarlink', // name of the link that is used to toggle[m
[32m+[m[32m        clockDivName: 'clockbox', // name of clock <div> that gets toggled[m
[32m+[m[32m        clockLinkName: 'clocklink', // name of the link that is used to toggle[m
[32m+[m[32m        shortCutsClass: 'datetimeshortcuts', // class of the clock and cal shortcuts[m
[32m+[m[32m        timezoneWarningClass: 'timezonewarning', // class of the warning for timezone mismatch[m
[32m+[m[32m        timezoneOffset: 0,[m
[32m+[m[32m        init: function() {[m
[32m+[m[32m            const serverOffset = document.body.dataset.adminUtcOffset;[m
[32m+[m[32m            if (serverOffset) {[m
[32m+[m[32m                const localOffset = new Date().getTimezoneOffset() * -60;[m
[32m+[m[32m                DateTimeShortcuts.timezoneOffset = localOffset - serverOffset;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            for (const inp of document.getElementsByTagName('input')) {[m
[32m+[m[32m                if (inp.type === 'text' && inp.classList.contains('vTimeField')) {[m
[32m+[m[32m                    DateTimeShortcuts.addClock(inp);[m
[32m+[m[32m                    DateTimeShortcuts.addTimezoneWarning(inp);[m
[32m+[m[32m                }[m
[32m+[m[32m                else if (inp.type === 'text' && inp.classList.contains('vDateField')) {[m
[32m+[m[32m                    DateTimeShortcuts.addCalendar(inp);[m
[32m+[m[32m                    DateTimeShortcuts.addTimezoneWarning(inp);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        },[m
[32m+[m[32m        // Return the current time while accounting for the server timezone.[m
[32m+[m[32m        now: function() {[m
[32m+[m[32m            const serverOffset = document.body.dataset.adminUtcOffset;[m
[32m+[m[32m            if (serverOffset) {[m
[32m+[m[32m                const localNow = new Date();[m
[32m+[m[32m                const localOffset = localNow.getTimezoneOffset() * -60;[m
[32m+[m[32m                localNow.setTime(localNow.getTime() + 1000 * (serverOffset - localOffset));[m
[32m+[m[32m                return localNow;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                return new Date();[m
[32m+[m[32m            }[m
[32m+[m[32m        },[m
[32m+[m[32m        // Add a warning when the time zone in the browser and backend do not match.[m
[32m+[m[32m        addTimezoneWarning: function(inp) {[m
[32m+[m[32m            const warningClass = DateTimeShortcuts.timezoneWarningClass;[m
[32m+[m[32m            let timezoneOffset = DateTimeShortcuts.timezoneOffset / 3600;[m
[32m+[m
[32m+[m[32m            // Only warn if there is a time zone mismatch.[m
[32m+[m[32m            if (!timezoneOffset) {[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // Check if warning is already there.[m
[32m+[m[32m            if (inp.parentNode.querySelectorAll('.' + warningClass).length) {[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            let message;[m
[32m+[m[32m            if (timezoneOffset > 0) {[m
[32m+[m[32m                message = ngettext([m
[32m+[m[32m                    'Note: You are %s hour ahead of server time.',[m
[32m+[m[32m                    'Note: You are %s hours ahead of server time.',[m
[32m+[m[32m                    timezoneOffset[m
[32m+[m[32m                );[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                timezoneOffset *= -1;[m
[32m+[m[32m                message = ngettext([m
[32m+[m[32m                    'Note: You are %s hour behind server time.',[m
[32m+[m[32m                    'Note: You are %s hours behind server time.',[m
[32m+[m[32m                    timezoneOffset[m
[32m+[m[32m                );[m
[32m+[m[32m            }[m
[32m+[m[32m            message = interpolate(message, [timezoneOffset]);[m
[32m+[m
[32m+[m[32m            const warning = document.createElement('div');[m
[32m+[m[32m            warning.classList.add('help', warningClass);[m
[32m+[m[32m            warning.textContent = message;[m
[32m+[m[32m            inp.parentNode.appendChild(warning);[m
[32m+[m[32m        },[m
[32m+[m[32m        // Add clock widget to a given field[m
[32m+[m[32m        addClock: function(inp) {[m
[32m+[m[32m            const num = DateTimeShortcuts.clockInputs.length;[m
[32m+[m[32m            DateTimeShortcuts.clockInputs[num] = inp;[m
[32m+[m[32m            DateTimeShortcuts.dismissClockFunc[num] = function() { DateTimeShortcuts.dismissClock(num); return true; };[m
[32m+[m
[32m+[m[32m            // Shortcut links (clock icon and "Now" link)[m
[32m+[m[32m            const shortcuts_span = document.createElement('span');[m
[32m+[m[32m            shortcuts_span.className = DateTimeShortcuts.shortCutsClass;[m
[32m+[m[32m            inp.parentNode.insertBefore(shortcuts_span, inp.nextSibling);[m
[32m+[m[32m            const now_link = document.createElement('a');[m
[32m+[m[32m            now_link.href = "#";[m
[32m+[m[32m            now_link.textContent = gettext('Now');[m
[32m+[m[32m            now_link.addEventListener('click', function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                DateTimeShortcuts.handleClockQuicklink(num, -1);[m
[32m+[m[32m            });[m
[32m+[m[32m            const clock_link = document.createElement('a');[m
[32m+[m[32m            clock_link.href = '#';[m
[32m+[m[32m            clock_link.id = DateTimeShortcuts.clockLinkName + num;[m
[32m+[m[32m            clock_link.addEventListener('click', function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                // avoid triggering the document click handler to dismiss the clock[m
[32m+[m[32m                e.stopPropagation();[m
[32m+[m[32m                DateTimeShortcuts.openClock(num);[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            quickElement([m
[32m+[m[32m                'span', clock_link, '',[m
[32m+[m[32m                'class', 'clock-icon',[m
[32m+[m[32m                'title', gettext('Choose a Time')[m
[32m+[m[32m            );[m
[32m+[m[32m            shortcuts_span.appendChild(document.createTextNode('\u00A0'));[m
[32m+[m[32m            shortcuts_span.appendChild(now_link);[m
[32m+[m[32m            shortcuts_span.appendChild(document.createTextNode('\u00A0|\u00A0'));[m
[32m+[m[32m            shortcuts_span.appendChild(clock_link);[m
[32m+[m
[32m+[m[32m            // Create clock link div[m
[32m+[m[32m            //[m
[32m+[m[32m            // Markup looks like:[m
[32m+[m[32m            // <div id="clockbox1" class="clockbox module">[m
[32m+[m[32m            //     <h2>Choose a time</h2>[m
[32m+[m[32m            //     <ul class="timelist">[m
[32m+[m[32m            //         <li><a href="#">Now</a></li>[m
[32m+[m[32m            //         <li><a href="#">Midnight</a></li>[m
[32m+[m[32m            //         <li><a href="#">6 a.m.</a></li>[m
[32m+[m[32m            //         <li><a href="#">Noon</a></li>[m
[32m+[m[32m            //         <li><a href="#">6 p.m.</a></li>[m
[32m+[m[32m            //     </ul>[m
[32m+[m[32m            //     <p class="calendar-cancel"><a href="#">Cancel</a></p>[m
[32m+[m[32m            // </div>[m
[32m+[m
[32m+[m[32m            const clock_box = document.createElement('div');[m
[32m+[m[32m            clock_box.style.display = 'none';[m
[32m+[m[32m            clock_box.style.position = 'absolute';[m
[32m+[m[32m            clock_box.className = 'clockbox module';[m
[32m+[m[32m            clock_box.id = DateTimeShortcuts.clockDivName + num;[m
[32m+[m[32m            document.body.appendChild(clock_box);[m
[32m+[m[32m            clock_box.addEventListener('click', function(e) { e.stopPropagation(); });[m
[32m+[m
[32m+[m[32m            quickElement('h2', clock_box, gettext('Choose a time'));[m
[32m+[m[32m            const time_list = quickElement('ul', clock_box);[m
[32m+[m[32m            time_list.className = 'timelist';[m
[32m+[m[32m            // The list of choices can be overridden in JavaScript like this:[m
[32m+[m[32m            // DateTimeShortcuts.clockHours.name = [['3 a.m.', 3]];[m
[32m+[m[32m            // where name is the name attribute of the <input>.[m
[32m+[m[32m            const name = typeof DateTimeShortcuts.clockHours[inp.name] === 'undefined' ? 'default_' : inp.name;[m
[32m+[m[32m            DateTimeShortcuts.clockHours[name].forEach(function(element) {[m
[32m+[m[32m                const time_link = quickElement('a', quickElement('li', time_list), gettext(element[0]), 'href', '#');[m
[32m+[m[32m                time_link.addEventListener('click', function(e) {[m
[32m+[m[32m                    e.preventDefault();[m
[32m+[m[32m                    DateTimeShortcuts.handleClockQuicklink(num, element[1]);[m
[32m+[m[32m                });[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            const cancel_p = quickElement('p', clock_box);[m
[32m+[m[32m            cancel_p.className = 'calendar-cancel';[m
[32m+[m[32m            const cancel_link = quickElement('a', cancel_p, gettext('Cancel'), 'href', '#');[m
[32m+[m[32m            cancel_link.addEventListener('click', function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                DateTimeShortcuts.dismissClock(num);[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            document.addEventListener('keyup', function(event) {[m
[32m+[m[32m                if (event.which === 27) {[m
[32m+[m[32m                    // ESC key closes popup[m
[32m+[m[32m                    DateTimeShortcuts.dismissClock(num);[m
[32m+[m[32m                    event.preventDefault();[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        },[m
[32m+[m[32m        openClock: function(num) {[m
[32m+[m[32m            const clock_box = document.getElementById(DateTimeShortcuts.clockDivName + num);[m
[32m+[m[32m            const clock_link = document.getElementById(DateTimeShortcuts.clockLinkName + num);[m
[32m+[m
[32m+[m[32m            // Recalculate the clockbox position[m
[32m+[m[32m            // is it left-to-right or right-to-left layout ?[m
[32m+[m[32m            if (window.getComputedStyle(document.body).direction !== 'rtl') {[m
[32m+[m[32m                clock_box.style.left = findPosX(clock_link) + 17 + 'px';[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                // since style's width is in em, it'd be tough to calculate[m
[32m+[m[32m                // px value of it. let's use an estimated px for now[m
[32m+[m[32m                clock_box.style.left = findPosX(clock_link) - 110 + 'px';[m
[32m+[m[32m            }[m
[32m+[m[32m            clock_box.style.top = Math.max(0, findPosY(clock_link) - 30) + 'px';[m
[32m+[m
[32m+[m[32m            // Show the clock box[m
[32m+[m[32m            clock_box.style.display = 'block';[m
[32m+[m[32m            document.addEventListener('click', DateTimeShortcuts.dismissClockFunc[num]);[m
[32m+[m[32m        },[m
[32m+[m[32m        dismissClock: function(num) {[m
[32m+[m[32m            document.getElementById(DateTimeShortcuts.clockDivName + num).style.display = 'none';[m
[32m+[m[32m            document.removeEventListener('click', DateTimeShortcuts.dismissClockFunc[num]);[m
[32m+[m[32m        },[m
[32m+[m[32m        handleClockQuicklink: function(num, val) {[m
[32m+[m[32m            let d;[m
[32m+[m[32m            if (val === -1) {[m
[32m+[m[32m                d = DateTimeShortcuts.now();[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                d = new Date(1970, 1, 1, val, 0, 0, 0);[m
[32m+[m[32m            }[m
[32m+[m[32m            DateTimeShortcuts.clockInputs[num].value = d.strftime(get_format('TIME_INPUT_FORMATS')[0]);[m
[32m+[m[32m            DateTimeShortcuts.clockInputs[num].focus();[m
[32m+[m[32m            DateTimeShortcuts.dismissClock(num);[m
[32m+[m[32m        },[m
[32m+[m[32m        // Add calendar widget to a given field.[m
[32m+[m[32m        addCalendar: function(inp) {[m
[32m+[m[32m            const num = DateTimeShortcuts.calendars.length;[m
[32m+[m
[32m+[m[32m            DateTimeShortcuts.calendarInputs[num] = inp;[m
[32m+[m[32m            DateTimeShortcuts.dismissCalendarFunc[num] = function() { DateTimeShortcuts.dismissCalendar(num); return true; };[m
[32m+[m
[32m+[m[32m            // Shortcut links (calendar icon and "Today" link)[m
[32m+[m[32m            const shortcuts_span = document.createElement('span');[m
[32m+[m[32m            shortcuts_span.className = DateTimeShortcuts.shortCutsClass;[m
[32m+[m[32m            inp.parentNode.insertBefore(shortcuts_span, inp.nextSibling);[m
[32m+[m[32m            const today_link = document.createElement('a');[m
[32m+[m[32m            today_link.href = '#';[m
[32m+[m[32m            today_link.appendChild(document.createTextNode(gettext('Today')));[m
[32m+[m[32m            today_link.addEventListener('click', function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                DateTimeShortcuts.handleCalendarQuickLink(num, 0);[m
[32m+[m[32m            });[m
[32m+[m[32m            const cal_link = document.createElement('a');[m
[32m+[m[32m            cal_link.href = '#';[m
[32m+[m[32m            cal_link.id = DateTimeShortcuts.calendarLinkName + num;[m
[32m+[m[32m            cal_link.addEventListener('click', function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                // avoid triggering the document click handler to dismiss the calendar[m
[32m+[m[32m                e.stopPropagation();[m
[32m+[m[32m                DateTimeShortcuts.openCalendar(num);[m
[32m+[m[32m            });[m
[32m+[m[32m            quickElement([m
[32m+[m[32m                'span', cal_link, '',[m
[32m+[m[32m                'class', 'date-icon',[m
[32m+[m[32m                'title', gettext('Choose a Date')[m
[32m+[m[32m            );[m
[32m+[m[32m            shortcuts_span.appendChild(document.createTextNode('\u00A0'));[m
[32m+[m[32m            shortcuts_span.appendChild(today_link);[m
[32m+[m[32m            shortcuts_span.appendChild(document.createTextNode('\u00A0|\u00A0'));[m
[32m+[m[32m            shortcuts_span.appendChild(cal_link);[m
[32m+[m
[32m+[m[32m            // Create calendarbox div.[m
[32m+[m[32m            //[m
[32m+[m[32m            // Markup looks like:[m
[32m+[m[32m            //[m
[32m+[m[32m            // <div id="calendarbox3" class="calendarbox module">[m
[32m+[m[32m            //     <h2>[m
[32m+[m[32m            //           <a href="#" class="link-previous">&lsaquo;</a>[m
[32m+[m[32m            //           <a href="#" class="link-next">&rsaquo;</a> February 2003[m
[32m+[m[32m            //     </h2>[m
[32m+[m[32m            //     <div class="calendar" id="calendarin3">[m
[32m+[m[32m            //         <!-- (cal) -->[m
[32m+[m[32m            //     </div>[m
[32m+[m[32m            //     <div class="calendar-shortcuts">[m
[32m+[m[32m            //          <a href="#">Yesterday</a> | <a href="#">Today</a> | <a href="#">Tomorrow</a>[m
[32m+[m[32m            //     </div>[m
[32m+[m[32m            //     <p class="calendar-cancel"><a href="#">Cancel</a></p>[m
[32m+[m[32m            // </div>[m
[32m+[m[32m            const cal_box = document.createElement('div');[m
[32m+[m[32m            cal_box.style.display = 'none';[m
[32m+[m[32m            cal_box.style.position = 'absolute';[m
[32m+[m[32m            cal_box.className = 'calendarbox module';[m
[32m+[m[32m            cal_box.id = DateTimeShortcuts.calendarDivName1 + num;[m
[32m+[m[32m            document.body.appendChild(cal_box);[m
[32m+[m[32m            cal_box.addEventListener('click', function(e) { e.stopPropagation(); });[m
[32m+[m
[32m+[m[32m            // next-prev links[m
[32m+[m[32m            const cal_nav = quickElement('div', cal_box);[m
[32m+[m[32m            const cal_nav_prev = quickElement('a', cal_nav, '<', 'href', '#');[m
[32m+[m[32m            cal_nav_prev.className = 'calendarnav-previous';[m
[32m+[m[32m            cal_nav_prev.addEventListener('click', function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                DateTimeShortcuts.drawPrev(num);[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            const cal_nav_next = quickElement('a', cal_nav, '>', 'href', '#');[m
[32m+[m[32m            cal_nav_next.className = 'calendarnav-next';[m
[32m+[m[32m            cal_nav_next.addEventListener('click', function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                DateTimeShortcuts.drawNext(num);[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            // main box[m
[32m+[m[32m            const cal_main = quickElement('div', cal_box, '', 'id', DateTimeShortcuts.calendarDivName2 + num);[m
[32m+[m[32m            cal_main.className = 'calendar';[m
[32m+[m[32m            DateTimeShortcuts.calendars[num] = new Calendar(DateTimeShortcuts.calendarDivName2 + num, DateTimeShortcuts.handleCalendarCallback(num));[m
[32m+[m[32m            DateTimeShortcuts.calendars[num].drawCurrent();[m
[32m+[m
[32m+[m[32m            // calendar shortcuts[m
[32m+[m[32m            const shortcuts = quickElement('div', cal_box);[m
[32m+[m[32m            shortcuts.className = 'calendar-shortcuts';[m
[32m+[m[32m            let day_link = quickElement('a', shortcuts, gettext('Yesterday'), 'href', '#');[m
[32m+[m[32m            day_link.addEventListener('click', function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                DateTimeShortcuts.handleCalendarQuickLink(num, -1);[m
[32m+[m[32m            });[m
[32m+[m[32m            shortcuts.appendChild(document.createTextNode('\u00A0|\u00A0'));[m
[32m+[m[32m            day_link = quickElement('a', shortcuts, gettext('Today'), 'href', '#');[m
[32m+[m[32m            day_link.addEventListener('click', function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                DateTimeShortcuts.handleCalendarQuickLink(num, 0);[m
[32m+[m[32m            });[m
[32m+[m[32m            shortcuts.appendChild(document.createTextNode('\u00A0|\u00A0'));[m
[32m+[m[32m            day_link = quickElement('a', shortcuts, gettext('Tomorrow'), 'href', '#');[m
[32m+[m[32m            day_link.addEventListener('click', function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                DateTimeShortcuts.handleCalendarQuickLink(num, +1);[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            // cancel bar[m
[32m+[m[32m            const cancel_p = quickElement('p', cal_box);[m
[32m+[m[32m            cancel_p.className = 'calendar-cancel';[m
[32m+[m[32m            const cancel_link = quickElement('a', cancel_p, gettext('Cancel'), 'href', '#');[m
[32m+[m[32m            cancel_link.addEventListener('click', function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m                DateTimeShortcuts.dismissCalendar(num);[m
[32m+[m[32m            });[m
[32m+[m[32m            document.addEventListener('keyup', function(event) {[m
[32m+[m[32m                if (event.which === 27) {[m
[32m+[m[32m                    // ESC key closes popup[m
[32m+[m[32m                    DateTimeShortcuts.dismissCalendar(num);[m
[32m+[m[32m                    event.preventDefault();[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        },[m
[32m+[m[32m        openCalendar: function(num) {[m
[32m+[m[32m            const cal_box = document.getElementById(DateTimeShortcuts.calendarDivName1 + num);[m
[32m+[m[32m            const cal_link = document.getElementById(DateTimeShortcuts.calendarLinkName + num);[m
[32m+[m[32m            const inp = DateTimeShortcuts.calendarInputs[num];[m
[32m+[m
[32m+[m[32m            // Determine if the current value in the input has a valid date.[m
[32m+[m[32m            // If so, draw the calendar with that date's year and month.[m
[32m+[m[32m            if (inp.value) {[m
[32m+[m[32m                const format = get_format('DATE_INPUT_FORMATS')[0];[m
[32m+[m[32m                const selected = inp.value.strptime(format);[m
[32m+[m[32m                const year = selected.getUTCFullYear();[m
[32m+[m[32m                const month = selected.getUTCMonth() + 1;[m
[32m+[m[32m                const re = /\d{4}/;[m
[32m+[m[32m                if (re.test(year.toString()) && month >= 1 && month <= 12) {[m
[32m+[m[32m                    DateTimeShortcuts.calendars[num].drawDate(month, year, selected);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // Recalculate the clockbox position[m
[32m+[m[32m            // is it left-to-right or right-to-left layout ?[m
[32m+[m[32m            if (window.getComputedStyle(document.body).direction !== 'rtl') {[m
[32m+[m[32m                cal_box.style.left = findPosX(cal_link) + 17 + 'px';[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                // since style's width is in em, it'd be tough to calculate[m
[32m+[m[32m                // px value of it. let's use an estimated px for now[m
[32m+[m[32m                cal_box.style.left = findPosX(cal_link) - 180 + 'px';[m
[32m+[m[32m            }[m
[32m+[m[32m            cal_box.style.top = Math.max(0, findPosY(cal_link) - 75) + 'px';[m
[32m+[m
[32m+[m[32m            cal_box.style.display = 'block';[m
[32m+[m[32m            document.addEventListener('click', DateTimeShortcuts.dismissCalendarFunc[num]);[m
[32m+[m[32m        },[m
[32m+[m[32m        dismissCalendar: function(num) {[m
[32m+[m[32m            document.getElementById(DateTimeShortcuts.calendarDivName1 + num).style.display = 'none';[m
[32m+[m[32m            document.removeEventListener('click', DateTimeShortcuts.dismissCalendarFunc[num]);[m
[32m+[m[32m        },[m
[32m+[m[32m        drawPrev: function(num) {[m
[32m+[m[32m            DateTimeShortcuts.calendars[num].drawPreviousMonth();[m
[32m+[m[32m        },[m
[32m+[m[32m        drawNext: function(num) {[m
[32m+[m[32m            DateTimeShortcuts.calendars[num].drawNextMonth();[m
[32m+[m[32m        },[m
[32m+[m[32m        handleCalendarCallback: function(num) {[m
[32m+[m[32m            const format = get_format('DATE_INPUT_FORMATS')[0];[m
[32m+[m[32m            return function(y, m, d) {[m
[32m+[m[32m                DateTimeShortcuts.calendarInputs[num].value = new Date(y, m - 1, d).strftime(format);[m
[32m+[m[32m                DateTimeShortcuts.calendarInputs[num].focus();[m
[32m+[m[32m                document.getElementById(DateTimeShortcuts.calendarDivName1 + num).style.display = 'none';[m
[32m+[m[32m            };[m
[32m+[m[32m        },[m
[32m+[m[32m        handleCalendarQuickLink: function(num, offset) {[m
[32m+[m[32m            const d = DateTimeShortcuts.now();[m
[32m+[m[32m            d.setDate(d.getDate() + offset);[m
[32m+[m[32m            DateTimeShortcuts.calendarInputs[num].value = d.strftime(get_format('DATE_INPUT_FORMATS')[0]);[m
[32m+[m[32m            DateTimeShortcuts.calendarInputs[num].focus();[m
[32m+[m[32m            DateTimeShortcuts.dismissCalendar(num);[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    window.addEventListener('load', DateTimeShortcuts.init);[m
[32m+[m[32m    window.DateTimeShortcuts = DateTimeShortcuts;[m
[32m+[m[32m}[m
