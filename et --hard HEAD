[32m+[m[32mfrom django import forms[m
[32m+[m
[32m+[m[32mfrom .models import Comment, Article, ArticleImage[m
[32m+[m[32mfrom django.forms import inlineformset_factory[m
[32m+[m
[32m+[m[32mclass CommentForm(forms.ModelForm):[m
[32m+[m[32m    class Meta:[m
[32m+[m[32m        model = Comment[m
[32m+[m[32m        fields = ('comment',)[m
[32m+[m[41m        [m
[32m+[m[32m# formulario de que permite actualizar los articulos[m
[32m+[m[32mclass ArticleForm(forms.ModelForm):[m
[32m+[m[32m    class Meta:[m
[32m+[m[32m        # indeca que el formulario se base en el modelo(tabla) article[m
[32m+[m[32m        model = Article[m
[32m+[m[32m        # indica los campos que se incluiran en el formulario[m
[32m+[m[32m        fields = ['title', 'body',][m
[32m+[m
[32m+[m[32m# permite crear varios formularios que es util para manejar modelos relacionados[m
[32m+[m[32mArticleImageFormSet = inlineformset_factory([m
[32m+[m[32m    # modelo al que esta asociado las imagenes[m
[32m+[m[32m    Article,[m
[32m+[m[32m    # modelo que contiene la relacion[m
[32m+[m[32m    ArticleImage,[m
[32m+[m[32m    # campo del formulario[m
[32m+[m[32m    fields=['image'],[m
[32m+[m[32m    # Indica que se mostrar√° un formulario vac√≠o adicional para a√±adir una nueva imagen.[m
[32m+[m[32m    extra=1,[m
[32m+[m[32m    #opcion para eliminar una imagen de un articulo[m
[32m+[m[32m    can_delete=True[m
[32m+[m[32m)[m
