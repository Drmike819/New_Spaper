[32m+[m[32m/*! Select2 4.0.13 | https://github.com/select2/select2/blob/master/LICENSE.md */[m
[32m+[m
[32m+[m[32m!function(){if(jQuery&&jQuery.fn&&jQuery.fn.select2&&jQuery.fn.select2.amd)var n=jQuery.fn.select2.amd;n.define("select2/i18n/zh-TW",[],function(){return{inputTooLong:function(n){return"Ë´ãÂà™Êéâ"+(n.input.length-n.maximum)+"ÂÄãÂ≠óÂÖÉ"},inputTooShort:function(n){return"Ë´ãÂÜçËº∏ÂÖ•"+(n.minimum-n.input.length)+"ÂÄãÂ≠óÂÖÉ"},loadingMore:function(){return"ËºâÂÖ•‰∏≠‚Ä¶"},maximumSelected:function(n){return"‰Ω†Âè™ËÉΩÈÅ∏ÊìáÊúÄÂ§ö"+n.maximum+"È†Ö"},noResults:function(){return"Ê≤íÊúâÊâæÂà∞Áõ∏Á¨¶ÁöÑÈ†ÖÁõÆ"},searching:function(){return"ÊêúÂ∞ã‰∏≠‚Ä¶"},removeAllItems:function(){return"Âà™Èô§ÊâÄÊúâÈ†ÖÁõÆ"}}}),n.define,n.require}();[m
\ No newline at end of file[m
[1mdiff --git a/staticfiles/admin/js/vendor/select2/select2.full.js b/staticfiles/admin/js/vendor/select2/select2.full.js[m
[1mnew file mode 100644[m
[1mindex 0000000..358572a[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/vendor/select2/select2.full.js[m
[36m@@ -0,0 +1,6820 @@[m
[32m+[m[32m/*![m
[32m+[m[32m * Select2 4.0.13[m
[32m+[m[32m * https://select2.github.io[m
[32m+[m[32m *[m
[32m+[m[32m * Released under the MIT license[m
[32m+[m[32m * https://github.com/select2/select2/blob/master/LICENSE.md[m
[32m+[m[32m */[m
[32m+[m[32m;(function (factory) {[m
[32m+[m[32m  if (typeof define === 'function' && define.amd) {[m
[32m+[m[32m    // AMD. Register as an anonymous module.[m
[32m+[m[32m    define(['jquery'], factory);[m
[32m+[m[32m  } else if (typeof module === 'object' && module.exports) {[m
[32m+[m[32m    // Node/CommonJS[m
[32m+[m[32m    module.exports = function (root, jQuery) {[m
[32m+[m[32m      if (jQuery === undefined) {[m
[32m+[m[32m        // require('jQuery') returns a factory that requires window to[m
[32m+[m[32m        // build a jQuery instance, we normalize how we use modules[m
[32m+[m[32m        // that require this pattern but the window provided is a noop[m
[32m+[m[32m        // if it's defined (how jquery works)[m
[32m+[m[32m        if (typeof window !== 'undefined') {[m
[32m+[m[32m          jQuery = require('jquery');[m
[32m+[m[32m        }[m
[32m+[m[32m        else {[m
[32m+[m[32m          jQuery = require('jquery')(root);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      factory(jQuery);[m
[32m+[m[32m      return jQuery;[m
[32m+[m[32m    };[m
[32m+[m[32m  } else {[m
[32m+[m[32m    // Browser globals[m
[32m+[m[32m    factory(jQuery);[m
[32m+[m[32m  }[m
[32m+[m[32m} (function (jQuery) {[m
[32m+[m[32m  // This is needed so we can catch the AMD loader configuration and use it[m
[32m+[m[32m  // The inner file should be wrapped (by `banner.start.js`) in a function that[m
[32m+[m[32m  // returns the AMD loader references.[m
[32m+[m[32m  var S2 =(function () {[m
[32m+[m[32m  // Restore the Select2 AMD loader so it can be used[m
[32m+[m[32m  // Needed mostly in the language files, where the loader is not inserted[m
[32m+[m[32m  if (jQuery && jQuery.fn && jQuery.fn.select2 && jQuery.fn.select2.amd) {[m
[32m+[m[32m    var S2 = jQuery.fn.select2.amd;[m
[32m+[m[32m  }[m
[32m+[m[32mvar S2;(function () { if (!S2 || !S2.requirejs) {[m
[32m+[m[32mif (!S2) { S2 = {}; } else { require = S2; }[m
[32m+[m[32m/**[m
[32m+[m[32m * @license almond 0.3.3 Copyright jQuery Foundation and other contributors.[m
[32m+[m[32m * Released under MIT license, http://github.com/requirejs/almond/LICENSE[m
[32m+[m[32m */[m
[32m+[m[32m//Going sloppy to avoid 'use strict' string cost, but strict practices should[m
[32m+[m[32m//be followed.[m
[32m+[m[32m/*global setTimeout: false */[m
[32m+[m
[32m+[m[32mvar requirejs, require, define;[m
[32m+[m[32m(function (undef) {[m
[32m+[m[32m    var main, req, makeMap, handlers,[m
[32m+[m[32m        defined = {},[m
[32m+[m[32m        waiting = {},[m
[32m+[m[32m        config = {},[m
[32m+[m[32m        defining = {},[m
[32m+[m[32m        hasOwn = Object.prototype.hasOwnProperty,[m
[32m+[m[32m        aps = [].slice,[m
[32m+[m[32m        jsSuffixRegExp = /\.js$/;[m
[32m+[m
[32m+[m[32m    function hasProp(obj, prop) {[m
[32m+[m[32m        return hasOwn.call(obj, prop);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Given a relative module name, like ./something, normalize it to[m
[32m+[m[32m     * a real name that can be mapped to a path.[m
[32m+[m[32m     * @param {String} name the relative name[m
[32m+[m[32m     * @param {String} baseName a real name that the name arg is relative[m
[32m+[m[32m     * to.[m
[32m+[m[32m     * @returns {String} normalized name[m
[32m+[m[32m     */[m
[32m+[m[32m    function normalize(name, baseName) {[m
[32m+[m[32m        var nameParts, nameSegment, mapValue, foundMap, lastIndex,[m
[32m+[m[32m            foundI, foundStarMap, starI, i, j, part, normalizedBaseParts,[m
[32m+[m[32m            baseParts = baseName && baseName.split("/"),[m
[32m+[m[32m            map = config.map,[m
[32m+[m[32m            starMap = (map && map['*']) || {};[m
[32m+[m
[32m+[m[32m        //Adjust any relative paths.[m
[32m+[m[32m        if (name) {[m
[32m+[m[32m            name = name.split('/');[m
[32m+[m[32m            lastIndex = name.length - 1;[m
[32m+[m
[32m+[m[32m            // If wanting node ID compatibility, strip .js from end[m
[32m+[m[32m            // of IDs. Have to do this here, and not in nameToUrl[m
[32m+[m[32m            // because node allows either .js or non .js to map[m
[32m+[m[32m            // to same file.[m
[32m+[m[32m            if (config.nodeIdCompat && jsSuffixRegExp.test(name[lastIndex])) {[m
[32m+[m[32m                name[lastIndex] = name[lastIndex].replace(jsSuffixRegExp, '');[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // Starts with a '.' so need the baseName[m
[32m+[m[32m            if (name[0].charAt(0) === '.' && baseParts) {[m
[32m+[m[32m                //Convert baseName to array, and lop off the last part,[m
[32m+[m[32m                //so that . matches that 'directory' and not name of the baseName's[m
[32m+[m[32m                //module. For instance, baseName of 'one/two/three', maps to[m
[32m+[m[32m                //'one/two/three.js', but we want the directory, 'one/two' for[m
[32m+[m[32m                //this normalization.[m
[32m+[m[32m                normalizedBaseParts = baseParts.slice(0, baseParts.length - 1);[m
[32m+[m[32m                name = normalizedBaseParts.concat(name);[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            //start trimDots[m
[32m+[m[32m            for (i = 0; i < name.length; i++) {[m
[32m+[m[32m                part = name[i];[m
[32m+[m[32m                if (part === '.') {[m
[32m+[m[32m                    name.splice(i, 1);[m
[32m+[m[32m                    i -= 1;[m
[32m+[m[32m                } else if (part === '..') {[m
[32m+[m[32m                    // If at the start, or previous value is still ..,[m
[32m+[m[32m                    // keep them so that when converted to a path it may[m
[32m+[m[32m                    // still work when converted to a path, even though[m
[32m+[m[32m                    // as an ID it is less than ideal. In larger point[m
[32m+[m[32m                    // releases, may be better to just kick out an error.[m
[32m+[m[32m                    if (i === 0 || (i === 1 && name[2] === '..') || name[i - 1] === '..') {[m
[32m+[m[32m                        continue;[m
[32m+[m[32m                    } else if (i > 0) {[m
[32m+[m[32m                        name.splice(i - 1, 2);[m
[32m+[m[32m                        i -= 2;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            //end trimDots[m
[32m+[m
[32m+[m[32m            name = name.join('/');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        //Apply map config if available.[m
[32m+[m[32m        if ((baseParts || starMap) && map) {[m
[32m+[m[32m            nameParts = name.split('/');[m
[32m+[m
[32m+[m[32m            for (i = nameParts.length; i > 0; i -= 1) {[m
[32m+[m[32m                nameSegment = nameParts.slice(0, i).join("/");[m
[32m+[m
[32m+[m[32m                if (baseParts) {[m
[32m+[m[32m                    //Find the longest baseName segment match in the config.[m
[32m+[m[32m                    //So, do joins on the biggest to smallest lengths of baseParts.[m
[32m+[m[32m                    for (j = baseParts.length; j > 0; j -= 1) {[m
[32m+[m[32m                        mapValue = map[baseParts.slice(0, j).join('/')];[m
[32m+[m
[32m+[m[32m                        //baseName segment has  config, find if it has one for[m
[32m+[m[32m                        //this name.[m
[32m+[m[32m                        if (mapValue) {[m
[32m+[m[32m                            mapValue = mapValue[nameSegment];[m
[32m+[m[32m                            if (mapValue) {[m
[32m+[m[32m                                //Match, update name to the new value.[m
[32m+[m[32m                                foundMap = mapValue;[m
[32m+[m[32m                                foundI = i;[m
[32m+[m[32m                                break;[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                if (foundMap) {[m
[32m+[m[32m                    break;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                //Check for a star map match, but just hold on to it,[m
[32m+[m[32m                //if there is a shorter segment match later in a matching[m
[32m+[m[32m                //config, then favor over this star map.[m
[32m+[m[32m                if (!foundStarMap && starMap && starMap[nameSegment]) {[m
[32m+[m[32m                    foundStarMap = starMap[nameSegment];[m
[32m+[m[32m                    starI = i;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (!foundMap && foundStarMap) {[m
[32m+[m[32m                foundMap = foundStarMap;[m
[32m+[m[32m                foundI = starI;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (foundMap) {[m
[32m+[m[32m                nameParts.splice(0, foundI, foundMap);[m
[32m+[m[32m                name = nameParts.join('/');[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return name;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function makeRequire(relName, forceSync) {[m
[32m+[m[32m        return function () {[m
[32m+[m[32m            //A version of a require function that passes a moduleName[m
[32m+[m[32m            //value for items that may need to[m
[32m+[m[32m            //look up paths relative to the moduleName[m
[32m+[m[32m            var args = aps.call(arguments, 0);[m
[32m+[m
[32m+[m[32m            //If first arg is not require('string'), and there is only[m
[32m+[m[32m            //one arg, it is the array form without a callback. Insert[m
[32m+[m[32m            //a null so that the following concat is correct.[m
[32m+[m[32m            if (typeof args[0] !== 'string' && args.length === 1) {[m
[32m+[m[32m                args.push(null);[m
[32m+[m[32m            }[m
[32m+[m[32m            return req.apply(undef, args.concat([relName, forceSync]));[m
[32m+[m[32m        };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function makeNormalize(relName) {[m
[32m+[m[32m        return function (name) {[m
[32m+[m[32m            return normalize(name, relName);[m
[32m+[m[32m        };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function makeLoad(depName) {[m
[32m+[m[32m        return function (value) {[m
[32m+[m[32m            defined[depName] = value;[m
[32m+[m[32m        };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function callDep(name) {[m
[32m+[m[32m        if (hasProp(waiting, name)) {[m
[32m+[m[32m            var args = waiting[name];[m
[32m+[m[32m            delete waiting[name];[m
[32m+[m[32m            defining[name] = true;[m
[32m+[m[32m            main.apply(undef, args);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!hasProp(defined, name) && !hasProp(defining, name)) {[m
[32m+[m[32m            throw new Error('No ' + name);[m
[32m+[m[32m        }[m
[32m+[m[32m        return defined[name];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    //Turns a plugin!resource to [plugin, resource][m
[32m+[m[32m    //with the plugin being undefined if the name[m
[32m+[m[32m    //did not have a plugin prefix.[m
[32m+[m[32m    function splitPrefix(name) {[m
[32m+[m[32m        var prefix,[m
[32m+[m[32m            index = name ? name.indexOf('!') : -1;[m
[32m+[m[32m        if (index > -1) {[m
[32m+[m[32m            prefix = name.substring(0, index);[m
[32m+[m[32m            name = name.substring(index + 1, name.length);[m
[32m+[m[32m        }[m
[32m+[m[32m        return [prefix, name];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    //Creates a parts array for a relName where first part is plugin ID,[m
[32m+[m[32m    //second part is resource ID. Assumes relName has already been normalized.[m
[32m+[m[32m    function makeRelParts(relName) {[m
[32m+[m[32m        return relName ? splitPrefix(relName) : [];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Makes a name map, normalizing the name, and using a plugin[m
[32m+[m[32m     * for normalization if necessary. Grabs a ref to plugin[m
[32m+[m[32m     * too, as an optimization.[m
[32m+[m[32m     */[m
[32m+[m[32m    makeMap = function (name, relParts) {[m
[32m+[m[32m        var plugin,[m
[32m+[m[32m            parts = splitPrefix(name),[m
[32m+[m[32m            prefix = parts[0],[m
[32m+[m[32m            relResourceName = relParts[1];[m
[32m+[m
[32m+[m[32m        name = parts[1];[m
[32m+[m
[32m+[m[32m        if (prefix) {[m
[32m+[m[32m            prefix = normalize(prefix, relResourceName);[m
[32m+[m[32m            plugin = callDep(prefix);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        //Normalize according[m
[32m+[m[32m        if (prefix) {[m
[32m+[m[32m            if (plugin && plugin.normalize) {[m
[32m+[m[32m                name = plugin.normalize(name, makeNormalize(relResourceName));[m
[32m+[m[32m            } else {[m
[32m+[m[32m                name = normalize(name, relResourceName);[m
[32m+[m[32m            }[m
[32m+[m[32m        } else {[m
[32m+[m[32m            name = normalize(name, relResourceName);[m
[32m+[m[32m            parts = splitPrefix(name);[m
[32m+[m[32m            prefix = parts[0];[m
[32m+[m[32m            name = parts[1];[m
[32m+[m[32m            if (prefix) {[m
[32m+[m[32m                plugin = callDep(prefix);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        //Using ridiculous property names for space reasons[m
[32m+[m[32m        return {[m
[32m+[m[32m            f: prefix ? prefix + '!' + name : name, //fullName[m
[32m+[m[32m            n: name,[m
[32m+[m[32m            pr: prefix,[m
[32m+[m[32m            p: plugin[m
[32m+[m[32m        };[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    function makeConfig(name) {[m
[32m+[m[32m        return function () {[m
[32m+[m[32m            return (config && config.config && config.config[name]) || {};[m
[32m+[m[32m        };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    handlers = {[m
[32m+[m[32m        require: function (name) {[m
[32m+[m[32m            return makeRequire(name);[m
[32m+[m[32m        },[m
[32m+[m[32m        exports: function (name) {[m
[32m+[m[32m            var e = defined[name];[m
[32m+[m[32m            if (typeof e !== 'undefined') {[m
[32m+[m[32m                return e;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                return (defined[name] = {});[m
[32m+[m[32m            }[m
[32m+[m[32m        },[m
[32m+[m[32m        module: function (name) {[m
[32m+[m[32m            return {[m
[32m+[m[32m                id: name,[m
[32m+[m[32m                uri: '',[m
[32m+[m[32m                exports: defined[name],[m
[32m+[m[32m                config: makeConfig(name)[m
[32m+[m[32m            };[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    main = function (name, deps, callback, relName) {[m
[32m+[m[32m        var cjsModule, depName, ret, map, i, relParts,[m
[32m+[m[32m            args = [],[m
[32m+[m[32m            callbackType = typeof callback,[m
[32m+[m[32m            usingExports;[m
[32m+[m
[32m+[m[32m        //Use name if no relName[m
[32m+[m[32m        relName = relName || name;[m
[32m+[m[32m        relParts = makeRelParts(relName);[m
[32m+[m
[32m+[m[32m        //Call the callback to define the module, if necessary.[m
[32m+[m[32m        if (callbackType === 'undefined' || callbackType === 'function') {[m
[32m+[m[32m            //Pull out the defined dependencies and pass the ordered[m
[32m+[m[32m            //values to the callback.[m
[32m+[m[32m            //Default to [require, exports, module] if no deps[m
[32m+[m[32m            deps = !deps.length && callback.length ? ['require', 'exports', 'module'] : deps;[m
[32m+[m[32m            for (i = 0; i < deps.length; i += 1) {[m
[32m+[m[32m                map = makeMap(deps[i], relParts);[m
[32m+[m[32m                depName = map.f;[m
[32m+[m
[32m+[m[32m                //Fast path CommonJS standard dependencies.[m
[32m+[m[32m                if (depName === "require") {[m
[32m+[m[32m                    args[i] = handlers.require(name);[m
[32m+[m[32m                } else if (depName === "exports") {[m
[32m+[m[32m                    //CommonJS module spec 1.1[m
[32m+[m[32m                    args[i] = handlers.exports(name);[m
[32m+[m[32m                    usingExports = true;[m
[32m+[m[32m                } else if (depName === "module") {[m
[32m+[m[32m                    //CommonJS module spec 1.1[m
[32m+[m[32m                    cjsModule = args[i] = handlers.module(name);[m
[32m+[m[32m                } else if (hasProp(defined, depName) ||[m
[32m+[m[32m                           hasProp(waiting, depName) ||[m
[32m+[m[32m                           hasProp(defining, depName)) {[m
[32m+[m[32m                    args[i] = callDep(depName);[m
[32m+[m[32m                } else if (map.p) {[m
[32m+[m[32m                    map.p.load(map.n, makeRequire(relName, true), makeLoad(depName), {});[m
[32m+[m[32m                    args[i] = defined[depName];[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    throw new Error(name + ' missing ' + depName);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            ret = callback ? callback.apply(defined[name], args) : undefined;[m
[32m+[m
[32m+[m[32m            if (name) {[m
[32m+[m[32m                //If setting exports via "module" is in play,[m
[32m+[m[32m                //favor that over return value and exports. After that,[m
[32m+[m[32m                //favor a non-undefined return value over exports use.[m
[32m+[m[32m                if (cjsModule && cjsModule.exports !== undef &&[m
[32m+[m[32m                        cjsModule.exports !== defined[name]) {[m
[32m+[m[32m                    defined[name] = cjsModule.exports;[m
[32m+[m[32m                } else if (ret !== undef || !usingExports) {[m
[32m+[m[32m                    //Use the return value from the function.[m
[32m+[m[32m                    defined[name] = ret;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        } else if (name) {[m
[32m+[m[32m            //May just be an object definition for the module. Only[m
[32m+[m[32m            //worry about defining if have a module name.[m
[32m+[m[32m            defined[name] = callback;[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    requirejs = require = req = function (deps, callback, relName, forceSync, alt) {[m
[32m+[m[32m        if (typeof deps === "string") {[m
[32m+[m[32m            if (handlers[deps]) {[m
[32m+[m[32m                //callback in this case is really relName[m
[32m+[m[32m                return handlers[deps](callback);[m
[32m+[m[32m            }[m
[32m+[m[32m            //Just return the module wanted. In this scenario, the[m
[32m+[m[32m            //deps arg is the module name, and second arg (if passed)[m
[32m+[m[32m            //is just the relName.[m
[32m+[m[32m            //Normalize module name, if it contains . or ..[m
[32m+[m[32m            return callDep(makeMap(deps, makeRelParts(callback)).f);[m
[32m+[m[32m        } else if (!deps.splice) {[m
[32m+[m[32m            //deps is a config object, not an array.[m
[32m+[m[32m            config = deps;[m
[32m+[m[32m            if (config.deps) {[m
[32m+[m[32m                req(config.deps, config.callback);[m
[32m+[m[32m            }[m
[32m+[m[32m            if (!callback) {[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (callback.splice) {[m
[32m+[m[32m                //callback is an array, which means it is a dependency list.[m
[32m+[m[32m                //Adjust args if there are dependencies[m
[32m+[m[32m                deps = callback;[m
[32m+[m[32m                callback = relName;[m
[32m+[m[32m                relName = null;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                deps = undef;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        //Support require(['a'])[m
[32m+[m[32m        callback = callback || function () {};[m
[32m+[m
[32m+[m[32m        //If relName is a function, it is an errback handler,[m
[32m+[m[32m        //so remove it.[m
[32m+[m[32m        if (typeof relName === 'function') {[m
[32m+[m[32m            relName = forceSync;[m
[32m+[m[32m            forceSync = alt;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        //Simulate async callback;[m
[32m+[m[32m        if (forceSync) {[m
[32m+[m[32m            main(undef, deps, callback, relName);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            //Using a non-zero value because of concern for what old browsers[m
[32m+[m[32m            //do, and latest browsers "upgrade" to 4 if lower value is used:[m
[32m+[m[32m            //http://www.whatwg.org/specs/web-apps/current-work/multipage/timers.html#dom-windowtimers-settimeout:[m
[32m+[m[32m            //If want a value immediately, use require('id') instead -- something[m
[32m+[m[32m            //that works in almond on the global level, but not guaranteed and[m
[32m+[m[32m            //unlikely to work in other AMD implementations.[m
[32m+[m[32m            setTimeout(function () {[m
[32m+[m[32m                main(undef, deps, callback, relName);[m
[32m+[m[32m            }, 4);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return req;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Just drops the config on the floor, but returns req in case[m
[32m+[m[32m     * the config return value is used.[m
[32m+[m[32m     */[m
[32m+[m[32m    req.config = function (cfg) {[m
[32m+[m[32m        return req(cfg);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Expose module registry for debugging and tooling[m
[32m+[m[32m     */[m
[32m+[m[32m    requirejs._defined = defined;[m
[32m+[m
[32m+[m[32m    define = function (name, deps, callback) {[m
[32m+[m[32m        if (typeof name !== 'string') {[m
[32m+[m[32m            throw new Error('See almond README: incorrect module build, no module name');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        //This module may not have dependencies[m
[32m+[m[32m        if (!deps.splice) {[m
[32m+[m[32m            //deps is not an array, so probably means[m
[32m+[m[32m            //an object literal or factory function for[m
[32m+[m[32m            //the value. Adjust args.[m
[32m+[m[32m            callback = deps;[m
[32m+[m[32m            deps = [];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!hasProp(defined, name) && !hasProp(waiting, name)) {[m
[32m+[m[32m            waiting[name] = [name, deps, callback];[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    define.amd = {[m
[32m+[m[32m        jQuery: true[m
[32m+[m[32m    };[m
[32m+[m[32m}());[m
[32m+[m
[32m+[m[32mS2.requirejs = requirejs;S2.require = require;S2.define = define;[m
[32m+[m[32m}[m
[32m+[m[32m}());[m
[32m+[m[32mS2.define("almond", function(){});[m
[32m+[m
[32m+[m[32m/* global jQuery:false, $:false */[m
[32m+[m[32mS2.define('jquery',[],function () {[m
[32m+[m[32m  var _$ = jQuery || $;[m
[32m+[m
[32m+[m[32m  if (_$ == null && console && console.error) {[m
[32m+[m[32m    console.error([m
[32m+[m[32m      'Select2: An instance of jQuery or a jQuery-compatible library was not ' +[m
[32m+[m[32m      'found. Make sure that you are including jQuery before Select2 on your ' +[m
[32m+[m[32m      'web page.'[m
[32m+[m[32m    );[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return _$;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/utils',[[m
[32m+[m[32m  'jquery'[m
[32m+[m[32m], function ($) {[m
[32m+[m[32m  var Utils = {};[m
[32m+[m
[32m+[m[32m  Utils.Extend = function (ChildClass, SuperClass) {[m
[32m+[m[32m    var __hasProp = {}.hasOwnProperty;[m
[32m+[m
[32m+[m[32m    function BaseConstructor () {[m
[32m+[m[32m      this.constructor = ChildClass;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    for (var key in SuperClass) {[m
[32m+[m[32m      if (__hasProp.call(SuperClass, key)) {[m
[32m+[m[32m        ChildClass[key] = SuperClass[key];[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    BaseConstructor.prototype = SuperClass.prototype;[m
[32m+[m[32m    ChildClass.prototype = new BaseConstructor();[m
[32m+[m[32m    ChildClass.__super__ = SuperClass.prototype;[m
[32m+[m
[32m+[m[32m    return ChildClass;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  function getMethods (theClass) {[m
[32m+[m[32m    var proto = theClass.prototype;[m
[32m+[m
[32m+[m[32m    var methods = [];[m
[32m+[m
[32m+[m[32m    for (var methodName in proto) {[m
[32m+[m[32m      var m = proto[methodName];[m
[32m+[m
[32m+[m[32m      if (typeof m !== 'function') {[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (methodName === 'constructor') {[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      methods.push(methodName);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return methods;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Utils.Decorate = function (SuperClass, DecoratorClass) {[m
[32m+[m[32m    var decoratedMethods = getMethods(DecoratorClass);[m
[32m+[m[32m    var superMethods = getMethods(SuperClass);[m
[32m+[m
[32m+[m[32m    function DecoratedClass () {[m
[32m+[m[32m      var unshift = Array.prototype.unshift;[m
[32m+[m
[32m+[m[32m      var argCount = DecoratorClass.prototype.constructor.length;[m
[32m+[m
[32m+[m[32m      var calledConstructor = SuperClass.prototype.constructor;[m
[32m+[m
[32m+[m[32m      if (argCount > 0) {[m
[32m+[m[32m        unshift.call(arguments, SuperClass.prototype.constructor);[m
[32m+[m
[32m+[m[32m        calledConstructor = DecoratorClass.prototype.constructor;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      calledConstructor.apply(this, arguments);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    DecoratorClass.displayName = SuperClass.displayName;[m
[32m+[m
[32m+[m[32m    function ctr () {[m
[32m+[m[32m      this.constructor = DecoratedClass;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    DecoratedClass.prototype = new ctr();[m
[32m+[m
[32m+[m[32m    for (var m = 0; m < superMethods.length; m++) {[m
[32m+[m[32m      var superMethod = superMethods[m];[m
[32m+[m
[32m+[m[32m      DecoratedClass.prototype[superMethod] =[m
[32m+[m[32m        SuperClass.prototype[superMethod];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var calledMethod = function (methodName) {[m
[32m+[m[32m      // Stub out the original method if it's not decorating an actual method[m
[32m+[m[32m      var originalMethod = function () {};[m
[32m+[m
[32m+[m[32m      if (methodName in DecoratedClass.prototype) {[m
[32m+[m[32m        originalMethod = DecoratedClass.prototype[methodName];[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var decoratedMethod = DecoratorClass.prototype[methodName];[m
[32m+[m
[32m+[m[32m      return function () {[m
[32m+[m[32m        var unshift = Array.prototype.unshift;[m
[32m+[m
[32m+[m[32m        unshift.call(arguments, originalMethod);[m
[32m+[m
[32m+[m[32m        return decoratedMethod.apply(this, arguments);[m
[32m+[m[32m      };[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    for (var d = 0; d < decoratedMethods.length; d++) {[m
[32m+[m[32m      var decoratedMethod = decoratedMethods[d];[m
[32m+[m
[32m+[m[32m      DecoratedClass.prototype[decoratedMethod] = calledMethod(decoratedMethod);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return DecoratedClass;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  var Observable = function () {[m
[32m+[m[32m    this.listeners = {};[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Observable.prototype.on = function (event, callback) {[m
[32m+[m[32m    this.listeners = this.listeners || {};[m
[32m+[m
[32m+[m[32m    if (event in this.listeners) {[m
[32m+[m[32m      this.listeners[event].push(callback);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      this.listeners[event] = [callback];[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Observable.prototype.trigger = function (event) {[m
[32m+[m[32m    var slice = Array.prototype.slice;[m
[32m+[m[32m    var params = slice.call(arguments, 1);[m
[32m+[m
[32m+[m[32m    this.listeners = this.listeners || {};[m
[32m+[m
[32m+[m[32m    // Params should always come in as an array[m
[32m+[m[32m    if (params == null) {[m
[32m+[m[32m      params = [];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // If there are no arguments to the event, use a temporary object[m
[32m+[m[32m    if (params.length === 0) {[m
[32m+[m[32m      params.push({});[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Set the `_type` of the first object to the event[m
[32m+[m[32m    params[0]._type = event;[m
[32m+[m
[32m+[m[32m    if (event in this.listeners) {[m
[32m+[m[32m      this.invoke(this.listeners[event], slice.call(arguments, 1));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if ('*' in this.listeners) {[m
[32m+[m[32m      this.invoke(this.listeners['*'], arguments);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Observable.prototype.invoke = function (listeners, params) {[m
[32m+[m[32m    for (var i = 0, len = listeners.length; i < len; i++) {[m
[32m+[m[32m      listeners[i].apply(this, params);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Utils.Observable = Observable;[m
[32m+[m
[32m+[m[32m  Utils.generateChars = function (length) {[m
[32m+[m[32m    var chars = '';[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < length; i++) {[m
[32m+[m[32m      var randomChar = Math.floor(Math.random() * 36);[m
[32m+[m[32m      chars += randomChar.toString(36);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return chars;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Utils.bind = function (func, context) {[m
[32m+[m[32m    return function () {[m
[32m+[m[32m      func.apply(context, arguments);[m
[32m+[m[32m    };[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Utils._convertData = function (data) {[m
[32m+[m[32m    for (var originalKey in data) {[m
[32m+[m[32m      var keys = originalKey.split('-');[m
[32m+[m
[32m+[m[32m      var dataLevel = data;[m
[32m+[m
[32m+[m[32m      if (keys.length === 1) {[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      for (var k = 0; k < keys.length; k++) {[m
[32m+[m[32m        var key = keys[k];[m
[32m+[m
[32m+[m[32m        // Lowercase the first letter[m
[32m+[m[32m        // By default, dash-separated becomes camelCase[m
[32m+[m[32m        key = key.substring(0, 1).toLowerCase() + key.substring(1);[m
[32m+[m
[32m+[m[32m        if (!(key in dataLevel)) {[m
[32m+[m[32m          dataLevel[key] = {};[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (k == keys.length - 1) {[m
[32m+[m[32m          dataLevel[key] = data[originalKey];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        dataLevel = dataLevel[key];[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      delete data[originalKey];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return data;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Utils.hasScroll = function (index, el) {[m
[32m+[m[32m    // Adapted from the function created by @ShadowScripter[m
[32m+[m[32m    // and adapted by @BillBarry on the Stack Exchange Code Review website.[m
[32m+[m[32m    // The original code can be found at[m
[32m+[m[32m    // http://codereview.stackexchange.com/q/13338[m
[32m+[m[32m    // and was designed to be used with the Sizzle selector engine.[m
[32m+[m
[32m+[m[32m    var $el = $(el);[m
[32m+[m[32m    var overflowX = el.style.overflowX;[m
[32m+[m[32m    var overflowY = el.style.overflowY;[m
[32m+[m
[32m+[m[32m    //Check both x and y declarations[m
[32m+[m[32m    if (overflowX === overflowY &&[m
[32m+[m[32m        (overflowY === 'hidden' || overflowY === 'visible')) {[m
[32m+[m[32m      return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (overflowX === 'scroll' || overflowY === 'scroll') {[m
[32m+[m[32m      return true;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return ($el.innerHeight() < el.scrollHeight ||[m
[32m+[m[32m      $el.innerWidth() < el.scrollWidth);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Utils.escapeMarkup = function (markup) {[m
[32m+[m[32m    var replaceMap = {[m
[32m+[m[32m      '\\': '&#92;',[m
[32m+[m[32m      '&': '&amp;',[m
[32m+[m[32m      '<': '&lt;',[m
[32m+[m[32m      '>': '&gt;',[m
[32m+[m[32m      '"': '&quot;',[m
[32m+[m[32m      '\'': '&#39;',[m
[32m+[m[32m      '/': '&#47;'[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Do not try to escape the markup if it's not a string[m
[32m+[m[32m    if (typeof markup !== 'string') {[m
[32m+[m[32m      return markup;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return String(markup).replace(/[&<>"'\/\\]/g, function (match) {[m
[32m+[m[32m      return replaceMap[match];[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Append an array of jQuery nodes to a given element.[m
[32m+[m[32m  Utils.appendMany = function ($element, $nodes) {[m
[32m+[m[32m    // jQuery 1.7.x does not support $.fn.append() with an array[m
[32m+[m[32m    // Fall back to a jQuery object collection using $.fn.add()[m
[32m+[m[32m    if ($.fn.jquery.substr(0, 3) === '1.7') {[m
[32m+[m[32m      var $jqNodes = $();[m
[32m+[m
[32m+[m[32m      $.map($nodes, function (node) {[m
[32m+[m[32m        $jqNodes = $jqNodes.add(node);[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      $nodes = $jqNodes;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $element.append($nodes);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Cache objects in Utils.__cache instead of $.data (see #4346)[m
[32m+[m[32m  Utils.__cache = {};[m
[32m+[m
[32m+[m[32m  var id = 0;[m
[32m+[m[32m  Utils.GetUniqueElementId = function (element) {[m
[32m+[m[32m    // Get a unique element Id. If element has no id,[m
[32m+[m[32m    // creates a new unique number, stores it in the id[m
[32m+[m[32m    // attribute and returns the new id.[m
[32m+[m[32m    // If an id already exists, it simply returns it.[m
[32m+[m
[32m+[m[32m    var select2Id = element.getAttribute('data-select2-id');[m
[32m+[m[32m    if (select2Id == null) {[m
[32m+[m[32m      // If element has id, use it.[m
[32m+[m[32m      if (element.id) {[m
[32m+[m[32m        select2Id = element.id;[m
[32m+[m[32m        element.setAttribute('data-select2-id', select2Id);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        element.setAttribute('data-select2-id', ++id);[m
[32m+[m[32m        select2Id = id.toString();[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    return select2Id;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Utils.StoreData = function (element, name, value) {[m
[32m+[m[32m    // Stores an item in the cache for a specified element.[m
[32m+[m[32m    // name is the cache key.[m
[32m+[m[32m    var id = Utils.GetUniqueElementId(element);[m
[32m+[m[32m    if (!Utils.__cache[id]) {[m
[32m+[m[32m      Utils.__cache[id] = {};[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    Utils.__cache[id][name] = value;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Utils.GetData = function (element, name) {[m
[32m+[m[32m    // Retrieves a value from the cache by its key (name)[m
[32m+[m[32m    // name is optional. If no name specified, return[m
[32m+[m[32m    // all cache items for the specified element.[m
[32m+[m[32m    // and for a specified element.[m
[32m+[m[32m    var id = Utils.GetUniqueElementId(element);[m
[32m+[m[32m    if (name) {[m
[32m+[m[32m      if (Utils.__cache[id]) {[m
[32m+[m[32m        if (Utils.__cache[id][name] != null) {[m
[32m+[m[32m          return Utils.__cache[id][name];[m
[32m+[m[32m        }[m
[32m+[m[32m        return $(element).data(name); // Fallback to HTML5 data attribs.[m
[32m+[m[32m      }[m
[32m+[m[32m      return $(element).data(name); // Fallback to HTML5 data attribs.[m
[32m+[m[32m    } else {[m
[32m+[m[32m      return Utils.__cache[id];[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Utils.RemoveData = function (element) {[m
[32m+[m[32m    // Removes all cached items for a specified element.[m
[32m+[m[32m    var id = Utils.GetUniqueElementId(element);[m
[32m+[m[32m    if (Utils.__cache[id] != null) {[m
[32m+[m[32m      delete Utils.__cache[id];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    element.removeAttribute('data-select2-id');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return Utils;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/results',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  './utils'[m
[32m+[m[32m], function ($, Utils) {[m
[32m+[m[32m  function Results ($element, options, dataAdapter) {[m
[32m+[m[32m    this.$element = $element;[m
[32m+[m[32m    this.data = dataAdapter;[m
[32m+[m[32m    this.options = options;[m
[32m+[m
[32m+[m[32m    Results.__super__.constructor.call(this);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Utils.Extend(Results, Utils.Observable);[m
[32m+[m
[32m+[m[32m  Results.prototype.render = function () {[m
[32m+[m[32m    var $results = $([m
[32m+[m[32m      '<ul class="select2-results__options" role="listbox"></ul>'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    if (this.options.get('multiple')) {[m
[32m+[m[32m      $results.attr('aria-multiselectable', 'true');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.$results = $results;[m
[32m+[m
[32m+[m[32m    return $results;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.clear = function () {[m
[32m+[m[32m    this.$results.empty();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.displayMessage = function (params) {[m
[32m+[m[32m    var escapeMarkup = this.options.get('escapeMarkup');[m
[32m+[m
[32m+[m[32m    this.clear();[m
[32m+[m[32m    this.hideLoading();[m
[32m+[m
[32m+[m[32m    var $message = $([m
[32m+[m[32m      '<li role="alert" aria-live="assertive"' +[m
[32m+[m[32m      ' class="select2-results__option"></li>'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    var message = this.options.get('translations').get(params.message);[m
[32m+[m
[32m+[m[32m    $message.append([m
[32m+[m[32m      escapeMarkup([m
[32m+[m[32m        message(params.args)[m
[32m+[m[32m      )[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    $message[0].className += ' select2-results__message';[m
[32m+[m
[32m+[m[32m    this.$results.append($message);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.hideMessages = function () {[m
[32m+[m[32m    this.$results.find('.select2-results__message').remove();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.append = function (data) {[m
[32m+[m[32m    this.hideLoading();[m
[32m+[m
[32m+[m[32m    var $options = [];[m
[32m+[m
[32m+[m[32m    if (data.results == null || data.results.length === 0) {[m
[32m+[m[32m      if (this.$results.children().length === 0) {[m
[32m+[m[32m        this.trigger('results:message', {[m
[32m+[m[32m          message: 'noResults'[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    data.results = this.sort(data.results);[m
[32m+[m
[32m+[m[32m    for (var d = 0; d < data.results.length; d++) {[m
[32m+[m[32m      var item = data.results[d];[m
[32m+[m
[32m+[m[32m      var $option = this.option(item);[m
[32m+[m
[32m+[m[32m      $options.push($option);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.$results.append($options);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.position = function ($results, $dropdown) {[m
[32m+[m[32m    var $resultsContainer = $dropdown.find('.select2-results');[m
[32m+[m[32m    $resultsContainer.append($results);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.sort = function (data) {[m
[32m+[m[32m    var sorter = this.options.get('sorter');[m
[32m+[m
[32m+[m[32m    return sorter(data);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.highlightFirstItem = function () {[m
[32m+[m[32m    var $options = this.$results[m
[32m+[m[32m      .find('.select2-results__option[aria-selected]');[m
[32m+[m
[32m+[m[32m    var $selected = $options.filter('[aria-selected=true]');[m
[32m+[m
[32m+[m[32m    // Check if there are any selected options[m
[32m+[m[32m    if ($selected.length > 0) {[m
[32m+[m[32m      // If there are selected options, highlight the first[m
[32m+[m[32m      $selected.first().trigger('mouseenter');[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // If there are no selected options, highlight the first option[m
[32m+[m[32m      // in the dropdown[m
[32m+[m[32m      $options.first().trigger('mouseenter');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.ensureHighlightVisible();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.setClasses = function () {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    this.data.current(function (selected) {[m
[32m+[m[32m      var selectedIds = $.map(selected, function (s) {[m
[32m+[m[32m        return s.id.toString();[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      var $options = self.$results[m
[32m+[m[32m        .find('.select2-results__option[aria-selected]');[m
[32m+[m
[32m+[m[32m      $options.each(function () {[m
[32m+[m[32m        var $option = $(this);[m
[32m+[m
[32m+[m[32m        var item = Utils.GetData(this, 'data');[m
[32m+[m
[32m+[m[32m        // id needs to be converted to a string when comparing[m
[32m+[m[32m        var id = '' + item.id;[m
[32m+[m
[32m+[m[32m        if ((item.element != null && item.element.selected) ||[m
[32m+[m[32m            (item.element == null && $.inArray(id, selectedIds) > -1)) {[m
[32m+[m[32m          $option.attr('aria-selected', 'true');[m
[32m+[m[32m        } else {[m
[32m+[m[32m          $option.attr('aria-selected', 'false');[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.showLoading = function (params) {[m
[32m+[m[32m    this.hideLoading();[m
[32m+[m
[32m+[m[32m    var loadingMore = this.options.get('translations').get('searching');[m
[32m+[m
[32m+[m[32m    var loading = {[m
[32m+[m[32m      disabled: true,[m
[32m+[m[32m      loading: true,[m
[32m+[m[32m      text: loadingMore(params)[m
[32m+[m[32m    };[m
[32m+[m[32m    var $loading = this.option(loading);[m
[32m+[m[32m    $loading.className += ' loading-results';[m
[32m+[m
[32m+[m[32m    this.$results.prepend($loading);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.hideLoading = function () {[m
[32m+[m[32m    this.$results.find('.loading-results').remove();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.option = function (data) {[m
[32m+[m[32m    var option = document.createElement('li');[m
[32m+[m[32m    option.className = 'select2-results__option';[m
[32m+[m
[32m+[m[32m    var attrs = {[m
[32m+[m[32m      'role': 'option',[m
[32m+[m[32m      'aria-selected': 'false'[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var matches = window.Element.prototype.matches ||[m
[32m+[m[32m      window.Element.prototype.msMatchesSelector ||[m
[32m+[m[32m      window.Element.prototype.webkitMatchesSelector;[m
[32m+[m
[32m+[m[32m    if ((data.element != null && matches.call(data.element, ':disabled')) ||[m
[32m+[m[32m        (data.element == null && data.disabled)) {[m
[32m+[m[32m      delete attrs['aria-selected'];[m
[32m+[m[32m      attrs['aria-disabled'] = 'true';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (data.id == null) {[m
[32m+[m[32m      delete attrs['aria-selected'];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (data._resultId != null) {[m
[32m+[m[32m      option.id = data._resultId;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (data.title) {[m
[32m+[m[32m      option.title = data.title;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (data.children) {[m
[32m+[m[32m      attrs.role = 'group';[m
[32m+[m[32m      attrs['aria-label'] = data.text;[m
[32m+[m[32m      delete attrs['aria-selected'];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    for (var attr in attrs) {[m
[32m+[m[32m      var val = attrs[attr];[m
[32m+[m
[32m+[m[32m      option.setAttribute(attr, val);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (data.children) {[m
[32m+[m[32m      var $option = $(option);[m
[32m+[m
[32m+[m[32m      var label = document.createElement('strong');[m
[32m+[m[32m      label.className = 'select2-results__group';[m
[32m+[m
[32m+[m[32m      var $label = $(label);[m
[32m+[m[32m      this.template(data, label);[m
[32m+[m
[32m+[m[32m      var $children = [];[m
[32m+[m
[32m+[m[32m      for (var c = 0; c < data.children.length; c++) {[m
[32m+[m[32m        var child = data.children[c];[m
[32m+[m
[32m+[m[32m        var $child = this.option(child);[m
[32m+[m
[32m+[m[32m        $children.push($child);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var $childrenContainer = $('<ul></ul>', {[m
[32m+[m[32m        'class': 'select2-results__options select2-results__options--nested'[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      $childrenContainer.append($children);[m
[32m+[m
[32m+[m[32m      $option.append(label);[m
[32m+[m[32m      $option.append($childrenContainer);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      this.template(data, option);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    Utils.StoreData(option, 'data', data);[m
[32m+[m
[32m+[m[32m    return option;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.bind = function (container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    var id = container.id + '-results';[m
[32m+[m
[32m+[m[32m    this.$results.attr('id', id);[m
[32m+[m
[32m+[m[32m    container.on('results:all', function (params) {[m
[32m+[m[32m      self.clear();[m
[32m+[m[32m      self.append(params.data);[m
[32m+[m
[32m+[m[32m      if (container.isOpen()) {[m
[32m+[m[32m        self.setClasses();[m
[32m+[m[32m        self.highlightFirstItem();[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:append', function (params) {[m
[32m+[m[32m      self.append(params.data);[m
[32m+[m
[32m+[m[32m      if (container.isOpen()) {[m
[32m+[m[32m        self.setClasses();[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('query', function (params) {[m
[32m+[m[32m      self.hideMessages();[m
[32m+[m[32m      self.showLoading(params);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('select', function () {[m
[32m+[m[32m      if (!container.isOpen()) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      self.setClasses();[m
[32m+[m
[32m+[m[32m      if (self.options.get('scrollAfterSelect')) {[m
[32m+[m[32m        self.highlightFirstItem();[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('unselect', function () {[m
[32m+[m[32m      if (!container.isOpen()) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      self.setClasses();[m
[32m+[m
[32m+[m[32m      if (self.options.get('scrollAfterSelect')) {[m
[32m+[m[32m        self.highlightFirstItem();[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('open', function () {[m
[32m+[m[32m      // When the dropdown is open, aria-expended="true"[m
[32m+[m[32m      self.$results.attr('aria-expanded', 'true');[m
[32m+[m[32m      self.$results.attr('aria-hidden', 'false');[m
[32m+[m
[32m+[m[32m      self.setClasses();[m
[32m+[m[32m      self.ensureHighlightVisible();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('close', function () {[m
[32m+[m[32m      // When the dropdown is closed, aria-expended="false"[m
[32m+[m[32m      self.$results.attr('aria-expanded', 'false');[m
[32m+[m[32m      self.$results.attr('aria-hidden', 'true');[m
[32m+[m[32m      self.$results.removeAttr('aria-activedescendant');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:toggle', function () {[m
[32m+[m[32m      var $highlighted = self.getHighlightedResults();[m
[32m+[m
[32m+[m[32m      if ($highlighted.length === 0) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      $highlighted.trigger('mouseup');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:select', function () {[m
[32m+[m[32m      var $highlighted = self.getHighlightedResults();[m
[32m+[m
[32m+[m[32m      if ($highlighted.length === 0) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var data = Utils.GetData($highlighted[0], 'data');[m
[32m+[m
[32m+[m[32m      if ($highlighted.attr('aria-selected') == 'true') {[m
[32m+[m[32m        self.trigger('close', {});[m
[32m+[m[32m      } else {[m
[32m+[m[32m        self.trigger('select', {[m
[32m+[m[32m          data: data[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:previous', function () {[m
[32m+[m[32m      var $highlighted = self.getHighlightedResults();[m
[32m+[m
[32m+[m[32m      var $options = self.$results.find('[aria-selected]');[m
[32m+[m
[32m+[m[32m      var currentIndex = $options.index($highlighted);[m
[32m+[m
[32m+[m[32m      // If we are already at the top, don't move further[m
[32m+[m[32m      // If no options, currentIndex will be -1[m
[32m+[m[32m      if (currentIndex <= 0) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var nextIndex = currentIndex - 1;[m
[32m+[m
[32m+[m[32m      // If none are highlighted, highlight the first[m
[32m+[m[32m      if ($highlighted.length === 0) {[m
[32m+[m[32m        nextIndex = 0;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var $next = $options.eq(nextIndex);[m
[32m+[m
[32m+[m[32m      $next.trigger('mouseenter');[m
[32m+[m
[32m+[m[32m      var currentOffset = self.$results.offset().top;[m
[32m+[m[32m      var nextTop = $next.offset().top;[m
[32m+[m[32m      var nextOffset = self.$results.scrollTop() + (nextTop - currentOffset);[m
[32m+[m
[32m+[m[32m      if (nextIndex === 0) {[m
[32m+[m[32m        self.$results.scrollTop(0);[m
[32m+[m[32m      } else if (nextTop - currentOffset < 0) {[m
[32m+[m[32m        self.$results.scrollTop(nextOffset);[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:next', function () {[m
[32m+[m[32m      var $highlighted = self.getHighlightedResults();[m
[32m+[m
[32m+[m[32m      var $options = self.$results.find('[aria-selected]');[m
[32m+[m
[32m+[m[32m      var currentIndex = $options.index($highlighted);[m
[32m+[m
[32m+[m[32m      var nextIndex = currentIndex + 1;[m
[32m+[m
[32m+[m[32m      // If we are at the last option, stay there[m
[32m+[m[32m      if (nextIndex >= $options.length) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var $next = $options.eq(nextIndex);[m
[32m+[m
[32m+[m[32m      $next.trigger('mouseenter');[m
[32m+[m
[32m+[m[32m      var currentOffset = self.$results.offset().top +[m
[32m+[m[32m        self.$results.outerHeight(false);[m
[32m+[m[32m      var nextBottom = $next.offset().top + $next.outerHeight(false);[m
[32m+[m[32m      var nextOffset = self.$results.scrollTop() + nextBottom - currentOffset;[m
[32m+[m
[32m+[m[32m      if (nextIndex === 0) {[m
[32m+[m[32m        self.$results.scrollTop(0);[m
[32m+[m[32m      } else if (nextBottom > currentOffset) {[m
[32m+[m[32m        self.$results.scrollTop(nextOffset);[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:focus', function (params) {[m
[32m+[m[32m      params.element.addClass('select2-results__option--highlighted');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:message', function (params) {[m
[32m+[m[32m      self.displayMessage(params);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    if ($.fn.mousewheel) {[m
[32m+[m[32m      this.$results.on('mousewheel', function (e) {[m
[32m+[m[32m        var top = self.$results.scrollTop();[m
[32m+[m
[32m+[m[32m        var bottom = self.$results.get(0).scrollHeight - top + e.deltaY;[m
[32m+[m
[32m+[m[32m        var isAtTop = e.deltaY > 0 && top - e.deltaY <= 0;[m
[32m+[m[32m        var isAtBottom = e.deltaY < 0 && bottom <= self.$results.height();[m
[32m+[m
[32m+[m[32m        if (isAtTop) {[m
[32m+[m[32m          self.$results.scrollTop(0);[m
[32m+[m
[32m+[m[32m          e.preventDefault();[m
[32m+[m[32m          e.stopPropagation();[m
[32m+[m[32m        } else if (isAtBottom) {[m
[32m+[m[32m          self.$results.scrollTop([m
[32m+[m[32m            self.$results.get(0).scrollHeight - self.$results.height()[m
[32m+[m[32m          );[m
[32m+[m
[32m+[m[32m          e.preventDefault();[m
[32m+[m[32m          e.stopPropagation();[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.$results.on('mouseup', '.select2-results__option[aria-selected]',[m
[32m+[m[32m      function (evt) {[m
[32m+[m[32m      var $this = $(this);[m
[32m+[m
[32m+[m[32m      var data = Utils.GetData(this, 'data');[m
[32m+[m
[32m+[m[32m      if ($this.attr('aria-selected') === 'true') {[m
[32m+[m[32m        if (self.options.get('multiple')) {[m
[32m+[m[32m          self.trigger('unselect', {[m
[32m+[m[32m            originalEvent: evt,[m
[32m+[m[32m            data: data[m
[32m+[m[32m          });[m
[32m+[m[32m        } else {[m
[32m+[m[32m          self.trigger('close', {});[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      self.trigger('select', {[m
[32m+[m[32m        originalEvent: evt,[m
[32m+[m[32m        data: data[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$results.on('mouseenter', '.select2-results__option[aria-selected]',[m
[32m+[m[32m      function (evt) {[m
[32m+[m[32m      var data = Utils.GetData(this, 'data');[m
[32m+[m
[32m+[m[32m      self.getHighlightedResults()[m
[32m+[m[32m          .removeClass('select2-results__option--highlighted');[m
[32m+[m
[32m+[m[32m      self.trigger('results:focus', {[m
[32m+[m[32m        data: data,[m
[32m+[m[32m        element: $(this)[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.getHighlightedResults = function () {[m
[32m+[m[32m    var $highlighted = this.$results[m
[32m+[m[32m    .find('.select2-results__option--highlighted');[m
[32m+[m
[32m+[m[32m    return $highlighted;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.destroy = function () {[m
[32m+[m[32m    this.$results.remove();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.ensureHighlightVisible = function () {[m
[32m+[m[32m    var $highlighted = this.getHighlightedResults();[m
[32m+[m
[32m+[m[32m    if ($highlighted.length === 0) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var $options = this.$results.find('[aria-selected]');[m
[32m+[m
[32m+[m[32m    var currentIndex = $options.index($highlighted);[m
[32m+[m
[32m+[m[32m    var currentOffset = this.$results.offset().top;[m
[32m+[m[32m    var nextTop = $highlighted.offset().top;[m
[32m+[m[32m    var nextOffset = this.$results.scrollTop() + (nextTop - currentOffset);[m
[32m+[m
[32m+[m[32m    var offsetDelta = nextTop - currentOffset;[m
[32m+[m[32m    nextOffset -= $highlighted.outerHeight(false) * 2;[m
[32m+[m
[32m+[m[32m    if (currentIndex <= 2) {[m
[32m+[m[32m      this.$results.scrollTop(0);[m
[32m+[m[32m    } else if (offsetDelta > this.$results.outerHeight() || offsetDelta < 0) {[m
[32m+[m[32m      this.$results.scrollTop(nextOffset);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Results.prototype.template = function (result, container) {[m
[32m+[m[32m    var template = this.options.get('templateResult');[m
[32m+[m[32m    var escapeMarkup = this.options.get('escapeMarkup');[m
[32m+[m
[32m+[m[32m    var content = template(result, container);[m
[32m+[m
[32m+[m[32m    if (content == null) {[m
[32m+[m[32m      container.style.display = 'none';[m
[32m+[m[32m    } else if (typeof content === 'string') {[m
[32m+[m[32m      container.innerHTML = escapeMarkup(content);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      $(container).append(content);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return Results;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/keys',[[m
[32m+[m
[32m+[m[32m], function () {[m
[32m+[m[32m  var KEYS = {[m
[32m+[m[32m    BACKSPACE: 8,[m
[32m+[m[32m    TAB: 9,[m
[32m+[m[32m    ENTER: 13,[m
[32m+[m[32m    SHIFT: 16,[m
[32m+[m[32m    CTRL: 17,[m
[32m+[m[32m    ALT: 18,[m
[32m+[m[32m    ESC: 27,[m
[32m+[m[32m    SPACE: 32,[m
[32m+[m[32m    PAGE_UP: 33,[m
[32m+[m[32m    PAGE_DOWN: 34,[m
[32m+[m[32m    END: 35,[m
[32m+[m[32m    HOME: 36,[m
[32m+[m[32m    LEFT: 37,[m
[32m+[m[32m    UP: 38,[m
[32m+[m[32m    RIGHT: 39,[m
[32m+[m[32m    DOWN: 40,[m
[32m+[m[32m    DELETE: 46[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return KEYS;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/selection/base',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  '../utils',[m
[32m+[m[32m  '../keys'[m
[32m+[m[32m], function ($, Utils, KEYS) {[m
[32m+[m[32m  function BaseSelection ($element, options) {[m
[32m+[m[32m    this.$element = $element;[m
[32m+[m[32m    this.options = options;[m
[32m+[m
[32m+[m[32m    BaseSelection.__super__.constructor.call(this);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Utils.Extend(BaseSelection, Utils.Observable);[m
[32m+[m
[32m+[m[32m  BaseSelection.prototype.render = function () {[m
[32m+[m[32m    var $selection = $([m
[32m+[m[32m      '<span class="select2-selection" role="combobox" ' +[m
[32m+[m[32m      ' aria-haspopup="true" aria-expanded="false">' +[m
[32m+[m[32m      '</span>'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    this._tabindex = 0;[m
[32m+[m
[32m+[m[32m    if (Utils.GetData(this.$element[0], 'old-tabindex') != null) {[m
[32m+[m[32m      this._tabindex = Utils.GetData(this.$element[0], 'old-tabindex');[m
[32m+[m[32m    } else if (this.$element.attr('tabindex') != null) {[m
[32m+[m[32m      this._tabindex = this.$element.attr('tabindex');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $selection.attr('title', this.$element.attr('title'));[m
[32m+[m[32m    $selection.attr('tabindex', this._tabindex);[m
[32m+[m[32m    $selection.attr('aria-disabled', 'false');[m
[32m+[m
[32m+[m[32m    this.$selection = $selection;[m
[32m+[m
[32m+[m[32m    return $selection;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  BaseSelection.prototype.bind = function (container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    var resultsId = container.id + '-results';[m
[32m+[m
[32m+[m[32m    this.container = container;[m
[32m+[m
[32m+[m[32m    this.$selection.on('focus', function (evt) {[m
[32m+[m[32m      self.trigger('focus', evt);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$selection.on('blur', function (evt) {[m
[32m+[m[32m      self._handleBlur(evt);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$selection.on('keydown', function (evt) {[m
[32m+[m[32m      self.trigger('keypress', evt);[m
[32m+[m
[32m+[m[32m      if (evt.which === KEYS.SPACE) {[m
[32m+[m[32m        evt.preventDefault();[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:focus', function (params) {[m
[32m+[m[32m      self.$selection.attr('aria-activedescendant', params.data._resultId);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('selection:update', function (params) {[m
[32m+[m[32m      self.update(params.data);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('open', function () {[m
[32m+[m[32m      // When the dropdown is open, aria-expanded="true"[m
[32m+[m[32m      self.$selection.attr('aria-expanded', 'true');[m
[32m+[m[32m      self.$selection.attr('aria-owns', resultsId);[m
[32m+[m
[32m+[m[32m      self._attachCloseHandler(container);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('close', function () {[m
[32m+[m[32m      // When the dropdown is closed, aria-expanded="false"[m
[32m+[m[32m      self.$selection.attr('aria-expanded', 'false');[m
[32m+[m[32m      self.$selection.removeAttr('aria-activedescendant');[m
[32m+[m[32m      self.$selection.removeAttr('aria-owns');[m
[32m+[m
[32m+[m[32m      self.$selection.trigger('focus');[m
[32m+[m
[32m+[m[32m      self._detachCloseHandler(container);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('enable', function () {[m
[32m+[m[32m      self.$selection.attr('tabindex', self._tabindex);[m
[32m+[m[32m      self.$selection.attr('aria-disabled', 'false');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('disable', function () {[m
[32m+[m[32m      self.$selection.attr('tabindex', '-1');[m
[32m+[m[32m      self.$selection.attr('aria-disabled', 'true');[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  BaseSelection.prototype._handleBlur = function (evt) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    // This needs to be delayed as the active element is the body when the tab[m
[32m+[m[32m    // key is pressed, possibly along with others.[m
[32m+[m[32m    window.setTimeout(function () {[m
[32m+[m[32m      // Don't trigger `blur` if the focus is still in the selection[m
[32m+[m[32m      if ([m
[32m+[m[32m        (document.activeElement == self.$selection[0]) ||[m
[32m+[m[32m        ($.contains(self.$selection[0], document.activeElement))[m
[32m+[m[32m      ) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      self.trigger('blur', evt);[m
[32m+[m[32m    }, 1);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  BaseSelection.prototype._attachCloseHandler = function (container) {[m
[32m+[m
[32m+[m[32m    $(document.body).on('mousedown.select2.' + container.id, function (e) {[m
[32m+[m[32m      var $target = $(e.target);[m
[32m+[m
[32m+[m[32m      var $select = $target.closest('.select2');[m
[32m+[m
[32m+[m[32m      var $all = $('.select2.select2-container--open');[m
[32m+[m
[32m+[m[32m      $all.each(function () {[m
[32m+[m[32m        if (this == $select[0]) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var $element = Utils.GetData(this, 'element');[m
[32m+[m
[32m+[m[32m        $element.select2('close');[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  BaseSelection.prototype._detachCloseHandler = function (container) {[m
[32m+[m[32m    $(document.body).off('mousedown.select2.' + container.id);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  BaseSelection.prototype.position = function ($selection, $container) {[m
[32m+[m[32m    var $selectionContainer = $container.find('.selection');[m
[32m+[m[32m    $selectionContainer.append($selection);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  BaseSelection.prototype.destroy = function () {[m
[32m+[m[32m    this._detachCloseHandler(this.container);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  BaseSelection.prototype.update = function (data) {[m
[32m+[m[32m    throw new Error('The `update` method must be defined in child classes.');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Helper method to abstract the "enabled" (not "disabled") state of this[m
[32m+[m[32m   * object.[m
[32m+[m[32m   *[m
[32m+[m[32m   * @return {true} if the instance is not disabled.[m
[32m+[m[32m   * @return {false} if the instance is disabled.[m
[32m+[m[32m   */[m
[32m+[m[32m  BaseSelection.prototype.isEnabled = function () {[m
[32m+[m[32m    return !this.isDisabled();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Helper method to abstract the "disabled" state of this object.[m
[32m+[m[32m   *[m
[32m+[m[32m   * @return {true} if the disabled option is true.[m
[32m+[m[32m   * @return {false} if the disabled option is false.[m
[32m+[m[32m   */[m
[32m+[m[32m  BaseSelection.prototype.isDisabled = function () {[m
[32m+[m[32m    return this.options.get('disabled');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return BaseSelection;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/selection/single',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  './base',[m
[32m+[m[32m  '../utils',[m
[32m+[m[32m  '../keys'[m
[32m+[m[32m], function ($, BaseSelection, Utils, KEYS) {[m
[32m+[m[32m  function SingleSelection () {[m
[32m+[m[32m    SingleSelection.__super__.constructor.apply(this, arguments);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Utils.Extend(SingleSelection, BaseSelection);[m
[32m+[m
[32m+[m[32m  SingleSelection.prototype.render = function () {[m
[32m+[m[32m    var $selection = SingleSelection.__super__.render.call(this);[m
[32m+[m
[32m+[m[32m    $selection.addClass('select2-selection--single');[m
[32m+[m
[32m+[m[32m    $selection.html([m
[32m+[m[32m      '<span class="select2-selection__rendered"></span>' +[m
[32m+[m[32m      '<span class="select2-selection__arrow" role="presentation">' +[m
[32m+[m[32m        '<b role="presentation"></b>' +[m
[32m+[m[32m      '</span>'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    return $selection;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SingleSelection.prototype.bind = function (container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    SingleSelection.__super__.bind.apply(this, arguments);[m
[32m+[m
[32m+[m[32m    var id = container.id + '-container';[m
[32m+[m
[32m+[m[32m    this.$selection.find('.select2-selection__rendered')[m
[32m+[m[32m      .attr('id', id)[m
[32m+[m[32m      .attr('role', 'textbox')[m
[32m+[m[32m      .attr('aria-readonly', 'true');[m
[32m+[m[32m    this.$selection.attr('aria-labelledby', id);[m
[32m+[m
[32m+[m[32m    this.$selection.on('mousedown', function (evt) {[m
[32m+[m[32m      // Only respond to left clicks[m
[32m+[m[32m      if (evt.which !== 1) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      self.trigger('toggle', {[m
[32m+[m[32m        originalEvent: evt[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$selection.on('focus', function (evt) {[m
[32m+[m[32m      // User focuses on the container[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$selection.on('blur', function (evt) {[m
[32m+[m[32m      // User exits the container[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('focus', function (evt) {[m
[32m+[m[32m      if (!container.isOpen()) {[m
[32m+[m[32m        self.$selection.trigger('focus');[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SingleSelection.prototype.clear = function () {[m
[32m+[m[32m    var $rendered = this.$selection.find('.select2-selection__rendered');[m
[32m+[m[32m    $rendered.empty();[m
[32m+[m[32m    $rendered.removeAttr('title'); // clear tooltip on empty[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SingleSelection.prototype.display = function (data, container) {[m
[32m+[m[32m    var template = this.options.get('templateSelection');[m
[32m+[m[32m    var escapeMarkup = this.options.get('escapeMarkup');[m
[32m+[m
[32m+[m[32m    return escapeMarkup(template(data, container));[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SingleSelection.prototype.selectionContainer = function () {[m
[32m+[m[32m    return $('<span></span>');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SingleSelection.prototype.update = function (data) {[m
[32m+[m[32m    if (data.length === 0) {[m
[32m+[m[32m      this.clear();[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var selection = data[0];[m
[32m+[m
[32m+[m[32m    var $rendered = this.$selection.find('.select2-selection__rendered');[m
[32m+[m[32m    var formatted = this.display(selection, $rendered);[m
[32m+[m
[32m+[m[32m    $rendered.empty().append(formatted);[m
[32m+[m
[32m+[m[32m    var title = selection.title || selection.text;[m
[32m+[m
[32m+[m[32m    if (title) {[m
[32m+[m[32m      $rendered.attr('title', title);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      $rendered.removeAttr('title');[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return SingleSelection;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/selection/multiple',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  './base',[m
[32m+[m[32m  '../utils'[m
[32m+[m[32m], function ($, BaseSelection, Utils) {[m
[32m+[m[32m  function MultipleSelection ($element, options) {[m
[32m+[m[32m    MultipleSelection.__super__.constructor.apply(this, arguments);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Utils.Extend(MultipleSelection, BaseSelection);[m
[32m+[m
[32m+[m[32m  MultipleSelection.prototype.render = function () {[m
[32m+[m[32m    var $selection = MultipleSelection.__super__.render.call(this);[m
[32m+[m
[32m+[m[32m    $selection.addClass('select2-selection--multiple');[m
[32m+[m
[32m+[m[32m    $selection.html([m
[32m+[m[32m      '<ul class="select2-selection__rendered"></ul>'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    return $selection;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  MultipleSelection.prototype.bind = function (container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    MultipleSelection.__super__.bind.apply(this, arguments);[m
[32m+[m
[32m+[m[32m    this.$selection.on('click', function (evt) {[m
[32m+[m[32m      self.trigger('toggle', {[m
[32m+[m[32m        originalEvent: evt[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$selection.on([m
[32m+[m[32m      'click',[m
[32m+[m[32m      '.select2-selection__choice__remove',[m
[32m+[m[32m      function (evt) {[m
[32m+[m[32m        // Ignore the event if it is disabled[m
[32m+[m[32m        if (self.isDisabled()) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var $remove = $(this);[m
[32m+[m[32m        var $selection = $remove.parent();[m
[32m+[m
[32m+[m[32m        var data = Utils.GetData($selection[0], 'data');[m
[32m+[m
[32m+[m[32m        self.trigger('unselect', {[m
[32m+[m[32m          originalEvent: evt,[m
[32m+[m[32m          data: data[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m[32m    );[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  MultipleSelection.prototype.clear = function () {[m
[32m+[m[32m    var $rendered = this.$selection.find('.select2-selection__rendered');[m
[32m+[m[32m    $rendered.empty();[m
[32m+[m[32m    $rendered.removeAttr('title');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  MultipleSelection.prototype.display = function (data, container) {[m
[32m+[m[32m    var template = this.options.get('templateSelection');[m
[32m+[m[32m    var escapeMarkup = this.options.get('escapeMarkup');[m
[32m+[m
[32m+[m[32m    return escapeMarkup(template(data, container));[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  MultipleSelection.prototype.selectionContainer = function () {[m
[32m+[m[32m    var $container = $([m
[32m+[m[32m      '<li class="select2-selection__choice">' +[m
[32m+[m[32m        '<span class="select2-selection__choice__remove" role="presentation">' +[m
[32m+[m[32m          '&times;' +[m
[32m+[m[32m        '</span>' +[m
[32m+[m[32m      '</li>'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    return $container;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  MultipleSelection.prototype.update = function (data) {[m
[32m+[m[32m    this.clear();[m
[32m+[m
[32m+[m[32m    if (data.length === 0) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var $selections = [];[m
[32m+[m
[32m+[m[32m    for (var d = 0; d < data.length; d++) {[m
[32m+[m[32m      var selection = data[d];[m
[32m+[m
[32m+[m[32m      var $selection = this.selectionContainer();[m
[32m+[m[32m      var formatted = this.display(selection, $selection);[m
[32m+[m
[32m+[m[32m      $selection.append(formatted);[m
[32m+[m
[32m+[m[32m      var title = selection.title || selection.text;[m
[32m+[m
[32m+[m[32m      if (title) {[m
[32m+[m[32m        $selection.attr('title', title);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      Utils.StoreData($selection[0], 'data', selection);[m
[32m+[m
[32m+[m[32m      $selections.push($selection);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var $rendered = this.$selection.find('.select2-selection__rendered');[m
[32m+[m
[32m+[m[32m    Utils.appendMany($rendered, $selections);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return MultipleSelection;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/selection/placeholder',[[m
[32m+[m[32m  '../utils'[m
[32m+[m[32m], function (Utils) {[m
[32m+[m[32m  function Placeholder (decorated, $element, options) {[m
[32m+[m[32m    this.placeholder = this.normalizePlaceholder(options.get('placeholder'));[m
[32m+[m
[32m+[m[32m    decorated.call(this, $element, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Placeholder.prototype.normalizePlaceholder = function (_, placeholder) {[m
[32m+[m[32m    if (typeof placeholder === 'string') {[m
[32m+[m[32m      placeholder = {[m
[32m+[m[32m        id: '',[m
[32m+[m[32m        text: placeholder[m
[32m+[m[32m      };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return placeholder;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Placeholder.prototype.createPlaceholder = function (decorated, placeholder) {[m
[32m+[m[32m    var $placeholder = this.selectionContainer();[m
[32m+[m
[32m+[m[32m    $placeholder.html(this.display(placeholder));[m
[32m+[m[32m    $placeholder.addClass('select2-selection__placeholder')[m
[32m+[m[32m                .removeClass('select2-selection__choice');[m
[32m+[m
[32m+[m[32m    return $placeholder;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Placeholder.prototype.update = function (decorated, data) {[m
[32m+[m[32m    var singlePlaceholder = ([m
[32m+[m[32m      data.length == 1 && data[0].id != this.placeholder.id[m
[32m+[m[32m    );[m
[32m+[m[32m    var multipleSelections = data.length > 1;[m
[32m+[m
[32m+[m[32m    if (multipleSelections || singlePlaceholder) {[m
[32m+[m[32m      return decorated.call(this, data);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.clear();[m
[32m+[m
[32m+[m[32m    var $placeholder = this.createPlaceholder(this.placeholder);[m
[32m+[m
[32m+[m[32m    this.$selection.find('.select2-selection__rendered').append($placeholder);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return Placeholder;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/selection/allowClear',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  '../keys',[m
[32m+[m[32m  '../utils'[m
[32m+[m[32m], function ($, KEYS, Utils) {[m
[32m+[m[32m  function AllowClear () { }[m
[32m+[m
[32m+[m[32m  AllowClear.prototype.bind = function (decorated, container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    decorated.call(this, container, $container);[m
[32m+[m
[32m+[m[32m    if (this.placeholder == null) {[m
[32m+[m[32m      if (this.options.get('debug') && window.console && console.error) {[m
[32m+[m[32m        console.error([m
[32m+[m[32m          'Select2: The `allowClear` option should be used in combination ' +[m
[32m+[m[32m          'with the `placeholder` option.'[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.$selection.on('mousedown', '.select2-selection__clear',[m
[32m+[m[32m      function (evt) {[m
[32m+[m[32m        self._handleClear(evt);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('keypress', function (evt) {[m
[32m+[m[32m      self._handleKeyboardClear(evt, container);[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AllowClear.prototype._handleClear = function (_, evt) {[m
[32m+[m[32m    // Ignore the event if it is disabled[m
[32m+[m[32m    if (this.isDisabled()) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var $clear = this.$selection.find('.select2-selection__clear');[m
[32m+[m
[32m+[m[32m    // Ignore the event if nothing has been selected[m
[32m+[m[32m    if ($clear.length === 0) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    evt.stopPropagation();[m
[32m+[m
[32m+[m[32m    var data = Utils.GetData($clear[0], 'data');[m
[32m+[m
[32m+[m[32m    var previousVal = this.$element.val();[m
[32m+[m[32m    this.$element.val(this.placeholder.id);[m
[32m+[m
[32m+[m[32m    var unselectData = {[m
[32m+[m[32m      data: data[m
[32m+[m[32m    };[m
[32m+[m[32m    this.trigger('clear', unselectData);[m
[32m+[m[32m    if (unselectData.prevented) {[m
[32m+[m[32m      this.$element.val(previousVal);[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    for (var d = 0; d < data.length; d++) {[m
[32m+[m[32m      unselectData = {[m
[32m+[m[32m        data: data[d][m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      // Trigger the `unselect` event, so people can prevent it from being[m
[32m+[m[32m      // cleared.[m
[32m+[m[32m      this.trigger('unselect', unselectData);[m
[32m+[m
[32m+[m[32m      // If the event was prevented, don't clear it out.[m
[32m+[m[32m      if (unselectData.prevented) {[m
[32m+[m[32m        this.$element.val(previousVal);[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.$element.trigger('input').trigger('change');[m
[32m+[m
[32m+[m[32m    this.trigger('toggle', {});[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AllowClear.prototype._handleKeyboardClear = function (_, evt, container) {[m
[32m+[m[32m    if (container.isOpen()) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (evt.which == KEYS.DELETE || evt.which == KEYS.BACKSPACE) {[m
[32m+[m[32m      this._handleClear(evt);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AllowClear.prototype.update = function (decorated, data) {[m
[32m+[m[32m    decorated.call(this, data);[m
[32m+[m
[32m+[m[32m    if (this.$selection.find('.select2-selection__placeholder').length > 0 ||[m
[32m+[m[32m        data.length === 0) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var removeAll = this.options.get('translations').get('removeAllItems');[m
[32m+[m
[32m+[m[32m    var $remove = $([m
[32m+[m[32m      '<span class="select2-selection__clear" title="' + removeAll() +'">' +[m
[32m+[m[32m        '&times;' +[m
[32m+[m[32m      '</span>'[m
[32m+[m[32m    );[m
[32m+[m[32m    Utils.StoreData($remove[0], 'data', data);[m
[32m+[m
[32m+[m[32m    this.$selection.find('.select2-selection__rendered').prepend($remove);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return AllowClear;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/selection/search',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  '../utils',[m
[32m+[m[32m  '../keys'[m
[32m+[m[32m], function ($, Utils, KEYS) {[m
[32m+[m[32m  function Search (decorated, $element, options) {[m
[32m+[m[32m    decorated.call(this, $element, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Search.prototype.render = function (decorated) {[m
[32m+[m[32m    var $search = $([m
[32m+[m[32m      '<li class="select2-search select2-search--inline">' +[m
[32m+[m[32m        '<input class="select2-search__field" type="search" tabindex="-1"' +[m
[32m+[m[32m        ' autocomplete="off" autocorrect="off" autocapitalize="none"' +[m
[32m+[m[32m        ' spellcheck="false" role="searchbox" aria-autocomplete="list" />' +[m
[32m+[m[32m      '</li>'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    this.$searchContainer = $search;[m
[32m+[m[32m    this.$search = $search.find('input');[m
[32m+[m
[32m+[m[32m    var $rendered = decorated.call(this);[m
[32m+[m
[32m+[m[32m    this._transferTabIndex();[m
[32m+[m
[32m+[m[32m    return $rendered;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Search.prototype.bind = function (decorated, container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    var resultsId = container.id + '-results';[m
[32m+[m
[32m+[m[32m    decorated.call(this, container, $container);[m
[32m+[m
[32m+[m[32m    container.on('open', function () {[m
[32m+[m[32m      self.$search.attr('aria-controls', resultsId);[m
[32m+[m[32m      self.$search.trigger('focus');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('close', function () {[m
[32m+[m[32m      self.$search.val('');[m
[32m+[m[32m      self.$search.removeAttr('aria-controls');[m
[32m+[m[32m      self.$search.removeAttr('aria-activedescendant');[m
[32m+[m[32m      self.$search.trigger('focus');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('enable', function () {[m
[32m+[m[32m      self.$search.prop('disabled', false);[m
[32m+[m
[32m+[m[32m      self._transferTabIndex();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('disable', function () {[m
[32m+[m[32m      self.$search.prop('disabled', true);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('focus', function (evt) {[m
[32m+[m[32m      self.$search.trigger('focus');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:focus', function (params) {[m
[32m+[m[32m      if (params.data._resultId) {[m
[32m+[m[32m        self.$search.attr('aria-activedescendant', params.data._resultId);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        self.$search.removeAttr('aria-activedescendant');[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$selection.on('focusin', '.select2-search--inline', function (evt) {[m
[32m+[m[32m      self.trigger('focus', evt);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$selection.on('focusout', '.select2-search--inline', function (evt) {[m
[32m+[m[32m      self._handleBlur(evt);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$selection.on('keydown', '.select2-search--inline', function (evt) {[m
[32m+[m[32m      evt.stopPropagation();[m
[32m+[m
[32m+[m[32m      self.trigger('keypress', evt);[m
[32m+[m
[32m+[m[32m      self._keyUpPrevented = evt.isDefaultPrevented();[m
[32m+[m
[32m+[m[32m      var key = evt.which;[m
[32m+[m
[32m+[m[32m      if (key === KEYS.BACKSPACE && self.$search.val() === '') {[m
[32m+[m[32m        var $previousChoice = self.$searchContainer[m
[32m+[m[32m          .prev('.select2-selection__choice');[m
[32m+[m
[32m+[m[32m        if ($previousChoice.length > 0) {[m
[32m+[m[32m          var item = Utils.GetData($previousChoice[0], 'data');[m
[32m+[m
[32m+[m[32m          self.searchRemoveChoice(item);[m
[32m+[m
[32m+[m[32m          evt.preventDefault();[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$selection.on('click', '.select2-search--inline', function (evt) {[m
[32m+[m[32m      if (self.$search.val()) {[m
[32m+[m[32m        evt.stopPropagation();[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Try to detect the IE version should the `documentMode` property that[m
[32m+[m[32m    // is stored on the document. This is only implemented in IE and is[m
[32m+[m[32m    // slightly cleaner than doing a user agent check.[m
[32m+[m[32m    // This property is not available in Edge, but Edge also doesn't have[m
[32m+[m[32m    // this bug.[m
[32m+[m[32m    var msie = document.documentMode;[m
[32m+[m[32m    var disableInputEvents = msie && msie <= 11;[m
[32m+[m
[32m+[m[32m    // Workaround for browsers which do not support the `input` event[m
[32m+[m[32m    // This will prevent double-triggering of events for browsers which support[m
[32m+[m[32m    // both the `keyup` and `input` events.[m
[32m+[m[32m    this.$selection.on([m
[32m+[m[32m      'input.searchcheck',[m
[32m+[m[32m      '.select2-search--inline',[m
[32m+[m[32m      function (evt) {[m
[32m+[m[32m        // IE will trigger the `input` event when a placeholder is used on a[m
[32m+[m[32m        // search box. To get around this issue, we are forced to ignore all[m
[32m+[m[32m        // `input` events in IE and keep using `keyup`.[m
[32m+[m[32m        if (disableInputEvents) {[m
[32m+[m[32m          self.$selection.off('input.search input.searchcheck');[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Unbind the duplicated `keyup` event[m
[32m+[m[32m        self.$selection.off('keyup.search');[m
[32m+[m[32m      }[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    this.$selection.on([m
[32m+[m[32m      'keyup.search input.search',[m
[32m+[m[32m      '.select2-search--inline',[m
[32m+[m[32m      function (evt) {[m
[32m+[m[32m        // IE will trigger the `input` event when a placeholder is used on a[m
[32m+[m[32m        // search box. To get around this issue, we are forced to ignore all[m
[32m+[m[32m        // `input` events in IE and keep using `keyup`.[m
[32m+[m[32m        if (disableInputEvents && evt.type === 'input') {[m
[32m+[m[32m          self.$selection.off('input.search input.searchcheck');[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var key = evt.which;[m
[32m+[m
[32m+[m[32m        // We can freely ignore events from modifier keys[m
[32m+[m[32m        if (key == KEYS.SHIFT || key == KEYS.CTRL || key == KEYS.ALT) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Tabbing will be handled during the `keydown` phase[m
[32m+[m[32m        if (key == KEYS.TAB) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        self.handleSearch(evt);[m
[32m+[m[32m      }[m
[32m+[m[32m    );[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * This method will transfer the tabindex attribute from the rendered[m
[32m+[m[32m   * selection to the search box. This allows for the search box to be used as[m
[32m+[m[32m   * the primary focus instead of the selection container.[m
[32m+[m[32m   *[m
[32m+[m[32m   * @private[m
[32m+[m[32m   */[m
[32m+[m[32m  Search.prototype._transferTabIndex = function (decorated) {[m
[32m+[m[32m    this.$search.attr('tabindex', this.$selection.attr('tabindex'));[m
[32m+[m[32m    this.$selection.attr('tabindex', '-1');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Search.prototype.createPlaceholder = function (decorated, placeholder) {[m
[32m+[m[32m    this.$search.attr('placeholder', placeholder.text);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Search.prototype.update = function (decorated, data) {[m
[32m+[m[32m    var searchHadFocus = this.$search[0] == document.activeElement;[m
[32m+[m
[32m+[m[32m    this.$search.attr('placeholder', '');[m
[32m+[m
[32m+[m[32m    decorated.call(this, data);[m
[32m+[m
[32m+[m[32m    this.$selection.find('.select2-selection__rendered')[m
[32m+[m[32m                   .append(this.$searchContainer);[m
[32m+[m
[32m+[m[32m    this.resizeSearch();[m
[32m+[m[32m    if (searchHadFocus) {[m
[32m+[m[32m      this.$search.trigger('focus');[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Search.prototype.handleSearch = function () {[m
[32m+[m[32m    this.resizeSearch();[m
[32m+[m
[32m+[m[32m    if (!this._keyUpPrevented) {[m
[32m+[m[32m      var input = this.$search.val();[m
[32m+[m
[32m+[m[32m      this.trigger('query', {[m
[32m+[m[32m        term: input[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this._keyUpPrevented = false;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Search.prototype.searchRemoveChoice = function (decorated, item) {[m
[32m+[m[32m    this.trigger('unselect', {[m
[32m+[m[32m      data: item[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$search.val(item.text);[m
[32m+[m[32m    this.handleSearch();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Search.prototype.resizeSearch = function () {[m
[32m+[m[32m    this.$search.css('width', '25px');[m
[32m+[m
[32m+[m[32m    var width = '';[m
[32m+[m
[32m+[m[32m    if (this.$search.attr('placeholder') !== '') {[m
[32m+[m[32m      width = this.$selection.find('.select2-selection__rendered').width();[m
[32m+[m[32m    } else {[m
[32m+[m[32m      var minimumWidth = this.$search.val().length + 1;[m
[32m+[m
[32m+[m[32m      width = (minimumWidth * 0.75) + 'em';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.$search.css('width', width);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return Search;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/selection/eventRelay',[[m
[32m+[m[32m  'jquery'[m
[32m+[m[32m], function ($) {[m
[32m+[m[32m  function EventRelay () { }[m
[32m+[m
[32m+[m[32m  EventRelay.prototype.bind = function (decorated, container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m[32m    var relayEvents = [[m
[32m+[m[32m      'open', 'opening',[m
[32m+[m[32m      'close', 'closing',[m
[32m+[m[32m      'select', 'selecting',[m
[32m+[m[32m      'unselect', 'unselecting',[m
[32m+[m[32m      'clear', 'clearing'[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    var preventableEvents = [[m
[32m+[m[32m      'opening', 'closing', 'selecting', 'unselecting', 'clearing'[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    decorated.call(this, container, $container);[m
[32m+[m
[32m+[m[32m    container.on('*', function (name, params) {[m
[32m+[m[32m      // Ignore events that should not be relayed[m
[32m+[m[32m      if ($.inArray(name, relayEvents) === -1) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // The parameters should always be an object[m
[32m+[m[32m      params = params || {};[m
[32m+[m
[32m+[m[32m      // Generate the jQuery event for the Select2 event[m
[32m+[m[32m      var evt = $.Event('select2:' + name, {[m
[32m+[m[32m        params: params[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      self.$element.trigger(evt);[m
[32m+[m
[32m+[m[32m      // Only handle preventable events if it was one[m
[32m+[m[32m      if ($.inArray(name, preventableEvents) === -1) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      params.prevented = evt.isDefaultPrevented();[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return EventRelay;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/translation',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  'require'[m
[32m+[m[32m], function ($, require) {[m
[32m+[m[32m  function Translation (dict) {[m
[32m+[m[32m    this.dict = dict || {};[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Translation.prototype.all = function () {[m
[32m+[m[32m    return this.dict;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Translation.prototype.get = function (key) {[m
[32m+[m[32m    return this.dict[key];[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Translation.prototype.extend = function (translation) {[m
[32m+[m[32m    this.dict = $.extend({}, translation.all(), this.dict);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // Static functions[m
[32m+[m
[32m+[m[32m  Translation._cache = {};[m
[32m+[m
[32m+[m[32m  Translation.loadPath = function (path) {[m
[32m+[m[32m    if (!(path in Translation._cache)) {[m
[32m+[m[32m      var translations = require(path);[m
[32m+[m
[32m+[m[32m      Translation._cache[path] = translations;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return new Translation(Translation._cache[path]);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return Translation;[m
[32m+[m[32m});[m
[32m+[m
