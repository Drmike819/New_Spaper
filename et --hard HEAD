[32m+[m[32m  var kind = state.kind;[m
[32m+[m[32m  var index = state.index++;[m
[32m+[m[32m  if (!target || index >= target.length) {[m
[32m+[m[32m    state.target = undefined;[m
[32m+[m[32m    return { value: undefined, done: true };[m
[32m+[m[32m  }[m
[32m+[m[32m  if (kind == 'keys') return { value: index, done: false };[m
[32m+[m[32m  if (kind == 'values') return { value: target[index], done: false };[m
[32m+[m[32m  return { value: [index, target[index]], done: false };[m
