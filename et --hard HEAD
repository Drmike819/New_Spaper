[32m+[m[32mvar CONFIGURABLE_FUNCTION_NAME = FunctionName.CONFIGURABLE;[m
[32m+[m[32mvar IteratorPrototype = IteratorsCore.IteratorPrototype;[m
[32m+[m[32mvar BUGGY_SAFARI_ITERATORS = IteratorsCore.BUGGY_SAFARI_ITERATORS;[m
[32m+[m[32mvar ITERATOR = wellKnownSymbol('iterator');[m
[32m+[m[32mvar KEYS = 'keys';[m
[32m+[m[32mvar VALUES = 'values';[m
[32m+[m[32mvar ENTRIES = 'entries';[m
[32m+[m
[32m+[m[32mvar returnThis = function () { return this; };[m
[32m+[m
[32m+[m[32mmodule.exports = function (Iterable, NAME, IteratorConstructor, next, DEFAULT, IS_SET, FORCED) {[m
[32m+[m[32m  createIteratorConstructor(IteratorConstructor, NAME, next);[m
[32m+[m
[32m+[m[32m  var getIterationMethod = function (KIND) {[m
[32m+[m[32m    if (KIND === DEFAULT && defaultIterator) return defaultIterator;[m
[32m+[m[32m    if (!BUGGY_SAFARI_ITERATORS && KIND in IterablePrototype) return IterablePrototype[KIND];[m
[32m+[m[32m    switch (KIND) {[m
[32m+[m[32m      case KEYS: return function keys() { return new IteratorConstructor(this, KIND); };[m
[32m+[m[32m      case VALUES: return function values() { return new IteratorConstructor(this, KIND); };[m
[32m+[m[32m      case ENTRIES: return function entries() { return new IteratorConstructor(this, KIND); };[m
[32m+[m[32m    } return function () { return new IteratorConstructor(this); };[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  var TO_STRING_TAG = NAME + ' Iterator';[m
[32m+[m[32m  var INCORRECT_VALUES_NAME = false;[m
[32m+[m[32m  var IterablePrototype = Iterable.prototype;[m
[32m+[m[32m  var nativeIterator = IterablePrototype[ITERATOR][m
[32m+[m[32m    || IterablePrototype['@@iterator'][m
[32m+[m[32m    || DEFAULT && IterablePrototype[DEFAULT];[m
[32m+[m[32m  var defaultIterator = !BUGGY_SAFARI_ITERATORS && nativeIterator || getIterationMethod(DEFAULT);[m
[32m+[m[32m  var anyNativeIterator = NAME == 'Array' ? IterablePrototype.entries || nativeIterator : nativeIterator;[m
[32m+[m[32m  var CurrentIteratorPrototype, methods, KEY;[m
[32m+[m
[32m+[m[32m  // fix native[m
[32m+[m[32m  if (anyNativeIterator) {[m
[32m+[m[32m    CurrentIteratorPrototype = getPrototypeOf(anyNativeIterator.call(new Iterable()));[m
[32m+[m[32m    if (CurrentIteratorPrototype !== Object.prototype && CurrentIteratorPrototype.next) {[m
[32m+[m[32m      if (!IS_PURE && getPrototypeOf(CurrentIteratorPrototype) !== IteratorPrototype) {[m
[32m+[m[32m        if (setPrototypeOf) {[m
[32m+[m[32m          setPrototypeOf(CurrentIteratorPrototype, IteratorPrototype);[m
[32m+[m[32m        } else if (!isCallable(CurrentIteratorPrototype[ITERATOR])) {[m
[32m+[m[32m          redefine(CurrentIteratorPrototype, ITERATOR, returnThis);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      // Set @@toStringTag to native iterators[m
[32m+[m[32m      setToStringTag(CurrentIteratorPrototype, TO_STRING_TAG, true, true);[m
[32m+[m[32m      if (IS_PURE) Iterators[TO_STRING_TAG] = returnThis;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // fix Array.prototype.{ values, @@iterator }.name in V8 / FF[m
[32m+[m[32m  if (PROPER_FUNCTION_NAME && DEFAULT == VALUES && nativeIterator && nativeIterator.name !== VALUES) {[m
[32m+[m[32m    if (!IS_PURE && CONFIGURABLE_FUNCTION_NAME) {[m
[32m+[m[32m      createNonEnumerableProperty(IterablePrototype, 'name', VALUES);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      INCORRECT_VALUES_NAME = true;[m
[32m+[m[32m      defaultIterator = function values() { return call(nativeIterator, this); };[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // export additional methods[m
[32m+[m[32m  if (DEFAULT) {[m
[32m+[m[32m    methods = {[m
[32m+[m[32m      values: getIterationMethod(VALUES),[m
[32m+[m[32m      keys: IS_SET ? defaultIterator : getIterationMethod(KEYS),[m
[32m+[m[32m      entries: getIterationMethod(ENTRIES)[m
[32m+[m[32m    };[m
[32m+[m[32m    if (FORCED) for (KEY in methods) {[m
[32m+[m[32m      if (BUGGY_SAFARI_ITERATORS || INCORRECT_VALUES_NAME || !(KEY in IterablePrototype)) {[m
[32m+[m[32m        redefine(IterablePrototype, KEY, methods[KEY]);[m
[32m+[m[32m      }[m
[32m+[m[32m    } else $({ target: NAME, proto: true, forced: BUGGY_SAFARI_ITERATORS || INCORRECT_VALUES_NAME }, methods);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // define iterator[m
[32m+[m[32m  if ((!IS_PURE || FORCED) && IterablePrototype[ITERATOR] !== defaultIterator) {[m
[32m+[m[32m    redefine(IterablePrototype, ITERATOR, defaultIterator, { name: DEFAULT });[m
[32m+[m[32m  }[m
[32m+[m[32m  Iterators[NAME] = defaultIterator;[m
[32m+[m
[32m+[m[32m  return methods;[m
[32m+[m[32m};[m
