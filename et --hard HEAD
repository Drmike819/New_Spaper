[32m+[m
[32m+[m[32m/*![m
[32m+[m[32m * XRegExp 5.1.1[m
[32m+[m[32m * <xregexp.com>[m
[32m+[m[32m * Steven Levithan (c) 2007-present MIT License[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * XRegExp provides augmented, extensible regular expressions. You get additional regex syntax and[m
[32m+[m[32m * flags, beyond what browsers support natively. XRegExp is also a regex utility belt with tools to[m
[32m+[m[32m * make your client-side grepping simpler and more powerful, while freeing you from related[m
[32m+[m[32m * cross-browser inconsistencies.[m
[32m+[m[32m */[m
[32m+[m[32m// ==--------------------------==[m
[32m+[m[32m// Private stuff[m
[32m+[m[32m// ==--------------------------==[m
[32m+[m[32m// Property name used for extended regex instance data[m
[32m+[m[32mvar REGEX_DATA = 'xregexp'; // Optional features that can be installed and uninstalled[m
[32m+[m
[32m+[m[32mvar features = {[m
[32m+[m[32m  astral: false,[m
[32m+[m[32m  namespacing: true[m
[32m+[m[32m}; // Storage for fixed/extended native methods[m
[32m+[m
[32m+[m[32mvar fixed = {}; // Storage for regexes cached by `XRegExp.cache`[m
[32m+[m
[32m+[m[32mvar regexCache = {}; // Storage for pattern details cached by the `XRegExp` constructor[m
[32m+[m
[32m+[m[32mvar patternCache = {}; // Storage for regex syntax tokens added internally or by `XRegExp.addToken`[m
[32m+[m
[32m+[m[32mvar tokens = []; // Token scopes[m
[32m+[m
[32m+[m[32mvar defaultScope = 'default';[m
[32m+[m[32mvar classScope = 'class'; // Regexes that match native regex syntax, including octals[m
[32m+[m
[32m+[m[32mvar nativeTokens = {[m
[32m+[m[32m  // Any native multicharacter token in default scope, or any single character[m
[32m+[m[32m  'default': /\\(?:0(?:[0-3][0-7]{0,2}|[4-7][0-7]?)?|[1-9]\d*|x[\dA-Fa-f]{2}|u(?:[\dA-Fa-f]{4}|{[\dA-Fa-f]+})|c[A-Za-z]|[\s\S])|\(\?(?:[:=!]|<[=!])|[?*+]\?|{\d+(?:,\d*)?}\??|[\s\S]/,[m
[32m+[m[32m  // Any native multicharacter token in character class scope, or any single character[m
[32m+[m[32m  'class': /\\(?:[0-3][0-7]{0,2}|[4-7][0-7]?|x[\dA-Fa-f]{2}|u(?:[\dA-Fa-f]{4}|{[\dA-Fa-f]+})|c[A-Za-z]|[\s\S])|[\s\S]/[m
[32m+[m[32m}; // Any backreference or dollar-prefixed character in replacement strings[m
[32m+[m
[32m+[m[32mvar replacementToken = /\$(?:\{([^\}]+)\}|<([^>]+)>|(\d\d?|[\s\S]?))/g; // Check for correct `exec` handling of nonparticipating capturing groups[m
[32m+[m
[32m+[m[32mvar correctExecNpcg = /()??/.exec('')[1] === undefined; // Check for ES6 `flags` prop support[m
[32m+[m
[32m+[m[32mvar hasFlagsProp = (0, _flags["default"])(/x/) !== undefined;[m
[32m+[m
[32m+[m[32mfunction hasNativeFlag(flag) {[m
[32m+[m[32m  // Can't check based on the presence of properties/getters since browsers might support such[m
[32m+[m[32m  // properties even when they don't support the corresponding flag in regex construction (tested[m
[32m+[m[32m  // in Chrome 48, where `'unicode' in /x/` is true but trying to construct a regex with flag `u`[m
[32m+[m[32m  // throws an error)[m
[32m+[m[32m  var isSupported = true;[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    // Can't use regex literals for testing even in a `try` because regex literals with[m
[32m+[m[32m    // unsupported flags cause a compilation error in IE[m
[32m+[m[32m    new RegExp('', flag); // Work around a broken/incomplete IE11 polyfill for sticky introduced in core-js 3.6.0[m
[32m+[m
[32m+[m[32m    if (flag === 'y') {[m
[32m+[m[32m      // Using function to avoid babel transform to regex literal[m
[32m+[m[32m      var gy = function () {[m
[32m+[m[32m        return 'gy';[m
[32m+[m[32m      }();[m
[32m+[m
[32m+[m[32m      var incompleteY = '.a'.replace(new RegExp('a', gy), '.') === '..';[m
[32m+[m
[32m+[m[32m      if (incompleteY) {[m
[32m+[m[32m        isSupported = false;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  } catch (exception) {[m
[32m+[m[32m    isSupported = false;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return isSupported;[m
[32m+[m[32m} // Check for ES2021 `d` flag support[m
[32m+[m
[32m+[m
[32m+[m[32mvar hasNativeD = hasNativeFlag('d'); // Check for ES2018 `s` flag support[m
[32m+[m
[32m+[m[32mvar hasNativeS = hasNativeFlag('s'); // Check for ES6 `u` flag support[m
[32m+[m
[32m+[m[32mvar hasNativeU = hasNativeFlag('u'); // Check for ES6 `y` flag support[m
[32m+[m
[32m+[m[32mvar hasNativeY = hasNativeFlag('y'); // Tracker for known flags, including addon flags[m
[32m+[m
[32m+[m[32mvar registeredFlags = {[m
[32m+[m[32m  d: hasNativeD,[m
[32m+[m[32m  g: true,[m
[32m+[m[32m  i: true,[m
[32m+[m[32m  m: true,[m
[32m+[m[32m  s: hasNativeS,[m
[32m+[m[32m  u: hasNativeU,[m
[32m+[m[32m  y: hasNativeY[m
[32m+[m[32m}; // Flags to remove when passing to native `RegExp` constructor[m
[32m+[m
[32m+[m[32mvar nonnativeFlags = hasNativeS ? /[^dgimsuy]+/g : /[^dgimuy]+/g;[m
[32m+[m[32m/**[m
[32m+[m[32m * Attaches extended data and `XRegExp.prototype` properties to a regex object.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {RegExp} regex Regex to augment.[m
[32m+[m[32m * @param {Array} captureNames Array with capture names, or `null`.[m
[32m+[m[32m * @param {String} xSource XRegExp pattern used to generate `regex`, or `null` if N/A.[m
[32m+[m[32m * @param {String} xFlags XRegExp flags used to generate `regex`, or `null` if N/A.[m
[32m+[m[32m * @param {Boolean} [isInternalOnly=false] Whether the regex will be used only for internal[m
[32m+[m[32m *   operations, and never exposed to users. For internal-only regexes, we can improve perf by[m
[32m+[m[32m *   skipping some operations like attaching `XRegExp.prototype` properties.[m
[32m+[m[32m * @returns {!RegExp} Augmented regex.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mfunction augment(regex, captureNames, xSource, xFlags, isInternalOnly) {[m
[32m+[m[32m  var _context;[m
[32m+[m
[32m+[m[32m  regex[REGEX_DATA] = {[m
[32m+[m[32m    captureNames: captureNames[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  if (isInternalOnly) {[m
[32m+[m[32m    return regex;[m
[32m+[m[32m  } // Can't auto-inherit these since the XRegExp constructor returns a nonprimitive value[m
[32m+[m
[32m+[m
[32m+[m[32m  if (regex.__proto__) {[m
[32m+[m[32m    regex.__proto__ = XRegExp.prototype;[m
[32m+[m[32m  } else {[m
[32m+[m[32m    for (var p in XRegExp.prototype) {[m
[32m+[m[32m      // An `XRegExp.prototype.hasOwnProperty(p)` check wouldn't be worth it here, since this[m
[32m+[m[32m      // is performance sensitive, and enumerable `Object.prototype` or `RegExp.prototype`[m
[32m+[m[32m      // extensions exist on `regex.prototype` anyway[m
[32m+[m[32m      regex[p] = XRegExp.prototype[p];[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  regex[REGEX_DATA].source = xSource; // Emulate the ES6 `flags` prop by ensuring flags are in alphabetical order[m
[32m+[m
[32m+[m[32m  regex[REGEX_DATA].flags = xFlags ? (0, _sort["default"])(_context = xFlags.split('')).call(_context).join('') : xFlags;[m
[32m+[m[32m  return regex;[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Removes any duplicate characters from the provided string.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {String} str String to remove duplicate characters from.[m
[32m+[m[32m * @returns {string} String with any duplicate characters removed.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction clipDuplicates(str) {[m
[32m+[m[32m  return str.replace(/([\s\S])(?=[\s\S]*\1)/g, '');[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Copies a regex object while preserving extended data and augmenting with `XRegExp.prototype`[m
[32m+[m[32m * properties. The copy has a fresh `lastIndex` property (set to zero). Allows adding and removing[m
[32m+[m[32m * flags g and y while copying the regex.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {RegExp} regex Regex to copy.[m
[32m+[m[32m * @param {Object} [options] Options object with optional properties:[m
[32m+[m[32m *   - `addG` {Boolean} Add flag g while copying the regex.[m
[32m+[m[32m *   - `addY` {Boolean} Add flag y while copying the regex.[m
[32m+[m[32m *   - `removeG` {Boolean} Remove flag g while copying the regex.[m
[32m+[m[32m *   - `removeY` {Boolean} Remove flag y while copying the regex.[m
[32m+[m[32m *   - `isInternalOnly` {Boolean} Whether the copied regex will be used only for internal[m
[32m+[m[32m *     operations, and never exposed to users. For internal-only regexes, we can improve perf by[m
[32m+[m[32m *     skipping some operations like attaching `XRegExp.prototype` properties.[m
[32m+[m[32m *   - `source` {String} Overrides `<regex>.source`, for special cases.[m
[32m+[m[32m * @returns {RegExp} Copy of the provided regex, possibly with modified flags.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction copyRegex(regex, options) {[m
[32m+[m[32m  var _context2;[m
[32m+[m
[32m+[m[32m  if (!XRegExp.isRegExp(regex)) {[m
[32m+[m[32m    throw new TypeError('Type RegExp expected');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var xData = regex[REGEX_DATA] || {};[m
[32m+[m[32m  var flags = getNativeFlags(regex);[m
[32m+[m[32m  var flagsToAdd = '';[m
[32m+[m[32m  var flagsToRemove = '';[m
[32m+[m[32m  var xregexpSource = null;[m
[32m+[m[32m  var xregexpFlags = null;[m
[32m+[m[32m  options = options || {};[m
[32m+[m
[32m+[m[32m  if (options.removeG) {[m
[32m+[m[32m    flagsToRemove += 'g';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (options.removeY) {[m
[32m+[m[32m    flagsToRemove += 'y';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (flagsToRemove) {[m
[32m+[m[32m    flags = flags.replace(new RegExp("[".concat(flagsToRemove, "]+"), 'g'), '');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (options.addG) {[m
[32m+[m[32m    flagsToAdd += 'g';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (options.addY) {[m
[32m+[m[32m    flagsToAdd += 'y';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (flagsToAdd) {[m
[32m+[m[32m    flags = clipDuplicates(flags + flagsToAdd);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!options.isInternalOnly) {[m
[32m+[m[32m    if (xData.source !== undefined) {[m
[32m+[m[32m      xregexpSource = xData.source;[m
[32m+[m[32m    } // null or undefined; don't want to add to `flags` if the previous value was null, since[m
[32m+[m[32m    // that indicates we're not tracking original precompilation flags[m
[32m+[m
[32m+[m
[32m+[m[32m    if ((0, _flags["default"])(xData) != null) {[m
[32m+[m[32m      // Flags are only added for non-internal regexes by `XRegExp.globalize`. Flags are never[m
[32m+[m[32m      // removed for non-internal regexes, so don't need to handle it[m
[32m+[m[32m      xregexpFlags = flagsToAdd ? clipDuplicates((0, _flags["default"])(xData) + flagsToAdd) : (0, _flags["default"])(xData);[m
[32m+[m[32m    }[m
[32m+[m[32m  } // Augment with `XRegExp.prototype` properties, but use the native `RegExp` constructor to avoid[m
[32m+[m[32m  // searching for special tokens. That would be wrong for regexes constructed by `RegExp`, and[m
[32m+[m[32m  // unnecessary for regexes constructed by `XRegExp` because the regex has already undergone the[m
[32m+[m[32m  // translation to native regex syntax[m
[32m+[m
[32m+[m
[32m+[m[32m  regex = augment(new RegExp(options.source || regex.source, flags), hasNamedCapture(regex) ? (0, _slice["default"])(_context2 = xData.captureNames).call(_context2, 0) : null, xregexpSource, xregexpFlags, options.isInternalOnly);[m
[32m+[m[32m  return regex;[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Converts hexadecimal to decimal.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {String} hex[m
[32m+[m[32m * @returns {number}[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction dec(hex) {[m
[32m+[m[32m  return (0, _parseInt2["default"])(hex, 16);[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns a pattern that can be used in a native RegExp in place of an ignorable token such as an[m
[32m+[m[32m * inline comment or whitespace with flag x. This is used directly as a token handler function[m
[32m+[m[32m * passed to `XRegExp.addToken`.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {String} match Match arg of `XRegExp.addToken` handler[m
[32m+[m[32m * @param {String} scope Scope arg of `XRegExp.addToken` handler[m
[32m+[m[32m * @param {String} flags Flags arg of `XRegExp.addToken` handler[m
[32m+[m[32m * @returns {string} Either '' or '(?:)', depending on which is needed in the context of the match.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction getContextualTokenSeparator(match, scope, flags) {[m
[32m+[m[32m  var matchEndPos = match.index + match[0].length;[m
[32m+[m[32m  var precedingChar = match.input[match.index - 1];[m
[32m+[m[32m  var followingChar = match.input[matchEndPos];[m
[32m+[m
[32m+[m[32m  if ( // No need to separate tokens if at the beginning or end of a group, before or after a[m
[32m+[m[32m  // group, or before or after a `|`[m
[32m+[m[32m  /^[()|]$/.test(precedingChar) || /^[()|]$/.test(followingChar) || // No need to separate tokens if at the beginning or end of the pattern[m
[32m+[m[32m  match.index === 0 || matchEndPos === match.input.length || // No need to separate tokens if at the beginning of a noncapturing group or lookaround.[m
[32m+[m[32m  // Looks only at the last 4 chars (at most) for perf when constructing long regexes.[m
[32m+[m[32m  /\(\?(?:[:=!]|<[=!])$/.test(match.input.substring(match.index - 4, match.index)) || // Avoid separating tokens when the following token is a quantifier[m
[32m+[m[32m  isQuantifierNext(match.input, matchEndPos, flags)) {[m
[32m+[m[32m    return '';[m
[32m+[m[32m  } // Keep tokens separated. This avoids e.g. inadvertedly changing `\1 1` or `\1(?#)1` to `\11`.[m
[32m+[m[32m  // This also ensures all tokens remain as discrete atoms, e.g. it prevents converting the[m
[32m+[m[32m  // syntax error `(? :` into `(?:`.[m
[32m+[m
[32m+[m
[32m+[m[32m  return '(?:)';[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns native `RegExp` flags used by a regex object.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {RegExp} regex Regex to check.[m
[32m+[m[32m * @returns {string} Native flags in use.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction getNativeFlags(regex) {[m
[32m+[m[32m  return hasFlagsProp ? (0, _flags["default"])(regex) : // Explicitly using `RegExp.prototype.toString` (rather than e.g. `String` or concatenation[m
[32m+[m[32m  // with an empty string) allows this to continue working predictably when[m
[32m+[m[32m  // `XRegExp.proptotype.toString` is overridden[m
[32m+[m[32m  /\/([a-z]*)$/i.exec(RegExp.prototype.toString.call(regex))[1];[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Determines whether a regex has extended instance data used to track capture names.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {RegExp} regex Regex to check.[m
[32m+[m[32m * @returns {boolean} Whether the regex uses named capture.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction hasNamedCapture(regex) {[m
[32m+[m[32m  return !!(regex[REGEX_DATA] && regex[REGEX_DATA].captureNames);[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Converts decimal to hexadecimal.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {Number|String} dec[m
[32m+[m[32m * @returns {string}[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction hex(dec) {[m
[32m+[m[32m  return (0, _parseInt2["default"])(dec, 10).toString(16);[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Checks whether the next nonignorable token after the specified position is a quantifier.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {String} pattern Pattern to search within.[m
[32m+[m[32m * @param {Number} pos Index in `pattern` to search at.[m
[32m+[m[32m * @param {String} flags Flags used by the pattern.[m
[32m+[m[32m * @returns {Boolean} Whether the next nonignorable token is a quantifier.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction isQuantifierNext(pattern, pos, flags) {[m
[32m+[m[32m  var inlineCommentPattern = '\\(\\?#[^)]*\\)';[m
[32m+[m[32m  var lineCommentPattern = '#[^#\\n]*';[m
[32m+[m[32m  var quantifierPattern = '[?*+]|{\\d+(?:,\\d*)?}';[m
[32m+[m[32m  var regex = (0, _indexOf["default"])(flags).call(flags, 'x') !== -1 ? // Ignore any leading whitespace, line comments, and inline comments[m
[32m+[m[32m  /^(?:\s|#[^#\n]*|\(\?#[^)]*\))*(?:[?*+]|{\d+(?:,\d*)?})/ : // Ignore any leading inline comments[m
[32m+[m[32m  /^(?:\(\?#[^)]*\))*(?:[?*+]|{\d+(?:,\d*)?})/;[m
[32m+[m[32m  return regex.test((0, _slice["default"])(pattern).call(pattern, pos));[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Determines whether a value is of the specified type, by resolving its internal [[Class]].[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {*} value Object to check.[m
[32m+[m[32m * @param {String} type Type to check for, in TitleCase.[m
[32m+[m[32m * @returns {boolean} Whether the object matches the type.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction isType(value, type) {[m
[32m+[m[32m  return Object.prototype.toString.call(value) === "[object ".concat(type, "]");[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns the object, or throws an error if it is `null` or `undefined`. This is used to follow[m
[32m+[m[32m * the ES5 abstract operation `ToObject`.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {*} value Object to check and return.[m
[32m+[m[32m * @returns {*} The provided object.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction nullThrows(value) {[m
[32m+[m[32m  // null or undefined[m
[32m+[m[32m  if (value == null) {[m
[32m+[m[32m    throw new TypeError('Cannot convert null or undefined to object');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return value;[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Adds leading zeros if shorter than four characters. Used for fixed-length hexadecimal values.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {String} str[m
[32m+[m[32m * @returns {string}[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction pad4(str) {[m
[32m+[m[32m  while (str.length < 4) {[m
[32m+[m[32m    str = "0".concat(str);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return str;[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Checks for flag-related errors, and strips/applies flags in a leading mode modifier. Offloads[m
[32m+[m[32m * the flag preparation logic from the `XRegExp` constructor.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {String} pattern Regex pattern, possibly with a leading mode modifier.[m
[32m+[m[32m * @param {String} flags Any combination of flags.[m
[32m+[m[32m * @returns {!Object} Object with properties `pattern` and `flags`.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction prepareFlags(pattern, flags) {[m
[32m+[m[32m  // Recent browsers throw on duplicate flags, so copy this behavior for nonnative flags[m
[32m+[m[32m  if (clipDuplicates(flags) !== flags) {[m
[32m+[m[32m    throw new SyntaxError("Invalid duplicate regex flag ".concat(flags));[m
[32m+[m[32m  } // Strip and apply a leading mode modifier with any combination of flags except `dgy`[m
[32m+[m
[32m+[m
[32m+[m[32m  pattern = pattern.replace(/^\(\?([\w$]+)\)/, function ($0, $1) {[m
[32m+[m[32m    if (/[dgy]/.test($1)) {[m
[32m+[m[32m      throw new SyntaxError("Cannot use flags dgy in mode modifier ".concat($0));[m
[32m+[m[32m    } // Allow duplicate flags within the mode modifier[m
[32m+[m
[32m+[m
[32m+[m[32m    flags = clipDuplicates(flags + $1);[m
[32m+[m[32m    return '';[m
[32m+[m[32m  }); // Throw on unknown native or nonnative flags[m
[32m+[m
[32m+[m[32m  var _iterator = _createForOfIteratorHelper(flags),[m
[32m+[m[32m      _step;[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    for (_iterator.s(); !(_step = _iterator.n()).done;) {[m
[32m+[m[32m      var flag = _step.value;[m
[32m+[m
[32m+[m[32m      if (!registeredFlags[flag]) {[m
[32m+[m[32m        throw new SyntaxError("Unknown regex flag ".concat(flag));[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    _iterator.e(err);[m
[32m+[m[32m  } finally {[m
[32m+[m[32m    _iterator.f();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    pattern: pattern,[m
[32m+[m[32m    flags: flags[m
[32m+[m[32m  };[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Prepares an options object from the given value.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {String|Object} value Value to convert to an options object.[m
[32m+[m[32m * @returns {Object} Options object.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction prepareOptions(value) {[m
[32m+[m[32m  var options = {};[m
[32m+[m
[32m+[m[32m  if (isType(value, 'String')) {[m
[32m+[m[32m    (0, _forEach["default"])(XRegExp).call(XRegExp, value, /[^\s,]+/, function (match) {[m
[32m+[m[32m      options[match] = true;[m
[32m+[m[32m    });[m
[32m+[m[32m    return options;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return value;[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Registers a flag so it doesn't throw an 'unknown flag' error.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {String} flag Single-character flag to register.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction registerFlag(flag) {[m
[32m+[m[32m  if (!/^[\w$]$/.test(flag)) {[m
[32m+[m[32m    throw new Error('Flag must be a single character A-Za-z0-9_$');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  registeredFlags[flag] = true;[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Runs built-in and custom regex syntax tokens in reverse insertion order at the specified[m
[32m+[m[32m * position, until a match is found.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {String} pattern Original pattern from which an XRegExp object is being built.[m
[32m+[m[32m * @param {String} flags Flags being used to construct the regex.[m
[32m+[m[32m * @param {Number} pos Position to search for tokens within `pattern`.[m
[32m+[m[32m * @param {Number} scope Regex scope to apply: 'default' or 'class'.[m
[32m+[m[32m * @param {Object} context Context object to use for token handler functions.[m
[32m+[m[32m * @returns {Object} Object with properties `matchLength`, `output`, and `reparse`; or `null`.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction runTokens(pattern, flags, pos, scope, context) {[m
[32m+[m[32m  var i = tokens.length;[m
[32m+[m[32m  var leadChar = pattern[pos];[m
[32m+[m[32m  var result = null;[m
[32m+[m[32m  var match;[m
[32m+[m[32m  var t; // Run in reverse insertion order[m
[32m+[m
[32m+[m[32m  while (i--) {[m
[32m+[m[32m    t = tokens[i];[m
[32m+[m
[32m+[m[32m    if (t.leadChar && t.leadChar !== leadChar || t.scope !== scope && t.scope !== 'all' || t.flag && !((0, _indexOf["default"])(flags).call(flags, t.flag) !== -1)) {[m
[32m+[m[32m      continue;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    match = XRegExp.exec(pattern, t.regex, pos, 'sticky');[m
[32m+[m
[32m+[m[32m    if (match) {[m
[32m+[m[32m      result = {[m
[32m+[m[32m        matchLength: match[0].length,[m
[32m+[m[32m        output: t.handler.call(context, match, scope, flags),[m
[32m+[m[32m        reparse: t.reparse[m
[32m+[m[32m      }; // Finished with token tests[m
[32m+[m
[32m+[m[32m      break;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return result;[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Enables or disables implicit astral mode opt-in. When enabled, flag A is automatically added to[m
[32m+[m[32m * all new regexes created by XRegExp. This causes an error to be thrown when creating regexes if[m
[32m+[m[32m * the Unicode Base addon is not available, since flag A is registered by that addon.[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {Boolean} on `true` to enable; `false` to disable.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction setAstral(on) {[m
[32m+[m[32m  features.astral = on;[m
[32m+[m[32m}[m
[32m+[m[32m/**[m
[32m+[m[32m * Adds named capture groups to the `groups` property of match arrays. See here for details:[m
[32m+[m[32m * https://github.com/tc39/proposal-regexp-named-groups[m
[32m+[m[32m *[m
[32m+[m[32m * @private[m
[32m+[m[32m * @param {Boolean} on `true` to enable; `false` to disable.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction setNamespacing(on) {[m
[32m+[m[32m  features.namespacing = on;[m
[32m+[m[32m} // ==--------------------------==[m
[32m+[m[32m// Constructor[m
[32m+[m[32m// ==--------------------------==[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Creates an extended regular expression object for matching text with a pattern. Differs from a[m
[32m+[m[32m * native regular expression in that additional syntax and flags are supported. The returned object[m
[32m+[m[32m * is in fact a native `RegExp` and works with all native methods.[m
[32m+[m[32m *[m
[32m+[m[32m * @class XRegExp[m
[32m+[m[32m * @constructor[m
[32m+[m[32m * @param {String|RegExp} pattern Regex pattern string, or an existing regex object to copy.[m
[32m+[m[32m * @param {String} [flags] Any combination of flags.[m
[32m+[m[32m *   Native flags:[m
[32m+[m[32m *     - `d` - indices for capturing groups (ES2021)[m
[32m+[m[32m *     - `g` - global[m
[32m+[m[32m *     - `i` - ignore case[m
[32m+[m[32m *     - `m` - multiline anchors[m
[32m+[m[32m *     - `u` - unicode (ES6)[m
[32m+[m[32m *     - `y` - sticky (Firefox 3+, ES6)[m
[32m+[m[32m *   Additional XRegExp flags:[m
[32m+[m[32m *     - `n` - named capture only[m
[32m+[m[32m *     - `s` - dot matches all (aka singleline) - works even when not natively supported[m
[32m+[m[32m *     - `x` - free-spacing and line comments (aka extended)[m
[32m+[m[32m *     - `A` - 21-bit Unicode properties (aka astral) - requires the Unicode Base addon[m
[32m+[m[32m *   Flags cannot be provided when constructing one `RegExp` from another.[m
[32m+[m[32m * @returns {RegExp} Extended regular expression object.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * // With named capture and flag x[m
[32m+[m[32m * XRegExp(`(?<year>  [0-9]{4} ) -?  # year[m
[32m+[m[32m *          (?<month> [0-9]{2} ) -?  # month[m
[32m+[m[32m *          (?<day>   [0-9]{2} )     # day`, 'x');[m
[32m+[m[32m *[m
[32m+[m[32m * // Providing a regex object copies it. Native regexes are recompiled using native (not XRegExp)[m
[32m+[m[32m * // syntax. Copies maintain extended data, are augmented with `XRegExp.prototype` properties, and[m
[32m+[m[32m * // have fresh `lastIndex` properties (set to zero).[m
[32m+[m[32m * XRegExp(/regex/);[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction XRegExp(pattern, flags) {[m
[32m+[m[32m  if (XRegExp.isRegExp(pattern)) {[m
[32m+[m[32m    if (flags !== undefined) {[m
[32m+[m[32m      throw new TypeError('Cannot supply flags when copying a RegExp');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return copyRegex(pattern);[m
[32m+[m[32m  } // Copy the argument behavior of `RegExp`[m
[32m+[m
[32m+[m
[32m+[m[32m  pattern = pattern === undefined ? '' : String(pattern);[m
[32m+[m[32m  flags = flags === undefined ? '' : String(flags);[m
[32m+[m
[32m+[m[32m  if (XRegExp.isInstalled('astral') && !((0, _indexOf["default"])(flags).call(flags, 'A') !== -1)) {[m
[32m+[m[32m    // This causes an error to be thrown if the Unicode Base addon is not available[m
[32m+[m[32m    flags += 'A';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!patternCache[pattern]) {[m
[32m+[m[32m    patternCache[pattern] = {};[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!patternCache[pattern][flags]) {[m
[32m+[m[32m    var context = {[m
[32m+[m[32m      hasNamedCapture: false,[m
[32m+[m[32m      captureNames: [][m
[32m+[m[32m    };[m
[32m+[m[32m    var scope = defaultScope;[m
[32m+[m[32m    var output = '';[m
[32m+[m[32m    var pos = 0;[m
[32m+[m[32m    var result; // Check for flag-related errors, and strip/apply flags in a leading mode modifier[m
[32m+[m
[32m+[m[32m    var applied = prepareFlags(pattern, flags);[m
[32m+[m[32m    var appliedPattern = applied.pattern;[m
[32m+[m[32m    var appliedFlags = (0, _flags["default"])(applied); // Use XRegExp's tokens to translate the pattern to a native regex pattern.[m
[32m+[m[32m    // `appliedPattern.length` may change on each iteration if tokens use `reparse`[m
[32m+[m
[32m+[m[32m    while (pos < appliedPattern.length) {[m
[32m+[m[32m      do {[m
[32m+[m[32m        // Check for custom tokens at the current position[m
[32m+[m[32m        result = runTokens(appliedPattern, appliedFlags, pos, scope, context); // If the matched token used the `reparse` option, splice its output into the[m
[32m+[m[32m        // pattern before running tokens again at the same position[m
[32m+[m
[32m+[m[32m        if (result && result.reparse) {[m
[32m+[m[32m          appliedPattern = (0, _slice["default"])(appliedPattern).call(appliedPattern, 0, pos) + result.output + (0, _slice["default"])(appliedPattern).call(appliedPattern, pos + result.matchLength);[m
[32m+[m[32m        }[m
[32m+[m[32m      } while (result && result.reparse);[m
[32m+[m
[32m+[m[32m      if (result) {[m
[32m+[m[32m        output += result.output;[m
[32m+[m[32m        pos += result.matchLength || 1;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        // Get the native token at the current position[m
[32m+[m[32m        var _XRegExp$exec = XRegExp.exec(appliedPattern, nativeTokens[scope], pos, 'sticky'),[m
[32m+[m[32m            _XRegExp$exec2 = (0, _slicedToArray2["default"])(_XRegExp$exec, 1),[m
[32m+[m[32m            token = _XRegExp$exec2[0];[m
[32m+[m
[32m+[m[32m        output += token;[m
[32m+[m[32m        pos += token.length;[m
[32m+[m
[32m+[m[32m        if (token === '[' && scope === defaultScope) {[m
[32m+[m[32m          scope = classScope;[m
[32m+[m[32m        } else if (token === ']' && scope === classScope) {[m
[32m+[m[32m          scope = defaultScope;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    patternCache[pattern][flags] = {[m
[32m+[m[32m      // Use basic cleanup to collapse repeated empty groups like `(?:)(?:)` to `(?:)`. Empty[m
[32m+[m[32m      // groups are sometimes inserted during regex transpilation in order to keep tokens[m
[32m+[m[32m      // separated. However, more than one empty group in a row is never needed.[m
[32m+[m[32m      pattern: output.replace(/(?:\(\?:\))+/g, '(?:)'),[m
[32m+[m[32m      // Strip all but native flags[m
[32m+[m[32m      flags: appliedFlags.replace(nonnativeFlags, ''),[m
[32m+[m[32m      // `context.captureNames` has an item for each capturing group, even if unnamed[m
[32m+[m[32m      captures: context.hasNamedCapture ? context.captureNames : null[m
[32m+[m[32m    };[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var generated = patternCache[pattern][flags];[m
[32m+[m[32m  return augment(new RegExp(generated.pattern, (0, _flags["default"])(generated)), generated.captures, pattern, flags);[m
[32m+[m[32m} // Add `RegExp.prototype` to the prototype chain[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.prototype = /(?:)/; // ==--------------------------==[m
[32m+[m[32m// Public properties[m
[32m+[m[32m// ==--------------------------==[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * The XRegExp version number as a string containing three dot-separated parts. For example,[m
[32m+[m[32m * '2.0.0-beta-3'.[m
[32m+[m[32m *[m
[32m+[m[32m * @static[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @type String[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mXRegExp.version = '5.1.1'; // ==--------------------------==[m
[32m+[m[32m// Public methods[m
[32m+[m[32m// ==--------------------------==[m
[32m+[m[32m// Intentionally undocumented; used in tests and addons[m
[32m+[m
[32m+[m[32mXRegExp._clipDuplicates = clipDuplicates;[m
[32m+[m[32mXRegExp._hasNativeFlag = hasNativeFlag;[m
[32m+[m[32mXRegExp._dec = dec;[m
[32m+[m[32mXRegExp._hex = hex;[m
[32m+[m[32mXRegExp._pad4 = pad4;[m
[32m+[m[32m/**[m
[32m+[m[32m * Extends XRegExp syntax and allows custom flags. This is used internally and can be used to[m
[32m+[m[32m * create XRegExp addons. If more than one token can match the same string, the last added wins.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {RegExp} regex Regex object that matches the new token.[m
[32m+[m[32m * @param {Function} handler Function that returns a new pattern string (using native regex syntax)[m
[32m+[m[32m *   to replace the matched token within all future XRegExp regexes. Has access to persistent[m
[32m+[m[32m *   properties of the regex being built, through `this`. Invoked with three arguments:[m
[32m+[m[32m *   - The match array, with named backreference properties.[m
[32m+[m[32m *   - The regex scope where the match was found: 'default' or 'class'.[m
[32m+[m[32m *   - The flags used by the regex, including any flags in a leading mode modifier.[m
[32m+[m[32m *   The handler function becomes part of the XRegExp construction process, so be careful not to[m
[32m+[m[32m *   construct XRegExps within the function or you will trigger infinite recursion.[m
[32m+[m[32m * @param {Object} [options] Options object with optional properties:[m
[32m+[m[32m *   - `scope` {String} Scope where the token applies: 'default', 'class', or 'all'.[m
[32m+[m[32m *   - `flag` {String} Single-character flag that triggers the token. This also registers the[m
[32m+[m[32m *     flag, which prevents XRegExp from throwing an 'unknown flag' error when the flag is used.[m
[32m+[m[32m *   - `optionalFlags` {String} Any custom flags checked for within the token `handler` that are[m
[32m+[m[32m *     not required to trigger the token. This registers the flags, to prevent XRegExp from[m
[32m+[m[32m *     throwing an 'unknown flag' error when any of the flags are used.[m
[32m+[m[32m *   - `reparse` {Boolean} Whether the `handler` function's output should not be treated as[m
[32m+[m[32m *     final, and instead be reparseable by other tokens (including the current token). Allows[m
[32m+[m[32m *     token chaining or deferring.[m
[32m+[m[32m *   - `leadChar` {String} Single character that occurs at the beginning of any successful match[m
[32m+[m[32m *     of the token (not always applicable). This doesn't change the behavior of the token unless[m
[32m+[m[32m *     you provide an erroneous value. However, providing it can increase the token's performance[m
[32m+[m[32m *     since the token can be skipped at any positions where this character doesn't appear.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * // Basic usage: Add \a for the ALERT control code[m
[32m+[m[32m * XRegExp.addToken([m
[32m+[m[32m *   /\\a/,[m
[32m+[m[32m *   () => '\\x07',[m
[32m+[m[32m *   {scope: 'all'}[m
[32m+[m[32m * );[m
[32m+[m[32m * XRegExp('\\a[\\a-\\n]+').test('\x07\n\x07'); // -> true[m
[32m+[m[32m *[m
[32m+[m[32m * // Add the U (ungreedy) flag from PCRE and RE2, which reverses greedy and lazy quantifiers.[m
[32m+[m[32m * // Since `scope` is not specified, it uses 'default' (i.e., transformations apply outside of[m
[32m+[m[32m * // character classes only)[m
[32m+[m[32m * XRegExp.addToken([m
[32m+[m[32m *   /([?*+]|{\d+(?:,\d*)?})(\??)/,[m
[32m+[m[32m *   (match) => `${match[1]}${match[2] ? '' : '?'}`,[m
[32m+[m[32m *   {flag: 'U'}[m
[32m+[m[32m * );[m
[32m+[m[32m * XRegExp('a+', 'U').exec('aaa')[0]; // -> 'a'[m
[32m+[m[32m * XRegExp('a+?', 'U').exec('aaa')[0]; // -> 'aaa'[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mXRegExp.addToken = function (regex, handler, options) {[m
[32m+[m[32m  options = options || {};[m
[32m+[m[32m  var _options = options,[m
[32m+[m[32m      optionalFlags = _options.optionalFlags;[m
[32m+[m
[32m+[m[32m  if (options.flag) {[m
[32m+[m[32m    registerFlag(options.flag);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (optionalFlags) {[m
[32m+[m[32m    optionalFlags = optionalFlags.split('');[m
[32m+[m
[32m+[m[32m    var _iterator2 = _createForOfIteratorHelper(optionalFlags),[m
[32m+[m[32m        _step2;[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m      for (_iterator2.s(); !(_step2 = _iterator2.n()).done;) {[m
[32m+[m[32m        var flag = _step2.value;[m
[32m+[m[32m        registerFlag(flag);[m
[32m+[m[32m      }[m
[32m+[m[32m    } catch (err) {[m
[32m+[m[32m      _iterator2.e(err);[m
[32m+[m[32m    } finally {[m
[32m+[m[32m      _iterator2.f();[m
[32m+[m[32m    }[m
[32m+[m[32m  } // Add to the private list of syntax tokens[m
[32m+[m
[32m+[m
[32m+[m[32m  tokens.push({[m
[32m+[m[32m    regex: copyRegex(regex, {[m
[32m+[m[32m      addG: true,[m
[32m+[m[32m      addY: hasNativeY,[m
[32m+[m[32m      isInternalOnly: true[m
[32m+[m[32m    }),[m
[32m+[m[32m    handler: handler,[m
[32m+[m[32m    scope: options.scope || defaultScope,[m
[32m+[m[32m    flag: options.flag,[m
[32m+[m[32m    reparse: options.reparse,[m
[32m+[m[32m    leadChar: options.leadChar[m
[32m+[m[32m  }); // Reset the pattern cache used by the `XRegExp` constructor, since the same pattern and flags[m
[32m+[m[32m  // might now produce different results[m
[32m+[m
[32m+[m[32m  XRegExp.cache.flush('patterns');[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Caches and returns the result of calling `XRegExp(pattern, flags)`. On any subsequent call with[m
[32m+[m[32m * the same pattern and flag combination, the cached copy of the regex is returned.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {String} pattern Regex pattern string.[m
[32m+[m[32m * @param {String} [flags] Any combination of XRegExp flags.[m
[32m+[m[32m * @returns {RegExp} Cached XRegExp object.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * let match;[m
[32m+[m[32m * while (match = XRegExp.cache('.', 'gs').exec('abc')) {[m
[32m+[m[32m *   // The regex is compiled once only[m
[32m+[m[32m * }[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.cache = function (pattern, flags) {[m
[32m+[m[32m  if (!regexCache[pattern]) {[m
[32m+[m[32m    regexCache[pattern] = {};[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return regexCache[pattern][flags] || (regexCache[pattern][flags] = XRegExp(pattern, flags));[m
[32m+[m[32m}; // Intentionally undocumented; used in tests[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.cache.flush = function (cacheName) {[m
[32m+[m[32m  if (cacheName === 'patterns') {[m
[32m+[m[32m    // Flush the pattern cache used by the `XRegExp` constructor[m
[32m+[m[32m    patternCache = {};[m
[32m+[m[32m  } else {[m
[32m+[m[32m    // Flush the regex cache populated by `XRegExp.cache`[m
[32m+[m[32m    regexCache = {};[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Escapes any regular expression metacharacters, for use when matching literal strings. The result[m
[32m+[m[32m * can safely be used at any position within a regex that uses any flags.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {String} str String to escape.[m
[32m+[m[32m * @returns {string} String with regex metacharacters escaped.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * XRegExp.escape('Escaped? <.>');[m
[32m+[m[32m * // -> 'Escaped\?\u0020<\.>'[m
[32m+[m[32m */[m
[32m+[m[32m// Following are the contexts where each metacharacter needs to be escaped because it would[m
[32m+[m[32m// otherwise have a special meaning, change the meaning of surrounding characters, or cause an[m
[32m+[m[32m// error. Context 'default' means outside character classes only.[m
[32m+[m[32m// - `\` - context: all[m
[32m+[m[32m// - `[()*+?.$|` - context: default[m
[32m+[m[32m// - `]` - context: default with flag u or if forming the end of a character class[m
[32m+[m[32m// - `{}` - context: default with flag u or if part of a valid/complete quantifier pattern[m
[32m+[m[32m// - `,` - context: default if in a position that causes an unescaped `{` to turn into a quantifier.[m
[32m+[m[32m//   Ex: `/^a{1\,2}$/` matches `'a{1,2}'`, but `/^a{1,2}$/` matches `'a'` or `'aa'`[m
[32m+[m[32m// - `#` and <whitespace> - context: default with flag x[m
[32m+[m[32m// - `^` - context: default, and context: class if it's the first character in the class[m
[32m+[m[32m// - `-` - context: class if part of a valid character class range[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.escape = function (str) {[m
[32m+[m[32m  return String(nullThrows(str)). // Escape most special chars with a backslash[m
[32m+[m[32m  replace(/[\\\[\]{}()*+?.^$|]/g, '\\$&'). // Convert to \uNNNN for special chars that can't be escaped when used with ES6 flag `u`[m
[32m+[m[32m  replace(/[\s#\-,]/g, function (match) {[m
[32m+[m[32m    return "\\u".concat(pad4(hex(match.charCodeAt(0))));[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Executes a regex search in a specified string. Returns a match array or `null`. If the provided[m
[32m+[m[32m * regex uses named capture, named capture properties are included on the match array's `groups`[m
[32m+[m[32m * property. Optional `pos` and `sticky` arguments specify the search start position, and whether[m
[32m+[m[32m * the match must start at the specified position only. The `lastIndex` property of the provided[m
[32m+[m[32m * regex is not used, but is updated for compatibility. Also fixes browser bugs compared to the[m
[32m+[m[32m * native `RegExp.prototype.exec` and can be used reliably cross-browser.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {String} str String to search.[m
[32m+[m[32m * @param {RegExp} regex Regex to search with.[m
[32m+[m[32m * @param {Number} [pos=0] Zero-based index at which to start the search.[m
[32m+[m[32m * @param {Boolean|String} [sticky=false] Whether the match must start at the specified position[m
[32m+[m[32m *   only. The string `'sticky'` is accepted as an alternative to `true`.[m
[32m+[m[32m * @returns {Array} Match array with named capture properties on the `groups` object, or `null`. If[m
[32m+[m[32m *   the `namespacing` feature is off, named capture properties are directly on the match array.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * // Basic use, with named capturing group[m
[32m+[m[32m * let match = XRegExp.exec('U+2620', XRegExp('U\\+(?<hex>[0-9A-F]{4})'));[m
[32m+[m[32m * match.groups.hex; // -> '2620'[m
[32m+[m[32m *[m
[32m+[m[32m * // With pos and sticky, in a loop[m
[32m+[m[32m * let pos = 3, result = [], match;[m
[32m+[m[32m * while (match = XRegExp.exec('<1><2><3><4>5<6>', /<(\d)>/, pos, 'sticky')) {[m
[32m+[m[32m *   result.push(match[1]);[m
[32m+[m[32m *   pos = match.index + match[0].length;[m
[32m+[m[32m * }[m
[32m+[m[32m * // result -> ['2', '3', '4'][m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.exec = function (str, regex, pos, sticky) {[m
[32m+[m[32m  var cacheKey = 'g';[m
[32m+[m[32m  var addY = false;[m
[32m+[m[32m  var fakeY = false;[m
[32m+[m[32m  var match;[m
[32m+[m[32m  addY = hasNativeY && !!(sticky || regex.sticky && sticky !== false);[m
[32m+[m
[32m+[m[32m  if (addY) {[m
[32m+[m[32m    cacheKey += 'y';[m
[32m+[m[32m  } else if (sticky) {[m
[32m+[m[32m    // Simulate sticky matching by appending an empty capture to the original regex. The[m
[32m+[m[32m    // resulting regex will succeed no matter what at the current index (set with `lastIndex`),[m
[32m+[m[32m    // and will not search the rest of the subject string. We'll know that the original regex[m
[32m+[m[32m    // has failed if that last capture is `''` rather than `undefined` (i.e., if that last[m
[32m+[m[32m    // capture participated in the match).[m
[32m+[m[32m    fakeY = true;[m
[32m+[m[32m    cacheKey += 'FakeY';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  regex[REGEX_DATA] = regex[REGEX_DATA] || {}; // Shares cached copies with `XRegExp.match`/`replace`[m
[32m+[m
[32m+[m[32m  var r2 = regex[REGEX_DATA][cacheKey] || (regex[REGEX_DATA][cacheKey] = copyRegex(regex, {[m
[32m+[m[32m    addG: true,[m
[32m+[m[32m    addY: addY,[m
[32m+[m[32m    source: fakeY ? "".concat(regex.source, "|()") : undefined,[m
[32m+[m[32m    removeY: sticky === false,[m
[32m+[m[32m    isInternalOnly: true[m
[32m+[m[32m  }));[m
[32m+[m[32m  pos = pos || 0;[m
[32m+[m[32m  r2.lastIndex = pos; // Fixed `exec` required for `lastIndex` fix, named backreferences, etc.[m
[32m+[m
[32m+[m[32m  match = fixed.exec.call(r2, str); // Get rid of the capture added by the pseudo-sticky matcher if needed. An empty string means[m
[32m+[m[32m  // the original regexp failed (see above).[m
[32m+[m
[32m+[m[32m  if (fakeY && match && match.pop() === '') {[m
[32m+[m[32m    match = null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (regex.global) {[m
[32m+[m[32m    regex.lastIndex = match ? r2.lastIndex : 0;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return match;[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Executes a provided function once per regex match. Searches always start at the beginning of the[m
[32m+[m[32m * string and continue until the end, regardless of the state of the regex's `global` property and[m
[32m+[m[32m * initial `lastIndex`.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {String} str String to search.[m
[32m+[m[32m * @param {RegExp} regex Regex to search with.[m
[32m+[m[32m * @param {Function} callback Function to execute for each match. Invoked with four arguments:[m
[32m+[m[32m *   - The match array, with named backreference properties.[m
[32m+[m[32m *   - The zero-based match index.[m
[32m+[m[32m *   - The string being traversed.[m
[32m+[m[32m *   - The regex object being used to traverse the string.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * // Extracts every other digit from a string[m
[32m+[m[32m * const evens = [];[m
[32m+[m[32m * XRegExp.forEach('1a2345', /\d/, (match, i) => {[m
[32m+[m[32m *   if (i % 2) evens.push(+match[0]);[m
[32m+[m[32m * });[m
[32m+[m[32m * // evens -> [2, 4][m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.forEach = function (str, regex, callback) {[m
[32m+[m[32m  var pos = 0;[m
[32m+[m[32m  var i = -1;[m
[32m+[m[32m  var match;[m
[32m+[m
[32m+[m[32m  while (match = XRegExp.exec(str, regex, pos)) {[m
[32m+[m[32m    // Because `regex` is provided to `callback`, the function could use the deprecated/[m
[32m+[m[32m    // nonstandard `RegExp.prototype.compile` to mutate the regex. However, since `XRegExp.exec`[m
[32m+[m[32m    // doesn't use `lastIndex` to set the search position, this can't lead to an infinite loop,[m
[32m+[m[32m    // at least. Actually, because of the way `XRegExp.exec` caches globalized versions of[m
[32m+[m[32m    // regexes, mutating the regex will not have any effect on the iteration or matched strings,[m
[32m+[m[32m    // which is a nice side effect that brings extra safety.[m
[32m+[m[32m    callback(match, ++i, str, regex);[m
[32m+[m[32m    pos = match.index + (match[0].length || 1);[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Copies a regex object and adds flag `g`. The copy maintains extended data, is augmented with[m
[32m+[m[32m * `XRegExp.prototype` properties, and has a fresh `lastIndex` property (set to zero). Native[m
[32m+[m[32m * regexes are not recompiled using XRegExp syntax.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {RegExp} regex Regex to globalize.[m
[32m+[m[32m * @returns {RegExp} Copy of the provided regex with flag `g` added.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * const globalCopy = XRegExp.globalize(/regex/);[m
[32m+[m[32m * globalCopy.global; // -> true[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.globalize = function (regex) {[m
[32m+[m[32m  return copyRegex(regex, {[m
[32m+[m[32m    addG: true[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Installs optional features according to the specified options. Can be undone using[m
[32m+[m[32m * `XRegExp.uninstall`.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {Object|String} options Options object or string.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * // With an options object[m
[32m+[m[32m * XRegExp.install({[m
[32m+[m[32m *   // Enables support for astral code points in Unicode addons (implicitly sets flag A)[m
[32m+[m[32m *   astral: true,[m
[32m+[m[32m *[m
[32m+[m[32m *   // Adds named capture groups to the `groups` property of matches[m
[32m+[m[32m *   namespacing: true[m
[32m+[m[32m * });[m
[32m+[m[32m *[m
[32m+[m[32m * // With an options string[m
[32m+[m[32m * XRegExp.install('astral namespacing');[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.install = function (options) {[m
[32m+[m[32m  options = prepareOptions(options);[m
[32m+[m
[32m+[m[32m  if (!features.astral && options.astral) {[m
[32m+[m[32m    setAstral(true);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!features.namespacing && options.namespacing) {[m
[32m+[m[32m    setNamespacing(true);[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Checks whether an individual optional feature is installed.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {String} feature Name of the feature to check. One of:[m
[32m+[m[32m *   - `astral`[m
[32m+[m[32m *   - `namespacing`[m
[32m+[m[32m * @returns {boolean} Whether the feature is installed.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * XRegExp.isInstalled('astral');[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.isInstalled = function (feature) {[m
[32m+[m[32m  return !!features[feature];[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns `true` if an object is a regex; `false` if it isn't. This works correctly for regexes[m
[32m+[m[32m * created in another frame, when `instanceof` and `constructor` checks would fail.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {*} value Object to check.[m
[32m+[m[32m * @returns {boolean} Whether the object is a `RegExp` object.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * XRegExp.isRegExp('string'); // -> false[m
[32m+[m[32m * XRegExp.isRegExp(/regex/i); // -> true[m
[32m+[m[32m * XRegExp.isRegExp(RegExp('^', 'm')); // -> true[m
[32m+[m[32m * XRegExp.isRegExp(XRegExp('(?s).')); // -> true[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.isRegExp = function (value) {[m
[32m+[m[32m  return Object.prototype.toString.call(value) === '[object RegExp]';[m
[32m+[m[32m}; // Same as `isType(value, 'RegExp')`, but avoiding that function call here for perf since[m
[32m+[m[32m// `isRegExp` is used heavily by internals including regex construction[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns the first matched string, or in global mode, an array containing all matched strings.[m
[32m+[m[32m * This is essentially a more convenient re-implementation of `String.prototype.match` that gives[m
[32m+[m[32m * the result types you actually want (string instead of `exec`-style array in match-first mode,[m
[32m+[m[32m * and an empty array instead of `null` when no matches are found in match-all mode). It also lets[m
[32m+[m[32m * you override flag g and ignore `lastIndex`, and fixes browser bugs.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {String} str String to search.[m
[32m+[m[32m * @param {RegExp} regex Regex to search with.[m
[32m+[m[32m * @param {String} [scope='one'] Use 'one' to return the first match as a string. Use 'all' to[m
[32m+[m[32m *   return an array of all matched strings. If not explicitly specified and `regex` uses flag g,[m
[32m+[m[32m *   `scope` is 'all'.[m
[32m+[m[32m * @returns {String|Array} In match-first mode: First match as a string, or `null`. In match-all[m
[32m+[m[32m *   mode: Array of all matched strings, or an empty array.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * // Match first[m
[32m+[m[32m * XRegExp.match('abc', /\w/); // -> 'a'[m
[32m+[m[32m * XRegExp.match('abc', /\w/g, 'one'); // -> 'a'[m
[32m+[m[32m * XRegExp.match('abc', /x/g, 'one'); // -> null[m
[32m+[m[32m *[m
[32m+[m[32m * // Match all[m
[32m+[m[32m * XRegExp.match('abc', /\w/g); // -> ['a', 'b', 'c'][m
[32m+[m[32m * XRegExp.match('abc', /\w/, 'all'); // -> ['a', 'b', 'c'][m
[32m+[m[32m * XRegExp.match('abc', /x/, 'all'); // -> [][m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.match = function (str, regex, scope) {[m
[32m+[m[32m  var global = regex.global && scope !== 'one' || scope === 'all';[m
[32m+[m[32m  var cacheKey = (global ? 'g' : '') + (regex.sticky ? 'y' : '') || 'noGY';[m
[32m+[m[32m  regex[REGEX_DATA] = regex[REGEX_DATA] || {}; // Shares cached copies with `XRegExp.exec`/`replace`[m
[32m+[m
[32m+[m[32m  var r2 = regex[REGEX_DATA][cacheKey] || (regex[REGEX_DATA][cacheKey] = copyRegex(regex, {[m
[32m+[m[32m    addG: !!global,[m
[32m+[m[32m    removeG: scope === 'one',[m
[32m+[m[32m    isInternalOnly: true[m
[32m+[m[32m  }));[m
[32m+[m[32m  var result = String(nullThrows(str)).match(r2);[m
[32m+[m
[32m+[m[32m  if (regex.global) {[m
[32m+[m[32m    regex.lastIndex = scope === 'one' && result ? // Can't use `r2.lastIndex` since `r2` is nonglobal in this case[m
[32m+[m[32m    result.index + result[0].length : 0;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return global ? result || [] : result && result[0];[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Retrieves the matches from searching a string using a chain of regexes that successively search[m
[32m+[m[32m * within previous matches. The provided `chain` array can contain regexes and or objects with[m
[32m+[m[32m * `regex` and `backref` properties. When a backreference is specified, the named or numbered[m
[32m+[m[32m * backreference is passed forward to the next regex or returned.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {String} str String to search.[m
[32m+[m[32m * @param {Array} chain Regexes that each search for matches within preceding results.[m
[32m+[m[32m * @returns {Array} Matches by the last regex in the chain, or an empty array.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * // Basic usage; matches numbers within <b> tags[m
[32m+[m[32m * XRegExp.matchChain('1 <b>2</b> 3 <b>4 a 56</b>', [[m
[32m+[m[32m *   XRegExp('(?is)<b>.*?</b>'),[m
[32m+[m[32m *   /\d+/[m
[32m+[m[32m * ]);[m
[32m+[m[32m * // -> ['2', '4', '56'][m
[32m+[m[32m *[m
[32m+[m[32m * // Passing forward and returning specific backreferences[m
[32m+[m[32m * const html = `<a href="http://xregexp.com/api/">XRegExp</a>[m
[32m+[m[32m *               <a href="http://www.google.com/">Google</a>`;[m
[32m+[m[32m * XRegExp.matchChain(html, [[m
[32m+[m[32m *   {regex: /<a href="([^"]+)">/i, backref: 1},[m
[32m+[m[32m *   {regex: XRegExp('(?i)^https?://(?<domain>[^/?#]+)'), backref: 'domain'}[m
[32m+[m[32m * ]);[m
[32m+[m[32m * // -> ['xregexp.com', 'www.google.com'][m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.matchChain = function (str, chain) {[m
[32m+[m[32m  return function recurseChain(values, level) {[m
[32m+[m[32m    var item = chain[level].regex ? chain[level] : {[m
[32m+[m[32m      regex: chain[level][m
[32m+[m[32m    };[m
[32m+[m[32m    var matches = [];[m
[32m+[m
[32m+[m[32m    function addMatch(match) {[m
[32m+[m[32m      if (item.backref) {[m
[32m+[m[32m        var ERR_UNDEFINED_GROUP = "Backreference to undefined group: ".concat(item.backref);[m
[32m+[m[32m        var isNamedBackref = isNaN(item.backref);[m
[32m+[m
[32m+[m[32m        if (isNamedBackref && XRegExp.isInstalled('namespacing')) {[m
[32m+[m[32m          // `groups` has `null` as prototype, so using `in` instead of `hasOwnProperty`[m
[32m+[m[32m          if (!(match.groups && item.backref in match.groups)) {[m
[32m+[m[32m            throw new ReferenceError(ERR_UNDEFINED_GROUP);[m
[32m+[m[32m          }[m
[32m+[m[32m        } else if (!match.hasOwnProperty(item.backref)) {[m
[32m+[m[32m          throw new ReferenceError(ERR_UNDEFINED_GROUP);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var backrefValue = isNamedBackref && XRegExp.isInstalled('namespacing') ? match.groups[item.backref] : match[item.backref];[m
[32m+[m[32m        matches.push(backrefValue || '');[m
[32m+[m[32m      } else {[m
[32m+[m[32m        matches.push(match[0]);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var _iterator3 = _createForOfIteratorHelper(values),[m
[32m+[m[32m        _step3;[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m      for (_iterator3.s(); !(_step3 = _iterator3.n()).done;) {[m
[32m+[m[32m        var value = _step3.value;[m
[32m+[m[32m        (0, _forEach["default"])(XRegExp).call(XRegExp, value, item.regex, addMatch);[m
[32m+[m[32m      }[m
[32m+[m[32m    } catch (err) {[m
[32m+[m[32m      _iterator3.e(err);[m
[32m+[m[32m    } finally {[m
[32m+[m[32m      _iterator3.f();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return level === chain.length - 1 || !matches.length ? matches : recurseChain(matches, level + 1);[m
[32m+[m[32m  }([str], 0);[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns a new string with one or all matches of a pattern replaced. The pattern can be a string[m
[32m+[m[32m * or regex, and the replacement can be a string or a function to be called for each match. To[m
[32m+[m[32m * perform a global search and replace, use the optional `scope` argument or include flag g if using[m
[32m+[m[32m * a regex. Replacement strings can use `$<n>` or `${n}` for named and numbered backreferences.[m
[32m+[m[32m * Replacement functions can use named backreferences via the last argument. Also fixes browser bugs[m
[32m+[m[32m * compared to the native `String.prototype.replace` and can be used reliably cross-browser.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {String} str String to search.[m
[32m+[m[32m * @param {RegExp|String} search Search pattern to be replaced.[m
[32m+[m[32m * @param {String|Function} replacement Replacement string or a function invoked to create it.[m
[32m+[m[32m *   Replacement strings can include special replacement syntax:[m
[32m+[m[32m *     - $$ - Inserts a literal $ character.[m
[32m+[m[32m *     - $&, $0 - Inserts the matched substring.[m
[32m+[m[32m *     - $` - Inserts the string that precedes the matched substring (left context).[m
[32m+[m[32m *     - $' - Inserts the string that follows the matched substring (right context).[m
[32m+[m[32m *     - $n, $nn - Where n/nn are digits referencing an existing capturing group, inserts[m
[32m+[m[32m *       backreference n/nn.[m
[32m+[m[32m *     - $<n>, ${n} - Where n is a name or any number of digits that reference an existing capturing[m
[32m+[m[32m *       group, inserts backreference n.[m
[32m+[m[32m *   Replacement functions are invoked with three or more arguments:[m
[32m+[m[32m *     - args[0] - The matched substring (corresponds to `$&` above). If the `namespacing` feature[m
[32m+[m[32m *       is off, named backreferences are accessible as properties of this argument.[m
[32m+[m[32m *     - args[1..n] - One argument for each backreference (corresponding to `$1`, `$2`, etc. above).[m
[32m+[m[32m *       If the regex has no capturing groups, no arguments appear in this position.[m
[32m+[m[32m *     - args[n+1] - The zero-based index of the match within the entire search string.[m
[32m+[m[32m *     - args[n+2] - The total string being searched.[m
[32m+[m[32m *     - args[n+3] - If the the search pattern is a regex with named capturing groups, the last[m
[32m+[m[32m *       argument is the groups object. Its keys are the backreference names and its values are the[m
[32m+[m[32m *       backreference values. If the `namespacing` feature is off, this argument is not present.[m
[32m+[m[32m * @param {String} [scope] Use 'one' to replace the first match only, or 'all'. Defaults to 'one'.[m
[32m+[m[32m *   Defaults to 'all' if using a regex with flag g.[m
[32m+[m[32m * @returns {String} New string with one or all matches replaced.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * // Regex search, using named backreferences in replacement string[m
[32m+[m[32m * const name = XRegExp('(?<first>\\w+) (?<last>\\w+)');[m
[32m+[m[32m * XRegExp.replace('John Smith', name, '$<last>, $<first>');[m
[32m+[m[32m * // -> 'Smith, John'[m
[32m+[m[32m *[m
[32m+[m[32m * // Regex search, using named backreferences in replacement function[m
[32m+[m[32m * XRegExp.replace('John Smith', name, (...args) => {[m
[32m+[m[32m *   const groups = args[args.length - 1];[m
[32m+[m[32m *   return `${groups.last}, ${groups.first}`;[m
[32m+[m[32m * });[m
[32m+[m[32m * // -> 'Smith, John'[m
[32m+[m[32m *[m
[32m+[m[32m * // String search, with replace-all[m
[32m+[m[32m * XRegExp.replace('RegExp builds RegExps', 'RegExp', 'XRegExp', 'all');[m
[32m+[m[32m * // -> 'XRegExp builds XRegExps'[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.replace = function (str, search, replacement, scope) {[m
[32m+[m[32m  var isRegex = XRegExp.isRegExp(search);[m
[32m+[m[32m  var global = search.global && scope !== 'one' || scope === 'all';[m
[32m+[m[32m  var cacheKey = (global ? 'g' : '') + (search.sticky ? 'y' : '') || 'noGY';[m
[32m+[m[32m  var s2 = search;[m
[32m+[m
[32m+[m[32m  if (isRegex) {[m
[32m+[m[32m    search[REGEX_DATA] = search[REGEX_DATA] || {}; // Shares cached copies with `XRegExp.exec`/`match`. Since a copy is used, `search`'s[m
[32m+[m[32m    // `lastIndex` isn't updated *during* replacement iterations[m
[32m+[m
[32m+[m[32m    s2 = search[REGEX_DATA][cacheKey] || (search[REGEX_DATA][cacheKey] = copyRegex(search, {[m
[32m+[m[32m      addG: !!global,[m
[32m+[m[32m      removeG: scope === 'one',[m
[32m+[m[32m      isInternalOnly: true[m
[32m+[m[32m    }));[m
[32m+[m[32m  } else if (global) {[m
[32m+[m[32m    s2 = new RegExp(XRegExp.escape(String(search)), 'g');[m
[32m+[m[32m  } // Fixed `replace` required for named backreferences, etc.[m
[32m+[m
[32m+[m
[32m+[m[32m  var result = fixed.replace.call(nullThrows(str), s2, replacement);[m
[32m+[m
[32m+[m[32m  if (isRegex && search.global) {[m
[32m+[m[32m    // Fixes IE, Safari bug (last tested IE 9, Safari 5.1)[m
[32m+[m[32m    search.lastIndex = 0;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return result;[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Performs batch processing of string replacements. Used like `XRegExp.replace`, but accepts an[m
[32m+[m[32m * array of replacement details. Later replacements operate on the output of earlier replacements.[m
[32m+[m[32m * Replacement details are accepted as an array with a regex or string to search for, the[m
[32m+[m[32m * replacement string or function, and an optional scope of 'one' or 'all'. Uses the XRegExp[m
[32m+[m[32m * replacement text syntax, which supports named backreference properties via `$<name>` or[m
[32m+[m[32m * `${name}`.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {String} str String to search.[m
[32m+[m[32m * @param {Array} replacements Array of replacement detail arrays.[m
[32m+[m[32m * @returns {String} New string with all replacements.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * str = XRegExp.replaceEach(str, [[m
[32m+[m[32m *   [XRegExp('(?<name>a)'), 'z$<name>'],[m
[32m+[m[32m *   [/b/gi, 'y'],[m
[32m+[m[32m *   [/c/g, 'x', 'one'], // scope 'one' overrides /g[m
[32m+[m[32m *   [/d/, 'w', 'all'],  // scope 'all' overrides lack of /g[m
[32m+[m[32m *   ['e', 'v', 'all'],  // scope 'all' allows replace-all for strings[m
[32m+[m[32m *   [/f/g, (match) => match.toUpperCase()][m
[32m+[m[32m * ]);[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.replaceEach = function (str, replacements) {[m
[32m+[m[32m  var _iterator4 = _createForOfIteratorHelper(replacements),[m
[32m+[m[32m      _step4;[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    for (_iterator4.s(); !(_step4 = _iterator4.n()).done;) {[m
[32m+[m[32m      var r = _step4.value;[m
[32m+[m[32m      str = XRegExp.replace(str, r[0], r[1], r[2]);[m
[32m+[m[32m    }[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    _iterator4.e(err);[m
[32m+[m[32m  } finally {[m
[32m+[m[32m    _iterator4.f();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return str;[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Splits a string into an array of strings using a regex or string separator. Matches of the[m
[32m+[m[32m * separator are not included in the result array. However, if `separator` is a regex that contains[m
[32m+[m[32m * capturing groups, backreferences are spliced into the result each time `separator` is matched.[m
[32m+[m[32m * Fixes browser bugs compared to the native `String.prototype.split` and can be used reliably[m
[32m+[m[32m * cross-browser.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {String} str String to split.[m
[32m+[m[32m * @param {RegExp|String} separator Regex or string to use for separating the string.[m
[32m+[m[32m * @param {Number} [limit] Maximum number of items to include in the result array.[m
[32m+[m[32m * @returns {Array} Array of substrings.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * // Basic use[m
[32m+[m[32m * XRegExp.split('a b c', ' ');[m
[32m+[m[32m * // -> ['a', 'b', 'c'][m
[32m+[m[32m *[m
[32m+[m[32m * // With limit[m
[32m+[m[32m * XRegExp.split('a b c', ' ', 2);[m
[32m+[m[32m * // -> ['a', 'b'][m
[32m+[m[32m *[m
[32m+[m[32m * // Backreferences in result array[m
[32m+[m[32m * XRegExp.split('..word1..', /([a-z]+)(\d+)/i);[m
[32m+[m[32m * // -> ['..', 'word', '1', '..'][m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.split = function (str, separator, limit) {[m
[32m+[m[32m  return fixed.split.call(nullThrows(str), separator, limit);[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Executes a regex search in a specified string. Returns `true` or `false`. Optional `pos` and[m
[32m+[m[32m * `sticky` arguments specify the search start position, and whether the match must start at the[m
[32m+[m[32m * specified position only. The `lastIndex` property of the provided regex is not used, but is[m
[32m+[m[32m * updated for compatibility. Also fixes browser bugs compared to the native[m
[32m+[m[32m * `RegExp.prototype.test` and can be used reliably cross-browser.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {String} str String to search.[m
[32m+[m[32m * @param {RegExp} regex Regex to search with.[m
[32m+[m[32m * @param {Number} [pos=0] Zero-based index at which to start the search.[m
[32m+[m[32m * @param {Boolean|String} [sticky=false] Whether the match must start at the specified position[m
[32m+[m[32m *   only. The string `'sticky'` is accepted as an alternative to `true`.[m
[32m+[m[32m * @returns {boolean} Whether the regex matched the provided value.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * // Basic use[m
[32m+[m[32m * XRegExp.test('abc', /c/); // -> true[m
[32m+[m[32m *[m
[32m+[m[32m * // With pos and sticky[m
[32m+[m[32m * XRegExp.test('abc', /c/, 0, 'sticky'); // -> false[m
[32m+[m[32m * XRegExp.test('abc', /c/, 2, 'sticky'); // -> true[m
[32m+[m[32m */[m
[32m+[m[32m// Do this the easy way :-)[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.test = function (str, regex, pos, sticky) {[m
[32m+[m[32m  return !!XRegExp.exec(str, regex, pos, sticky);[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Uninstalls optional features according to the specified options. Used to undo the actions of[m
[32m+[m[32m * `XRegExp.install`.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {Object|String} options Options object or string.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * // With an options object[m
[32m+[m[32m * XRegExp.uninstall({[m
[32m+[m[32m *   // Disables support for astral code points in Unicode addons (unless enabled per regex)[m
[32m+[m[32m *   astral: true,[m
[32m+[m[32m *[m
[32m+[m[32m *   // Don't add named capture groups to the `groups` property of matches[m
[32m+[m[32m *   namespacing: true[m
[32m+[m[32m * });[m
[32m+[m[32m *[m
[32m+[m[32m * // With an options string[m
[32m+[m[32m * XRegExp.uninstall('astral namespacing');[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.uninstall = function (options) {[m
[32m+[m[32m  options = prepareOptions(options);[m
[32m+[m
[32m+[m[32m  if (features.astral && options.astral) {[m
[32m+[m[32m    setAstral(false);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (features.namespacing && options.namespacing) {[m
[32m+[m[32m    setNamespacing(false);[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns an XRegExp object that is the union of the given patterns. Patterns can be provided as[m
[32m+[m[32m * regex objects or strings. Metacharacters are escaped in patterns provided as strings.[m
[32m+[m[32m * Backreferences in provided regex objects are automatically renumbered to work correctly within[m
[32m+[m[32m * the larger combined pattern. Native flags used by provided regexes are ignored in favor of the[m
[32m+[m[32m * `flags` argument.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf XRegExp[m
[32m+[m[32m * @param {Array} patterns Regexes and strings to combine.[m
[32m+[m[32m * @param {String} [flags] Any combination of XRegExp flags.[m
[32m+[m[32m * @param {Object} [options] Options object with optional properties:[m
[32m+[m[32m *   - `conjunction` {String} Type of conjunction to use: 'or' (default) or 'none'.[m
[32m+[m[32m * @returns {RegExp} Union of the provided regexes and strings.[m
[32m+[m[32m * @example[m
[32m+[m[32m *[m
[32m+[m[32m * XRegExp.union(['a+b*c', /(dogs)\1/, /(cats)\1/], 'i');[m
[32m+[m[32m * // -> /a\+b\*c|(dogs)\1|(cats)\2/i[m
[32m+[m[32m *[m
[32m+[m[32m * XRegExp.union([/man/, /bear/, /pig/], 'i', {conjunction: 'none'});[m
[32m+[m[32m * // -> /manbearpig/i[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.union = function (patterns, flags, options) {[m
[32m+[m[32m  options = options || {};[m
[32m+[m[32m  var conjunction = options.conjunction || 'or';[m
[32m+[m[32m  var numCaptures = 0;[m
[32m+[m[32m  var numPriorCaptures;[m
[32m+[m[32m  var captureNames;[m
[32m+[m
[32m+[m[32m  function rewrite(match, paren, backref) {[m
[32m+[m[32m    var name = captureNames[numCaptures - numPriorCaptures]; // Capturing group[m
[32m+[m
[32m+[m[32m    if (paren) {[m
[32m+[m[32m      ++numCaptures; // If the current capture has a name, preserve the name[m
[32m+[m
[32m+[m[32m      if (name) {[m
[32m+[m[32m        return "(?<".concat(name, ">");[m
[32m+[m[32m      } // Backreference[m
[32m+[m
[32m+[m[32m    } else if (backref) {[m
[32m+[m[32m      // Rewrite the backreference[m
[32m+[m[32m      return "\\".concat(+backref + numPriorCaptures);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return match;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!(isType(patterns, 'Array') && patterns.length)) {[m
[32m+[m[32m    throw new TypeError('Must provide a nonempty array of patterns to merge');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var parts = /(\()(?!\?)|\\([1-9]\d*)|\\[\s\S]|\[(?:[^\\\]]|\\[\s\S])*\]/g;[m
[32m+[m[32m  var output = [];[m
[32m+[m
[32m+[m[32m  var _iterator5 = _createForOfIteratorHelper(patterns),[m
[32m+[m[32m      _step5;[m
[32m+[m
[32m+[m[32m  try {[m
[32m+[m[32m    for (_iterator5.s(); !(_step5 = _iterator5.n()).done;) {[m
[32m+[m[32m      var pattern = _step5.value;[m
[32m+[m
[32m+[m[32m      if (XRegExp.isRegExp(pattern)) {[m
[32m+[m[32m        numPriorCaptures = numCaptures;[m
[32m+[m[32m        captureNames = pattern[REGEX_DATA] && pattern[REGEX_DATA].captureNames || []; // Rewrite backreferences. Passing to XRegExp dies on octals and ensures patterns are[m
[32m+[m[32m        // independently valid; helps keep this simple. Named captures are put back[m
[32m+[m
[32m+[m[32m        output.push(XRegExp(pattern.source).source.replace(parts, rewrite));[m
[32m+[m[32m      } else {[m
[32m+[m[32m        output.push(XRegExp.escape(pattern));[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    _iterator5.e(err);[m
[32m+[m[32m  } finally {[m
[32m+[m[32m    _iterator5.f();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var separator = conjunction === 'none' ? '' : '|';[m
[32m+[m[32m  return XRegExp(output.join(separator), flags);[m
[32m+[m[32m}; // ==--------------------------==[m
[32m+[m[32m// Fixed/extended native methods[m
[32m+[m[32m// ==--------------------------==[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Adds named capture support (with backreferences returned as `result.name`), and fixes browser[m
[32m+[m[32m * bugs in the native `RegExp.prototype.exec`. Use via `XRegExp.exec`.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf RegExp[m
[32m+[m[32m * @param {String} str String to search.[m
[32m+[m[32m * @returns {Array} Match array with named backreference properties, or `null`.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfixed.exec = function (str) {[m
[32m+[m[32m  var origLastIndex = this.lastIndex;[m
[32m+[m[32m  var match = RegExp.prototype.exec.apply(this, arguments);[m
[32m+[m
[32m+[m[32m  if (match) {[m
[32m+[m[32m    // Fix browsers whose `exec` methods don't return `undefined` for nonparticipating capturing[m
[32m+[m[32m    // groups. This fixes IE 5.5-8, but not IE 9's quirks mode or emulation of older IEs. IE 9[m
[32m+[m[32m    // in standards mode follows the spec.[m
[32m+[m[32m    if (!correctExecNpcg && match.length > 1 && (0, _indexOf["default"])(match).call(match, '') !== -1) {[m
[32m+[m[32m      var _context3;[m
[32m+[m
[32m+[m[32m      var r2 = copyRegex(this, {[m
[32m+[m[32m        removeG: true,[m
[32m+[m[32m        isInternalOnly: true[m
[32m+[m[32m      }); // Using `str.slice(match.index)` rather than `match[0]` in case lookahead allowed[m
[32m+[m[32m      // matching due to characters outside the match[m
[32m+[m
[32m+[m[32m      (0, _slice["default"])(_context3 = String(str)).call(_context3, match.index).replace(r2, function () {[m
[32m+[m[32m        var len = arguments.length; // Skip index 0 and the last 2[m
[32m+[m
[32m+[m[32m        for (var i = 1; i < len - 2; ++i) {[m
[32m+[m[32m          if ((i < 0 || arguments.length <= i ? undefined : arguments[i]) === undefined) {[m
[32m+[m[32m            match[i] = undefined;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    } // Attach named capture properties[m
[32m+[m
[32m+[m
[32m+[m[32m    if (this[REGEX_DATA] && this[REGEX_DATA].captureNames) {[m
[32m+[m[32m      var groupsObject = match;[m
[32m+[m
[32m+[m[32m      if (XRegExp.isInstalled('namespacing')) {[m
[32m+[m[32m        // https://tc39.github.io/proposal-regexp-named-groups/#sec-regexpbuiltinexec[m
[32m+[m[32m        match.groups = (0, _create["default"])(null);[m
[32m+[m[32m        groupsObject = match.groups;[m
[32m+[m[32m      } // Skip index 0[m
[32m+[m
[32m+[m
[32m+[m[32m      for (var i = 1; i < match.length; ++i) {[m
[32m+[m[32m        var name = this[REGEX_DATA].captureNames[i - 1];[m
[32m+[m
[32m+[m[32m        if (name) {[m
[32m+[m[32m          groupsObject[name] = match[i];[m
[32m+[m[32m        }[m
[32m+[m[32m      } // Preserve any existing `groups` obj that came from native ES2018 named capture[m
[32m+[m
[32m+[m[32m    } else if (!match.groups && XRegExp.isInstalled('namespacing')) {[m
[32m+[m[32m      match.groups = undefined;[m
[32m+[m[32m    } // Fix browsers that increment `lastIndex` after zero-length matches[m
[32m+[m
[32m+[m
[32m+[m[32m    if (this.global && !match[0].length && this.lastIndex > match.index) {[m
[32m+[m[32m      this.lastIndex = match.index;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!this.global) {[m
[32m+[m[32m    // Fixes IE, Opera bug (last tested IE 9, Opera 11.6)[m
[32m+[m[32m    this.lastIndex = origLastIndex;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return match;[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Fixes browser bugs in the native `RegExp.prototype.test`.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf RegExp[m
[32m+[m[32m * @param {String} str String to search.[m
[32m+[m[32m * @returns {boolean} Whether the regex matched the provided value.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfixed.test = function (str) {[m
[32m+[m[32m  // Do this the easy way :-)[m
[32m+[m[32m  return !!fixed.exec.call(this, str);[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Adds named capture support (with backreferences returned as `result.name`), and fixes browser[m
[32m+[m[32m * bugs in the native `String.prototype.match`.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf String[m
[32m+[m[32m * @param {RegExp|*} regex Regex to search with. If not a regex object, it is passed to `RegExp`.[m
[32m+[m[32m * @returns {Array} If `regex` uses flag g, an array of match strings or `null`. Without flag g,[m
[32m+[m[32m *   the result of calling `regex.exec(this)`.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfixed.match = function (regex) {[m
[32m+[m[32m  if (!XRegExp.isRegExp(regex)) {[m
[32m+[m[32m    // Use the native `RegExp` rather than `XRegExp`[m
[32m+[m[32m    regex = new RegExp(regex);[m
[32m+[m[32m  } else if (regex.global) {[m
[32m+[m[32m    var result = String.prototype.match.apply(this, arguments); // Fixes IE bug[m
[32m+[m
[32m+[m[32m    regex.lastIndex = 0;[m
[32m+[m[32m    return result;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return fixed.exec.call(regex, nullThrows(this));[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Adds support for `${n}` (or `$<n>`) tokens for named and numbered backreferences in replacement[m
[32m+[m[32m * text, and provides named backreferences to replacement functions as `arguments[0].name`. Also[m
[32m+[m[32m * fixes browser bugs in replacement text syntax when performing a replacement using a nonregex[m
[32m+[m[32m * search value, and the value of a replacement regex's `lastIndex` property during replacement[m
[32m+[m[32m * iterations and upon completion. Note that this doesn't support SpiderMonkey's proprietary third[m
[32m+[m[32m * (`flags`) argument. Use via `XRegExp.replace`.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf String[m
[32m+[m[32m * @param {RegExp|String} search Search pattern to be replaced.[m
[32m+[m[32m * @param {String|Function} replacement Replacement string or a function invoked to create it.[m
[32m+[m[32m * @returns {string} New string with one or all matches replaced.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfixed.replace = function (search, replacement) {[m
[32m+[m[32m  var isRegex = XRegExp.isRegExp(search);[m
[32m+[m[32m  var origLastIndex;[m
[32m+[m[32m  var captureNames;[m
[32m+[m[32m  var result;[m
[32m+[m
[32m+[m[32m  if (isRegex) {[m
[32m+[m[32m    if (search[REGEX_DATA]) {[m
[32m+[m[32m      captureNames = search[REGEX_DATA].captureNames;[m
[32m+[m[32m    } // Only needed if `search` is nonglobal[m
[32m+[m
[32m+[m
[32m+[m[32m    origLastIndex = search.lastIndex;[m
[32m+[m[32m  } else {[m
[32m+[m[32m    search += ''; // Type-convert[m
[32m+[m[32m  } // Don't use `typeof`; some older browsers return 'function' for regex objects[m
[32m+[m
[32m+[m
[32m+[m[32m  if (isType(replacement, 'Function')) {[m
[32m+[m[32m    // Stringifying `this` fixes a bug in IE < 9 where the last argument in replacement[m
[32m+[m[32m    // functions isn't type-converted to a string[m
[32m+[m[32m    result = String(this).replace(search, function () {[m
[32m+[m[32m      for (var _len = arguments.length, args = new Array(_len), _key = 0; _key < _len; _key++) {[m
[32m+[m[32m        args[_key] = arguments[_key];[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (captureNames) {[m
[32m+[m[32m        var groupsObject;[m
[32m+[m
[32m+[m[32m        if (XRegExp.isInstalled('namespacing')) {[m
[32m+[m[32m          // https://tc39.github.io/proposal-regexp-named-groups/#sec-regexpbuiltinexec[m
[32m+[m[32m          groupsObject = (0, _create["default"])(null);[m
[32m+[m[32m          args.push(groupsObject);[m
[32m+[m[32m        } else {[m
[32m+[m[32m          // Change the `args[0]` string primitive to a `String` object that can store[m
[32m+[m[32m          // properties. This really does need to use `String` as a constructor[m
[32m+[m[32m          args[0] = new String(args[0]);[m
[32m+[m[32m          groupsObject = args[0];[m
[32m+[m[32m        } // Store named backreferences[m
[32m+[m
[32m+[m
[32m+[m[32m        for (var i = 0; i < captureNames.length; ++i) {[m
[32m+[m[32m          if (captureNames[i]) {[m
[32m+[m[32m            groupsObject[captureNames[i]] = args[i + 1];[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      } // ES6 specs the context for replacement functions as `undefined`[m
[32m+[m
[32m+[m
[32m+[m[32m      return replacement.apply(void 0, args);[m
[32m+[m[32m    });[m
[32m+[m[32m  } else {[m
[32m+[m[32m    // Ensure that the last value of `args` will be a string when given nonstring `this`,[m
[32m+[m[32m    // while still throwing on null or undefined context[m
[32m+[m[32m    result = String(nullThrows(this)).replace(search, function () {[m
[32m+[m[32m      for (var _len2 = arguments.length, args = new Array(_len2), _key2 = 0; _key2 < _len2; _key2++) {[m
[32m+[m[32m        args[_key2] = arguments[_key2];[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return String(replacement).replace(replacementToken, replacer);[m
[32m+[m
[32m+[m[32m      function replacer($0, bracketed, angled, dollarToken) {[m
[32m+[m[32m        bracketed = bracketed || angled; // ES2018 added a new trailing `groups` arg that's passed to replacement functions[m
[32m+[m[32m        // when the search regex uses native named capture[m
[32m+[m
[32m+[m[32m        var numNonCaptureArgs = isType(args[args.length - 1], 'Object') ? 4 : 3;[m
[32m+[m[32m        var numCaptures = args.length - numNonCaptureArgs; // Handle named or numbered backreference with curly or angled braces: ${n}, $<n>[m
[32m+[m
[32m+[m[32m        if (bracketed) {[m
[32m+[m[32m          // Handle backreference to numbered capture, if `bracketed` is an integer. Use[m
[32m+[m[32m          // `0` for the entire match. Any number of leading zeros may be used.[m
[32m+[m[32m          if (/^\d+$/.test(bracketed)) {[m
[32m+[m[32m            // Type-convert and drop leading zeros[m
[32m+[m[32m            var _n = +bracketed;[m
[32m+[m
[32m+[m[32m            if (_n <= numCaptures) {[m
[32m+[m[32m              return args[_n] || '';[m
[32m+[m[32m            }[m
[32m+[m[32m          } // Handle backreference to named capture. If the name does not refer to an[m
[32m+[m[32m          // existing capturing group, it's an error. Also handles the error for numbered[m
[32m+[m[32m          // backference that does not refer to an existing group.[m
[32m+[m[32m          // Using `indexOf` since having groups with the same name is already an error,[m
[32m+[m[32m          // otherwise would need `lastIndexOf`.[m
[32m+[m
[32m+[m
[32m+[m[32m          var n = captureNames ? (0, _indexOf["default"])(captureNames).call(captureNames, bracketed) : -1;[m
[32m+[m
[32m+[m[32m          if (n < 0) {[m
[32m+[m[32m            throw new SyntaxError("Backreference to undefined group ".concat($0));[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          return args[n + 1] || '';[m
[32m+[m[32m        } // Handle `$`-prefixed variable[m
[32m+[m[32m        // Handle space/blank first because type conversion with `+` drops space padding[m
[32m+[m[32m        // and converts spaces and empty strings to `0`[m
[32m+[m
[32m+[m
[32m+[m[32m        if (dollarToken === '' || dollarToken === ' ') {[m
[32m+[m[32m          throw new SyntaxError("Invalid token ".concat($0));[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (dollarToken === '&' || +dollarToken === 0) {[m
[32m+[m[32m          // $&, $0 (not followed by 1-9), $00[m
[32m+[m[32m          return args[0];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (dollarToken === '$') {[m
[32m+[m[32m          // $$[m
[32m+[m[32m          return '$';[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (dollarToken === '`') {[m
[32m+[m[32m          var _context4;[m
[32m+[m
[32m+[m[32m          // $` (left context)[m
[32m+[m[32m          return (0, _slice["default"])(_context4 = args[args.length - 1]).call(_context4, 0, args[args.length - 2]);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (dollarToken === "'") {[m
[32m+[m[32m          var _context5;[m
[32m+[m
[32m+[m[32m          // $' (right context)[m
[32m+[m[32m          return (0, _slice["default"])(_context5 = args[args.length - 1]).call(_context5, args[args.length - 2] + args[0].length);[m
[32m+[m[32m        } // Handle numbered backreference without braces[m
[32m+[m[32m        // Type-convert and drop leading zero[m
[32m+[m
[32m+[m
[32m+[m[32m        dollarToken = +dollarToken; // XRegExp behavior for `$n` and `$nn`:[m
[32m+[m[32m        // - Backrefs end after 1 or 2 digits. Use `${..}` or `$<..>` for more digits.[m
[32m+[m[32m        // - `$1` is an error if no capturing groups.[m
[32m+[m[32m        // - `$10` is an error if less than 10 capturing groups. Use `${1}0` or `$<1>0`[m
[32m+[m[32m        //   instead.[m
[32m+[m[32m        // - `$01` is `$1` if at least one capturing group, else it's an error.[m
[32m+[m[32m        // - `$0` (not followed by 1-9) and `$00` are the entire match.[m
[32m+[m[32m        // Native behavior, for comparison:[m
[32m+[m[32m        // - Backrefs end after 1 or 2 digits. Cannot reference capturing group 100+.[m
[32m+[m[32m        // - `$1` is a literal `$1` if no capturing groups.[m
[32m+[m[32m        // - `$10` is `$1` followed by a literal `0` if less than 10 capturing groups.[m
[32m+[m[32m        // - `$01` is `$1` if at least one capturing group, else it's a literal `$01`.[m
[32m+[m[32m        // - `$0` is a literal `$0`.[m
[32m+[m
[32m+[m[32m        if (!isNaN(dollarToken)) {[m
[32m+[m[32m          if (dollarToken > numCaptures) {[m
[32m+[m[32m            throw new SyntaxError("Backreference to undefined group ".concat($0));[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          return args[dollarToken] || '';[m
[32m+[m[32m        } // `$` followed by an unsupported char is an error, unlike native JS[m
[32m+[m
[32m+[m
[32m+[m[32m        throw new SyntaxError("Invalid token ".concat($0));[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (isRegex) {[m
[32m+[m[32m    if (search.global) {[m
[32m+[m[32m      // Fixes IE, Safari bug (last tested IE 9, Safari 5.1)[m
[32m+[m[32m      search.lastIndex = 0;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // Fixes IE, Opera bug (last tested IE 9, Opera 11.6)[m
[32m+[m[32m      search.lastIndex = origLastIndex;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return result;[m
[32m+[m[32m};[m
[32m+[m[32m/**[m
[32m+[m[32m * Fixes browser bugs in the native `String.prototype.split`. Use via `XRegExp.split`.[m
[32m+[m[32m *[m
[32m+[m[32m * @memberOf String[m
[32m+[m[32m * @param {RegExp|String} separator Regex or string to use for separating the string.[m
[32m+[m[32m * @param {Number} [limit] Maximum number of items to include in the result array.[m
[32m+[m[32m * @returns {!Array} Array of substrings.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mfixed.split = function (separator, limit) {[m
[32m+[m[32m  if (!XRegExp.isRegExp(separator)) {[m
[32m+[m[32m    // Browsers handle nonregex split correctly, so use the faster native method[m
[32m+[m[32m    return String.prototype.split.apply(this, arguments);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var str = String(this);[m
[32m+[m[32m  var output = [];[m
[32m+[m[32m  var origLastIndex = separator.lastIndex;[m
[32m+[m[32m  var lastLastIndex = 0;[m
[32m+[m[32m  var lastLength; // Values for `limit`, per the spec:[m
[32m+[m[32m  // If undefined: pow(2,32) - 1[m
[32m+[m[32m  // If 0, Infinity, or NaN: 0[m
[32m+[m[32m  // If positive number: limit = floor(limit); if (limit >= pow(2,32)) limit -= pow(2,32);[m
[32m+[m[32m  // If negative number: pow(2,32) - floor(abs(limit))[m
[32m+[m[32m  // If other: Type-convert, then use the above rules[m
[32m+[m[32m  // This line fails in very strange ways for some values of `limit` in Opera 10.5-10.63, unless[m
[32m+[m[32m  // Opera Dragonfly is open (go figure). It works in at least Opera 9.5-10.1 and 11+[m
[32m+[m
[32m+[m[32m  limit = (limit === undefined ? -1 : limit) >>> 0;[m
[32m+[m[32m  (0, _forEach["default"])(XRegExp).call(XRegExp, str, separator, function (match) {[m
[32m+[m[32m    // This condition is not the same as `if (match[0].length)`[m
[32m+[m[32m    if (match.index + match[0].length > lastLastIndex) {[m
[32m+[m[32m      output.push((0, _slice["default"])(str).call(str, lastLastIndex, match.index));[m
[32m+[m
[32m+[m[32m      if (match.length > 1 && match.index < str.length) {[m
[32m+[m[32m        Array.prototype.push.apply(output, (0, _slice["default"])(match).call(match, 1));[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      lastLength = match[0].length;[m
[32m+[m[32m      lastLastIndex = match.index + lastLength;[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  if (lastLastIndex === str.length) {[m
[32m+[m[32m    if (!separator.test('') || lastLength) {[m
[32m+[m[32m      output.push('');[m
[32m+[m[32m    }[m
[32m+[m[32m  } else {[m
[32m+[m[32m    output.push((0, _slice["default"])(str).call(str, lastLastIndex));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  separator.lastIndex = origLastIndex;[m
[32m+[m[32m  return output.length > limit ? (0, _slice["default"])(output).call(output, 0, limit) : output;[m
[32m+[m[32m}; // ==--------------------------==[m
[32m+[m[32m// Built-in syntax/flag tokens[m
[32m+[m[32m// ==--------------------------==[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * Letter escapes that natively match literal characters: `\a`, `\A`, etc. These should be[m
[32m+[m[32m * SyntaxErrors but are allowed in web reality. XRegExp makes them errors for cross-browser[m
[32m+[m[32m * consistency and to reserve their syntax, but lets them be superseded by addons.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.addToken(/\\([ABCE-RTUVXYZaeg-mopqyz]|c(?![A-Za-z])|u(?![\dA-Fa-f]{4}|{[\dA-Fa-f]+})|x(?![\dA-Fa-f]{2}))/, function (match, scope) {[m
[32m+[m[32m  // \B is allowed in default scope only[m
[32m+[m[32m  if (match[1] === 'B' && scope === defaultScope) {[m
[32m+[m[32m    return match[0];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  throw new SyntaxError("Invalid escape ".concat(match[0]));[m
[32m+[m[32m}, {[m
[32m+[m[32m  scope: 'all',[m
[32m+[m[32m  leadChar: '\\'[m
[32m+[m[32m});[m
[32m+[m[32m/*[m
[32m+[m[32m * Unicode code point escape with curly braces: `\u{N..}`. `N..` is any one or more digit[m
[32m+[m[32m * hexadecimal number from 0-10FFFF, and can include leading zeros. Requires the native ES6 `u` flag[m
[32m+[m[32m * to support code points greater than U+FFFF. Avoids converting code points above U+FFFF to[m
[32m+[m[32m * surrogate pairs (which could be done without flag `u`), since that could lead to broken behavior[m
[32m+[m[32m * if you follow a `\u{N..}` token that references a code point above U+FFFF with a quantifier, or[m
[32m+[m[32m * if you use the same in a character class.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mXRegExp.addToken(/\\u{([\dA-Fa-f]+)}/, function (match, scope, flags) {[m
[32m+[m[32m  var code = dec(match[1]);[m
[32m+[m
[32m+[m[32m  if (code > 0x10FFFF) {[m
[32m+[m[32m    throw new SyntaxError("Invalid Unicode code point ".concat(match[0]));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (code <= 0xFFFF) {[m
[32m+[m[32m    // Converting to \uNNNN avoids needing to escape the literal character and keep it[m
[32m+[m[32m    // separate from preceding tokens[m
[32m+[m[32m    return "\\u".concat(pad4(hex(code)));[m
[32m+[m[32m  } // If `code` is between 0xFFFF and 0x10FFFF, require and defer to native handling[m
[32m+[m
[32m+[m
[32m+[m[32m  if (hasNativeU && (0, _indexOf["default"])(flags).call(flags, 'u') !== -1) {[m
[32m+[m[32m    return match[0];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  throw new SyntaxError('Cannot use Unicode code point above \\u{FFFF} without flag u');[m
[32m+[m[32m}, {[m
[32m+[m[32m  scope: 'all',[m
[32m+[m[32m  leadChar: '\\'[m
[32m+[m[32m});[m
[32m+[m[32m/*[m
[32m+[m[32m * Comment pattern: `(?# )`. Inline comments are an alternative to the line comments allowed in[m
[32m+[m[32m * free-spacing mode (flag x).[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mXRegExp.addToken(/\(\?#[^)]*\)/, getContextualTokenSeparator, {[m
[32m+[m[32m  leadChar: '('[m
[32m+[m[32m});[m
[32m+[m[32m/*[m
[32m+[m[32m * Whitespace and line comments, in free-spacing mode (aka extended mode, flag x) only.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mXRegExp.addToken(/\s+|#[^\n]*\n?/, getContextualTokenSeparator, {[m
[32m+[m[32m  flag: 'x'[m
[32m+[m[32m});[m
[32m+[m[32m/*[m
[32m+[m[32m * Dot, in dotAll mode (aka singleline mode, flag s) only.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mif (!hasNativeS) {[m
[32m+[m[32m  XRegExp.addToken(/\./, function () {[m
[32m+[m[32m    return '[\\s\\S]';[m
[32m+[m[32m  }, {[m
[32m+[m[32m    flag: 's',[m
[32m+[m[32m    leadChar: '.'[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m[32m/*[m
[32m+[m[32m * Named backreference: `\k<name>`. Backreference names can use RegExpIdentifierName characters[m
[32m+[m[32m * only. Also allows numbered backreferences as `\k<n>`.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m
[32m+[m[32mXRegExp.addToken(/\\k<([^>]+)>/, function (match) {[m
[32m+[m[32m  var _context6, _context7;[m
[32m+[m
[32m+[m[32m  // Groups with the same name is an error, else would need `lastIndexOf`[m
[32m+[m[32m  var index = isNaN(match[1]) ? (0, _indexOf["default"])(_context6 = this.captureNames).call(_context6, match[1]) + 1 : +match[1];[m
[32m+[m[32m  var endIndex = match.index + match[0].length;[m
[32m+[m
[32m+[m[32m  if (!index || index > this.captureNames.length) {[m
[32m+[m[32m    throw new SyntaxError("Backreference to undefined group ".concat(match[0]));[m
[32m+[m[32m  } // Keep backreferences separate from subsequent literal numbers. This avoids e.g.[m
[32m+[m[32m  // inadvertedly changing `(?<n>)\k<n>1` to `()\11`.[m
[32m+[m
[32m+[m
[32m+[m[32m  return (0, _concat["default"])(_context7 = "\\".concat(index)).call(_context7, endIndex === match.input.length || isNaN(match.input[endIndex]) ? '' : '(?:)');[m
[32m+[m[32m}, {[m
[32m+[m[32m  leadChar: '\\'[m
[32m+[m[32m});[m
[32m+[m[32m/*[m
[32m+[m[32m * Numbered backreference or octal, plus any following digits: `\0`, `\11`, etc. Octals except `\0`[m
[32m+[m[32m * not followed by 0-9 and backreferences to unopened capture groups throw an error. Other matches[m
[32m+[m[32m * are returned unaltered. IE < 9 doesn't support backreferences above `\99` in regex syntax.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mXRegExp.addToken(/\\(\d+)/, function (match, scope) {[m
[32m+[m[32m  if (!(scope === defaultScope && /^[1-9]/.test(match[1]) && +match[1] <= this.captureNames.length) && match[1] !== '0') {[m
[32m+[m[32m    throw new SyntaxError("Cannot use octal escape or backreference to undefined group ".concat(match[0]));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return match[0];[m
[32m+[m[32m}, {[m
[32m+[m[32m  scope: 'all',[m
[32m+[m[32m  leadChar: '\\'[m
[32m+[m[32m});[m
[32m+[m[32m/*[m
[32m+[m[32m * Named capturing group; match the opening delimiter only: `(?<name>`. Capture names can use the[m
[32m+[m[32m * RegExpIdentifierName characters only. Names can't be integers. Supports Python-style[m
[32m+[m[32m * `(?P<name>` as an alternate syntax to avoid issues in some older versions of Opera which natively[m
[32m+[m[32m * supported the Python-style syntax. Otherwise, XRegExp might treat numbered backreferences to[m
[32m+[m[32m * Python-style named capture as octals.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mXRegExp.addToken(/\(\?P?<((?:[\$A-Z_a-z\xAA\xB5\xBA\xC0-\xD6\xD8-\xF6\xF8-\u02C1\u02C6-\u02D1\u02E0-\u02E4\u02EC\u02EE\u0370-\u0374\u0376\u0377\u037A-\u037D\u037F\u0386\u0388-\u038A\u038C\u038E-\u03A1\u03A3-\u03F5\u03F7-\u0481\u048A-\u052F\u0531-\u0556\u0559\u0560-\u0588\u05D0-\u05EA\u05EF-\u05F2\u0620-\u064A\u066E\u066F\u0671-\u06D3\u06D5\u06E5\u06E6\u06EE\u06EF\u06FA-\u06FC\u06FF\u0710\u0712-\u072F\u074D-\u07A5\u07B1\u07CA-\u07EA\u07F4\u07F5\u07FA\u0800-\u0815\u081A\u0824\u0828\u0840-\u0858\u0860-\u086A\u0870-\u0887\u0889-\u088E\u08A0-\u08C9\u0904-\u0939\u093D\u0950\u0958-\u0961\u0971-\u0980\u0985-\u098C\u098F\u0990\u0993-\u09A8\u09AA-\u09B0\u09B2\u09B6-\u09B9\u09BD\u09CE\u09DC\u09DD\u09DF-\u09E1\u09F0\u09F1\u09FC\u0A05-\u0A0A\u0A0F\u0A10\u0A13-\u0A28\u0A2A-\u0A30\u0A32\u0A33\u0A35\u0A36\u0A38\u0A39\u0A59-\u0A5C\u0A5E\u0A72-\u0A74\u0A85-\u0A8D\u0A8F-\u0A91\u0A93-\u0AA8\u0AAA-\u0AB0\u0AB2\u0AB3\u0AB5-\u0AB9\u0ABD\u0AD0\u0AE0\u0AE1\u0AF9\u0B05-\u0B0C\u0B0F\u0B10\u0B13-\u0B28\u0B2A-\u0B30\u0B32\u0B33\u0B35-\u0B39\u0B3D\u0B5C\u0B5D\u0B5F-\u0B61\u0B71\u0B83\u0B85-\u0B8A\u0B8E-\u0B90\u0B92-\u0B95\u0B99\u0B9A\u0B9C\u0B9E\u0B9F\u0BA3\u0BA4\u0BA8-\u0BAA\u0BAE-\u0BB9\u0BD0\u0C05-\u0C0C\u0C0E-\u0C10\u0C12-\u0C28\u0C2A-\u0C39\u0C3D\u0C58-\u0C5A\u0C5D\u0C60\u0C61\u0C80\u0C85-\u0C8C\u0C8E-\u0C90\u0C92-\u0CA8\u0CAA-\u0CB3\u0CB5-\u0CB9\u0CBD\u0CDD\u0CDE\u0CE0\u0CE1\u0CF1\u0CF2\u0D04-\u0D0C\u0D0E-\u0D10\u0D12-\u0D3A\u0D3D\u0D4E\u0D54-\u0D56\u0D5F-\u0D61\u0D7A-\u0D7F\u0D85-\u0D96\u0D9A-\u0DB1\u0DB3-\u0DBB\u0DBD\u0DC0-\u0DC6\u0E01-\u0E30\u0E32\u0E33\u0E40-\u0E46\u0E81\u0E82\u0E84\u0E86-\u0E8A\u0E8C-\u0EA3\u0EA5\u0EA7-\u0EB0\u0EB2\u0EB3\u0EBD\u0EC0-\u0EC4\u0EC6\u0EDC-\u0EDF\u0F00\u0F40-\u0F47\u0F49-\u0F6C\u0F88-\u0F8C\u1000-\u102A\u103F\u1050-\u1055\u105A-\u105D\u1061\u1065\u1066\u106E-\u1070\u1075-\u1081\u108E\u10A0-\u10C5\u10C7\u10CD\u10D0-\u10FA\u10FC-\u1248\u124A-\u124D\u1250-\u1256\u1258\u125A-\u125D\u1260-\u1288\u128A-\u128D\u1290-\u12B0\u12B2-\u12B5\u12B8-\u12BE\