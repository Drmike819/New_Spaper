[32m+[m[32m/*global gettext, interpolate, ngettext*/[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    function show(selector) {[m
[32m+[m[32m        document.querySelectorAll(selector).forEach(function(el) {[m
[32m+[m[32m            el.classList.remove('hidden');[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function hide(selector) {[m
[32m+[m[32m        document.querySelectorAll(selector).forEach(function(el) {[m
[32m+[m[32m            el.classList.add('hidden');[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function showQuestion(options) {[m
[32m+[m[32m        hide(options.acrossClears);[m
[32m+[m[32m        show(options.acrossQuestions);[m
[32m+[m[32m        hide(options.allContainer);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function showClear(options) {[m
[32m+[m[32m        show(options.acrossClears);[m
[32m+[m[32m        hide(options.acrossQuestions);[m
[32m+[m[32m        document.querySelector(options.actionContainer).classList.remove(options.selectedClass);[m
[32m+[m[32m        show(options.allContainer);[m
[32m+[m[32m        hide(options.counterContainer);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function reset(options) {[m
[32m+[m[32m        hide(options.acrossClears);[m
[32m+[m[32m        hide(options.acrossQuestions);[m
[32m+[m[32m        hide(options.allContainer);[m
[32m+[m[32m        show(options.counterContainer);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function clearAcross(options) {[m
[32m+[m[32m        reset(options);[m
[32m+[m[32m        const acrossInputs = document.querySelectorAll(options.acrossInput);[m
[32m+[m[32m        acrossInputs.forEach(function(acrossInput) {[m
[32m+[m[32m            acrossInput.value = 0;[m
[32m+[m[32m        });[m
[32m+[m[32m        document.querySelector(options.actionContainer).classList.remove(options.selectedClass);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function checker(actionCheckboxes, options, checked) {[m
[32m+[m[32m        if (checked) {[m
[32m+[m[32m            showQuestion(options);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            reset(options);[m
[32m+[m[32m        }[m
[32m+[m[32m        actionCheckboxes.forEach(function(el) {[m
[32m+[m[32m            el.checked = checked;[m
[32m+[m[32m            el.closest('tr').classList.toggle(options.selectedClass, checked);[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function updateCounter(actionCheckboxes, options) {[m
[32m+[m[32m        const sel = Array.from(actionCheckboxes).filter(function(el) {[m
[32m+[m[32m            return el.checked;[m
[32m+[m[32m        }).length;[m
[32m+[m[32m        const counter = document.querySelector(options.counterContainer);[m
[32m+[m[32m        // data-actions-icnt is defined in the generated HTML[m
[32m+[m[32m        // and contains the total amount of objects in the queryset[m
[32m+[m[32m        const actions_icnt = Number(counter.dataset.actionsIcnt);[m
[32m+[m[32m        counter.textContent = interpolate([m
[32m+[m[32m            ngettext('%(sel)s of %(cnt)s selected', '%(sel)s of %(cnt)s selected', sel), {[m
[32m+[m[32m                sel: sel,[m
[32m+[m[32m                cnt: actions_icnt[m
[32m+[m[32m            }, true);[m
[32m+[m[32m        const allToggle = document.getElementById(options.allToggleId);[m
[32m+[m[32m        allToggle.checked = sel === actionCheckboxes.length;[m
[32m+[m[32m        if (allToggle.checked) {[m
[32m+[m[32m            showQuestion(options);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            clearAcross(options);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    const defaults = {[m
[32m+[m[32m        actionContainer: "div.actions",[m
[32m+[m[32m        counterContainer: "span.action-counter",[m
[32m+[m[32m        allContainer: "div.actions span.all",[m
[32m+[m[32m        acrossInput: "div.actions input.select-across",[m
[32m+[m[32m        acrossQuestions: "div.actions span.question",[m
[32m+[m[32m        acrossClears: "div.actions span.clear",[m
[32m+[m[32m        allToggleId: "action-toggle",[m
[32m+[m[32m        selectedClass: "selected"[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    window.Actions = function(actionCheckboxes, options) {[m
[32m+[m[32m        options = Object.assign({}, defaults, options);[m
[32m+[m[32m        let list_editable_changed = false;[m
[32m+[m[32m        let lastChecked = null;[m
[32m+[m[32m        let shiftPressed = false;[m
[32m+[m
[32m+[m[32m        document.addEventListener('keydown', (event) => {[m
[32m+[m[32m            shiftPressed = event.shiftKey;[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        document.addEventListener('keyup', (event) => {[m
[32m+[m[32m            shiftPressed = event.shiftKey;[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        document.getElementById(options.allToggleId).addEventListener('click', function(event) {[m
[32m+[m[32m            checker(actionCheckboxes, options, this.checked);[m
[32m+[m[32m            updateCounter(actionCheckboxes, options);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        document.querySelectorAll(options.acrossQuestions + " a").forEach(function(el) {[m
[32m+[m[32m            el.addEventListener('click', function(event) {[m
[32m+[m[32m                event.preventDefault();[m
[32m+[m[32m                const acrossInputs = document.querySelectorAll(options.acrossInput);[m
[32m+[m[32m                acrossInputs.forEach(function(acrossInput) {[m
[32m+[m[32m                    acrossInput.value = 1;[m
[32m+[m[32m                });[m
[32m+[m[32m                showClear(options);[m
[32m+[m[32m            });[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        document.querySelectorAll(options.acrossClears + " a").forEach(function(el) {[m
[32m+[m[32m            el.addEventListener('click', function(event) {[m
[32m+[m[32m                event.preventDefault();[m
[32m+[m[32m                document.getElementById(options.allToggleId).checked = false;[m
[32m+[m[32m                clearAcross(options);[m
[32m+[m[32m                checker(actionCheckboxes, options, false);[m
[32m+[m[32m                updateCounter(actionCheckboxes, options);[m
[32m+[m[32m            });[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        function affectedCheckboxes(target, withModifier) {[m
[32m+[m[32m            const multiSelect = (lastChecked && withModifier && lastChecked !== target);[m
[32m+[m[32m            if (!multiSelect) {[m
[32m+[m[32m                return [target];[m
[32m+[m[32m            }[m
[32m+[m[32m            const checkboxes = Array.from(actionCheckboxes);[m
[32m+[m[32m            const targetIndex = checkboxes.findIndex(el => el === target);[m
[32m+[m[32m            const lastCheckedIndex = checkboxes.findIndex(el => el === lastChecked);[m
[32m+[m[32m            const startIndex = Math.min(targetIndex, lastCheckedIndex);[m
[32m+[m[32m            const endIndex = Math.max(targetIndex, lastCheckedIndex);[m
[32m+[m[32m            const filtered = checkboxes.filter((el, index) => (startIndex <= index) && (index <= endIndex));[m
[32m+[m[32m            return filtered;[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        Array.from(document.getElementById('result_list').tBodies).forEach(function(el) {[m
[32m+[m[32m            el.addEventListener('change', function(event) {[m
[32m+[m[32m                const target = event.target;[m
[32m+[m[32m                if (target.classList.contains('action-select')) {[m
[32m+[m[32m                    const checkboxes = affectedCheckboxes(target, shiftPressed);[m
[32m+[m[32m                    checker(checkboxes, options, target.checked);[m
[32m+[m[32m                    updateCounter(actionCheckboxes, options);[m
[32m+[m[32m                    lastChecked = target;[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    list_editable_changed = true;[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        document.querySelector('#changelist-form button[name=index]').addEventListener('click', function(event) {[m
[32m+[m[32m            if (list_editable_changed) {[m
[32m+[m[32m                const confirmed = confirm(gettext("You have unsaved changes on individual editable fields. If you run an action, your unsaved changes will be lost."));[m
[32m+[m[32m                if (!confirmed) {[m
[32m+[m[32m                    event.preventDefault();[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        const el = document.querySelector('#changelist-form input[name=_save]');[m
[32m+[m[32m        // The button does not exist if no fields are editable.[m
[32m+[m[32m        if (el) {[m
[32m+[m[32m            el.addEventListener('click', function(event) {[m
[32m+[m[32m                if (document.querySelector('[name=action]').value) {[m
[32m+[m[32m                    const text = list_editable_changed[m
[32m+[m[32m                        ? gettext("You have selected an action, but you haven‚Äôt saved your changes to individual fields yet. Please click OK to save. You‚Äôll need to re-run the action.")[m
[32m+[m[32m                        : gettext("You have selected an action, and you haven‚Äôt made any changes on individual fields. You‚Äôre probably looking for the Go button rather than the Save button.");[m
[32m+[m[32m                    if (!confirm(text)) {[m
[32m+[m[32m                        event.preventDefault();[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m        // Sync counter when navigating to the page, such as through the back[m
[32m+[m[32m        // button.[m
[32m+[m[32m        window.addEventListener('pageshow', (event) => updateCounter(actionCheckboxes, options));[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Call function fn when the DOM is loaded and ready. If it is already[m
[32m+[m[32m    // loaded, call the function now.[m
[32m+[m[32m    // http://youmightnotneedjquery.com/#ready[m
[32m+[m[32m    function ready(fn) {[m
[32m+[m[32m        if (document.readyState !== 'loading') {[m
[32m+[m[32m            fn();[m
[32m+[m[32m        } else {[m
[32m+[m[32m            document.addEventListener('DOMContentLoaded', fn);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    ready(function() {[m
[32m+[m[32m        const actionsEls = document.querySelectorAll('tr input.action-select');[m
[32m+[m[32m        if (actionsEls.length > 0) {[m
[32m+[m[32m            Actions(actionsEls);[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
