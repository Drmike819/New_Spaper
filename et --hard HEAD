[32m+[m[32m/*global SelectBox, interpolate*/[m
[32m+[m[32m// Handles related-objects functionality: lookup link for raw_id_fields[m
[32m+[m[32m// and Add Another links.[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    const $ = django.jQuery;[m
[32m+[m[32m    let popupIndex = 0;[m
[32m+[m[32m    const relatedWindows = [];[m
[32m+[m
[32m+[m[32m    function dismissChildPopups() {[m
[32m+[m[32m        relatedWindows.forEach(function(win) {[m
[32m+[m[32m            if(!win.closed) {[m
[32m+[m[32m                win.dismissChildPopups();[m
[32m+[m[32m                win.close();[m[41m    [m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function setPopupIndex() {[m
[32m+[m[32m        if(document.getElementsByName("_popup").length > 0) {[m
[32m+[m[32m            const index = window.name.lastIndexOf("__") + 2;[m
[32m+[m[32m            popupIndex = parseInt(window.name.substring(index));[m[41m   [m
[32m+[m[32m        } else {[m
[32m+[m[32m            popupIndex = 0;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function addPopupIndex(name) {[m
[32m+[m[32m        return name + "__" + (popupIndex + 1);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function removePopupIndex(name) {[m
[32m+[m[32m        return name.replace(new RegExp("__" + (popupIndex + 1) + "$"), '');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function showAdminPopup(triggeringLink, name_regexp, add_popup) {[m
[32m+[m[32m        const name = addPopupIndex(triggeringLink.id.replace(name_regexp, ''));[m
[32m+[m[32m        const href = new URL(triggeringLink.href);[m
[32m+[m[32m        if (add_popup) {[m
[32m+[m[32m            href.searchParams.set('_popup', 1);[m
[32m+[m[32m        }[m
[32m+[m[32m        const win = window.open(href, name, 'height=500,width=800,resizable=yes,scrollbars=yes');[m
[32m+[m[32m        relatedWindows.push(win);[m
[32m+[m[32m        win.focus();[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function showRelatedObjectLookupPopup(triggeringLink) {[m
[32m+[m[32m        return showAdminPopup(triggeringLink, /^lookup_/, true);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function dismissRelatedLookupPopup(win, chosenId) {[m
[32m+[m[32m        const name = removePopupIndex(win.name);[m
[32m+[m[32m        const elem = document.getElementById(name);[m
[32m+[m[32m        if (elem.classList.contains('vManyToManyRawIdAdminField') && elem.value) {[m
[32m+[m[32m            elem.value += ',' + chosenId;[m
[32m+[m[32m        } else {[m
[32m+[m[32m            document.getElementById(name).value = chosenId;[m
[32m+[m[32m        }[m
[32m+[m[32m        const index = relatedWindows.indexOf(win);[m
[32m+[m[32m        if (index > -1) {[m
[32m+[m[32m            relatedWindows.splice(index, 1);[m
[32m+[m[32m        }[m
[32m+[m[32m        win.close();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function showRelatedObjectPopup(triggeringLink) {[m
[32m+[m[32m        return showAdminPopup(triggeringLink, /^(change|add|delete)_/, false);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function updateRelatedObjectLinks(triggeringLink) {[m
[32m+[m[32m        const $this = $(triggeringLink);[m
[32m+[m[32m        const siblings = $this.nextAll('.view-related, .change-related, .delete-related');[m
[32m+[m[32m        if (!siblings.length) {[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        const value = $this.val();[m
[32m+[m[32m        if (value) {[m
[32m+[m[32m            siblings.each(function() {[m
[32m+[m[32m                const elm = $(this);[m
[32m+[m[32m                elm.attr('href', elm.attr('data-href-template').replace('__fk__', value));[m
[32m+[m[32m                elm.removeAttr('aria-disabled');[m
[32m+[m[32m            });[m
[32m+[m[32m        } else {[m
[32m+[m[32m            siblings.removeAttr('href');[m
[32m+[m[32m            siblings.attr('aria-disabled', true);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function updateRelatedSelectsOptions(currentSelect, win, objId, newRepr, newId) {[m
[32m+[m[32m        // After create/edit a model from the options next to the current[m
[32m+[m[32m        // select (+ or :pencil:) update ForeignKey PK of the rest of selects[m
[32m+[m[32m        // in the page.[m
[32m+[m
[32m+[m[32m        const path = win.location.pathname;[m
[32m+[m[32m        // Extract the model from the popup url '.../<model>/add/' or[m
[32m+[m[32m        // '.../<model>/<id>/change/' depending the action (add or change).[m
[32m+[m[32m        const modelName = path.split('/')[path.split('/').length - (objId ? 4 : 3)];[m
[32m+[m[32m        // Exclude autocomplete selects.[m
[32m+[m[32m        const selectsRelated = document.querySelectorAll(`[data-model-ref="${modelName}"] select:not(.admin-autocomplete)`);[m
[32m+[m
[32m+[m[32m        selectsRelated.forEach(function(select) {[m
[32m+[m[32m            if (currentSelect === select) {[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            let option = select.querySelector(`option[value="${objId}"]`);[m
[32m+[m
[32m+[m[32m            if (!option) {[m
[32m+[m[32m                option = new Option(newRepr, newId);[m
[32m+[m[32m                select.options.add(option);[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            option.textContent = newRepr;[m
[32m+[m[32m            option.value = newId;[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function dismissAddRelatedObjectPopup(win, newId, newRepr) {[m
[32m+[m[32m        const name = removePopupIndex(win.name);[m
[32m+[m[32m        const elem = document.getElementById(name);[m
[32m+[m[32m        if (elem) {[m
[32m+[m[32m            const elemName = elem.nodeName.toUpperCase();[m
[32m+[m[32m            if (elemName === 'SELECT') {[m
[32m+[m[32m                elem.options[elem.options.length] = new Option(newRepr, newId, true, true);[m
[32m+[m[32m                updateRelatedSelectsOptions(elem, win, null, newRepr, newId);[m
[32m+[m[32m            } else if (elemName === 'INPUT') {[m
[32m+[m[32m                if (elem.classList.contains('vManyToManyRawIdAdminField') && elem.value) {[m
[32m+[m[32m                    elem.value += ',' + newId;[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    elem.value = newId;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            // Trigger a change event to update related links if required.[m
[32m+[m[32m            $(elem).trigger('change');[m
[32m+[m[32m        } else {[m
[32m+[m[32m            const toId = name + "_to";[m
[32m+[m[32m            const o = new Option(newRepr, newId);[m
[32m+[m[32m            SelectBox.add_to_cache(toId, o);[m
[32m+[m[32m            SelectBox.redisplay(toId);[m
[32m+[m[32m        }[m
[32m+[m[32m        const index = relatedWindows.indexOf(win);[m
[32m+[m[32m        if (index > -1) {[m
[32m+[m[32m            relatedWindows.splice(index, 1);[m
[32m+[m[32m        }[m
[32m+[m[32m        win.close();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function dismissChangeRelatedObjectPopup(win, objId, newRepr, newId) {[m
[32m+[m[32m        const id = removePopupIndex(win.name.replace(/^edit_/, ''));[m
[32m+[m[32m        const selectsSelector = interpolate('#%s, #%s_from, #%s_to', [id, id, id]);[m
[32m+[m[32m        const selects = $(selectsSelector);[m
[32m+[m[32m        selects.find('option').each(function() {[m
[32m+[m[32m            if (this.value === objId) {[m
[32m+[m[32m                this.textContent = newRepr;[m
[32m+[m[32m                this.value = newId;[m
[32m+[m[32m            }[m
[32m+[m[32m        }).trigger('change');[m
[32m+[m[32m        updateRelatedSelectsOptions(selects[0], win, objId, newRepr, newId);[m
[32m+[m[32m        selects.next().find('.select2-selection__rendered').each(function() {[m
[32m+[m[32m            // The element can have a clear button as a child.[m
[32m+[m[32m            // Use the lastChild to modify only the displayed value.[m
[32m+[m[32m            this.lastChild.textContent = newRepr;[m
[32m+[m[32m            this.title = newRepr;[m
[32m+[m[32m        });[m
[32m+[m[32m        const index = relatedWindows.indexOf(win);[m
[32m+[m[32m        if (index > -1) {[m
[32m+[m[32m            relatedWindows.splice(index, 1);[m
[32m+[m[32m        }[m
[32m+[m[32m        win.close();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function dismissDeleteRelatedObjectPopup(win, objId) {[m
[32m+[m[32m        const id = removePopupIndex(win.name.replace(/^delete_/, ''));[m
[32m+[m[32m        const selectsSelector = interpolate('#%s, #%s_from, #%s_to', [id, id, id]);[m
[32m+[m[32m        const selects = $(selectsSelector);[m
[32m+[m[32m        selects.find('option').each(function() {[m
[32m+[m[32m            if (this.value === objId) {[m
[32m+[m[32m                $(this).remove();[m
[32m+[m[32m            }[m
[32m+[m[32m        }).trigger('change');[m
[32m+[m[32m        const index = relatedWindows.indexOf(win);[m
[32m+[m[32m        if (index > -1) {[m
[32m+[m[32m            relatedWindows.splice(index, 1);[m
[32m+[m[32m        }[m
[32m+[m[32m        win.close();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    window.showRelatedObjectLookupPopup = showRelatedObjectLookupPopup;[m
[32m+[m[32m    window.dismissRelatedLookupPopup = dismissRelatedLookupPopup;[m
[32m+[m[32m    window.showRelatedObjectPopup = showRelatedObjectPopup;[m
[32m+[m[32m    window.updateRelatedObjectLinks = updateRelatedObjectLinks;[m
[32m+[m[32m    window.dismissAddRelatedObjectPopup = dismissAddRelatedObjectPopup;[m
[32m+[m[32m    window.dismissChangeRelatedObjectPopup = dismissChangeRelatedObjectPopup;[m
[32m+[m[32m    window.dismissDeleteRelatedObjectPopup = dismissDeleteRelatedObjectPopup;[m
[32m+[m[32m    window.dismissChildPopups = dismissChildPopups;[m
[32m+[m
[32m+[m[32m    // Kept for backward compatibility[m
[32m+[m[32m    window.showAddAnotherPopup = showRelatedObjectPopup;[m
[32m+[m[32m    window.dismissAddAnotherPopup = dismissAddRelatedObjectPopup;[m
[32m+[m
[32m+[m[32m    window.addEventListener('unload', function(evt) {[m
[32m+[m[32m        window.dismissChildPopups();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    $(document).ready(function() {[m
[32m+[m[32m        setPopupIndex();[m
[32m+[m[32m        $("a[data-popup-opener]").on('click', function(event) {[m
[32m+[m[32m            event.preventDefault();[m
[32m+[m[32m            opener.dismissRelatedLookupPopup(window, $(this).data("popup-opener"));[m
[32m+[m[32m        });[m
[32m+[m[32m        $('body').on('click', '.related-widget-wrapper-link[data-popup="yes"]', function(e) {[m
[32m+[m[32m            e.preventDefault();[m
[32m+[m[32m            if (this.href) {[m
[32m+[m[32m                const event = $.Event('django:show-related', {href: this.href});[m
[32m+[m[32m                $(this).trigger(event);[m
[32m+[m[32m                if (!event.isDefaultPrevented()) {[m
[32m+[m[32m                    showRelatedObjectPopup(this);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m        $('body').on('change', '.related-widget-wrapper select', function(e) {[m
[32m+[m[32m            const event = $.Event('django:update-related');[m
[32m+[m[32m            $(this).trigger(event);[m
[32m+[m[32m            if (!event.isDefaultPrevented()) {[m
[32m+[m[32m                updateRelatedObjectLinks(this);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m        $('.related-widget-wrapper select').trigger('change');[m
[32m+[m[32m        $('body').on('click', '.related-lookup', function(e) {[m
[32m+[m[32m            e.preventDefault();[m
[32m+[m[32m            const event = $.Event('django:lookup-related');[m
[32m+[m[32m            $(this).trigger(event);[m
[32m+[m[32m            if (!event.isDefaultPrevented()) {[m
[32m+[m[32m                showRelatedObjectLookupPopup(this);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
[1mdiff --git a/staticfiles/admin/js/autocomplete.js b/staticfiles/admin/js/autocomplete.js[m
[1mnew file mode 100644[m
[1mindex 0000000..d3daeab[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/autocomplete.js[m
[36m@@ -0,0 +1,33 @@[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    const $ = django.jQuery;[m
[32m+[m
