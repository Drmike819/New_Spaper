[32m+[m[32m    '\u03C2': '\u03C3',[m
[32m+[m[32m    '\u2019': '\''[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return diacritics;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/data/base',[[m
[32m+[m[32m  '../utils'[m
[32m+[m[32m], function (Utils) {[m
[32m+[m[32m  function BaseAdapter ($element, options) {[m
[32m+[m[32m    BaseAdapter.__super__.constructor.call(this);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Utils.Extend(BaseAdapter, Utils.Observable);[m
[32m+[m
[32m+[m[32m  BaseAdapter.prototype.current = function (callback) {[m
[32m+[m[32m    throw new Error('The `current` method must be defined in child classes.');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  BaseAdapter.prototype.query = function (params, callback) {[m
[32m+[m[32m    throw new Error('The `query` method must be defined in child classes.');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  BaseAdapter.prototype.bind = function (container, $container) {[m
[32m+[m[32m    // Can be implemented in subclasses[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  BaseAdapter.prototype.destroy = function () {[m
[32m+[m[32m    // Can be implemented in subclasses[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  BaseAdapter.prototype.generateResultId = function (container, data) {[m
[32m+[m[32m    var id = container.id + '-result-';[m
[32m+[m
[32m+[m[32m    id += Utils.generateChars(4);[m
[32m+[m
[32m+[m[32m    if (data.id != null) {[m
[32m+[m[32m      id += '-' + data.id.toString();[m
[32m+[m[32m    } else {[m
[32m+[m[32m      id += '-' + Utils.generateChars(4);[m
[32m+[m[32m    }[m
[32m+[m[32m    return id;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return BaseAdapter;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/data/select',[[m
[32m+[m[32m  './base',[m
[32m+[m[32m  '../utils',[m
[32m+[m[32m  'jquery'[m
[32m+[m[32m], function (BaseAdapter, Utils, $) {[m
[32m+[m[32m  function SelectAdapter ($element, options) {[m
[32m+[m[32m    this.$element = $element;[m
[32m+[m[32m    this.options = options;[m
[32m+[m
[32m+[m[32m    SelectAdapter.__super__.constructor.call(this);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Utils.Extend(SelectAdapter, BaseAdapter);[m
[32m+[m
[32m+[m[32m  SelectAdapter.prototype.current = function (callback) {[m
[32m+[m[32m    var data = [];[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    this.$element.find(':selected').each(function () {[m
[32m+[m[32m      var $option = $(this);[m
[32m+[m
[32m+[m[32m      var option = self.item($option);[m
[32m+[m
[32m+[m[32m      data.push(option);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    callback(data);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SelectAdapter.prototype.select = function (data) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    data.selected = true;[m
[32m+[m
[32m+[m[32m    // If data.element is a DOM node, use it instead[m
[32m+[m[32m    if ($(data.element).is('option')) {[m
[32m+[m[32m      data.element.selected = true;[m
[32m+[m
[32m+[m[32m      this.$element.trigger('input').trigger('change');[m
[32m+[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (this.$element.prop('multiple')) {[m
[32m+[m[32m      this.current(function (currentData) {[m
[32m+[m[32m        var val = [];[m
[32m+[m
[32m+[m[32m        data = [data];[m
[32m+[m[32m        data.push.apply(data, currentData);[m
[32m+[m
[32m+[m[32m        for (var d = 0; d < data.length; d++) {[m
[32m+[m[32m          var id = data[d].id;[m
[32m+[m
[32m+[m[32m          if ($.inArray(id, val) === -1) {[m
[32m+[m[32m            val.push(id);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        self.$element.val(val);[m
[32m+[m[32m        self.$element.trigger('input').trigger('change');[m
[32m+[m[32m      });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      var val = data.id;[m
[32m+[m
[32m+[m[32m      this.$element.val(val);[m
[32m+[m[32m      this.$element.trigger('input').trigger('change');[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SelectAdapter.prototype.unselect = function (data) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    if (!this.$element.prop('multiple')) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    data.selected = false;[m
[32m+[m
[32m+[m[32m    if ($(data.element).is('option')) {[m
[32m+[m[32m      data.element.selected = false;[m
[32m+[m
[32m+[m[32m      this.$element.trigger('input').trigger('change');[m
[32m+[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.current(function (currentData) {[m
[32m+[m[32m      var val = [];[m
[32m+[m
[32m+[m[32m      for (var d = 0; d < currentData.length; d++) {[m
[32m+[m[32m        var id = currentData[d].id;[m
[32m+[m
[32m+[m[32m        if (id !== data.id && $.inArray(id, val) === -1) {[m
[32m+[m[32m          val.push(id);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      self.$element.val(val);[m
[32m+[m
[32m+[m[32m      self.$element.trigger('input').trigger('change');[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SelectAdapter.prototype.bind = function (container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    this.container = container;[m
[32m+[m
[32m+[m[32m    container.on('select', function (params) {[m
[32m+[m[32m      self.select(params.data);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('unselect', function (params) {[m
[32m+[m[32m      self.unselect(params.data);[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SelectAdapter.prototype.destroy = function () {[m
[32m+[m[32m    // Remove anything added to child elements[m
[32m+[m[32m    this.$element.find('*').each(function () {[m
[32m+[m[32m      // Remove any custom data set by Select2[m
[32m+[m[32m      Utils.RemoveData(this);[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SelectAdapter.prototype.query = function (params, callback) {[m
[32m+[m[32m    var data = [];[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    var $options = this.$element.children();[m
[32m+[m
[32m+[m[32m    $options.each(function () {[m
[32m+[m[32m      var $option = $(this);[m
[32m+[m
[32m+[m[32m      if (!$option.is('option') && !$option.is('optgroup')) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var option = self.item($option);[m
[32m+[m
[32m+[m[32m      var matches = self.matches(params, option);[m
[32m+[m
[32m+[m[32m      if (matches !== null) {[m
[32m+[m[32m        data.push(matches);[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    callback({[m
[32m+[m[32m      results: data[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SelectAdapter.prototype.addOptions = function ($options) {[m
[32m+[m[32m    Utils.appendMany(this.$element, $options);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SelectAdapter.prototype.option = function (data) {[m
[32m+[m[32m    var option;[m
[32m+[m
[32m+[m[32m    if (data.children) {[m
[32m+[m[32m      option = document.createElement('optgroup');[m
[32m+[m[32m      option.label = data.text;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      option = document.createElement('option');[m
[32m+[m
[32m+[m[32m      if (option.textContent !== undefined) {[m
[32m+[m[32m        option.textContent = data.text;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        option.innerText = data.text;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (data.id !== undefined) {[m
[32m+[m[32m      option.value = data.id;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (data.disabled) {[m
[32m+[m[32m      option.disabled = true;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (data.selected) {[m
[32m+[m[32m      option.selected = true;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (data.title) {[m
[32m+[m[32m      option.title = data.title;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var $option = $(option);[m
[32m+[m
[32m+[m[32m    var normalizedData = this._normalizeItem(data);[m
[32m+[m[32m    normalizedData.element = option;[m
[32m+[m
[32m+[m[32m    // Override the option's data with the combined data[m
[32m+[m[32m    Utils.StoreData(option, 'data', normalizedData);[m
[32m+[m
[32m+[m[32m    return $option;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SelectAdapter.prototype.item = function ($option) {[m
[32m+[m[32m    var data = {};[m
[32m+[m
[32m+[m[32m    data = Utils.GetData($option[0], 'data');[m
[32m+[m
[32m+[m[32m    if (data != null) {[m
[32m+[m[32m      return data;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if ($option.is('option')) {[m
[32m+[m[32m      data = {[m
[32m+[m[32m        id: $option.val(),[m
[32m+[m[32m        text: $option.text(),[m
[32m+[m[32m        disabled: $option.prop('disabled'),[m
[32m+[m[32m        selected: $option.prop('selected'),[m
[32m+[m[32m        title: $option.prop('title')[m
[32m+[m[32m      };[m
[32m+[m[32m    } else if ($option.is('optgroup')) {[m
[32m+[m[32m      data = {[m
[32m+[m[32m        text: $option.prop('label'),[m
[32m+[m[32m        children: [],[m
[32m+[m[32m        title: $option.prop('title')[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      var $children = $option.children('option');[m
[32m+[m[32m      var children = [];[m
[32m+[m
[32m+[m[32m      for (var c = 0; c < $children.length; c++) {[m
[32m+[m[32m        var $child = $($children[c]);[m
[32m+[m
[32m+[m[32m        var child = this.item($child);[m
[32m+[m
[32m+[m[32m        children.push(child);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      data.children = children;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    data = this._normalizeItem(data);[m
[32m+[m[32m    data.element = $option[0];[m
[32m+[m
[32m+[m[32m    Utils.StoreData($option[0], 'data', data);[m
[32m+[m
[32m+[m[32m    return data;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SelectAdapter.prototype._normalizeItem = function (item) {[m
[32m+[m[32m    if (item !== Object(item)) {[m
[32m+[m[32m      item = {[m
[32m+[m[32m        id: item,[m
[32m+[m[32m        text: item[m
[32m+[m[32m      };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    item = $.extend({}, {[m
[32m+[m[32m      text: ''[m
[32m+[m[32m    }, item);[m
[32m+[m
[32m+[m[32m    var defaults = {[m
[32m+[m[32m      selected: false,[m
[32m+[m[32m      disabled: false[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    if (item.id != null) {[m
[32m+[m[32m      item.id = item.id.toString();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (item.text != null) {[m
[32m+[m[32m      item.text = item.text.toString();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (item._resultId == null && item.id && this.container != null) {[m
[32m+[m[32m      item._resultId = this.generateResultId(this.container, item);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return $.extend({}, defaults, item);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SelectAdapter.prototype.matches = function (params, data) {[m
[32m+[m[32m    var matcher = this.options.get('matcher');[m
[32m+[m
[32m+[m[32m    return matcher(params, data);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return SelectAdapter;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/data/array',[[m
[32m+[m[32m  './select',[m
[32m+[m[32m  '../utils',[m
[32m+[m[32m  'jquery'[m
[32m+[m[32m], function (SelectAdapter, Utils, $) {[m
[32m+[m[32m  function ArrayAdapter ($element, options) {[m
[32m+[m[32m    this._dataToConvert = options.get('data') || [];[m
[32m+[m
[32m+[m[32m    ArrayAdapter.__super__.constructor.call(this, $element, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Utils.Extend(ArrayAdapter, SelectAdapter);[m
[32m+[m
[32m+[m[32m  ArrayAdapter.prototype.bind = function (container, $container) {[m
[32m+[m[32m    ArrayAdapter.__super__.bind.call(this, container, $container);[m
[32m+[m
[32m+[m[32m    this.addOptions(this.convertToOptions(this._dataToConvert));[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ArrayAdapter.prototype.select = function (data) {[m
[32m+[m[32m    var $option = this.$element.find('option').filter(function (i, elm) {[m
[32m+[m[32m      return elm.value == data.id.toString();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    if ($option.length === 0) {[m
[32m+[m[32m      $option = this.option(data);[m
[32m+[m
[32m+[m[32m      this.addOptions($option);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    ArrayAdapter.__super__.select.call(this, data);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  ArrayAdapter.prototype.convertToOptions = function (data) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    var $existing = this.$element.find('option');[m
[32m+[m[32m    var existingIds = $existing.map(function () {[m
[32m+[m[32m      return self.item($(this)).id;[m
[32m+[m[32m    }).get();[m
[32m+[m
[32m+[m[32m    var $options = [];[m
[32m+[m
[32m+[m[32m    // Filter out all items except for the one passed in the argument[m
[32m+[m[32m    function onlyItem (item) {[m
[32m+[m[32m      return function () {[m
[32m+[m[32m        return $(this).val() == item.id;[m
[32m+[m[32m      };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    for (var d = 0; d < data.length; d++) {[m
[32m+[m[32m      var item = this._normalizeItem(data[d]);[m
[32m+[m
[32m+[m[32m      // Skip items which were pre-loaded, only merge the data[m
[32m+[m[32m      if ($.inArray(item.id, existingIds) >= 0) {[m
[32m+[m[32m        var $existingOption = $existing.filter(onlyItem(item));[m
[32m+[m
[32m+[m[32m        var existingData = this.item($existingOption);[m
[32m+[m[32m        var newData = $.extend(true, {}, item, existingData);[m
[32m+[m
[32m+[m[32m        var $newOption = this.option(newData);[m
[32m+[m
[32m+[m[32m        $existingOption.replaceWith($newOption);[m
[32m+[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var $option = this.option(item);[m
[32m+[m
[32m+[m[32m      if (item.children) {[m
[32m+[m[32m        var $children = this.convertToOptions(item.children);[m
[32m+[m
[32m+[m[32m        Utils.appendMany($option, $children);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      $options.push($option);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return $options;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return ArrayAdapter;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/data/ajax',[[m
[32m+[m[32m  './array',[m
[32m+[m[32m  '../utils',[m
[32m+[m[32m  'jquery'[m
[32m+[m[32m], function (ArrayAdapter, Utils, $) {[m
[32m+[m[32m  function AjaxAdapter ($element, options) {[m
[32m+[m[32m    this.ajaxOptions = this._applyDefaults(options.get('ajax'));[m
[32m+[m
[32m+[m[32m    if (this.ajaxOptions.processResults != null) {[m
[32m+[m[32m      this.processResults = this.ajaxOptions.processResults;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    AjaxAdapter.__super__.constructor.call(this, $element, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Utils.Extend(AjaxAdapter, ArrayAdapter);[m
[32m+[m
[32m+[m[32m  AjaxAdapter.prototype._applyDefaults = function (options) {[m
[32m+[m[32m    var defaults = {[m
[32m+[m[32m      data: function (params) {[m
[32m+[m[32m        return $.extend({}, params, {[m
[32m+[m[32m          q: params.term[m
[32m+[m[32m        });[m
[32m+[m[32m      },[m
[32m+[m[32m      transport: function (params, success, failure) {[m
[32m+[m[32m        var $request = $.ajax(params);[m
[32m+[m
[32m+[m[32m        $request.then(success);[m
[32m+[m[32m        $request.fail(failure);[m
[32m+[m
[32m+[m[32m        return $request;[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    return $.extend({}, defaults, options, true);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AjaxAdapter.prototype.processResults = function (results) {[m
[32m+[m[32m    return results;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AjaxAdapter.prototype.query = function (params, callback) {[m
[32m+[m[32m    var matches = [];[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    if (this._request != null) {[m
[32m+[m[32m      // JSONP requests cannot always be aborted[m
[32m+[m[32m      if ($.isFunction(this._request.abort)) {[m
[32m+[m[32m        this._request.abort();[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this._request = null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var options = $.extend({[m
[32m+[m[32m      type: 'GET'[m
[32m+[m[32m    }, this.ajaxOptions);[m
[32m+[m
[32m+[m[32m    if (typeof options.url === 'function') {[m
[32m+[m[32m      options.url = options.url.call(this.$element, params);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof options.data === 'function') {[m
[32m+[m[32m      options.data = options.data.call(this.$element, params);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function request () {[m
[32m+[m[32m      var $request = options.transport(options, function (data) {[m
[32m+[m[32m        var results = self.processResults(data, params);[m
[32m+[m
[32m+[m[32m        if (self.options.get('debug') && window.console && console.error) {[m
[32m+[m[32m          // Check to make sure that the response included a `results` key.[m
[32m+[m[32m          if (!results || !results.results || !$.isArray(results.results)) {[m
[32m+[m[32m            console.error([m
[32m+[m[32m              'Select2: The AJAX results did not return an array in the ' +[m
[32m+[m[32m              '`results` key of the response.'[m
[32m+[m[32m            );[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        callback(results);[m
[32m+[m[32m      }, function () {[m
[32m+[m[32m        // Attempt to detect if a request was aborted[m
[32m+[m[32m        // Only works if the transport exposes a status property[m
[32m+[m[32m        if ('status' in $request &&[m
[32m+[m[32m            ($request.status === 0 || $request.status === '0')) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        self.trigger('results:message', {[m
[32m+[m[32m          message: 'errorLoading'[m
[32m+[m[32m        });[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      self._request = $request;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (this.ajaxOptions.delay && params.term != null) {[m
[32m+[m[32m      if (this._queryTimeout) {[m
[32m+[m[32m        window.clearTimeout(this._queryTimeout);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this._queryTimeout = window.setTimeout(request, this.ajaxOptions.delay);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      request();[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return AjaxAdapter;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/data/tags',[[m
[32m+[m[32m  'jquery'[m
[32m+[m[32m], function ($) {[m
[32m+[m[32m  function Tags (decorated, $element, options) {[m
[32m+[m[32m    var tags = options.get('tags');[m
[32m+[m
[32m+[m[32m    var createTag = options.get('createTag');[m
[32m+[m
[32m+[m[32m    if (createTag !== undefined) {[m
[32m+[m[32m      this.createTag = createTag;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var insertTag = options.get('insertTag');[m
[32m+[m
[32m+[m[32m    if (insertTag !== undefined) {[m
[32m+[m[32m        this.insertTag = insertTag;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    decorated.call(this, $element, options);[m
[32m+[m
[32m+[m[32m    if ($.isArray(tags)) {[m
[32m+[m[32m      for (var t = 0; t < tags.length; t++) {[m
[32m+[m[32m        var tag = tags[t];[m
[32m+[m[32m        var item = this._normalizeItem(tag);[m
[32m+[m
[32m+[m[32m        var $option = this.option(item);[m
[32m+[m
[32m+[m[32m        this.$element.append($option);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Tags.prototype.query = function (decorated, params, callback) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    this._removeOldTags();[m
[32m+[m
[32m+[m[32m    if (params.term == null || params.page != null) {[m
[32m+[m[32m      decorated.call(this, params, callback);[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function wrapper (obj, child) {[m
[32m+[m[32m      var data = obj.results;[m
[32m+[m
[32m+[m[32m      for (var i = 0; i < data.length; i++) {[m
[32m+[m[32m        var option = data[i];[m
[32m+[m
[32m+[m[32m        var checkChildren = ([m
[32m+[m[32m          option.children != null &&[m
[32m+[m[32m          !wrapper({[m
[32m+[m[32m            results: option.children[m
[32m+[m[32m          }, true)[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        var optionText = (option.text || '').toUpperCase();[m
[32m+[m[32m        var paramsTerm = (params.term || '').toUpperCase();[m
[32m+[m
[32m+[m[32m        var checkText = optionText === paramsTerm;[m
[32m+[m
[32m+[m[32m        if (checkText || checkChildren) {[m
[32m+[m[32m          if (child) {[m
[32m+[m[32m            return false;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          obj.data = data;[m
[32m+[m[32m          callback(obj);[m
[32m+[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (child) {[m
[32m+[m[32m        return true;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var tag = self.createTag(params);[m
[32m+[m
[32m+[m[32m      if (tag != null) {[m
[32m+[m[32m        var $option = self.option(tag);[m
[32m+[m[32m        $option.attr('data-select2-tag', true);[m
[32m+[m
[32m+[m[32m        self.addOptions([$option]);[m
[32m+[m
[32m+[m[32m        self.insertTag(data, tag);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      obj.results = data;[m
[32m+[m
[32m+[m[32m      callback(obj);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    decorated.call(this, params, wrapper);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Tags.prototype.createTag = function (decorated, params) {[m
[32m+[m[32m    var term = $.trim(params.term);[m
[32m+[m
[32m+[m[32m    if (term === '') {[m
[32m+[m[32m      return null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return {[m
[32m+[m[32m      id: term,[m
[32m+[m[32m      text: term[m
[32m+[m[32m    };[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Tags.prototype.insertTag = function (_, data, tag) {[m
[32m+[m[32m    data.unshift(tag);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Tags.prototype._removeOldTags = function (_) {[m
[32m+[m[32m    var $options = this.$element.find('option[data-select2-tag]');[m
[32m+[m
[32m+[m[32m    $options.each(function () {[m
[32m+[m[32m      if (this.selected) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      $(this).remove();[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return Tags;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/data/tokenizer',[[m
[32m+[m[32m  'jquery'[m
[32m+[m[32m], function ($) {[m
[32m+[m[32m  function Tokenizer (decorated, $element, options) {[m
[32m+[m[32m    var tokenizer = options.get('tokenizer');[m
[32m+[m
[32m+[m[32m    if (tokenizer !== undefined) {[m
[32m+[m[32m      this.tokenizer = tokenizer;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    decorated.call(this, $element, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Tokenizer.prototype.bind = function (decorated, container, $container) {[m
[32m+[m[32m    decorated.call(this, container, $container);[m
[32m+[m
[32m+[m[32m    this.$search =  container.dropdown.$search || container.selection.$search ||[m
[32m+[m[32m      $container.find('.select2-search__field');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Tokenizer.prototype.query = function (decorated, params, callback) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    function createAndSelect (data) {[m
[32m+[m[32m      // Normalize the data object so we can use it for checks[m
[32m+[m[32m      var item = self._normalizeItem(data);[m
[32m+[m
[32m+[m[32m      // Check if the data object already exists as a tag[m
[32m+[m[32m      // Select it if it doesn't[m
[32m+[m[32m      var $existingOptions = self.$element.find('option').filter(function () {[m
[32m+[m[32m        return $(this).val() === item.id;[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      // If an existing option wasn't found for it, create the option[m
[32m+[m[32m      if (!$existingOptions.length) {[m
[32m+[m[32m        var $option = self.option(item);[m
[32m+[m[32m        $option.attr('data-select2-tag', true);[m
[32m+[m
[32m+[m[32m        self._removeOldTags();[m
[32m+[m[32m        self.addOptions([$option]);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // Select the item, now that we know there is an option for it[m
[32m+[m[32m      select(item);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function select (data) {[m
[32m+[m[32m      self.trigger('select', {[m
[32m+[m[32m        data: data[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    params.term = params.term || '';[m
[32m+[m
[32m+[m[32m    var tokenData = this.tokenizer(params, this.options, createAndSelect);[m
[32m+[m
[32m+[m[32m    if (tokenData.term !== params.term) {[m
[32m+[m[32m      // Replace the search term if we have the search box[m
[32m+[m[32m      if (this.$search.length) {[m
[32m+[m[32m        this.$search.val(tokenData.term);[m
[32m+[m[32m        this.$search.trigger('focus');[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      params.term = tokenData.term;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    decorated.call(this, params, callback);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Tokenizer.prototype.tokenizer = function (_, params, options, callback) {[m
[32m+[m[32m    var separators = options.get('tokenSeparators') || [];[m
[32m+[m[32m    var term = params.term;[m
[32m+[m[32m    var i = 0;[m
[32m+[m
[32m+[m[32m    var createTag = this.createTag || function (params) {[m
[32m+[m[32m      return {[m
[32m+[m[32m        id: params.term,[m
[32m+[m[32m        text: params.term[m
[32m+[m[32m      };[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    while (i < term.length) {[m
[32m+[m[32m      var termChar = term[i];[m
[32m+[m
[32m+[m[32m      if ($.inArray(termChar, separators) === -1) {[m
[32m+[m[32m        i++;[m
[32m+[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var part = term.substr(0, i);[m
[32m+[m[32m      var partParams = $.extend({}, params, {[m
[32m+[m[32m        term: part[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      var data = createTag(partParams);[m
[32m+[m
[32m+[m[32m      if (data == null) {[m
[32m+[m[32m        i++;[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      callback(data);[m
[32m+[m
[32m+[m[32m      // Reset the term to not include the tokenized portion[m
[32m+[m[32m      term = term.substr(i + 1) || '';[m
[32m+[m[32m      i = 0;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return {[m
[32m+[m[32m      term: term[m
[32m+[m[32m    };[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return Tokenizer;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/data/minimumInputLength',[[m
[32m+[m
[32m+[m[32m], function () {[m
[32m+[m[32m  function MinimumInputLength (decorated, $e, options) {[m
[32m+[m[32m    this.minimumInputLength = options.get('minimumInputLength');[m
[32m+[m
[32m+[m[32m    decorated.call(this, $e, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  MinimumInputLength.prototype.query = function (decorated, params, callback) {[m
[32m+[m[32m    params.term = params.term || '';[m
[32m+[m
[32m+[m[32m    if (params.term.length < this.minimumInputLength) {[m
[32m+[m[32m      this.trigger('results:message', {[m
[32m+[m[32m        message: 'inputTooShort',[m
[32m+[m[32m        args: {[m
[32m+[m[32m          minimum: this.minimumInputLength,[m
[32m+[m[32m          input: params.term,[m
[32m+[m[32m          params: params[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    decorated.call(this, params, callback);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return MinimumInputLength;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/data/maximumInputLength',[[m
[32m+[m
[32m+[m[32m], function () {[m
[32m+[m[32m  function MaximumInputLength (decorated, $e, options) {[m
[32m+[m[32m    this.maximumInputLength = options.get('maximumInputLength');[m
[32m+[m
[32m+[m[32m    decorated.call(this, $e, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  MaximumInputLength.prototype.query = function (decorated, params, callback) {[m
[32m+[m[32m    params.term = params.term || '';[m
[32m+[m
[32m+[m[32m    if (this.maximumInputLength > 0 &&[m
[32m+[m[32m        params.term.length > this.maximumInputLength) {[m
[32m+[m[32m      this.trigger('results:message', {[m
[32m+[m[32m        message: 'inputTooLong',[m
[32m+[m[32m        args: {[m
[32m+[m[32m          maximum: this.maximumInputLength,[m
[32m+[m[32m          input: params.term,[m
[32m+[m[32m          params: params[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    decorated.call(this, params, callback);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return MaximumInputLength;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/data/maximumSelectionLength',[[m
[32m+[m
[32m+[m[32m], function (){[m
[32m+[m[32m  function MaximumSelectionLength (decorated, $e, options) {[m
[32m+[m[32m    this.maximumSelectionLength = options.get('maximumSelectionLength');[m
[32m+[m
[32m+[m[32m    decorated.call(this, $e, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  MaximumSelectionLength.prototype.bind =[m
[32m+[m[32m    function (decorated, container, $container) {[m
[32m+[m[32m      var self = this;[m
[32m+[m
[32m+[m[32m      decorated.call(this, container, $container);[m
[32m+[m
[32m+[m[32m      container.on('select', function () {[m
[32m+[m[32m        self._checkIfMaximumSelected();[m
[32m+[m[32m      });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  MaximumSelectionLength.prototype.query =[m
[32m+[m[32m    function (decorated, params, callback) {[m
[32m+[m[32m      var self = this;[m
[32m+[m
[32m+[m[32m      this._checkIfMaximumSelected(function () {[m
[32m+[m[32m        decorated.call(self, params, callback);[m
[32m+[m[32m      });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  MaximumSelectionLength.prototype._checkIfMaximumSelected =[m
[32m+[m[32m    function (_, successCallback) {[m
[32m+[m[32m      var self = this;[m
[32m+[m
[32m+[m[32m      this.current(function (currentData) {[m
[32m+[m[32m        var count = currentData != null ? currentData.length : 0;[m
[32m+[m[32m        if (self.maximumSelectionLength > 0 &&[m
[32m+[m[32m          count >= self.maximumSelectionLength) {[m
[32m+[m[32m          self.trigger('results:message', {[m
[32m+[m[32m            message: 'maximumSelected',[m
[32m+[m[32m            args: {[m
[32m+[m[32m              maximum: self.maximumSelectionLength[m
[32m+[m[32m            }[m
[32m+[m[32m          });[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (successCallback) {[m
[32m+[m[32m          successCallback();[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return MaximumSelectionLength;[m
[32m+[m[32m});[m
