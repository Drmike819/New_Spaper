[32m+[m[32m},{"../internals/create-property":80,"../internals/global":104,"../internals/length-of-array-like":123,"../internals/to-absolute-index":153}],68:[function(require,module,exports){[m
[32m+[m[32mvar uncurryThis = require('../internals/function-uncurry-this');[m
[32m+[m
[32m+[m[32mmodule.exports = uncurryThis([].slice);[m
[32m+[m
[32m+[m[32m},{"../internals/function-uncurry-this":99}],69:[function(require,module,exports){[m
[32m+[m[32mvar arraySlice = require('../internals/array-slice-simple');[m
[32m+[m
[32m+[m[32mvar floor = Math.floor;[m
[32m+[m
[32m+[m[32mvar mergeSort = function (array, comparefn) {[m
[32m+[m[32m  var length = array.length;[m
[32m+[m[32m  var middle = floor(length / 2);[m
[32m+[m[32m  return length < 8 ? insertionSort(array, comparefn) : merge([m
[32m+[m[32m    array,[m
[32m+[m[32m    mergeSort(arraySlice(array, 0, middle), comparefn),[m
[32m+[m[32m    mergeSort(arraySlice(array, middle), comparefn),[m
[32m+[m[32m    comparefn[m
[32m+[m[32m  );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar insertionSort = function (array, comparefn) {[m
[32m+[m[32m  var length = array.length;[m
[32m+[m[32m  var i = 1;[m
[32m+[m[32m  var element, j;[m
[32m+[m
[32m+[m[32m  while (i < length) {[m
[32m+[m[32m    j = i;[m
[32m+[m[32m    element = array[i];[m
[32m+[m[32m    while (j && comparefn(array[j - 1], element) > 0) {[m
[32m+[m[32m      array[j] = array[--j];[m
[32m+[m[32m    }[m
[32m+[m[32m    if (j !== i++) array[j] = element;[m
[32m+[m[32m  } return array;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar merge = function (array, left, right, comparefn) {[m
[32m+[m[32m  var llength = left.length;[m
[32m+[m[32m  var rlength = right.length;[m
[32m+[m[32m  var lindex = 0;[m
[32m+[m[32m  var rindex = 0;[m
[32m+[m
[32m+[m[32m  while (lindex < llength || rindex < rlength) {[m
[32m+[m[32m    array[lindex + rindex] = (lindex < llength && rindex < rlength)[m
[32m+[m[32m      ? comparefn(left[lindex], right[rindex]) <= 0 ? left[lindex++] : right[rindex++][m
[32m+[m[32m      : lindex < llength ? left[lindex++] : right[rindex++];[m
[32m+[m[32m  } return array;[m
[32m+[m[32m};[m
