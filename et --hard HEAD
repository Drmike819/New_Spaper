[32m+[m[32m},{"../internals/fails":94}],67:[function(require,module,exports){[m
[32m+[m[32mvar global = require('../internals/global');[m
[32m+[m[32mvar toAbsoluteIndex = require('../internals/to-absolute-index');[m
[32m+[m[32mvar lengthOfArrayLike = require('../internals/length-of-array-like');[m
[32m+[m[32mvar createProperty = require('../internals/create-property');[m
[32m+[m
[32m+[m[32mvar Array = global.Array;[m
[32m+[m[32mvar max = Math.max;[m
[32m+[m
[32m+[m[32mmodule.exports = function (O, start, end) {[m
[32m+[m[32m  var length = lengthOfArrayLike(O);[m
[32m+[m[32m  var k = toAbsoluteIndex(start, length);[m
