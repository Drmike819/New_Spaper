[32m+[m[32m#importamos todas la views y el path para crear las urls[m
[32m+[m[32mfrom django.urls import path[m
[32m+[m[32mfrom .views import ([m
[32m+[m[32m    ArticleCreateView,[m
[32m+[m[32m    ArticleDetailView,[m
[32m+[m[32m    ArticleUpdateView,[m
[32m+[m[32m    ArticleDeleteView,[m
[32m+[m[32m    ArticleCreateView,[m
[32m+[m[32m    CommentDeleteView,[m
[32m+[m[32m    CommentUpdateView,[m
[32m+[m[32m)[m
[32m+[m[32m#urls de los articulos[m
[32m+[m[32murlpatterns = [[m
[32m+[m[32m    path("", ArticleCreateView.as_view(), name="article_list"),[m
[32m+[m[32m    path("<int:pk>/edit/", ArticleUpdateView.as_view(), name="article_edit"),[m
[32m+[m[32m    path("<int:pk>/delete/", ArticleDeleteView.as_view(), name="article_delete"),[m
[32m+[m[32m    path("new/", ArticleCreateView.as_view(), name="article_new"),[m
[32m+[m[32m    path('comment/<int:pk>/edit/', CommentUpdateView.as_view(), name='comment_edit'),[m
[32m+[m[32m    path('comment/<int:pk>/delete/', CommentDeleteView.as_view(), name='comment_delete')[m
