[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    const $ = django.jQuery;[m
[32m+[m[32m    const fields = $('#django-admin-prepopulated-fields-constants').data('prepopulatedFields');[m
[32m+[m[32m    $.each(fields, function(index, field) {[m
[32m+[m[32m        $([m
[32m+[m[32m            '.empty-form .form-row .field-' + field.name +[m
[32m+[m[32m            ', .empty-form.form-row .field-' + field.name +[m
