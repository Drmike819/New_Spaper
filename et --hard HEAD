[32m+[m[32mvar fails = require('../internals/fails');[m
[32m+[m[32mvar internalSort = require('../internals/array-sort');[m
[32m+[m[32mvar arrayMethodIsStrict = require('../internals/array-method-is-strict');[m
[32m+[m[32mvar FF = require('../internals/engine-ff-version');[m
[32m+[m[32mvar IE_OR_EDGE = require('../internals/engine-is-ie-or-edge');[m
[32m+[m[32mvar V8 = require('../internals/engine-v8-version');[m
[32m+[m[32mvar WEBKIT = require('../internals/engine-webkit-version');[m
[32m+[m
[32m+[m[32mvar test = [];[m
[32m+[m[32mvar un$Sort = uncurryThis(test.sort);[m
[32m+[m[32mvar push = uncurryThis(test.push);[m
[32m+[m
[32m+[m[32m// IE8-[m
[32m+[m[32mvar FAILS_ON_UNDEFINED = fails(function () {[m
[32m+[m[32m  test.sort(undefined);[m
[32m+[m[32m});[m
[32m+[m[32m// V8 bug[m
[32m+[m[32mvar FAILS_ON_NULL = fails(function () {[m
[32m+[m[32m  test.sort(null);[m
[32m+[m[32m});[m
[32m+[m[32m// Old WebKit[m
[32m+[m[32mvar STRICT_METHOD = arrayMethodIsStrict('sort');[m
[32m+[m
[32m+[m[32mvar STABLE_SORT = !fails(function () {[m
[32m+[m[32m  // feature detection can be too slow, so check engines versions[m
[32m+[m[32m  if (V8) return V8 < 70;[m
[32m+[m[32m  if (FF && FF > 3) return;[m
[32m+[m[32m  if (IE_OR_EDGE) return true;[m
[32m+[m[32m  if (WEBKIT) return WEBKIT < 603;[m
[32m+[m
[32m+[m[32m  var result = '';[m
[32m+[m[32m  var code, chr, value, index;[m
[32m+[m
[32m+[m[32m  // generate an array with more 512 elements (Chakra and old V8 fails only in this case)[m
[32m+[m[32m  for (code = 65; code < 76; code++) {[m
[32m+[m[32m    chr = String.fromCharCode(code);[m
[32m+[m
[32m+[m[32m    switch (code) {[m
[32m+[m[32m      case 66: case 69: case 70: case 72: value = 3; break;[m
[32m+[m[32m      case 68: case 71: value = 4; break;[m
[32m+[m[32m      default: value = 2;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    for (index = 0; index < 47; index++) {[m
[32m+[m[32m      test.push({ k: chr + index, v: value });[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  test.sort(function (a, b) { return b.v - a.v; });[m
[32m+[m
[32m+[m[32m  for (index = 0; index < test.length; index++) {[m
[32m+[m[32m    chr = test[index].k.charAt(0);[m
[32m+[m[32m    if (result.charAt(result.length - 1) !== chr) result += chr;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return result !== 'DGBEFHACIJK';[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mvar FORCED = FAILS_ON_UNDEFINED || !FAILS_ON_NULL || !STRICT_METHOD || !STABLE_SORT;[m
[32m+[m
[32m+[m[32mvar getSortCompare = function (comparefn) {[m
[32m+[m[32m  return function (x, y) {[m
[32m+[m[32m    if (y === undefined) return -1;[m
[32m+[m[32m    if (x === undefined) return 1;[m
[32m+[m[32m    if (comparefn !== undefined) return +comparefn(x, y) || 0;[m
[32m+[m[32m    return toString(x) > toString(y) ? 1 : -1;[m
[32m+[m[32m  };[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// `Array.prototype.sort` method[m
