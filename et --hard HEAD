[1mdiff --git a/staticfiles/admin/js/calendar.js b/staticfiles/admin/js/calendar.js[m
[1mnew file mode 100644[m
[1mindex 0000000..776310f[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/calendar.js[m
[36m@@ -0,0 +1,239 @@[m
[32m+[m[32m/*global gettext, pgettext, get_format, quickElement, removeChildren*/[m
[32m+[m[32m/*[m
[32m+[m[32mcalendar.js - Calendar functions by Adrian Holovaty[m
[32m+[m[32mdepends on core.js for utility functions like removeChildren or quickElement[m
[32m+[m[32m*/[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    // CalendarNamespace -- Provides a collection of HTML calendar-related helper functions[m
[32m+[m[32m    const CalendarNamespace = {[m
[32m+[m[32m        monthsOfYear: [[m
[32m+[m[32m            gettext('January'),[m
[32m+[m[32m            gettext('February'),[m
[32m+[m[32m            gettext('March'),[m
[32m+[m[32m            gettext('April'),[m
[32m+[m[32m            gettext('May'),[m
[32m+[m[32m            gettext('June'),[m
[32m+[m[32m            gettext('July'),[m
[32m+[m[32m            gettext('August'),[m
[32m+[m[32m            gettext('September'),[m
[32m+[m[32m            gettext('October'),[m
[32m+[m[32m            gettext('November'),[m
[32m+[m[32m            gettext('December')[m
[32m+[m[32m        ],[m
[32m+[m[32m        monthsOfYearAbbrev: [[m
[32m+[m[32m            pgettext('abbrev. month January', 'Jan'),[m
[32m+[m[32m            pgettext('abbrev. month February', 'Feb'),[m
[32m+[m[32m            pgettext('abbrev. month March', 'Mar'),[m
[32m+[m[32m            pgettext('abbrev. month April', 'Apr'),[m
[32m+[m[32m            pgettext('abbrev. month May', 'May'),[m
[32m+[m[32m            pgettext('abbrev. month June', 'Jun'),[m
[32m+[m[32m            pgettext('abbrev. month July', 'Jul'),[m
[32m+[m[32m            pgettext('abbrev. month August', 'Aug'),[m
[32m+[m[32m            pgettext('abbrev. month September', 'Sep'),[m
[32m+[m[32m            pgettext('abbrev. month October', 'Oct'),[m
[32m+[m[32m            pgettext('abbrev. month November', 'Nov'),[m
[32m+[m[32m            pgettext('abbrev. month December', 'Dec')[m
[32m+[m[32m        ],[m
[32m+[m[32m        daysOfWeek: [[m
[32m+[m[32m            gettext('Sunday'),[m
[32m+[m[32m            gettext('Monday'),[m
[32m+[m[32m            gettext('Tuesday'),[m
[32m+[m[32m            gettext('Wednesday'),[m
[32m+[m[32m            gettext('Thursday'),[m
[32m+[m[32m            gettext('Friday'),[m
[32m+[m[32m            gettext('Saturday')[m
[32m+[m[32m        ],[m
[32m+[m[32m        daysOfWeekAbbrev: [[m
[32m+[m[32m            pgettext('abbrev. day Sunday', 'Sun'),[m
[32m+[m[32m            pgettext('abbrev. day Monday', 'Mon'),[m
[32m+[m[32m            pgettext('abbrev. day Tuesday', 'Tue'),[m
[32m+[m[32m            pgettext('abbrev. day Wednesday', 'Wed'),[m
[32m+[m[32m            pgettext('abbrev. day Thursday', 'Thur'),[m
[32m+[m[32m            pgettext('abbrev. day Friday', 'Fri'),[m
[32m+[m[32m            pgettext('abbrev. day Saturday', 'Sat')[m
[32m+[m[32m        ],[m
[32m+[m[32m        daysOfWeekInitial: [[m
[32m+[m[32m            pgettext('one letter Sunday', 'S'),[m
[32m+[m[32m            pgettext('one letter Monday', 'M'),[m
[32m+[m[32m            pgettext('one letter Tuesday', 'T'),[m
[32m+[m[32m            pgettext('one letter Wednesday', 'W'),[m
[32m+[m[32m            pgettext('one letter Thursday', 'T'),[m
[32m+[m[32m            pgettext('one letter Friday', 'F'),[m
[32m+[m[32m            pgettext('one letter Saturday', 'S')[m
[32m+[m[32m        ],[m
[32m+[m[32m        firstDayOfWeek: parseInt(get_format('FIRST_DAY_OF_WEEK')),[m
[32m+[m[32m        isLeapYear: function(year) {[m
[32m+[m[32m            return (((year % 4) === 0) && ((year % 100) !== 0 ) || ((year % 400) === 0));[m
[32m+[m[32m        },[m
[32m+[m[32m        getDaysInMonth: function(month, year) {[m
[32m+[m[32m            let days;[m
[32m+[m[32m            if (month === 1 || month === 3 || month === 5 || month === 7 || month === 8 || month === 10 || month === 12) {[m
[32m+[m[32m                days = 31;[m
[32m+[m[32m            }[m
[32m+[m[32m            else if (month === 4 || month === 6 || month === 9 || month === 11) {[m
[32m+[m[32m                days = 30;[m
[32m+[m[32m            }[m
[32m+[m[32m            else if (month === 2 && CalendarNamespace.isLeapYear(year)) {[m
[32m+[m[32m                days = 29;[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                days = 28;[m
[32m+[m[32m            }[m
[32m+[m[32m            return days;[m
[32m+[m[32m        },[m
[32m+[m[32m        draw: function(month, year, div_id, callback, selected) { // month = 1-12, year = 1-9999[m
[32m+[m[32m            const today = new Date();[m
[32m+[m[32m            const todayDay = today.getDate();[m
[32m+[m[32m            const todayMonth = today.getMonth() + 1;[m
[32m+[m[32m            const todayYear = today.getFullYear();[m
[32m+[m[32m            let todayClass = '';[m
[32m+[m
[32m+[m[32m            // Use UTC functions here because the date field does not contain time[m
[32m+[m[32m            // and using the UTC function variants prevent the local time offset[m
[32m+[m[32m            // from altering the date, specifically the day field.  For example:[m
[32m+[m[32m            //[m
[32m+[m[32m            // ```[m
[32m+[m[32m            // var x = new Date('2013-10-02');[m
[32m+[m[32m            // var day = x.getDate();[m
[32m+[m[32m            // ```[m
[32m+[m[32m            //[m
[32m+[m[32m            // The day variable above will be 1 instead of 2 in, say, US Pacific time[m
[32m+[m[32m            // zone.[m
[32m+[m[32m            let isSelectedMonth = false;[m
[32m+[m[32m            if (typeof selected !== 'undefined') {[m
[32m+[m[32m                isSelectedMonth = (selected.getUTCFullYear() === year && (selected.getUTCMonth() + 1) === month);[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            month = parseInt(month);[m
[32m+[m[32m            year = parseInt(year);[m
[32m+[m[32m            const calDiv = document.getElementById(div_id);[m
[32m+[m[32m            removeChildren(calDiv);[m
[32m+[m[32m            const calTable = document.createElement('table');[m
[32m+[m[32m            quickElement('caption', calTable, CalendarNamespace.monthsOfYear[month - 1] + ' ' + year);[m
[32m+[m[32m            const tableBody = quickElement('tbody', calTable);[m
[32m+[m
[32m+[m[32m            // Draw days-of-week header[m
[32m+[m[32m            let tableRow = quickElement('tr', tableBody);[m
[32m+[m[32m            for (let i = 0; i < 7; i++) {[m
[32m+[m[32m                quickElement('th', tableRow, CalendarNamespace.daysOfWeekInitial[(i + CalendarNamespace.firstDayOfWeek) % 7]);[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            const startingPos = new Date(year, month - 1, 1 - CalendarNamespace.firstDayOfWeek).getDay();[m
[32m+[m[32m            const days = CalendarNamespace.getDaysInMonth(month, year);[m
[32m+[m
[32m+[m[32m            let nonDayCell;[m
[32m+[m
[32m+[m[32m            // Draw blanks before first of month[m
[32m+[m[32m            tableRow = quickElement('tr', tableBody);[m
[32m+[m[32m            for (let i = 0; i < startingPos; i++) {[m
[32m+[m[32m                nonDayCell = quickElement('td', tableRow, ' ');[m
[32m+[m[32m                nonDayCell.className = "nonday";[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            function calendarMonth(y, m) {[m
[32m+[m[32m                function onClick(e) {[m
[32m+[m[32m                    e.preventDefault();[m
[32m+[m[32m                    callback(y, m, this.textContent);[m
[32m+[m[32m                }[m
[32m+[m[32m                return onClick;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // Draw days of month[m
[32m+[m[32m            let currentDay = 1;[m
[32m+[m[32m            for (let i = startingPos; currentDay <= days; i++) {[m
[32m+[m[32m                if (i % 7 === 0 && currentDay !== 1) {[m
[32m+[m[32m                    tableRow = quickElement('tr', tableBody);[m
[32m+[m[32m                }[m
[32m+[m[32m                if ((currentDay === todayDay) && (month === todayMonth) && (year === todayYear)) {[m
[32m+[m[32m                    todayClass = 'today';[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    todayClass = '';[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                // use UTC function; see above for explanation.[m
[32m+[m[32m                if (isSelectedMonth && currentDay === selected.getUTCDate()) {[m
[32m+[m[32m                    if (todayClass !== '') {[m
[32m+[m[32m                        todayClass += " ";[m
[32m+[m[32m                    }[m
[32m+[m[32m                    todayClass += "selected";[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                const cell = quickElement('td', tableRow, '', 'class', todayClass);[m
[32m+[m[32m                const link = quickElement('a', cell, currentDay, 'href', '#');[m
[32m+[m[32m                link.addEventListener('click', calendarMonth(year, month));[m
[32m+[m[32m                currentDay++;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // Draw blanks after end of month (optional, but makes for valid code)[m
[32m+[m[32m            while (tableRow.childNodes.length < 7) {[m
[32m+[m[32m                nonDayCell = quickElement('td', tableRow, ' ');[m
[32m+[m[32m                nonDayCell.className = "nonday";[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            calDiv.appendChild(calTable);[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Calendar -- A calendar instance[m
[32m+[m[32m    function Calendar(div_id, callback, selected) {[m
[32m+[m[32m        // div_id (string) is the ID of the element in which the calendar will[m
[32m+[m[32m        //     be displayed[m
[32m+[m[32m        // callback (string) is the name of a JavaScript function that will be[m
[32m+[m[32m        //     called with the parameters (year, month, day) when a day in the[m
[32m+[m[32m        //     calendar is clicked[m
[32m+[m[32m        this.div_id = div_id;[m
[32m+[m[32m        this.callback = callback;[m
[32m+[m[32m        this.today = new Date();[m
[32m+[m[32m        this.currentMonth = this.today.getMonth() + 1;[m
[32m+[m[32m        this.currentYear = this.today.getFullYear();[m
[32m+[m[32m        if (typeof selected !== 'undefined') {[m
[32m+[m[32m            this.selected = selected;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    Calendar.prototype = {[m
[32m+[m[32m        drawCurrent: function() {[m
[32m+[m[32m            CalendarNamespace.draw(this.currentMonth, this.currentYear, this.div_id, this.callback, this.selected);[m
[32m+[m[32m        },[m
[32m+[m[32m        drawDate: function(month, year, selected) {[m
[32m+[m[32m            this.currentMonth = month;[m
[32m+[m[32m            this.currentYear = year;[m
[32m+[m
[32m+[m[32m            if(selected) {[m
[32m+[m[32m                this.selected = selected;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            this.drawCurrent();[m
[32m+[m[32m        },[m
[32m+[m[32m        drawPreviousMonth: function() {[m
[32m+[m[32m            if (this.currentMonth === 1) {[m
[32m+[m[32m                this.currentMonth = 12;[m
[32m+[m[32m                this.currentYear--;[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                this.currentMonth--;[m
[32m+[m[32m            }[m
[32m+[m[32m            this.drawCurrent();[m
[32m+[m[32m        },[m
[32m+[m[32m        drawNextMonth: function() {[m
[32m+[m[32m            if (this.currentMonth === 12) {[m
[32m+[m[32m                this.currentMonth = 1;[m
[32m+[m[32m                this.currentYear++;[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                this.currentMonth++;[m
[32m+[m[32m            }[m
[32m+[m[32m            this.drawCurrent();[m
[32m+[m[32m        },[m
[32m+[m[32m        drawPreviousYear: function() {[m
[32m+[m[32m            this.currentYear--;[m
[32m+[m[32m            this.drawCurrent();[m
[32m+[m[32m        },[m
[32m+[m[32m        drawNextYear: function() {[m
[32m+[m[32m            this.currentYear++;[m
[32m+[m[32m            this.drawCurrent();[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m[32m    window.Calendar = Calendar;[m
[32m+[m[32m    window.CalendarNamespace = CalendarNamespace;[m
[32m+[m[32m}[m
[1mdiff --git a/staticfiles/admin/js/cancel.js b/staticfiles/admin/js/cancel.js[m
[1mnew file mode 100644[m
[1mindex 0000000..3069c6f[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/cancel.js[m
[36m@@ -0,0 +1,29 @@[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    // Call function fn when the DOM is loaded and ready. If it is already[m
[32m+[m[32m    // loaded, call the function now.[m
[32m+[m[32m    // http://youmightnotneedjquery.com/#ready[m
[32m+[m[32m    function ready(fn) {[m
[32m+[m[32m        if (document.readyState !== 'loading') {[m
[32m+[m[32m            fn();[m
[32m+[m[32m        } else {[m
[32m+[m[32m            document.addEventListener('DOMContentLoaded', fn);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    ready(function() {[m
[32m+[m[32m        function handleClick(event) {[m
[32m+[m[32m            event.preventDefault();[m
[32m+[m[32m            const params = new URLSearchParams(window.location.search);[m
[32m+[m[32m            if (params.has('_popup')) {[m
[32m+[m[32m                window.close(); // Close the popup.[m
[32m+[m[32m            } else {[m
[32m+[m[32m                window.history.back(); // Otherwise, go back.[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        document.querySelectorAll('.cancel-link').forEach(function(el) {[m
[32m+[m[32m            el.addEventListener('click', handleClick);[m
[32m+[m[32m        });[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
[1mdiff --git a/staticfiles/admin/js/change_form.js b/staticfiles/admin/js/change_form.js[m
[1mnew file mode 100644[m
[1mindex 0000000..96a4c62[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/change_form.js[m
[36m@@ -0,0 +1,16 @@[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    const inputTags = ['BUTTON', 'INPUT', 'SELECT', 'TEXTAREA'];[m
[32m+[m[32m    const modelName = document.getElementById('django-admin-form-add-constants').dataset.modelName;[m
[32m+[m[32m    if (modelName) {[m
[32m+[m[32m        const form = document.getElementById(modelName + '_form');[m
[32m+[m[32m        for (const element of form.elements) {[m
[32m+[m[32m            // HTMLElement.offsetParent returns null when the element is not[m
[32m+[m[32m            // rendered.[m
[32m+[m[32m            if (inputTags.includes(element.tagName) && !element.disabled && element.offsetParent) {[m
[32m+[m[32m                element.focus();[m
[32m+[m[32m                break;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[1mdiff --git a/staticfiles/admin/js/collapse.js b/staticfiles/admin/js/collapse.js[m
[1mnew file mode 100644[m
[1mindex 0000000..c6c7b0f[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/collapse.js[m
[36m@@ -0,0 +1,43 @@[m
[32m+[m[32m/*global gettext*/[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    window.addEventListener('load', function() {[m
[32m+[m[32m        // Add anchor tag for Show/Hide link[m
[32m+[m[32m        const fieldsets = document.querySelectorAll('fieldset.collapse');[m
[32m+[m[32m        for (const [i, elem] of fieldsets.entries()) {[m
[32m+[m[32m            // Don't hide if fields in this fieldset have errors[m
[32m+[m[32m            if (elem.querySelectorAll('div.errors, ul.errorlist').length === 0) {[m
[32m+[m[32m                elem.classList.add('collapsed');[m
[32m+[m[32m                const h2 = elem.querySelector('h2');[m
[32m+[m[32m                const link = document.createElement('a');[m
[32m+[m[32m                link.id = 'fieldsetcollapser' + i;[m
[32m+[m[32m                link.className = 'collapse-toggle';[m
[32m+[m[32m                link.href = '#';[m
[32m+[m[32m                link.textContent = gettext('Show');[m
[32m+[m[32m                h2.appendChild(document.createTextNode(' ('));[m
[32m+[m[32m                h2.appendChild(link);[m
[32m+[m[32m                h2.appendChild(document.createTextNode(')'));[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        // Add toggle to hide/show anchor tag[m
[32m+[m[32m        const toggleFunc = function(ev) {[m
[32m+[m[32m            if (ev.target.matches('.collapse-toggle')) {[m
[32m+[m[32m                ev.preventDefault();[m
[32m+[m[32m                ev.stopPropagation();[m
[32m+[m[32m                const fieldset = ev.target.closest('fieldset');[m
[32m+[m[32m                if (fieldset.classList.contains('collapsed')) {[m
[32m+[m[32m                    // Show[m
[32m+[m[32m                    ev.target.textContent = gettext('Hide');[m
[32m+[m[32m                    fieldset.classList.remove('collapsed');[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    // Hide[m
[32m+[m[32m                    ev.target.textContent = gettext('Show');[m
[32m+[m[32m                    fieldset.classList.add('collapsed');[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m[32m        document.querySelectorAll('fieldset.module').forEach(function(el) {[m
[32m+[m[32m            el.addEventListener('click', toggleFunc);[m
[32m+[m[32m        });[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
[1mdiff --git a/staticfiles/admin/js/core.js b/staticfiles/admin/js/core.js[m
[1mnew file mode 100644[m
[1mindex 0000000..10504d4[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/core.js[m
[36m@@ -0,0 +1,184 @@[m
[32m+[m[32m// Core JavaScript helper functions[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m// quickElement(tagType, parentReference [, textInChildNode, attribute, attributeValue ...]);[m
[32m+[m[32mfunction quickElement() {[m
[32m+[m[32m    const obj = document.createElement(arguments[0]);[m
[32m+[m[32m    if (arguments[2]) {[m
[32m+[m[32m        const textNode = document.createTextNode(arguments[2]);[m
[32m+[m[32m        obj.appendChild(textNode);[m
[32m+[m[32m    }[m
[32m+[m[32m    const len = arguments.length;[m
[32m+[m[32m    for (let i = 3; i < len; i += 2) {[m
[32m+[m[32m        obj.setAttribute(arguments[i], arguments[i + 1]);[m
[32m+[m[32m    }[m
[32m+[m[32m    arguments[1].appendChild(obj);[m
[32m+[m[32m    return obj;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// "a" is reference to an object[m
[32m+[m[32mfunction removeChildren(a) {[m
[32m+[m[32m    while (a.hasChildNodes()) {[m
[32m+[m[32m        a.removeChild(a.lastChild);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// ----------------------------------------------------------------------------[m
[32m+[m[32m// Find-position functions by PPK[m
[32m+[m[32m// See https://www.quirksmode.org/js/findpos.html[m
[32m+[m[32m// ----------------------------------------------------------------------------[m
[32m+[m[32mfunction findPosX(obj) {[m
[32m+[m[32m    let curleft = 0;[m
[32m+[m[32m    if (obj.offsetParent) {[m
[32m+[m[32m        while (obj.offsetParent) {[m
[32m+[m[32m            curleft += obj.offsetLeft - obj.scrollLeft;[m
[32m+[m[32m            obj = obj.offsetParent;[m
[32m+[m[32m        }[m
[32m+[m[32m    } else if (obj.x) {[m
[32m+[m[32m        curleft += obj.x;[m
[32m+[m[32m    }[m
[32m+[m[32m    return curleft;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction findPosY(obj) {[m
[32m+[m[32m    let curtop = 0;[m
[32m+[m[32m    if (obj.offsetParent) {[m
[32m+[m[32m        while (obj.offsetParent) {[m
[32m+[m[32m            curtop += obj.offsetTop - obj.scrollTop;[m
[32m+[m[32m            obj = obj.offsetParent;[m
[32m+[m[32m        }[m
[32m+[m[32m    } else if (obj.y) {[m
[32m+[m[32m        curtop += obj.y;[m
[32m+[m[32m    }[m
[32m+[m[32m    return curtop;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//-----------------------------------------------------------------------------[m
[32m+[m[32m// Date object extensions[m
[32m+[m[32m// ----------------------------------------------------------------------------[m
[32m+[m[32m{[m
[32m+[m[32m    Date.prototype.getTwelveHours = function() {[m
[32m+[m[32m        return this.getHours() % 12 || 12;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Date.prototype.getTwoDigitMonth = function() {[m
[32m+[m[32m        return (this.getMonth() < 9) ? '0' + (this.getMonth() + 1) : (this.getMonth() + 1);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Date.prototype.getTwoDigitDate = function() {[m
[32m+[m[32m        return (this.getDate() < 10) ? '0' + this.getDate() : this.getDate();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Date.prototype.getTwoDigitTwelveHour = function() {[m
[32m+[m[32m        return (this.getTwelveHours() < 10) ? '0' + this.getTwelveHours() : this.getTwelveHours();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Date.prototype.getTwoDigitHour = function() {[m
[32m+[m[32m        return (this.getHours() < 10) ? '0' + this.getHours() : this.getHours();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Date.prototype.getTwoDigitMinute = function() {[m
[32m+[m[32m        return (this.getMinutes() < 10) ? '0' + this.getMinutes() : this.getMinutes();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Date.prototype.getTwoDigitSecond = function() {[m
[32m+[m[32m        return (this.getSeconds() < 10) ? '0' + this.getSeconds() : this.getSeconds();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Date.prototype.getAbbrevDayName = function() {[m
[32m+[m[32m        return typeof window.CalendarNamespace === "undefined"[m
[32m+[m[32m            ? '0' + this.getDay()[m
[32m+[m[32m            : window.CalendarNamespace.daysOfWeekAbbrev[this.getDay()];[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Date.prototype.getFullDayName = function() {[m
[32m+[m[32m        return typeof window.CalendarNamespace === "undefined"[m
[32m+[m[32m            ? '0' + this.getDay()[m
[32m+[m[32m            : window.CalendarNamespace.daysOfWeek[this.getDay()];[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Date.prototype.getAbbrevMonthName = function() {[m
[32m+[m[32m        return typeof window.CalendarNamespace === "undefined"[m
[32m+[m[32m            ? this.getTwoDigitMonth()[m
[32m+[m[32m            : window.CalendarNamespace.monthsOfYearAbbrev[this.getMonth()];[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Date.prototype.getFullMonthName = function() {[m
[32m+[m[32m        return typeof window.CalendarNamespace === "undefined"[m
[32m+[m[32m            ? this.getTwoDigitMonth()[m
[32m+[m[32m            : window.CalendarNamespace.monthsOfYear[this.getMonth()];[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Date.prototype.strftime = function(format) {[m
[32m+[m[32m        const fields = {[m
[32m+[m[32m            a: this.getAbbrevDayName(),[m
[32m+[m[32m            A: this.getFullDayName(),[m
[32m+[m[32m            b: this.getAbbrevMonthName(),[m
[32m+[m[32m            B: this.getFullMonthName(),[m
[32m+[m[32m            c: this.toString(),[m
[32m+[m[32m            d: this.getTwoDigitDate(),[m
[32m+[m[32m            H: this.getTwoDigitHour(),[m
[32m+[m[32m            I: this.getTwoDigitTwelveHour(),[m
[32m+[m[32m            m: this.getTwoDigitMonth(),[m
[32m+[m[32m            M: this.getTwoDigitMinute(),[m
[32m+[m[32m            p: (this.getHours() >= 12) ? 'PM' : 'AM',[m
[32m+[m[32m            S: this.getTwoDigitSecond(),[m
[32m+[m[32m            w: '0' + this.getDay(),[m
[32m+[m[32m            x: this.toLocaleDateString(),[m
[32m+[m[32m            X: this.toLocaleTimeString(),[m
[32m+[m[32m            y: ('' + this.getFullYear()).substr(2, 4),[m
[32m+[m[32m            Y: '' + this.getFullYear(),[m
[32m+[m[32m            '%': '%'[m
[32m+[m[32m        };[m
[32m+[m[32m        let result = '', i = 0;[m
[32m+[m[32m        while (i < format.length) {[m
[32m+[m[32m            if (format.charAt(i) === '%') {[m
[32m+[m[32m                result += fields[format.charAt(i + 1)];[m
[32m+[m[32m                ++i;[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                result += format.charAt(i);[m
[32m+[m[32m            }[m
[32m+[m[32m            ++i;[m
[32m+[m[32m        }[m
[32m+[m[32m        return result;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // ----------------------------------------------------------------------------[m
[32m+[m[32m    // String object extensions[m
[32m+[m[32m    // ----------------------------------------------------------------------------[m
[32m+[m[32m    String.prototype.strptime = function(format) {[m
[32m+[m[32m        const split_format = format.split(/[.\-/]/);[m
[32m+[m[32m        const date = this.split(/[.\-/]/);[m
[32m+[m[32m        let i = 0;[m
[32m+[m[32m        let day, month, year;[m
[32m+[m[32m        while (i < split_format.length) {[m
[32m+[m[32m            switch (split_format[i]) {[m
[32m+[m[32m            case "%d":[m
[32m+[m[32m                day = date[i];[m
[32m+[m[32m                break;[m
[32m+[m[32m            case "%m":[m
[32m+[m[32m                month = date[i] - 1;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case "%Y":[m
[32m+[m[32m                year = date[i];[m
[32m+[m[32m                break;[m
[32m+[m[32m            case "%y":[m
[32m+[m[32m                // A %y value in the range of [00, 68] is in the current[m
[32m+[m[32m                // century, while [69, 99] is in the previous century,[m
[32m+[m[32m                // according to the Open Group Specification.[m
[32m+[m[32m                if (parseInt(date[i], 10) >= 69) {[m
[32m+[m[32m                    year = date[i];[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    year = (new Date(Date.UTC(date[i], 0))).getUTCFullYear() + 100;[m
[32m+[m[32m                }[m
[32m+[m[32m                break;[m
[32m+[m[32m            }[m
[32m+[m[32m            ++i;[m
[32m+[m[32m        }[m
[32m+[m[32m        // Create Date object from UTC since the parsed value is supposed to be[m
[32m+[m[32m        // in UTC, not local time. Also, the calendar uses UTC functions for[m
[32m+[m[32m        // date extraction.[m
[32m+[m[32m        return new Date(Date.UTC(year, month, day));[m
[32m+[m[32m    };[m
[32m+[m[32m}[m
[1mdiff --git a/staticfiles/admin/js/filters.js b/staticfiles/admin/js/filters.js[m
[1mnew file mode 100644[m
[1mindex 0000000..f5536eb[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/filters.js[m
[36m@@ -0,0 +1,30 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * Persist changelist filters state (collapsed/expanded).[m
[32m+[m[32m */[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    // Init filters.[m
[32m+[m[32m    let filters = JSON.parse(sessionStorage.getItem('django.admin.filtersState'));[m
[32m+[m
[32m+[m[32m    if (!filters) {[m
[32m+[m[32m        filters = {};[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    Object.entries(filters).forEach(([key, value]) => {[m
[32m+[m[32m        const detailElement = document.querySelector(`[data-filter-title='${CSS.escape(key)}']`);[m
[32m+[m
[32m+[m[32m        // Check if the filter is present, it could be from other view.[m
[32m+[m[32m        if (detailElement) {[m
[32m+[m[32m            value ? detailElement.setAttribute('open', '') : detailElement.removeAttribute('open');[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Save filter state when clicks.[m
[32m+[m[32m    const details = document.querySelectorAll('details');[m
[32m+[m[32m    details.forEach(detail => {[m
[32m+[m[32m        detail.addEventListener('toggle', event => {[m
[32m+[m[32m            filters[`${event.target.dataset.filterTitle}`] = detail.open;[m
[32m+[m[32m            sessionStorage.setItem('django.admin.filtersState', JSON.stringify(filters));[m
[32m+[m[32m        });[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
[1mdiff --git a/staticfiles/admin/js/inlines.js b/staticfiles/admin/js/inlines.js[m
[1mnew file mode 100644[m
[1mindex 0000000..e9a1dfe[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/inlines.js[m
[36m@@ -0,0 +1,359 @@[m
[32m+[m[32m/*global DateTimeShortcuts, SelectFilter*/[m
[32m+[m[32m/**[m
[32m+[m[32m * Django admin inlines[m
[32m+[m[32m *[m
[32m+[m[32m * Based on jQuery Formset 1.1[m
[32m+[m[32m * @author Stanislaus Madueke (stan DOT madueke AT gmail DOT com)[m
[32m+[m[32m * @requires jQuery 1.2.6 or later[m
[32m+[m[32m *[m
[32m+[m[32m * Copyright (c) 2009, Stanislaus Madueke[m
[32m+[m[32m * All rights reserved.[m
[32m+[m[32m *[m
[32m+[m[32m * Spiced up with Code from Zain Memon's GSoC project 2009[m
[32m+[m[32m * and modified for Django by Jannis Leidel, Travis Swicegood and Julien Phalip.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the New BSD License[m
[32m+[m[32m * See: https://opensource.org/licenses/bsd-license.php[m
[32m+[m[32m */[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    const $ = django.jQuery;[m
[32m+[m[32m    $.fn.formset = function(opts) {[m
[32m+[m[32m        const options = $.extend({}, $.fn.formset.defaults, opts);[m
[32m+[m[32m        const $this = $(this);[m
[32m+[m[32m        const $parent = $this.parent();[m
[32m+[m[32m        const updateElementIndex = function(el, prefix, ndx) {[m
[32m+[m[32m            const id_regex = new RegExp("(" + prefix + "-(\\d+|__prefix__))");[m
[32m+[m[32m            const replacement = prefix + "-" + ndx;[m
[32m+[m[32m            if ($(el).prop("for")) {[m
[32m+[m[32m                $(el).prop("for", $(el).prop("for").replace(id_regex, replacement));[m
[32m+[m[32m            }[m
[32m+[m[32m            if (el.id) {[m
[32m+[m[32m                el.id = el.id.replace(id_regex, replacement);[m
[32m+[m[32m            }[m
[32m+[m[32m            if (el.name) {[m
[32m+[m[32m                el.name = el.name.replace(id_regex, replacement);[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m[32m        const totalForms = $("#id_" + options.prefix + "-TOTAL_FORMS").prop("autocomplete", "off");[m
[32m+[m[32m        let nextIndex = parseInt(totalForms.val(), 10);[m
[32m+[m[32m        const maxForms = $("#id_" + options.prefix + "-MAX_NUM_FORMS").prop("autocomplete", "off");[m
[32m+[m[32m        const minForms = $("#id_" + options.prefix + "-MIN_NUM_FORMS").prop("autocomplete", "off");[m
[32m+[m[32m        let addButton;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * The "Add another MyModel" button below the inline forms.[m
[32m+[m[32m         */[m
[32m+[m[32m        const addInlineAddButton = function() {[m
[32m+[m[32m            if (addButton === null) {[m
[32m+[m[32m                if ($this.prop("tagName") === "TR") {[m
[32m+[m[32m                    // If forms are laid out as table rows, insert the[m
[32m+[m[32m                    // "add" button in a new table row:[m
[32m+[m[32m                    const numCols = $this.eq(-1).children().length;[m
[32m+[m[32m                    $parent.append('<tr class="' + options.addCssClass + '"><td colspan="' + numCols + '"><a href="#">' + options.addText + "</a></tr>");[m
[32m+[m[32m                    addButton = $parent.find("tr:last a");[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    // Otherwise, insert it immediately after the last form:[m
[32m+[m[32m                    $this.filter(":last").after('<div class="' + options.addCssClass + '"><a href="#">' + options.addText + "</a></div>");[m
[32m+[m[32m                    addButton = $this.filter(":last").next().find("a");[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            addButton.on('click', addInlineClickHandler);[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        const addInlineClickHandler = function(e) {[m
[32m+[m[32m            e.preventDefault();[m
[32m+[m[32m            const template = $("#" + options.prefix + "-empty");[m
[32m+[m[32m            const row = template.clone(true);[m
[32m+[m[32m            row.removeClass(options.emptyCssClass)[m
[32m+[m[32m                .addClass(options.formCssClass)[m
[32m+[m[32m                .attr("id", options.prefix + "-" + nextIndex);[m
[32m+[m[32m            addInlineDeleteButton(row);[m
[32m+[m[32m            row.find("*").each(function() {[m
[32m+[m[32m                updateElementIndex(this, options.prefix, totalForms.val());[m
[32m+[m[32m            });[m
[32m+[m[32m            // Insert the new form when it has been fully edited.[m
[32m+[m[32m            row.insertBefore($(template));[m
[32m+[m[32m            // Update number of total forms.[m
[32m+[m[32m            $(totalForms).val(parseInt(totalForms.val(), 10) + 1);[m
[32m+[m[32m            nextIndex += 1;[m
[32m+[m[32m            // Hide the add button if there's a limit and it's been reached.[m
[32m+[m[32m            if ((maxForms.val() !== '') && (maxForms.val() - totalForms.val()) <= 0) {[m
[32m+[m[32m                addButton.parent().hide();[m
[32m+[m[32m            }[m
[32m+[m[32m            // Show the remove buttons if there are more than min_num.[m
[32m+[m[32m            toggleDeleteButtonVisibility(row.closest('.inline-group'));[m
[32m+[m
[32m+[m[32m            // Pass the new form to the post-add callback, if provided.[m
[32m+[m[32m            if (options.added) {[m
[32m+[m[32m                options.added(row);[m
[32m+[m[32m            }[m
[32m+[m[32m            row.get(0).dispatchEvent(new CustomEvent("formset:added", {[m
[32m+[m[32m                bubbles: true,[m
[32m+[m[32m                detail: {[m
[32m+[m[32m                    formsetName: options.prefix[m
[32m+[m[32m                }[m
[32m+[m[32m            }));[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * The "X" button that is part of every unsaved inline.[m
[32m+[m[32m         * (When saved, it is replaced with a "Delete" checkbox.)[m
[32m+[m[32m         */[m
[32m+[m[32m        const addInlineDeleteButton = function(row) {[m
[32m+[m[32m            if (row.is("tr")) {[m
[32m+[m[32m                // If the forms are laid out in table rows, insert[m
[32m+[m[32m                // the remove button into the last table cell:[m
[32m+[m[32m                row.children(":last").append('<div><a class="' + options.deleteCssClass + '" href="#">' + options.deleteText + "</a></div>");[m
[32m+[m[32m            } else if (row.is("ul") || row.is("ol")) {[m
[32m+[m[32m                // If they're laid out as an ordered/unordered list,[m
[32m+[m[32m                // insert an <li> after the last list item:[m
[32m+[m[32m                row.append('<li><a class="' + options.deleteCssClass + '" href="#">' + options.deleteText + "</a></li>");[m
[32m+[m[32m            } else {[m
[32m+[m[32m                // Otherwise, just insert the remove button as the[m
[32m+[m[32m                // last child element of the form's container:[m
[32m+[m[32m                row.children(":first").append('<span><a class="' + options.deleteCssClass + '" href="#">' + options.deleteText + "</a></span>");[m
[32m+[m[32m            }[m
[32m+[m[32m            // Add delete handler for each row.[m
[32m+[m[32m            row.find("a." + options.deleteCssClass).on('click', inlineDeleteHandler.bind(this));[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        const inlineDeleteHandler = function(e1) {[m
[32m+[m[32m            e1.preventDefault();[m
[32m+[m[32m            const deleteButton = $(e1.target);[m
[32m+[m[32m            const row = deleteButton.closest('.' + options.formCssClass);[m
[32m+[m[32m            const inlineGroup = row.closest('.inline-group');[m
[32m+[m[32m            // Remove the parent form containing this button,[m
[32m+[m[32m            // and also remove the relevant row with non-field errors:[m
[32m+[m[32m            const prevRow = row.prev();[m
[32m+[m[32m            if (prevRow.length && prevRow.hasClass('row-form-errors')) {[m
[32m+[m[32m                prevRow.remove();[m
[32m+[m[32m            }[m
[32m+[m[32m            row.remove();[m
[32m+[m[32m            nextIndex -= 1;[m
[32m+[m[32m            // Pass the deleted form to the post-delete callback, if provided.[m
[32m+[m[32m            if (options.removed) {[m
[32m+[m[32m                options.removed(row);[m
[32m+[m[32m            }[m
[32m+[m[32m            document.dispatchEvent(new CustomEvent("formset:removed", {[m
[32m+[m[32m                detail: {[m
[32m+[m[32m                    formsetName: options.prefix[m
[32m+[m[32m                }[m
[32m+[m[32m            }));[m
[32m+[m[32m            // Update the TOTAL_FORMS form count.[m
[32m+[m[32m            const forms = $("." + options.formCssClass);[m
[32m+[m[32m            $("#id_" + options.prefix + "-TOTAL_FORMS").val(forms.length);[m
[32m+[m[32m            // Show add button again once below maximum number.[m
[32m+[m[32m            if ((maxForms.val() === '') || (maxForms.val() - forms.length) > 0) {[m
[32m+[m[32m                addButton.parent().show();[m
[32m+[m[32m            }[m
[32m+[m[32m            // Hide the remove buttons if at min_num.[m
[32m+[m[32m            toggleDeleteButtonVisibility(inlineGroup);[m
[32m+[m[32m            // Also, update names and ids for all remaining form controls so[m
[32m+[m[32m            // they remain in sequence:[m
[32m+[m[32m            let i, formCount;[m
[32m+[m[32m            const updateElementCallback = function() {[m
[32m+[m[32m                updateElementIndex(this, options.prefix, i);[m
[32m+[m[32m            };[m
[32m+[m[32m            for (i = 0, formCount = forms.length; i < formCount; i++) {[m
[32m+[m[32m                updateElementIndex($(forms).get(i), options.prefix, i);[m
[32m+[m[32m                $(forms.get(i)).find("*").each(updateElementCallback);[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        const toggleDeleteButtonVisibility = function(inlineGroup) {[m
[32m+[m[32m            if ((minForms.val() !== '') && (minForms.val() - totalForms.val()) >= 0) {[m
[32m+[m[32m                inlineGroup.find('.inline-deletelink').hide();[m
[32m+[m[32m            } else {[m
[32m+[m[32m                inlineGroup.find('.inline-deletelink').show();[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        $this.each(function(i) {[m
[32m+[m[32m            $(this).not("." + options.emptyCssClass).addClass(options.formCssClass);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        // Create the delete buttons for all unsaved inlines:[m
[32m+[m[32m        $this.filter('.' + options.formCssClass + ':not(.has_original):not(.' + options.emptyCssClass + ')').each(function() {[m
[32m+[m[32m            addInlineDeleteButton($(this));[m
[32m+[m[32m        });[m
[32m+[m[32m        toggleDeleteButtonVisibility($this);[m
[32m+[m
[32m+[m[32m        // Create the add button, initially hidden.[m
[32m+[m[32m        addButton = options.addButton;[m
[32m+[m[32m        addInlineAddButton();[m
[32m+[m
[32m+[m[32m        // Show the add button if allowed to add more items.[m
[32m+[m[32m        // Note that max_num = None translates to a blank string.[m
[32m+[m[32m        const showAddButton = maxForms.val() === '' || (maxForms.val() - totalForms.val()) > 0;[m
[32m+[m[32m        if ($this.length && showAddButton) {[m
[32m+[m[32m            addButton.parent().show();[m
[32m+[m[32m        } else {[m
[32m+[m[32m            addButton.parent().hide();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /* Setup plugin defaults */[m
[32m+[m[32m    $.fn.formset.defaults = {[m
[32m+[m[32m        prefix: "form", // The form prefix for your django formset[m
[32m+[m[32m        addText: "add another", // Text for the add link[m
[32m+[m[32m        deleteText: "remove", // Text for the delete link[m
[32m+[m[32m        addCssClass: "add-row", // CSS class applied to the add link[m
[32m+[m[32m        deleteCssClass: "delete-row", // CSS class applied to the delete link[m
[32m+[m[32m        emptyCssClass: "empty-row", // CSS class applied to the empty row[m
[32m+[m[32m        formCssClass: "dynamic-form", // CSS class applied to each form in a formset[m
[32m+[m[32m        added: null, // Function called each time a new form is added[m
[32m+[m[32m        removed: null, // Function called each time a form is deleted[m
[32m+[m[32m        addButton: null // Existing add button to use[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m
[32m+[m[32m    // Tabular inlines ---------------------------------------------------------[m
[32m+[m[32m    $.fn.tabularFormset = function(selector, options) {[m
[32m+[m[32m        const $rows = $(this);[m
[32m+[m
[32m+[m[32m        const reinitDateTimeShortCuts = function() {[m
[32m+[m[32m            // Reinitialize the calendar and clock widgets by force[m
[32m+[m[32m            if (typeof DateTimeShortcuts !== "undefined") {[m
[32m+[m[32m                $(".datetimeshortcuts").remove();[m
[32m+[m[32m                DateTimeShortcuts.init();[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        const updateSelectFilter = function() {[m
[32m+[m[32m            // If any SelectFilter widgets are a part of the new form,[m
[32m+[m[32m            // instantiate a new SelectFilter instance for it.[m
[32m+[m[32m            if (typeof SelectFilter !== 'undefined') {[m
[32m+[m[32m                $('.selectfilter').each(function(index, value) {[m
[32m+[m[32m                    SelectFilter.init(value.id, this.dataset.fieldName, false);[m
[32m+[m[32m                });[m
[32m+[m[32m                $('.selectfilterstacked').each(function(index, value) {[m
[32m+[m[32m                    SelectFilter.init(value.id, this.dataset.fieldName, true);[m
[32m+[m[32m                });[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        const initPrepopulatedFields = function(row) {[m
[32m+[m[32m            row.find('.prepopulated_field').each(function() {[m
[32m+[m[32m                const field = $(this),[m
[32m+[m[32m                    input = field.find('input, select, textarea'),[m
[32m+[m[32m                    dependency_list = input.data('dependency_list') || [],[m
[32m+[m[32m                    dependencies = [];[m
[32m+[m[32m                $.each(dependency_list, function(i, field_name) {[m
[32m+[m[32m                    dependencies.push('#' + row.find('.field-' + field_name).find('input, select, textarea').attr('id'));[m
[32m+[m[32m                });[m
[32m+[m[32m                if (dependencies.length) {[m
[32m+[m[32m                    input.prepopulate(dependencies, input.attr('maxlength'));[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        $rows.formset({[m
[32m+[m[32m            prefix: options.prefix,[m
[32m+[m[32m            addText: options.addText,[m
[32m+[m[32m            formCssClass: "dynamic-" + options.prefix,[m
[32m+[m[32m            deleteCssClass: "inline-deletelink",[m
[32m+[m[32m            deleteText: options.deleteText,[m
[32m+[m[32m            emptyCssClass: "empty-form",[m
[32m+[m[32m            added: function(row) {[m
[32m+[m[32m                initPrepopulatedFields(row);[m
[32m+[m[32m                reinitDateTimeShortCuts();[m
[32m+[m[32m                updateSelectFilter();[m
[32m+[m[32m            },[m
[32m+[m[32m            addButton: options.addButton[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        return $rows;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Stacked inlines ---------------------------------------------------------[m
[32m+[m[32m    $.fn.stackedFormset = function(selector, options) {[m
[32m+[m[32m        const $rows = $(this);[m
[32m+[m[32m        const updateInlineLabel = function(row) {[m
[32m+[m[32m            $(selector).find(".inline_label").each(function(i) {[m
[32m+[m[32m                const count = i + 1;[m
[32m+[m[32m                $(this).html($(this).html().replace(/(#\d+)/g, "#" + count));[m
[32m+[m[32m            });[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        const reinitDateTimeShortCuts = function() {[m
[32m+[m[32m            // Reinitialize the calendar and clock widgets by force, yuck.[m
[32m+[m[32m            if (typeof DateTimeShortcuts !== "undefined") {[m
[32m+[m[32m                $(".datetimeshortcuts").remove();[m
[32m+[m[32m                DateTimeShortcuts.init();[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        const updateSelectFilter = function() {[m
[32m+[m[32m            // If any SelectFilter widgets were added, instantiate a new instance.[m
[32m+[m[32m            if (typeof SelectFilter !== "undefined") {[m
[32m+[m[32m                $(".selectfilter").each(function(index, value) {[m
[32m+[m[32m                    SelectFilter.init(value.id, this.dataset.fieldName, false);[m
[32m+[m[32m                });[m
[32m+[m[32m                $(".selectfilterstacked").each(function(index, value) {[m
[32m+[m[32m                    SelectFilter.init(value.id, this.dataset.fieldName, true);[m
[32m+[m[32m                });[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        const initPrepopulatedFields = function(row) {[m
[32m+[m[32m            row.find('.prepopulated_field').each(function() {[m
[32m+[m[32m                const field = $(this),[m
[32m+[m[32m                    input = field.find('input, select, textarea'),[m
[32m+[m[32m                    dependency_list = input.data('dependency_list') || [],[m
[32m+[m[32m                    dependencies = [];[m
[32m+[m[32m                $.each(dependency_list, function(i, field_name) {[m
[32m+[m[32m                    // Dependency in a fieldset.[m
[32m+[m[32m                    let field_element = row.find('.form-row .field-' + field_name);[m
[32m+[m[32m                    // Dependency without a fieldset.[m
[32m+[m[32m                    if (!field_element.length) {[m
[32m+[m[32m                        field_element = row.find('.form-row.field-' + field_name);[m
[32m+[m[32m                    }[m
[32m+[m[32m                    dependencies.push('#' + field_element.find('input, select, textarea').attr('id'));[m
[32m+[m[32m                });[m
[32m+[m[32m                if (dependencies.length) {[m
[32m+[m[32m                    input.prepopulate(dependencies, input.attr('maxlength'));[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        $rows.formset({[m
[32m+[m[32m            prefix: options.prefix,[m
[32m+[m[32m            addText: options.addText,[m
[32m+[m[32m            formCssClass: "dynamic-" + options.prefix,[m
[32m+[m[32m            deleteCssClass: "inline-deletelink",[m
[32m+[m[32m            deleteText: options.deleteText,[m
[32m+[m[32m            emptyCssClass: "empty-form",[m
[32m+[m[32m            removed: updateInlineLabel,[m
[32m+[m[32m            added: function(row) {[m
[32m+[m[32m                initPrepopulatedFields(row);[m
[32m+[m[32m                reinitDateTimeShortCuts();[m
[32m+[m[32m                updateSelectFilter();[m
[32m+[m[32m                updateInlineLabel(row);[m
[32m+[m[32m            },[m
[32m+[m[32m            addButton: options.addButton[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        return $rows;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    $(document).ready(function() {[m
[32m+[m[32m        $(".js-inline-admin-formset").each(function() {[m
[32m+[m[32m            const data = $(this).data(),[m
[32m+[m[32m                inlineOptions = data.inlineFormset;[m
[32m+[m[32m            let selector;[m
[32m+[m[32m            switch(data.inlineType) {[m
[32m+[m[32m            case "stacked":[m
[32m+[m[32m                selector = inlineOptions.name + "-group .inline-related";[m
[32m+[m[32m                $(selector).stackedFormset(selector, inlineOptions.options);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case "tabular":[m
[32m+[m[32m                selector = inlineOptions.name + "-group .tabular.inline-related tbody:first > tr.form-row";[m
[32m+[m[32m                $(selector).tabularFormset(selector, inlineOptions.options);[m
[32m+[m[32m                break;[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
