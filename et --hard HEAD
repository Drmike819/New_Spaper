[32m+[m[32m/*global opener */[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    const initData = JSON.parse(document.getElementById('django-admin-popup-response-constants').dataset.popupResponse);[m
[32m+[m[32m    switch(initData.action) {[m
[32m+[m[32m    case 'change':[m
[32m+[m[32m        opener.dismissChangeRelatedObjectPopup(window, initData.value, initData.obj, initData.new_value);[m
[32m+[m[32m        break;[m
[32m+[m[32m    case 'delete':[m
[32m+[m[32m        opener.dismissDeleteRelatedObjectPopup(window, initData.value);[m
[32m+[m[32m        break;[m
[32m+[m[32m    default:[m
[32m+[m[32m        opener.dismissAddRelatedObjectPopup(window, initData.value, initData.obj);[m
[32m+[m[32m        break;[m
