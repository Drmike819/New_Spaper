[32m+[m[32mS2.define('select2/dropdown',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  './utils'[m
[32m+[m[32m], function ($, Utils) {[m
[32m+[m[32m  function Dropdown ($element, options) {[m
[32m+[m[32m    this.$element = $element;[m
[32m+[m[32m    this.options = options;[m
[32m+[m
[32m+[m[32m    Dropdown.__super__.constructor.call(this);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Utils.Extend(Dropdown, Utils.Observable);[m
[32m+[m
[32m+[m[32m  Dropdown.prototype.render = function () {[m
[32m+[m[32m    var $dropdown = $([m
[32m+[m[32m      '<span class="select2-dropdown">' +[m
[32m+[m[32m        '<span class="select2-results"></span>' +[m
[32m+[m[32m      '</span>'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    $dropdown.attr('dir', this.options.get('dir'));[m
[32m+[m
[32m+[m[32m    this.$dropdown = $dropdown;[m
[32m+[m
[32m+[m[32m    return $dropdown;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Dropdown.prototype.bind = function () {[m
[32m+[m[32m    // Should be implemented in subclasses[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Dropdown.prototype.position = function ($dropdown, $container) {[m
[32m+[m[32m    // Should be implemented in subclasses[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Dropdown.prototype.destroy = function () {[m
[32m+[m[32m    // Remove the dropdown from the DOM[m
[32m+[m[32m    this.$dropdown.remove();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return Dropdown;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/dropdown/search',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  '../utils'[m
[32m+[m[32m], function ($, Utils) {[m
[32m+[m[32m  function Search () { }[m
[32m+[m
[32m+[m[32m  Search.prototype.render = function (decorated) {[m
[32m+[m[32m    var $rendered = decorated.call(this);[m
[32m+[m
[32m+[m[32m    var $search = $([m
[32m+[m[32m      '<span class="select2-search select2-search--dropdown">' +[m
[32m+[m[32m        '<input class="select2-search__field" type="search" tabindex="-1"' +[m
[32m+[m[32m        ' autocomplete="off" autocorrect="off" autocapitalize="none"' +[m
[32m+[m[32m        ' spellcheck="false" role="searchbox" aria-autocomplete="list" />' +[m
[32m+[m[32m      '</span>'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    this.$searchContainer = $search;[m
[32m+[m[32m    this.$search = $search.find('input');[m
[32m+[m
[32m+[m[32m    $rendered.prepend($search);[m
[32m+[m
[32m+[m[32m    return $rendered;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Search.prototype.bind = function (decorated, container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    var resultsId = container.id + '-results';[m
[32m+[m
[32m+[m[32m    decorated.call(this, container, $container);[m
[32m+[m
[32m+[m[32m    this.$search.on('keydown', function (evt) {[m
[32m+[m[32m      self.trigger('keypress', evt);[m
[32m+[m
[32m+[m[32m      self._keyUpPrevented = evt.isDefaultPrevented();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Workaround for browsers which do not support the `input` event[m
[32m+[m[32m    // This will prevent double-triggering of events for browsers which support[m
[32m+[m[32m    // both the `keyup` and `input` events.[m
[32m+[m[32m    this.$search.on('input', function (evt) {[m
[32m+[m[32m      // Unbind the duplicated `keyup` event[m
[32m+[m[32m      $(this).off('keyup');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$search.on('keyup input', function (evt) {[m
[32m+[m[32m      self.handleSearch(evt);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('open', function () {[m
[32m+[m[32m      self.$search.attr('tabindex', 0);[m
[32m+[m[32m      self.$search.attr('aria-controls', resultsId);[m
[32m+[m
[32m+[m[32m      self.$search.trigger('focus');[m
[32m+[m
[32m+[m[32m      window.setTimeout(function () {[m
[32m+[m[32m        self.$search.trigger('focus');[m
[32m+[m[32m      }, 0);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('close', function () {[m
[32m+[m[32m      self.$search.attr('tabindex', -1);[m
[32m+[m[32m      self.$search.removeAttr('aria-controls');[m
[32m+[m[32m      self.$search.removeAttr('aria-activedescendant');[m
[32m+[m
[32m+[m[32m      self.$search.val('');[m
[32m+[m[32m      self.$search.trigger('blur');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('focus', function () {[m
[32m+[m[32m      if (!container.isOpen()) {[m
[32m+[m[32m        self.$search.trigger('focus');[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:all', function (params) {[m
[32m+[m[32m      if (params.query.term == null || params.query.term === '') {[m
[32m+[m[32m        var showSearch = self.showSearch(params);[m
[32m+[m
[32m+[m[32m        if (showSearch) {[m
[32m+[m[32m          self.$searchContainer.removeClass('select2-search--hide');[m
[32m+[m[32m        } else {[m
[32m+[m[32m          self.$searchContainer.addClass('select2-search--hide');[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:focus', function (params) {[m
[32m+[m[32m      if (params.data._resultId) {[m
[32m+[m[32m        self.$search.attr('aria-activedescendant', params.data._resultId);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        self.$search.removeAttr('aria-activedescendant');[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Search.prototype.handleSearch = function (evt) {[m
[32m+[m[32m    if (!this._keyUpPrevented) {[m
[32m+[m[32m      var input = this.$search.val();[m
[32m+[m
[32m+[m[32m      this.trigger('query', {[m
[32m+[m[32m        term: input[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this._keyUpPrevented = false;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Search.prototype.showSearch = function (_, params) {[m
[32m+[m[32m    return true;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return Search;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/dropdown/hidePlaceholder',[[m
[32m+[m
[32m+[m[32m], function () {[m
[32m+[m[32m  function HidePlaceholder (decorated, $element, options, dataAdapter) {[m
[32m+[m[32m    this.placeholder = this.normalizePlaceholder(options.get('placeholder'));[m
[32m+[m
[32m+[m[32m    decorated.call(this, $element, options, dataAdapter);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  HidePlaceholder.prototype.append = function (decorated, data) {[m
[32m+[m[32m    data.results = this.removePlaceholder(data.results);[m
[32m+[m
[32m+[m[32m    decorated.call(this, data);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  HidePlaceholder.prototype.normalizePlaceholder = function (_, placeholder) {[m
[32m+[m[32m    if (typeof placeholder === 'string') {[m
[32m+[m[32m      placeholder = {[m
[32m+[m[32m        id: '',[m
[32m+[m[32m        text: placeholder[m
[32m+[m[32m      };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return placeholder;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  HidePlaceholder.prototype.removePlaceholder = function (_, data) {[m
[32m+[m[32m    var modifiedData = data.slice(0);[m
[32m+[m
[32m+[m[32m    for (var d = data.length - 1; d >= 0; d--) {[m
[32m+[m[32m      var item = data[d];[m
[32m+[m
[32m+[m[32m      if (this.placeholder.id === item.id) {[m
[32m+[m[32m        modifiedData.splice(d, 1);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return modifiedData;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return HidePlaceholder;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/dropdown/infiniteScroll',[[m
[32m+[m[32m  'jquery'[m
[32m+[m[32m], function ($) {[m
[32m+[m[32m  function InfiniteScroll (decorated, $element, options, dataAdapter) {[m
[32m+[m[32m    this.lastParams = {};[m
[32m+[m
[32m+[m[32m    decorated.call(this, $element, options, dataAdapter);[m
[32m+[m
[32m+[m[32m    this.$loadingMore = this.createLoadingMore();[m
[32m+[m[32m    this.loading = false;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  InfiniteScroll.prototype.append = function (decorated, data) {[m
[32m+[m[32m    this.$loadingMore.remove();[m
[32m+[m[32m    this.loading = false;[m
[32m+[m
[32m+[m[32m    decorated.call(this, data);[m
[32m+[m
[32m+[m[32m    if (this.showLoadingMore(data)) {[m
[32m+[m[32m      this.$results.append(this.$loadingMore);[m
[32m+[m[32m      this.loadMoreIfNeeded();[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  InfiniteScroll.prototype.bind = function (decorated, container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    decorated.call(this, container, $container);[m
[32m+[m
[32m+[m[32m    container.on('query', function (params) {[m
[32m+[m[32m      self.lastParams = params;[m
[32m+[m[32m      self.loading = true;[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('query:append', function (params) {[m
[32m+[m[32m      self.lastParams = params;[m
[32m+[m[32m      self.loading = true;[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$results.on('scroll', this.loadMoreIfNeeded.bind(this));[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  InfiniteScroll.prototype.loadMoreIfNeeded = function () {[m
[32m+[m[32m    var isLoadMoreVisible = $.contains([m
[32m+[m[32m      document.documentElement,[m
[32m+[m[32m      this.$loadingMore[0][m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    if (this.loading || !isLoadMoreVisible) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var currentOffset = this.$results.offset().top +[m
[32m+[m[32m      this.$results.outerHeight(false);[m
[32m+[m[32m    var loadingMoreOffset = this.$loadingMore.offset().top +[m
[32m+[m[32m      this.$loadingMore.outerHeight(false);[m
[32m+[m
[32m+[m[32m    if (currentOffset + 50 >= loadingMoreOffset) {[m
[32m+[m[32m      this.loadMore();[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  InfiniteScroll.prototype.loadMore = function () {[m
[32m+[m[32m    this.loading = true;[m
[32m+[m
[32m+[m[32m    var params = $.extend({}, {page: 1}, this.lastParams);[m
[32m+[m
[32m+[m[32m    params.page++;[m
[32m+[m
[32m+[m[32m    this.trigger('query:append', params);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  InfiniteScroll.prototype.showLoadingMore = function (_, data) {[m
[32m+[m[32m    return data.pagination && data.pagination.more;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  InfiniteScroll.prototype.createLoadingMore = function () {[m
[32m+[m[32m    var $option = $([m
[32m+[m[32m      '<li ' +[m
[32m+[m[32m      'class="select2-results__option select2-results__option--load-more"' +[m
[32m+[m[32m      'role="option" aria-disabled="true"></li>'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    var message = this.options.get('translations').get('loadingMore');[m
[32m+[m
[32m+[m[32m    $option.html(message(this.lastParams));[m
[32m+[m
[32m+[m[32m    return $option;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return InfiniteScroll;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/dropdown/attachBody',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  '../utils'[m
[32m+[m[32m], function ($, Utils) {[m
[32m+[m[32m  function AttachBody (decorated, $element, options) {[m
[32m+[m[32m    this.$dropdownParent = $(options.get('dropdownParent') || document.body);[m
[32m+[m
[32m+[m[32m    decorated.call(this, $element, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  AttachBody.prototype.bind = function (decorated, container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    decorated.call(this, container, $container);[m
[32m+[m
[32m+[m[32m    container.on('open', function () {[m
[32m+[m[32m      self._showDropdown();[m
[32m+[m[32m      self._attachPositioningHandler(container);[m
[32m+[m
[32m+[m[32m      // Must bind after the results handlers to ensure correct sizing[m
[32m+[m[32m      self._bindContainerResultHandlers(container);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('close', function () {[m
[32m+[m[32m      self._hideDropdown();[m
[32m+[m[32m      self._detachPositioningHandler(container);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$dropdownContainer.on('mousedown', function (evt) {[m
[32m+[m[32m      evt.stopPropagation();[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AttachBody.prototype.destroy = function (decorated) {[m
[32m+[m[32m    decorated.call(this);[m
[32m+[m
[32m+[m[32m    this.$dropdownContainer.remove();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AttachBody.prototype.position = function (decorated, $dropdown, $container) {[m
[32m+[m[32m    // Clone all of the container classes[m
[32m+[m[32m    $dropdown.attr('class', $container.attr('class'));[m
[32m+[m
[32m+[m[32m    $dropdown.removeClass('select2');[m
[32m+[m[32m    $dropdown.addClass('select2-container--open');[m
[32m+[m
[32m+[m[32m    $dropdown.css({[m
[32m+[m[32m      position: 'absolute',[m
[32m+[m[32m      top: -999999[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$container = $container;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AttachBody.prototype.render = function (decorated) {[m
[32m+[m[32m    var $container = $('<span></span>');[m
[32m+[m
[32m+[m[32m    var $dropdown = decorated.call(this);[m
[32m+[m[32m    $container.append($dropdown);[m
[32m+[m
[32m+[m[32m    this.$dropdownContainer = $container;[m
[32m+[m
[32m+[m[32m    return $container;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AttachBody.prototype._hideDropdown = function (decorated) {[m
[32m+[m[32m    this.$dropdownContainer.detach();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AttachBody.prototype._bindContainerResultHandlers =[m
[32m+[m[32m      function (decorated, container) {[m
[32m+[m
[32m+[m[32m    // These should only be bound once[m
[32m+[m[32m    if (this._containerResultsHandlersBound) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    container.on('results:all', function () {[m
[32m+[m[32m      self._positionDropdown();[m
[32m+[m[32m      self._resizeDropdown();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:append', function () {[m
[32m+[m[32m      self._positionDropdown();[m
[32m+[m[32m      self._resizeDropdown();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('results:message', function () {[m
[32m+[m[32m      self._positionDropdown();[m
[32m+[m[32m      self._resizeDropdown();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('select', function () {[m
[32m+[m[32m      self._positionDropdown();[m
[32m+[m[32m      self._resizeDropdown();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('unselect', function () {[m
[32m+[m[32m      self._positionDropdown();[m
[32m+[m[32m      self._resizeDropdown();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this._containerResultsHandlersBound = true;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AttachBody.prototype._attachPositioningHandler =[m
[32m+[m[32m      function (decorated, container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    var scrollEvent = 'scroll.select2.' + container.id;[m
[32m+[m[32m    var resizeEvent = 'resize.select2.' + container.id;[m
[32m+[m[32m    var orientationEvent = 'orientationchange.select2.' + container.id;[m
[32m+[m
[32m+[m[32m    var $watchers = this.$container.parents().filter(Utils.hasScroll);[m
[32m+[m[32m    $watchers.each(function () {[m
[32m+[m[32m      Utils.StoreData(this, 'select2-scroll-position', {[m
[32m+[m[32m        x: $(this).scrollLeft(),[m
[32m+[m[32m        y: $(this).scrollTop()[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    $watchers.on(scrollEvent, function (ev) {[m
[32m+[m[32m      var position = Utils.GetData(this, 'select2-scroll-position');[m
[32m+[m[32m      $(this).scrollTop(position.y);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    $(window).on(scrollEvent + ' ' + resizeEvent + ' ' + orientationEvent,[m
[32m+[m[32m      function (e) {[m
[32m+[m[32m      self._positionDropdown();[m
[32m+[m[32m      self._resizeDropdown();[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AttachBody.prototype._detachPositioningHandler =[m
[32m+[m[32m      function (decorated, container) {[m
[32m+[m[32m    var scrollEvent = 'scroll.select2.' + container.id;[m
[32m+[m[32m    var resizeEvent = 'resize.select2.' + container.id;[m
[32m+[m[32m    var orientationEvent = 'orientationchange.select2.' + container.id;[m
[32m+[m
[32m+[m[32m    var $watchers = this.$container.parents().filter(Utils.hasScroll);[m
[32m+[m[32m    $watchers.off(scrollEvent);[m
[32m+[m
[32m+[m[32m    $(window).off(scrollEvent + ' ' + resizeEvent + ' ' + orientationEvent);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AttachBody.prototype._positionDropdown = function () {[m
[32m+[m[32m    var $window = $(window);[m
[32m+[m
[32m+[m[32m    var isCurrentlyAbove = this.$dropdown.hasClass('select2-dropdown--above');[m
[32m+[m[32m    var isCurrentlyBelow = this.$dropdown.hasClass('select2-dropdown--below');[m
[32m+[m
[32m+[m[32m    var newDirection = null;[m
[32m+[m
[32m+[m[32m    var offset = this.$container.offset();[m
[32m+[m
[32m+[m[32m    offset.bottom = offset.top + this.$container.outerHeight(false);[m
[32m+[m
[32m+[m[32m    var container = {[m
[32m+[m[32m      height: this.$container.outerHeight(false)[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    container.top = offset.top;[m
[32m+[m[32m    container.bottom = offset.top + container.height;[m
[32m+[m
[32m+[m[32m    var dropdown = {[m
[32m+[m[32m      height: this.$dropdown.outerHeight(false)[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var viewport = {[m
[32m+[m[32m      top: $window.scrollTop(),[m
[32m+[m[32m      bottom: $window.scrollTop() + $window.height()[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var enoughRoomAbove = viewport.top < (offset.top - dropdown.height);[m
[32m+[m[32m    var enoughRoomBelow = viewport.bottom > (offset.bottom + dropdown.height);[m
[32m+[m
[32m+[m[32m    var css = {[m
[32m+[m[32m      left: offset.left,[m
[32m+[m[32m      top: container.bottom[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // Determine what the parent element is to use for calculating the offset[m
[32m+[m[32m    var $offsetParent = this.$dropdownParent;[m
[32m+[m
[32m+[m[32m    // For statically positioned elements, we need to get the element[m
[32m+[m[32m    // that is determining the offset[m
[32m+[m[32m    if ($offsetParent.css('position') === 'static') {[m
[32m+[m[32m      $offsetParent = $offsetParent.offsetParent();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var parentOffset = {[m
[32m+[m[32m      top: 0,[m
[32m+[m[32m      left: 0[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    if ([m
[32m+[m[32m      $.contains(document.body, $offsetParent[0]) ||[m
[32m+[m[32m      $offsetParent[0].isConnected[m
[32m+[m[32m      ) {[m
[32m+[m[32m      parentOffset = $offsetParent.offset();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    css.top -= parentOffset.top;[m
[32m+[m[32m    css.left -= parentOffset.left;[m
[32m+[m
[32m+[m[32m    if (!isCurrentlyAbove && !isCurrentlyBelow) {[m
[32m+[m[32m      newDirection = 'below';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!enoughRoomBelow && enoughRoomAbove && !isCurrentlyAbove) {[m
[32m+[m[32m      newDirection = 'above';[m
[32m+[m[32m    } else if (!enoughRoomAbove && enoughRoomBelow && isCurrentlyAbove) {[m
[32m+[m[32m      newDirection = 'below';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (newDirection == 'above' ||[m
[32m+[m[32m      (isCurrentlyAbove && newDirection !== 'below')) {[m
[32m+[m[32m      css.top = container.top - parentOffset.top - dropdown.height;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (newDirection != null) {[m
[32m+[m[32m      this.$dropdown[m
[32m+[m[32m        .removeClass('select2-dropdown--below select2-dropdown--above')[m
[32m+[m[32m        .addClass('select2-dropdown--' + newDirection);[m
[32m+[m[32m      this.$container[m
[32m+[m[32m        .removeClass('select2-container--below select2-container--above')[m
[32m+[m[32m        .addClass('select2-container--' + newDirection);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.$dropdownContainer.css(css);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AttachBody.prototype._resizeDropdown = function () {[m
[32m+[m[32m    var css = {[m
[32m+[m[32m      width: this.$container.outerWidth(false) + 'px'[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    if (this.options.get('dropdownAutoWidth')) {[m
[32m+[m[32m      css.minWidth = css.width;[m
[32m+[m[32m      css.position = 'relative';[m
[32m+[m[32m      css.width = 'auto';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.$dropdown.css(css);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AttachBody.prototype._showDropdown = function (decorated) {[m
[32m+[m[32m    this.$dropdownContainer.appendTo(this.$dropdownParent);[m
[32m+[m
[32m+[m[32m    this._positionDropdown();[m
[32m+[m[32m    this._resizeDropdown();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return AttachBody;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/dropdown/minimumResultsForSearch',[[m
[32m+[m
[32m+[m[32m], function () {[m
[32m+[m[32m  function countResults (data) {[m
[32m+[m[32m    var count = 0;[m
[32m+[m
[32m+[m[32m    for (var d = 0; d < data.length; d++) {[m
[32m+[m[32m      var item = data[d];[m
[32m+[m
[32m+[m[32m      if (item.children) {[m
[32m+[m[32m        count += countResults(item.children);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        count++;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return count;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function MinimumResultsForSearch (decorated, $element, options, dataAdapter) {[m
[32m+[m[32m    this.minimumResultsForSearch = options.get('minimumResultsForSearch');[m
[32m+[m
[32m+[m[32m    if (this.minimumResultsForSearch < 0) {[m
[32m+[m[32m      this.minimumResultsForSearch = Infinity;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    decorated.call(this, $element, options, dataAdapter);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  MinimumResultsForSearch.prototype.showSearch = function (decorated, params) {[m
[32m+[m[32m    if (countResults(params.data.results) < this.minimumResultsForSearch) {[m
[32m+[m[32m      return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return decorated.call(this, params);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return MinimumResultsForSearch;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/dropdown/selectOnClose',[[m
[32m+[m[32m  '../utils'[m
[32m+[m[32m], function (Utils) {[m
[32m+[m[32m  function SelectOnClose () { }[m
[32m+[m
[32m+[m[32m  SelectOnClose.prototype.bind = function (decorated, container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    decorated.call(this, container, $container);[m
[32m+[m
[32m+[m[32m    container.on('close', function (params) {[m
[32m+[m[32m      self._handleSelectOnClose(params);[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SelectOnClose.prototype._handleSelectOnClose = function (_, params) {[m
[32m+[m[32m    if (params && params.originalSelect2Event != null) {[m
[32m+[m[32m      var event = params.originalSelect2Event;[m
[32m+[m
[32m+[m[32m      // Don't select an item if the close event was triggered from a select or[m
[32m+[m[32m      // unselect event[m
[32m+[m[32m      if (event._type === 'select' || event._type === 'unselect') {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var $highlightedResults = this.getHighlightedResults();[m
[32m+[m
[32m+[m[32m    // Only select highlighted results[m
[32m+[m[32m    if ($highlightedResults.length < 1) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var data = Utils.GetData($highlightedResults[0], 'data');[m
[32m+[m
[32m+[m[32m    // Don't re-select already selected resulte[m
[32m+[m[32m    if ([m
[32m+[m[32m      (data.element != null && data.element.selected) ||[m
[32m+[m[32m      (data.element == null && data.selected)[m
[32m+[m[32m    ) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.trigger('select', {[m
[32m+[m[32m        data: data[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return SelectOnClose;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/dropdown/closeOnSelect',[[m
[32m+[m
[32m+[m[32m], function () {[m
[32m+[m[32m  function CloseOnSelect () { }[m
[32m+[m
[32m+[m[32m  CloseOnSelect.prototype.bind = function (decorated, container, $container) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    decorated.call(this, container, $container);[m
[32m+[m
[32m+[m[32m    container.on('select', function (evt) {[m
[32m+[m[32m      self._selectTriggered(evt);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    container.on('unselect', function (evt) {[m
[32m+[m[32m      self._selectTriggered(evt);[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  CloseOnSelect.prototype._selectTriggered = function (_, evt) {[m
[32m+[m[32m    var originalEvent = evt.originalEvent;[m
[32m+[m
[32m+[m[32m    // Don't close if the control key is being held[m
[32m+[m[32m    if (originalEvent && (originalEvent.ctrlKey || originalEvent.metaKey)) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.trigger('close', {[m
[32m+[m[32m      originalEvent: originalEvent,[m
[32m+[m[32m      originalSelect2Event: evt[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return CloseOnSelect;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/i18n/en',[],function () {[m
[32m+[m[32m  // English[m
[32m+[m[32m  return {[m
[32m+[m[32m    errorLoading: function () {[m
[32m+[m[32m      return 'The results could not be loaded.';[m
[32m+[m[32m    },[m
[32m+[m[32m    inputTooLong: function (args) {[m
[32m+[m[32m      var overChars = args.input.length - args.maximum;[m
[32m+[m
[32m+[m[32m      var message = 'Please delete ' + overChars + ' character';[m
[32m+[m
[32m+[m[32m      if (overChars != 1) {[m
[32m+[m[32m        message += 's';[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return message;[m
[32m+[m[32m    },[m
[32m+[m[32m    inputTooShort: function (args) {[m
[32m+[m[32m      var remainingChars = args.minimum - args.input.length;[m
[32m+[m
[32m+[m[32m      var message = 'Please enter ' + remainingChars + ' or more characters';[m
[32m+[m
[32m+[m[32m      return message;[m
[32m+[m[32m    },[m
[32m+[m[32m    loadingMore: function () {[m
[32m+[m[32m      return 'Loading more results‚Ä¶';[m
[32m+[m[32m    },[m
[32m+[m[32m    maximumSelected: function (args) {[m
[32m+[m[32m      var message = 'You can only select ' + args.maximum + ' item';[m
[32m+[m
[32m+[m[32m      if (args.maximum != 1) {[m
[32m+[m[32m        message += 's';[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return message;[m
[32m+[m[32m    },[m
[32m+[m[32m    noResults: function () {[m
[32m+[m[32m      return 'No results found';[m
[32m+[m[32m    },[m
[32m+[m[32m    searching: function () {[m
[32m+[m[32m      return 'Searching‚Ä¶';[m
[32m+[m[32m    },[m
[32m+[m[32m    removeAllItems: function () {[m
[32m+[m[32m      return 'Remove all items';[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/defaults',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  'require',[m
[32m+[m
[32m+[m[32m  './results',[m
[32m+[m
[32m+[m[32m  './selection/single',[m
[32m+[m[32m  './selection/multiple',[m
[32m+[m[32m  './selection/placeholder',[m
[32m+[m[32m  './selection/allowClear',[m
[32m+[m[32m  './selection/search',[m
[32m+[m[32m  './selection/eventRelay',[m
[32m+[m
[32m+[m[32m  './utils',[m
[32m+[m[32m  './translation',[m
[32m+[m[32m  './diacritics',[m
[32m+[m
[32m+[m[32m  './data/select',[m
[32m+[m[32m  './data/array',[m
[32m+[m[32m  './data/ajax',[m
[32m+[m[32m  './data/tags',[m
[32m+[m[32m  './data/tokenizer',[m
[32m+[m[32m  './data/minimumInputLength',[m
[32m+[m[32m  './data/maximumInputLength',[m
[32m+[m[32m  './data/maximumSelectionLength',[m
[32m+[m
[32m+[m[32m  './dropdown',[m
[32m+[m[32m  './dropdown/search',[m
[32m+[m[32m  './dropdown/hidePlaceholder',[m
[32m+[m[32m  './dropdown/infiniteScroll',[m
[32m+[m[32m  './dropdown/attachBody',[m
[32m+[m[32m  './dropdown/minimumResultsForSearch',[m
[32m+[m[32m  './dropdown/selectOnClose',[m
[32m+[m[32m  './dropdown/closeOnSelect',[m
[32m+[m
[32m+[m[32m  './i18n/en'[m
[32m+[m[32m], function ($, require,[m
[32m+[m
[32m+[m[32m             ResultsList,[m
[32m+[m
[32m+[m[32m             SingleSelection, MultipleSelection, Placeholder, AllowClear,[m
[32m+[m[32m             SelectionSearch, EventRelay,[m
[32m+[m
[32m+[m[32m             Utils, Translation, DIACRITICS,[m
[32m+[m
[32m+[m[32m             SelectData, ArrayData, AjaxData, Tags, Tokenizer,[m
[32m+[m[32m             MinimumInputLength, MaximumInputLength, MaximumSelectionLength,[m
[32m+[m
[32m+[m[32m             Dropdown, DropdownSearch, HidePlaceholder, InfiniteScroll,[m
[32m+[m[32m             AttachBody, MinimumResultsForSearch, SelectOnClose, CloseOnSelect,[m
[32m+[m
[32m+[m[32m             EnglishTranslation) {[m
[32m+[m[32m  function Defaults () {[m
[32m+[m[32m    this.reset();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Defaults.prototype.apply = function (options) {[m
[32m+[m[32m    options = $.extend(true, {}, this.defaults, options);[m
[32m+[m
[32m+[m[32m    if (options.dataAdapter == null) {[m
[32m+[m[32m      if (options.ajax != null) {[m
[32m+[m[32m        options.dataAdapter = AjaxData;[m
[32m+[m[32m      } else if (options.data != null) {[m
[32m+[m[32m        options.dataAdapter = ArrayData;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        options.dataAdapter = SelectData;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.minimumInputLength > 0) {[m
[32m+[m[32m        options.dataAdapter = Utils.Decorate([m
[32m+[m[32m          options.dataAdapter,[m
[32m+[m[32m          MinimumInputLength[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.maximumInputLength > 0) {[m
[32m+[m[32m        options.dataAdapter = Utils.Decorate([m
[32m+[m[32m          options.dataAdapter,[m
[32m+[m[32m          MaximumInputLength[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.maximumSelectionLength > 0) {[m
[32m+[m[32m        options.dataAdapter = Utils.Decorate([m
[32m+[m[32m          options.dataAdapter,[m
[32m+[m[32m          MaximumSelectionLength[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.tags) {[m
[32m+[m[32m        options.dataAdapter = Utils.Decorate(options.dataAdapter, Tags);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.tokenSeparators != null || options.tokenizer != null) {[m
[32m+[m[32m        options.dataAdapter = Utils.Decorate([m
[32m+[m[32m          options.dataAdapter,[m
[32m+[m[32m          Tokenizer[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.query != null) {[m
[32m+[m[32m        var Query = require(options.amdBase + 'compat/query');[m
[32m+[m
[32m+[m[32m        options.dataAdapter = Utils.Decorate([m
[32m+[m[32m          options.dataAdapter,[m
[32m+[m[32m          Query[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.initSelection != null) {[m
[32m+[m[32m        var InitSelection = require(options.amdBase + 'compat/initSelection');[m
[32m+[m
[32m+[m[32m        options.dataAdapter = Utils.Decorate([m
[32m+[m[32m          options.dataAdapter,[m
[32m+[m[32m          InitSelection[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (options.resultsAdapter == null) {[m
[32m+[m[32m      options.resultsAdapter = ResultsList;[m
[32m+[m
[32m+[m[32m      if (options.ajax != null) {[m
[32m+[m[32m        options.resultsAdapter = Utils.Decorate([m
[32m+[m[32m          options.resultsAdapter,[m
[32m+[m[32m          InfiniteScroll[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.placeholder != null) {[m
[32m+[m[32m        options.resultsAdapter = Utils.Decorate([m
[32m+[m[32m          options.resultsAdapter,[m
[32m+[m[32m          HidePlaceholder[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.selectOnClose) {[m
[32m+[m[32m        options.resultsAdapter = Utils.Decorate([m
[32m+[m[32m          options.resultsAdapter,[m
[32m+[m[32m          SelectOnClose[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (options.dropdownAdapter == null) {[m
[32m+[m[32m      if (options.multiple) {[m
[32m+[m[32m        options.dropdownAdapter = Dropdown;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        var SearchableDropdown = Utils.Decorate(Dropdown, DropdownSearch);[m
[32m+[m
[32m+[m[32m        options.dropdownAdapter = SearchableDropdown;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.minimumResultsForSearch !== 0) {[m
[32m+[m[32m        options.dropdownAdapter = Utils.Decorate([m
[32m+[m[32m          options.dropdownAdapter,[m
[32m+[m[32m          MinimumResultsForSearch[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.closeOnSelect) {[m
[32m+[m[32m        options.dropdownAdapter = Utils.Decorate([m
[32m+[m[32m          options.dropdownAdapter,[m
[32m+[m[32m          CloseOnSelect[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if ([m
[32m+[m[32m        options.dropdownCssClass != null ||[m
[32m+[m[32m        options.dropdownCss != null ||[m
[32m+[m[32m        options.adaptDropdownCssClass != null[m
[32m+[m[32m      ) {[m
[32m+[m[32m        var DropdownCSS = require(options.amdBase + 'compat/dropdownCss');[m
[32m+[m
[32m+[m[32m        options.dropdownAdapter = Utils.Decorate([m
[32m+[m[32m          options.dropdownAdapter,[m
[32m+[m[32m          DropdownCSS[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      options.dropdownAdapter = Utils.Decorate([m
[32m+[m[32m        options.dropdownAdapter,[m
[32m+[m[32m        AttachBody[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (options.selectionAdapter == null) {[m
[32m+[m[32m      if (options.multiple) {[m
[32m+[m[32m        options.selectionAdapter = MultipleSelection;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        options.selectionAdapter = SingleSelection;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // Add the placeholder mixin if a placeholder was specified[m
[32m+[m[32m      if (options.placeholder != null) {[m
[32m+[m[32m        options.selectionAdapter = Utils.Decorate([m
[32m+[m[32m          options.selectionAdapter,[m
[32m+[m[32m          Placeholder[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.allowClear) {[m
[32m+[m[32m        options.selectionAdapter = Utils.Decorate([m
[32m+[m[32m          options.selectionAdapter,[m
[32m+[m[32m          AllowClear[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (options.multiple) {[m
[32m+[m[32m        options.selectionAdapter = Utils.Decorate([m
[32m+[m[32m          options.selectionAdapter,[m
[32m+[m[32m          SelectionSearch[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if ([m
[32m+[m[32m        options.containerCssClass != null ||[m
[32m+[m[32m        options.containerCss != null ||[m
[32m+[m[32m        options.adaptContainerCssClass != null[m
[32m+[m[32m      ) {[m
[32m+[m[32m        var ContainerCSS = require(options.amdBase + 'compat/containerCss');[m
[32m+[m
[32m+[m[32m        options.selectionAdapter = Utils.Decorate([m
[32m+[m[32m          options.selectionAdapter,[m
[32m+[m[32m          ContainerCSS[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      options.selectionAdapter = Utils.Decorate([m
[32m+[m[32m        options.selectionAdapter,[m
[32m+[m[32m        EventRelay[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // If the defaults were not previously applied from an element, it is[m
[32m+[m[32m    // possible for the language option to have not been resolved[m
[32m+[m[32m    options.language = this._resolveLanguage(options.language);[m
[32m+[m
[32m+[m[32m    // Always fall back to English since it will always be complete[m
[32m+[m[32m    options.language.push('en');[m
[32m+[m
[32m+[m[32m    var uniqueLanguages = [];[m
[32m+[m
[32m+[m[32m    for (var l = 0; l < options.language.length; l++) {[m
[32m+[m[32m      var language = options.language[l];[m
[32m+[m
[32m+[m[32m      if (uniqueLanguages.indexOf(language) === -1) {[m
[32m+[m[32m        uniqueLanguages.push(language);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    options.language = uniqueLanguages;[m
[32m+[m
[32m+[m[32m    options.translations = this._processTranslations([m
[32m+[m[32m      options.language,[m
[32m+[m[32m      options.debug[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    return options;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Defaults.prototype.reset = function () {[m
[32m+[m[32m    function stripDiacritics (text) {[m
[32m+[m[32m      // Used 'uni range + named function' from http://jsperf.com/diacritics/18[m
[32m+[m[32m      function match(a) {[m
[32m+[m[32m        return DIACRITICS[a] || a;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return text.replace(/[^\u0000-\u007E]/g, match);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function matcher (params, data) {[m
[32m+[m[32m      // Always return the object if there is nothing to compare[m
[32m+[m[32m      if ($.trim(params.term) === '') {[m
[32m+[m[32m        return data;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // Do a recursive check for options with children[m
[32m+[m[32m      if (data.children && data.children.length > 0) {[m
[32m+[m[32m        // Clone the data object if there are children[m
[32m+[m[32m        // This is required as we modify the object to remove any non-matches[m
[32m+[m[32m        var match = $.extend(true, {}, data);[m
[32m+[m
[32m+[m[32m        // Check each child of the option[m
[32m+[m[32m        for (var c = data.children.length - 1; c >= 0; c--) {[m
[32m+[m[32m          var child = data.children[c];[m
[32m+[m
[32m+[m[32m          var matches = matcher(params, child);[m
[32m+[m
[32m+[m[32m          // If there wasn't a match, remove the object in the array[m
[32m+[m[32m          if (matches == null) {[m
[32m+[m[32m            match.children.splice(c, 1);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // If any children matched, return the new object[m
[32m+[m[32m        if (match.children.length > 0) {[m
[32m+[m[32m          return match;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // If there were no matching children, check just the plain object[m
[32m+[m[32m        return matcher(params, match);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var original = stripDiacritics(data.text).toUpperCase();[m
[32m+[m[32m      var term = stripDiacritics(params.term).toUpperCase();[m
[32m+[m
[32m+[m[32m      // Check if the text contains the term[m
[32m+[m[32m      if (original.indexOf(term) > -1) {[m
[32m+[m[32m        return data;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      // If it doesn't contain the term, don't return anything[m
[32m+[m[32m      return null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.defaults = {[m
[32m+[m[32m      amdBase: './',[m
[32m+[m[32m      amdLanguageBase: './i18n/',[m
[32m+[m[32m      closeOnSelect: true,[m
[32m+[m[32m      debug: false,[m
[32m+[m[32m      dropdownAutoWidth: false,[m
[32m+[m[32m      escapeMarkup: Utils.escapeMarkup,[m
[32m+[m[32m      language: {},[m
[32m+[m[32m      matcher: matcher,[m
[32m+[m[32m      minimumInputLength: 0,[m
[32m+[m[32m      maximumInputLength: 0,[m
[32m+[m[32m      maximumSelectionLength: 0,[m
[32m+[m[32m      minimumResultsForSearch: 0,[m
[32m+[m[32m      selectOnClose: false,[m
[32m+[m[32m      scrollAfterSelect: false,[m
[32m+[m[32m      sorter: function (data) {[m
[32m+[m[32m        return data;[m
[32m+[m[32m      },[m
[32m+[m[32m      templateResult: function (result) {[m
[32m+[m[32m        return result.text;[m
[32m+[m[32m      },[m
[32m+[m[32m      templateSelection: function (selection) {[m
[32m+[m[32m        return selection.text;[m
[32m+[m[32m      },[m
[32m+[m[32m      theme: 'default',[m
[32m+[m[32m      width: 'resolve'[m
[32m+[m[32m    };[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Defaults.prototype.applyFromElement = function (options, $element) {[m
[32m+[m[32m    var optionLanguage = options.language;[m
[32m+[m[32m    var defaultLanguage = this.defaults.language;[m
[32m+[m[32m    var elementLanguage = $element.prop('lang');[m
[32m+[m[32m    var parentLanguage = $element.closest('[lang]').prop('lang');[m
[32m+[m
[32m+[m[32m    var languages = Array.prototype.concat.call([m
[32m+[m[32m      this._resolveLanguage(elementLanguage),[m
[32m+[m[32m      this._resolveLanguage(optionLanguage),[m
[32m+[m[32m      this._resolveLanguage(defaultLanguage),[m
[32m+[m[32m      this._resolveLanguage(parentLanguage)[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    options.language = languages;[m
[32m+[m
[32m+[m[32m    return options;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Defaults.prototype._resolveLanguage = function (language) {[m
[32m+[m[32m    if (!language) {[m
[32m+[m[32m      return [];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if ($.isEmptyObject(language)) {[m
[32m+[m[32m      return [];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if ($.isPlainObject(language)) {[m
[32m+[m[32m      return [language];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var languages;[m
[32m+[m
[32m+[m[32m    if (!$.isArray(language)) {[m
[32m+[m[32m      languages = [language];[m
[32m+[m[32m    } else {[m
[32m+[m[32m      languages = language;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var resolvedLanguages = [];[m
[32m+[m
[32m+[m[32m    for (var l = 0; l < languages.length; l++) {[m
[32m+[m[32m      resolvedLanguages.push(languages[l]);[m
[32m+[m
[32m+[m[32m      if (typeof languages[l] === 'string' && languages[l].indexOf('-') > 0) {[m
[32m+[m[32m        // Extract the region information if it is included[m
[32m+[m[32m        var languageParts = languages[l].split('-');[m
[32m+[m[32m        var baseLanguage = languageParts[0];[m
[32m+[m
[32m+[m[32m        resolvedLanguages.push(baseLanguage);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return resolvedLanguages;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Defaults.prototype._processTranslations = function (languages, debug) {[m
[32m+[m[32m    var translations = new Translation();[m
[32m+[m
[32m+[m[32m    for (var l = 0; l < languages.length; l++) {[m
[32m+[m[32m      var languageData = new Translation();[m
[32m+[m
[32m+[m[32m      var language = languages[l];[m
[32m+[m
[32m+[m[32m      if (typeof language === 'string') {[m
[32m+[m[32m        try {[m
[32m+[m[32m          // Try to load it with the original name[m
[32m+[m[32m          languageData = Translation.loadPath(language);[m
[32m+[m[32m        } catch (e) {[m
[32m+[m[32m          try {[m
[32m+[m[32m            // If we couldn't load it, check if it wasn't the full path[m
[32m+[m[32m            language = this.defaults.amdLanguageBase + language;[m
[32m+[m[32m            languageData = Translation.loadPath(language);[m
[32m+[m[32m          } catch (ex) {[m
[32m+[m[32m            // The translation could not be loaded at all. Sometimes this is[m
[32m+[m[32m            // because of a configuration problem, other times this can be[m
[32m+[m[32m            // because of how Select2 helps load all possible translation files[m
[32m+[m[32m            if (debug && window.console && console.warn) {[m
[32m+[m[32m              console.warn([m
[32m+[m[32m                'Select2: The language file for "' + language + '" could ' +[m
[32m+[m[32m                'not be automatically loaded. A fallback will be used instead.'[m
[32m+[m[32m              );[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      } else if ($.isPlainObject(language)) {[m
[32m+[m[32m        languageData = new Translation(language);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        languageData = language;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      translations.extend(languageData);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return translations;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Defaults.prototype.set = function (key, value) {[m
[32m+[m[32m    var camelKey = $.camelCase(key);[m
[32m+[m
[32m+[m[32m    var data = {};[m
[32m+[m[32m    data[camelKey] = value;[m
[32m+[m
[32m+[m[32m    var convertedData = Utils._convertData(data);[m
[32m+[m
[32m+[m[32m    $.extend(true, this.defaults, convertedData);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  var defaults = new Defaults();[m
[32m+[m
[32m+[m[32m  return defaults;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/options',[[m
[32m+[m[32m  'require',[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  './defaults',[m
[32m+[m[32m  './utils'[m
[32m+[m[32m], function (require, $, Defaults, Utils) {[m
[32m+[m[32m  function Options (options, $element) {[m
[32m+[m[32m    this.options = options;[m
[32m+[m
[32m+[m[32m    if ($element != null) {[m
[32m+[m[32m      this.fromElement($element);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if ($element != null) {[m
[32m+[m[32m      this.options = Defaults.applyFromElement(this.options, $element);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.options = Defaults.apply(this.options);[m
[32m+[m
[32m+[m[32m    if ($element && $element.is('input')) {[m
[32m+[m[32m      var InputCompat = require(this.get('amdBase') + 'compat/inputData');[m
[32m+[m
[32m+[m[32m      this.options.dataAdapter = Utils.Decorate([m
[32m+[m[32m        this.options.dataAdapter,[m
[32m+[m[32m        InputCompat[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Options.prototype.fromElement = function ($e) {[m
[32m+[m[32m    var excludedData = ['select2'];[m
[32m+[m
[32m+[m[32m    if (this.options.multiple == null) {[m
[32m+[m[32m      this.options.multiple = $e.prop('multiple');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (this.options.disabled == null) {[m
[32m+[m[32m      this.options.disabled = $e.prop('disabled');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (this.options.dir == null) {[m
[32m+[m[32m      if ($e.prop('dir')) {[m
[32m+[m[32m        this.options.dir = $e.prop('dir');[m
[32m+[m[32m      } else if ($e.closest('[dir]').prop('dir')) {[m
[32m+[m[32m        this.options.dir = $e.closest('[dir]').prop('dir');[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this.options.dir = 'ltr';[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $e.prop('disabled', this.options.disabled);[m
[32m+[m[32m    $e.prop('multiple', this.options.multiple);[m
[32m+[m
[32m+[m[32m    if (Utils.GetData($e[0], 'select2Tags')) {[m
[32m+[m[32m      if (this.options.debug && window.console && console.warn) {[m
[32m+[m[32m        console.warn([m
[32m+[m[32m          'Select2: The `data-select2-tags` attribute has been changed to ' +[m
[32m+[m[32m          'use the `data-data` and `data-tags="true"` attributes and will be ' +[m
[32m+[m[32m          'removed in future versions of Select2.'[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      Utils.StoreData($e[0], 'data', Utils.GetData($e[0], 'select2Tags'));[m
[32m+[m[32m      Utils.StoreData($e[0], 'tags', true);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (Utils.GetData($e[0], 'ajaxUrl')) {[m
[32m+[m[32m      if (this.options.debug && window.console && console.warn) {[m
[32m+[m[32m        console.warn([m
[32m+[m[32m          'Select2: The `data-ajax-url` attribute has been changed to ' +[m
[32m+[m[32m          '`data-ajax--url` and support for the old attribute will be removed' +[m
[32m+[m[32m          ' in future versions of Select2.'[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      $e.attr('ajax--url', Utils.GetData($e[0], 'ajaxUrl'));[m
[32m+[m[32m      Utils.StoreData($e[0], 'ajax-Url', Utils.GetData($e[0], 'ajaxUrl'));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var dataset = {};[m
[32m+[m
[32m+[m[32m    function upperCaseLetter(_, letter) {[m
[32m+[m[32m      return letter.toUpperCase();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Pre-load all of the attributes which are prefixed with `data-`[m
[32m+[m[32m    for (var attr = 0; attr < $e[0].attributes.length; attr++) {[m
[32m+[m[32m      var attributeName = $e[0].attributes[attr].name;[m
[32m+[m[32m      var prefix = 'data-';[m
[32m+[m
[32m+[m[32m      if (attributeName.substr(0, prefix.length) == prefix) {[m
[32m+[m[32m        // Get the contents of the attribute after `data-`[m
[32m+[m[32m        var dataName = attributeName.substring(prefix.length);[m
[32m+[m
[32m+[m[32m        // Get the data contents from the consistent source[m
[32m+[m[32m        // This is more than likely the jQuery data helper[m
[32m+[m[32m        var dataValue = Utils.GetData($e[0], dataName);[m
[32m+[m
[32m+[m[32m        // camelCase the attribute name to match the spec[m
[32m+[m[32m        var camelDataName = dataName.replace(/-([a-z])/g, upperCaseLetter);[m
[32m+[m
[32m+[m[32m        // Store the data attribute contents into the dataset since[m
[32m+[m[32m        dataset[camelDataName] = dataValue;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Prefer the element's `dataset` attribute if it exists[m
[32m+[m[32m    // jQuery 1.x does not correctly handle data attributes with multiple dashes[m
[32m+[m[32m    if ($.fn.jquery && $.fn.jquery.substr(0, 2) == '1.' && $e[0].dataset) {[m
[32m+[m[32m      dataset = $.extend(true, {}, $e[0].dataset, dataset);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Prefer our internal data cache if it exists[m
[32m+[m[32m    var data = $.extend(true, {}, Utils.GetData($e[0]), dataset);[m
[32m+[m
[32m+[m[32m    data = Utils._convertData(data);[m
[32m+[m
[32m+[m[32m    for (var key in data) {[m
[32m+[m[32m      if ($.inArray(key, excludedData) > -1) {[m
[32m+[m[32m        continue;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if ($.isPlainObject(this.options[key])) {[m
[32m+[m[32m        $.extend(this.options[key], data[key]);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this.options[key] = data[key];[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return this;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Options.prototype.get = function (key) {[m
[32m+[m[32m    return this.options[key];[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Options.prototype.set = function (key, val) {[m
[32m+[m[32m    this.options[key] = val;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return Options;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/core',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  './options',[m
[32m+[m[32m  './utils',[m
[32m+[m[32m  './keys'[m
[32m+[m[32m], function ($, Options, Utils, KEYS) {[m
[32m+[m[32m  var Select2 = function ($element, options) {[m
[32m+[m[32m    if (Utils.GetData($element[0], 'select2') != null) {[m
[32m+[m[32m      Utils.GetData($element[0], 'select2').destroy();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.$element = $element;[m
[32m+[m
[32m+[m[32m    this.id = this._generateId($element);[m
[32m+[m
[32m+[m[32m    options = options || {};[m
[32m+[m
[32m+[m[32m    this.options = new Options(options, $element);[m
[32m+[m
[32m+[m[32m    Select2.__super__.constructor.call(this);[m
[32m+[m
[32m+[m[32m    // Set up the tabindex[m
[32m+[m
[32m+[m[32m    var tabindex = $element.attr('tabindex') || 0;[m
[32m+[m[32m    Utils.StoreData($element[0], 'old-tabindex', tabindex);[m
[32m+[m[32m    $element.attr('tabindex', '-1');[m
[32m+[m
[32m+[m[32m    // Set up containers and adapters[m
[32m+[m
[32m+[m[32m    var DataAdapter = this.options.get('dataAdapter');[m
[32m+[m[32m    this.dataAdapter = new DataAdapter($element, this.options);[m
[32m+[m
[32m+[m[32m    var $container = this.render();[m
[32m+[m
[32m+[m[32m    this._placeContainer($container);[m
[32m+[m
[32m+[m[32m    var SelectionAdapter = this.options.get('selectionAdapter');[m
[32m+[m[32m    this.selection = new SelectionAdapter($element, this.options);[m
[32m+[m[32m    this.$selection = this.selection.render();[m
[32m+[m
[32m+[m[32m    this.selection.position(this.$selection, $container);[m
[32m+[m
[32m+[m[32m    var DropdownAdapter = this.options.get('dropdownAdapter');[m
[32m+[m[32m    this.dropdown = new DropdownAdapter($element, this.options);[m
[32m+[m[32m    this.$dropdown = this.dropdown.render();[m
[32m+[m
[32m+[m[32m    this.dropdown.position(this.$dropdown, $container);[m
[32m+[m
[32m+[m[32m    var ResultsAdapter = this.options.get('resultsAdapter');[m
[32m+[m[32m    this.results = new ResultsAdapter($element, this.options, this.dataAdapter);[m
[32m+[m[32m    this.$results = this.results.render();[m
[32m+[m
[32m+[m[32m    this.results.position(this.$results, this.$dropdown);[m
[32m+[m
[32m+[m[32m    // Bind events[m
[32m+[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    // Bind the container to all of the adapters[m
[32m+[m[32m    this._bindAdapters();[m
[32m+[m
[32m+[m[32m    // Register any DOM event handlers[m
[32m+[m[32m    this._registerDomEvents();[m
[32m+[m
[32m+[m[32m    // Register any internal event handlers[m
[32m+[m[32m    this._registerDataEvents();[m
[32m+[m[32m    this._registerSelectionEvents();[m
[32m+[m[32m    this._registerDropdownEvents();[m
[32m+[m[32m    this._registerResultsEvents();[m
[32m+[m[32m    this._registerEvents();[m
[32m+[m
[32m+[m[32m    // Set the initial state[m
[32m+[m[32m    this.dataAdapter.current(function (initialData) {[m
[32m+[m[32m      self.trigger('selection:update', {[m
[32m+[m[32m        data: initialData[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    // Hide the original select[m
[32m+[m[32m    $element.addClass('select2-hidden-accessible');[m
[32m+[m[32m    $element.attr('aria-hidden', 'true');[m
[32m+[m
[32m+[m[32m    // Synchronize any monitored attributes[m
[32m+[m[32m    this._syncAttributes();[m
[32m+[m
[32m+[m[32m    Utils.StoreData($element[0], 'select2', this);[m
[32m+[m
[32m+[m[32m    // Ensure backwards compatibility with $element.data('select2').[m
[32m+[m[32m    $element.data('select2', this);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Utils.Extend(Select2, Utils.Observable);[m
[32m+[m
[32m+[m[32m  Select2.prototype._generateId = function ($element) {[m
[32m+[m[32m    var id = '';[m
[32m+[m
[32m+[m[32m    if ($element.attr('id') != null) {[m
[32m+[m[32m      id = $element.attr('id');[m
[32m+[m[32m    } else if ($element.attr('name') != null) {[m
[32m+[m[32m      id = $element.attr('name') + '-' + Utils.generateChars(2);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      id = Utils.generateChars(4);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    id = id.replace(/(:|\.|\[|\]|,)/g, '');[m
[32m+[m[32m    id = 'select2-' + id;[m
[32m+[m
[32m+[m[32m    return id;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype._placeContainer = function ($container) {[m
[32m+[m[32m    $container.insertAfter(this.$element);[m
[32m+[m
[32m+[m[32m    var width = this._resolveWidth(this.$element, this.options.get('width'));[m
[32m+[m
[32m+[m[32m    if (width != null) {[m
[32m+[m[32m      $container.css('width', width);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype._resolveWidth = function ($element, method) {[m
[32m+[m[32m    var WIDTH = /^width:(([-+]?([0-9]*\.)?[0-9]+)(px|em|ex|%|in|cm|mm|pt|pc))/i;[m
[32m+[m
[32m+[m[32m    if (method == 'resolve') {[m
[32m+[m[32m      var styleWidth = this._resolveWidth($element, 'style');[m
[32m+[m
[32m+[m[32m      if (styleWidth != null) {[m
[32m+[m[32m        return styleWidth;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this._resolveWidth($element, 'element');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (method == 'element') {[m
[32m+[m[32m      var elementWidth = $element.outerWidth(false);[m
[32m+[m
[32m+[m[32m      if (elementWidth <= 0) {[m
[32m+[m[32m        return 'auto';[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return elementWidth + 'px';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (method == 'style') {[m
[32m+[m[32m      var style = $element.attr('style');[m
[32m+[m
[32m+[m[32m      if (typeof(style) !== 'string') {[m
[32m+[m[32m        return null;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var attrs = style.split(';');[m
[32m+[m
[32m+[m[32m      for (var i = 0, l = attrs.length; i < l; i = i + 1) {[m
[32m+[m[32m        var attr = attrs[i].replace(/\s/g, '');[m
[32m+[m[32m        var matches = attr.match(WIDTH);[m
[32m+[m
[32m+[m[32m        if (matches !== null && matches.length >= 1) {[m
[32m+[m[32m          return matches[1];[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (method == 'computedstyle') {[m
[32m+[m[32m      var computedStyle = window.getComputedStyle($element[0]);[m
[32m+[m
[32m+[m[32m      return computedStyle.width;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return method;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype._bindAdapters = function () {[m
[32m+[m[32m    this.dataAdapter.bind(this, this.$container);[m
[32m+[m[32m    this.selection.bind(this, this.$container);[m
[32m+[m
[32m+[m[32m    this.dropdown.bind(this, this.$container);[m
[32m+[m[32m    this.results.bind(this, this.$container);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype._registerDomEvents = function () {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    this.$element.on('change.select2', function () {[m
[32m+[m[32m      self.dataAdapter.current(function (data) {[m
[32m+[m[32m        self.trigger('selection:update', {[m
[32m+[m[32m          data: data[m
[32m+[m[32m        });[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.$element.on('focus.select2', function (evt) {[m
[32m+[m[32m      self.trigger('focus', evt);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this._syncA = Utils.bind(this._syncAttributes, this);[m
[32m+[m[32m    this._syncS = Utils.bind(this._syncSubtree, this);[m
[32m+[m
[32m+[m[32m    if (this.$element[0].attachEvent) {[m
[32m+[m[32m      this.$element[0].attachEvent('onpropertychange', this._syncA);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var observer = window.MutationObserver ||[m
[32m+[m[32m      window.WebKitMutationObserver ||[m
[32m+[m[32m      window.MozMutationObserver[m
[32m+[m[32m    ;[m
[32m+[m
[32m+[m[32m    if (observer != null) {[m
[32m+[m[32m      this._observer = new observer(function (mutations) {[m
[32m+[m[32m        self._syncA();[m
[32m+[m[32m        self._syncS(null, mutations);[m
[32m+[m[32m      });[m
[32m+[m[32m      this._observer.observe(this.$element[0], {[m
[32m+[m[32m        attributes: true,[m
[32m+[m[32m        childList: true,[m
[32m+[m[32m        subtree: false[m
[32m+[m[32m      });[m
[32m+[m[32m    } else if (this.$element[0].addEventListener) {[m
[32m+[m[32m      this.$element[0].addEventListener([m
[32m+[m[32m        'DOMAttrModified',[m
[32m+[m[32m        self._syncA,[m
[32m+[m[32m        false[m
[32m+[m[32m      );[m
[32m+[m[32m      this.$element[0].addEventListener([m
[32m+[m[32m        'DOMNodeInserted',[m
[32m+[m[32m        self._syncS,[m
[32m+[m[32m        false[m
[32m+[m[32m      );[m
[32m+[m[32m      this.$element[0].addEventListener([m
[32m+[m[32m        'DOMNodeRemoved',[m
[32m+[m[32m        self._syncS,[m
[32m+[m[32m        false[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype._registerDataEvents = function () {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    this.dataAdapter.on('*', function (name, params) {[m
[32m+[m[32m      self.trigger(name, params);[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype._registerSelectionEvents = function () {[m
[32m+[m[32m    var self = this;[m
[32m+[m[32m    var nonRelayEvents = ['toggle', 'focus'];[m
[32m+[m
[32m+[m[32m    this.selection.on('toggle', function () {[m
[32m+[m[32m      self.toggleDropdown();[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.selection.on('focus', function (params) {[m
[32m+[m[32m      self.focus(params);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.selection.on('*', function (name, params) {[m
[32m+[m[32m      if ($.inArray(name, nonRelayEvents) !== -1) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      self.trigger(name, params);[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype._registerDropdownEvents = function () {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    this.dropdown.on('*', function (name, params) {[m
[32m+[m[32m      self.trigger(name, params);[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype._registerResultsEvents = function () {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    this.results.on('*', function (name, params) {[m
[32m+[m[32m      self.trigger(name, params);[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype._registerEvents = function () {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    this.on('open', function () {[m
[32m+[m[32m      self.$container.addClass('select2-container--open');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.on('close', function () {[m
[32m+[m[32m      self.$container.removeClass('select2-container--open');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.on('enable', function () {[m
[32m+[m[32m      self.$container.removeClass('select2-container--disabled');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.on('disable', function () {[m
[32m+[m[32m      self.$container.addClass('select2-container--disabled');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.on('blur', function () {[m
[32m+[m[32m      self.$container.removeClass('select2-container--focus');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.on('query', function (params) {[m
[32m+[m[32m      if (!self.isOpen()) {[m
[32m+[m[32m        self.trigger('open', {});[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.dataAdapter.query(params, function (data) {[m
[32m+[m[32m        self.trigger('results:all', {[m
[32m+[m[32m          data: data,[m
[32m+[m[32m          query: params[m
[32m+[m[32m        });[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.on('query:append', function (params) {[m
[32m+[m[32m      this.dataAdapter.query(params, function (data) {[m
[32m+[m[32m        self.trigger('results:append', {[m
[32m+[m[32m          data: data,[m
[32m+[m[32m          query: params[m
[32m+[m[32m        });[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.on('keypress', function (evt) {[m
[32m+[m[32m      var key = evt.which;[m
[32m+[m
[32m+[m[32m      if (self.isOpen()) {[m
[32m+[m[32m        if (key === KEYS.ESC || key === KEYS.TAB ||[m
[32m+[m[32m            (key === KEYS.UP && evt.altKey)) {[m
[32m+[m[32m          self.close(evt);[m
[32m+[m
[32m+[m[32m          evt.preventDefault();[m
[32m+[m[32m        } else if (key === KEYS.ENTER) {[m
[32m+[m[32m          self.trigger('results:select', {});[m
[32m+[m
[32m+[m[32m          evt.preventDefault();[m
[32m+[m[32m        } else if ((key === KEYS.SPACE && evt.ctrlKey)) {[m
[32m+[m[32m          self.trigger('results:toggle', {});[m
[32m+[m
[32m+[m[32m          evt.preventDefault();[m
[32m+[m[32m        } else if (key === KEYS.UP) {[m
[32m+[m[32m          self.trigger('results:previous', {});[m
[32m+[m
[32m+[m[32m          evt.preventDefault();[m
[32m+[m[32m        } else if (key === KEYS.DOWN) {[m
[32m+[m[32m          self.trigger('results:next', {});[m
[32m+[m
[32m+[m[32m          evt.preventDefault();[m
[32m+[m[32m        }[m
[32m+[m[32m      } else {[m
[32m+[m[32m        if (key === KEYS.ENTER || key === KEYS.SPACE ||[m
[32m+[m[32m            (key === KEYS.DOWN && evt.altKey)) {[m
[32m+[m[32m          self.open();[m
[32m+[m
[32m+[m[32m          evt.preventDefault();[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype._syncAttributes = function () {[m
[32m+[m[32m    this.options.set('disabled', this.$element.prop('disabled'));[m
[32m+[m
[32m+[m[32m    if (this.isDisabled()) {[m
[32m+[m[32m      if (this.isOpen()) {[m
[32m+[m[32m        this.close();[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.trigger('disable', {});[m
[32m+[m[32m    } else {[m
[32m+[m[32m      this.trigger('enable', {});[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype._isChangeMutation = function (evt, mutations) {[m
[32m+[m[32m    var changed = false;[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    // Ignore any mutation events raised for elements that aren't options or[m
[32m+[m[32m    // optgroups. This handles the case when the select element is destroyed[m
[32m+[m[32m    if ([m
[32m+[m[32m      evt && evt.target && ([m
[32m+[m[32m        evt.target.nodeName !== 'OPTION' && evt.target.nodeName !== 'OPTGROUP'[m
[32m+[m[32m      )[m
[32m+[m[32m    ) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!mutations) {[m
[32m+[m[32m      // If mutation events aren't supported, then we can only assume that the[m
[32m+[m[32m      // change affected the selections[m
[32m+[m[32m      changed = true;[m
[32m+[m[32m    } else if (mutations.addedNodes && mutations.addedNodes.length > 0) {[m
[32m+[m[32m      for (var n = 0; n < mutations.addedNodes.length; n++) {[m
[32m+[m[32m        var node = mutations.addedNodes[n];[m
[32m+[m
[32m+[m[32m        if (node.selected) {[m
[32m+[m[32m          changed = true;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    } else if (mutations.removedNodes && mutations.removedNodes.length > 0) {[m
[32m+[m[32m      changed = true;[m
[32m+[m[32m    } else if ($.isArray(mutations)) {[m
[32m+[m[32m      $.each(mutations, function(evt, mutation) {[m
[32m+[m[32m        if (self._isChangeMutation(evt, mutation)) {[m
[32m+[m[32m          // We've found a change mutation.[m
[32m+[m[32m          // Let's escape from the loop and continue[m
[32m+[m[32m          changed = true;[m
[32m+[m[32m          return false;[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m    return changed;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype._syncSubtree = function (evt, mutations) {[m
[32m+[m[32m    var changed = this._isChangeMutation(evt, mutations);[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    // Only re-pull the data if we think there is a change[m
[32m+[m[32m    if (changed) {[m
[32m+[m[32m      this.dataAdapter.current(function (currentData) {[m
[32m+[m[32m        self.trigger('selection:update', {[m
[32m+[m[32m          data: currentData[m
[32m+[m[32m        });[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Override the trigger method to automatically trigger pre-events when[m
[32m+[m[32m   * there are events that can be prevented.[m
[32m+[m[32m   */[m
[32m+[m[32m  Select2.prototype.trigger = function (name, args) {[m
[32m+[m[32m    var actualTrigger = Select2.__super__.trigger;[m
[32m+[m[32m    var preTriggerMap = {[m
[32m+[m[32m      'open': 'opening',[m
[32m+[m[32m      'close': 'closing',[m
[32m+[m[32m      'select': 'selecting',[m
[32m+[m[32m      'unselect': 'unselecting',[m
[32m+[m[32m      'clear': 'clearing'[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    if (args === undefined) {[m
[32m+[m[32m      args = {};[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (name in preTriggerMap) {[m
[32m+[m[32m      var preTriggerName = preTriggerMap[name];[m
[32m+[m[32m      var preTriggerArgs = {[m
[32m+[m[32m        prevented: false,[m
[32m+[m[32m        name: name,[m
[32m+[m[32m        args: args[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      actualTrigger.call(this, preTriggerName, preTriggerArgs);[m
[32m+[m
[32m+[m[32m      if (preTriggerArgs.prevented) {[m
[32m+[m[32m        args.prevented = true;[m
[32m+[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    actualTrigger.call(this, name, args);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype.toggleDropdown = function () {[m
[32m+[m[32m    if (this.isDisabled()) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (this.isOpen()) {[m
[32m+[m[32m      this.close();[m
[32m+[m[32m    } else {[m
[32m+[m[32m      this.open();[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype.open = function () {[m
[32m+[m[32m    if (this.isOpen()) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (this.isDisabled()) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.trigger('query', {});[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype.close = function (evt) {[m
[32m+[m[32m    if (!this.isOpen()) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.trigger('close', { originalEvent : evt });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Helper method to abstract the "enabled" (not "disabled") state of this[m
[32m+[m[32m   * object.[m
[32m+[m[32m   *[m
[32m+[m[32m   * @return {true} if the instance is not disabled.[m
[32m+[m[32m   * @return {false} if the instance is disabled.[m
[32m+[m[32m   */[m
[32m+[m[32m  Select2.prototype.isEnabled = function () {[m
[32m+[m[32m    return !this.isDisabled();[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Helper method to abstract the "disabled" state of this object.[m
[32m+[m[32m   *[m
[32m+[m[32m   * @return {true} if the disabled option is true.[m
[32m+[m[32m   * @return {false} if the disabled option is false.[m
[32m+[m[32m   */[m
[32m+[m[32m  Select2.prototype.isDisabled = function () {[m
[32m+[m[32m    return this.options.get('disabled');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype.isOpen = function () {[m
[32m+[m[32m    return this.$container.hasClass('select2-container--open');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype.hasFocus = function () {[m
[32m+[m[32m    return this.$container.hasClass('select2-container--focus');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype.focus = function (data) {[m
[32m+[m[32m    // No need to re-trigger focus events if we are already focused[m
[32m+[m[32m    if (this.hasFocus()) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.$container.addClass('select2-container--focus');[m
[32m+[m[32m    this.trigger('focus', {});[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype.enable = function (args) {[m
[32m+[m[32m    if (this.options.get('debug') && window.console && console.warn) {[m
[32m+[m[32m      console.warn([m
[32m+[m[32m        'Select2: The `select2("enable")` method has been deprecated and will' +[m
[32m+[m[32m        ' be removed in later Select2 versions. Use $element.prop("disabled")' +[m
[32m+[m[32m        ' instead.'[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (args == null || args.length === 0) {[m
[32m+[m[32m      args = [true];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var disabled = !args[0];[m
[32m+[m
[32m+[m[32m    this.$element.prop('disabled', disabled);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype.data = function () {[m
[32m+[m[32m    if (this.options.get('debug') &&[m
[32m+[m[32m        arguments.length > 0 && window.console && console.warn) {[m
[32m+[m[32m      console.warn([m
[32m+[m[32m        'Select2: Data can no longer be set using `select2("data")`. You ' +[m
[32m+[m[32m        'should consider setting the value instead using `$element.val()`.'[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var data = [];[m
[32m+[m
[32m+[m[32m    this.dataAdapter.current(function (currentData) {[m
[32m+[m[32m      data = currentData;[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    return data;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype.val = function (args) {[m
[32m+[m[32m    if (this.options.get('debug') && window.console && console.warn) {[m
[32m+[m[32m      console.warn([m
[32m+[m[32m        'Select2: The `select2("val")` method has been deprecated and will be' +[m
[32m+[m[32m        ' removed in later Select2 versions. Use $element.val() instead.'[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (args == null || args.length === 0) {[m
[32m+[m[32m      return this.$element.val();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var newVal = args[0];[m
[32m+[m
[32m+[m[32m    if ($.isArray(newVal)) {[m
[32m+[m[32m      newVal = $.map(newVal, function (obj) {[m
[32m+[m[32m        return obj.toString();[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.$element.val(newVal).trigger('input').trigger('change');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype.destroy = function () {[m
[32m+[m[32m    this.$container.remove();[m
[32m+[m
[32m+[m[32m    if (this.$element[0].detachEvent) {[m
[32m+[m[32m      this.$element[0].detachEvent('onpropertychange', this._syncA);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (this._observer != null) {[m
[32m+[m[32m      this._observer.disconnect();[m
[32m+[m[32m      this._observer = null;[m
[32m+[m[32m    } else if (this.$element[0].removeEventListener) {[m
[32m+[m[32m      this.$element[0][m
[32m+[m[32m        .removeEventListener('DOMAttrModified', this._syncA, false);[m
[32m+[m[32m      this.$element[0][m
[32m+[m[32m        .removeEventListener('DOMNodeInserted', this._syncS, false);[m
[32m+[m[32m      this.$element[0][m
[32m+[m[32m        .removeEventListener('DOMNodeRemoved', this._syncS, false);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this._syncA = null;[m
[32m+[m[32m    this._syncS = null;[m
[32m+[m
[32m+[m[32m    this.$element.off('.select2');[m
[32m+[m[32m    this.$element.attr('tabindex',[m
[32m+[m[32m    Utils.GetData(this.$element[0], 'old-tabindex'));[m
[32m+[m
[32m+[m[32m    this.$element.removeClass('select2-hidden-accessible');[m
[32m+[m[32m    this.$element.attr('aria-hidden', 'false');[m
[32m+[m[32m    Utils.RemoveData(this.$element[0]);[m
[32m+[m[32m    this.$element.removeData('select2');[m
[32m+[m
[32m+[m[32m    this.dataAdapter.destroy();[m
[32m+[m[32m    this.selection.destroy();[m
[32m+[m[32m    this.dropdown.destroy();[m
[32m+[m[32m    this.results.destroy();[m
[32m+[m
[32m+[m[32m    this.dataAdapter = null;[m
[32m+[m[32m    this.selection = null;[m
[32m+[m[32m    this.dropdown = null;[m
[32m+[m[32m    this.results = null;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Select2.prototype.render = function () {[m
[32m+[m[32m    var $container = $([m
[32m+[m[32m      '<span class="select2 select2-container">' +[m
[32m+[m[32m        '<span class="selection"></span>' +[m
[32m+[m[32m        '<span class="dropdown-wrapper" aria-hidden="true"></span>' +[m
[32m+[m[32m      '</span>'[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    $container.attr('dir', this.options.get('dir'));[m
[32m+[m
[32m+[m[32m    this.$container = $container;[m
[32m+[m
[32m+[m[32m    this.$container.addClass('select2-container--' + this.options.get('theme'));[m
[32m+[m
[32m+[m[32m    Utils.StoreData($container[0], 'element', this.$element);[m
[32m+[m
[32m+[m[32m    return $container;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return Select2;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/compat/utils',[[m
[32m+[m[32m  'jquery'[m
[32m+[m[32m], function ($) {[m
[32m+[m[32m  function syncCssClasses ($dest, $src, adapter) {[m
[32m+[m[32m    var classes, replacements = [], adapted;[m
[32m+[m
[32m+[m[32m    classes = $.trim($dest.attr('class'));[m
[32m+[m
[32m+[m[32m    if (classes) {[m
[32m+[m[32m      classes = '' + classes; // for IE which returns object[m
[32m+[m
[32m+[m[32m      $(classes.split(/\s+/)).each(function () {[m
[32m+[m[32m        // Save all Select2 classes[m
[32m+[m[32m        if (this.indexOf('select2-') === 0) {[m
[32m+[m[32m          replacements.push(this);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    classes = $.trim($src.attr('class'));[m
[32m+[m
[32m+[m[32m    if (classes) {[m
[32m+[m[32m      classes = '' + classes; // for IE which returns object[m
[32m+[m
[32m+[m[32m      $(classes.split(/\s+/)).each(function () {[m
[32m+[m[32m        // Only adapt non-Select2 classes[m
[32m+[m[32m        if (this.indexOf('select2-') !== 0) {[m
[32m+[m[32m          adapted = adapter(this);[m
[32m+[m
[32m+[m[32m          if (adapted != null) {[m
[32m+[m[32m            replacements.push(adapted);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $dest.attr('class', replacements.join(' '));[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    syncCssClasses: syncCssClasses[m
[32m+[m[32m  };[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/compat/containerCss',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  './utils'[m
[32m+[m[32m], function ($, CompatUtils) {[m
[32m+[m[32m  // No-op CSS adapter that discards all classes by default[m
[32m+[m[32m  function _containerAdapter (clazz) {[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function ContainerCSS () { }[m
[32m+[m
[32m+[m[32m  ContainerCSS.prototype.render = function (decorated) {[m
[32m+[m[32m    var $container = decorated.call(this);[m
[32m+[m
[32m+[m[32m    var containerCssClass = this.options.get('containerCssClass') || '';[m
[32m+[m
[32m+[m[32m    if ($.isFunction(containerCssClass)) {[m
[32m+[m[32m      containerCssClass = containerCssClass(this.$element);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var containerCssAdapter = this.options.get('adaptContainerCssClass');[m
[32m+[m[32m    containerCssAdapter = containerCssAdapter || _containerAdapter;[m
[32m+[m
[32m+[m[32m    if (containerCssClass.indexOf(':all:') !== -1) {[m
[32m+[m[32m      containerCssClass = containerCssClass.replace(':all:', '');[m
[32m+[m
[32m+[m[32m      var _cssAdapter = containerCssAdapter;[m
[32m+[m
[32m+[m[32m      containerCssAdapter = function (clazz) {[m
[32m+[m[32m        var adapted = _cssAdapter(clazz);[m
[32m+[m
[32m+[m[32m        if (adapted != null) {[m
[32m+[m[32m          // Append the old one along with the adapted one[m
[32m+[m[32m          return adapted + ' ' + clazz;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return clazz;[m
[32m+[m[32m      };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var containerCss = this.options.get('containerCss') || {};[m
[32m+[m
[32m+[m[32m    if ($.isFunction(containerCss)) {[m
[32m+[m[32m      containerCss = containerCss(this.$element);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    CompatUtils.syncCssClasses($container, this.$element, containerCssAdapter);[m
[32m+[m
[32m+[m[32m    $container.css(containerCss);[m
[32m+[m[32m    $container.addClass(containerCssClass);[m
[32m+[m
[32m+[m[32m    return $container;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return ContainerCSS;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/compat/dropdownCss',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  './utils'[m
[32m+[m[32m], function ($, CompatUtils) {[m
[32m+[m[32m  // No-op CSS adapter that discards all classes by default[m
[32m+[m[32m  function _dropdownAdapter (clazz) {[m
[32m+[m[32m    return null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function DropdownCSS () { }[m
[32m+[m
[32m+[m[32m  DropdownCSS.prototype.render = function (decorated) {[m
[32m+[m[32m    var $dropdown = decorated.call(this);[m
[32m+[m
[32m+[m[32m    var dropdownCssClass = this.options.get('dropdownCssClass') || '';[m
[32m+[m
[32m+[m[32m    if ($.isFunction(dropdownCssClass)) {[m
[32m+[m[32m      dropdownCssClass = dropdownCssClass(this.$element);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var dropdownCssAdapter = this.options.get('adaptDropdownCssClass');[m
[32m+[m[32m    dropdownCssAdapter = dropdownCssAdapter || _dropdownAdapter;[m
[32m+[m
[32m+[m[32m    if (dropdownCssClass.indexOf(':all:') !== -1) {[m
[32m+[m[32m      dropdownCssClass = dropdownCssClass.replace(':all:', '');[m
[32m+[m
[32m+[m[32m      var _cssAdapter = dropdownCssAdapter;[m
[32m+[m
[32m+[m[32m      dropdownCssAdapter = function (clazz) {[m
[32m+[m[32m        var adapted = _cssAdapter(clazz);[m
[32m+[m
[32m+[m[32m        if (adapted != null) {[m
[32m+[m[32m          // Append the old one along with the adapted one[m
[32m+[m[32m          return adapted + ' ' + clazz;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return clazz;[m
[32m+[m[32m      };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var dropdownCss = this.options.get('dropdownCss') || {};[m
[32m+[m
[32m+[m[32m    if ($.isFunction(dropdownCss)) {[m
[32m+[m[32m      dropdownCss = dropdownCss(this.$element);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    CompatUtils.syncCssClasses($dropdown, this.$element, dropdownCssAdapter);[m
[32m+[m
[32m+[m[32m    $dropdown.css(dropdownCss);[m
[32m+[m[32m    $dropdown.addClass(dropdownCssClass);[m
[32m+[m
[32m+[m[32m    return $dropdown;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return DropdownCSS;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/compat/initSelection',[[m
[32m+[m[32m  'jquery'[m
[32m+[m[32m], function ($) {[m
[32m+[m[32m  function InitSelection (decorated, $element, options) {[m
[32m+[m[32m    if (options.get('debug') && window.console && console.warn) {[m
[32m+[m[32m      console.warn([m
[32m+[m[32m        'Select2: The `initSelection` option has been deprecated in favor' +[m
[32m+[m[32m        ' of a custom data adapter that overrides the `current` method. ' +[m
[32m+[m[32m        'This method is now called multiple times instead of a single ' +[m
[32m+[m[32m        'time when the instance is initialized. Support will be removed ' +[m
[32m+[m[32m        'for the `initSelection` option in future versions of Select2'[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.initSelection = options.get('initSelection');[m
[32m+[m[32m    this._isInitialized = false;[m
[32m+[m
[32m+[m[32m    decorated.call(this, $element, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  InitSelection.prototype.current = function (decorated, callback) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    if (this._isInitialized) {[m
[32m+[m[32m      decorated.call(this, callback);[m
[32m+[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.initSelection.call(null, this.$element, function (data) {[m
[32m+[m[32m      self._isInitialized = true;[m
[32m+[m
[32m+[m[32m      if (!$.isArray(data)) {[m
[32m+[m[32m        data = [data];[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      callback(data);[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return InitSelection;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/compat/inputData',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  '../utils'[m
[32m+[m[32m], function ($, Utils) {[m
[32m+[m[32m  function InputData (decorated, $element, options) {[m
[32m+[m[32m    this._currentData = [];[m
[32m+[m[32m    this._valueSeparator = options.get('valueSeparator') || ',';[m
[32m+[m
[32m+[m[32m    if ($element.prop('type') === 'hidden') {[m
[32m+[m[32m      if (options.get('debug') && console && console.warn) {[m
[32m+[m[32m        console.warn([m
[32m+[m[32m          'Select2: Using a hidden input with Select2 is no longer ' +[m
[32m+[m[32m          'supported and may stop working in the future. It is recommended ' +[m
[32m+[m[32m          'to use a `<select>` element instead.'[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    decorated.call(this, $element, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  InputData.prototype.current = function (_, callback) {[m
[32m+[m[32m    function getSelected (data, selectedIds) {[m
[32m+[m[32m      var selected = [];[m
[32m+[m
[32m+[m[32m      if (data.selected || $.inArray(data.id, selectedIds) !== -1) {[m
[32m+[m[32m        data.selected = true;[m
[32m+[m[32m        selected.push(data);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        data.selected = false;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (data.children) {[m
[32m+[m[32m        selected.push.apply(selected, getSelected(data.children, selectedIds));[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return selected;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var selected = [];[m
[32m+[m
[32m+[m[32m    for (var d = 0; d < this._currentData.length; d++) {[m
[32m+[m[32m      var data = this._currentData[d];[m
[32m+[m
[32m+[m[32m      selected.push.apply([m
[32m+[m[32m        selected,[m
[32m+[m[32m        getSelected([m
[32m+[m[32m          data,[m
[32m+[m[32m          this.$element.val().split([m
[32m+[m[32m            this._valueSeparator[m
[32m+[m[32m          )[m
[32m+[m[32m        )[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    callback(selected);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  InputData.prototype.select = function (_, data) {[m
[32m+[m[32m    if (!this.options.get('multiple')) {[m
[32m+[m[32m      this.current(function (allData) {[m
[32m+[m[32m        $.map(allData, function (data) {[m
[32m+[m[32m          data.selected = false;[m
[32m+[m[32m        });[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      this.$element.val(data.id);[m
[32m+[m[32m      this.$element.trigger('input').trigger('change');[m
[32m+[m[32m    } else {[m
[32m+[m[32m      var value = this.$element.val();[m
[32m+[m[32m      value += this._valueSeparator + data.id;[m
[32m+[m
[32m+[m[32m      this.$element.val(value);[m
[32m+[m[32m      this.$element.trigger('input').trigger('change');[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  InputData.prototype.unselect = function (_, data) {[m
[32m+[m[32m    var self = this;[m
[32m+[m
[32m+[m[32m    data.selected = false;[m
[32m+[m
[32m+[m[32m    this.current(function (allData) {[m
[32m+[m[32m      var values = [];[m
[32m+[m
[32m+[m[32m      for (var d = 0; d < allData.length; d++) {[m
[32m+[m[32m        var item = allData[d];[m
[32m+[m
[32m+[m[32m        if (data.id == item.id) {[m
[32m+[m[32m          continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        values.push(item.id);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      self.$element.val(values.join(self._valueSeparator));[m
[32m+[m[32m      self.$element.trigger('input').trigger('change');[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  InputData.prototype.query = function (_, params, callback) {[m
[32m+[m[32m    var results = [];[m
[32m+[m
[32m+[m[32m    for (var d = 0; d < this._currentData.length; d++) {[m
[32m+[m[32m      var data = this._currentData[d];[m
[32m+[m
[32m+[m[32m      var matches = this.matches(params, data);[m
[32m+[m
[32m+[m[32m      if (matches !== null) {[m
[32m+[m[32m        results.push(matches);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    callback({[m
[32m+[m[32m      results: results[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  InputData.prototype.addOptions = function (_, $options) {[m
[32m+[m[32m    var options = $.map($options, function ($option) {[m
[32m+[m[32m      return Utils.GetData($option[0], 'data');[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this._currentData.push.apply(this._currentData, options);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return InputData;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/compat/matcher',[[m
[32m+[m[32m  'jquery'[m
[32m+[m[32m], function ($) {[m
[32m+[m[32m  function oldMatcher (matcher) {[m
[32m+[m[32m    function wrappedMatcher (params, data) {[m
[32m+[m[32m      var match = $.extend(true, {}, data);[m
[32m+[m
[32m+[m[32m      if (params.term == null || $.trim(params.term) === '') {[m
[32m+[m[32m        return match;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (data.children) {[m
[32m+[m[32m        for (var c = data.children.length - 1; c >= 0; c--) {[m
[32m+[m[32m          var child = data.children[c];[m
[32m+[m
[32m+[m[32m          // Check if the child object matches[m
[32m+[m[32m          // The old matcher returned a boolean true or false[m
[32m+[m[32m          var doesMatch = matcher(params.term, child.text, child);[m
[32m+[m
[32m+[m[32m          // If the child didn't match, pop it off[m
[32m+[m[32m          if (!doesMatch) {[m
[32m+[m[32m            match.children.splice(c, 1);[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (match.children.length > 0) {[m
[32m+[m[32m          return match;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (matcher(params.term, data.text, data)) {[m
[32m+[m[32m        return match;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return wrappedMatcher;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return oldMatcher;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/compat/query',[[m
[32m+[m
[32m+[m[32m], function () {[m
[32m+[m[32m  function Query (decorated, $element, options) {[m
[32m+[m[32m    if (options.get('debug') && window.console && console.warn) {[m
[32m+[m[32m      console.warn([m
[32m+[m[32m        'Select2: The `query` option has been deprecated in favor of a ' +[m
[32m+[m[32m        'custom data adapter that overrides the `query` method. Support ' +[m
[32m+[m[32m        'will be removed for the `query` option in future versions of ' +[m
[32m+[m[32m        'Select2.'[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    decorated.call(this, $element, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Query.prototype.query = function (_, params, callback) {[m
[32m+[m[32m    params.callback = callback;[m
[32m+[m
[32m+[m[32m    var query = this.options.get('query');[m
[32m+[m
[32m+[m[32m    query.call(null, params);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return Query;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/dropdown/attachContainer',[[m
[32m+[m
[32m+[m[32m], function () {[m
[32m+[m[32m  function AttachContainer (decorated, $element, options) {[m
[32m+[m[32m    decorated.call(this, $element, options);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  AttachContainer.prototype.position =[m
[32m+[m[32m    function (decorated, $dropdown, $container) {[m
[32m+[m[32m    var $dropdownContainer = $container.find('.dropdown-wrapper');[m
[32m+[m[32m    $dropdownContainer.append($dropdown);[m
[32m+[m
[32m+[m[32m    $dropdown.addClass('select2-dropdown--below');[m
[32m+[m[32m    $container.addClass('select2-container--below');[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return AttachContainer;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/dropdown/stopPropagation',[[m
[32m+[m
[32m+[m[32m], function () {[m
[32m+[m[32m  function StopPropagation () { }[m
[32m+[m
[32m+[m[32m  StopPropagation.prototype.bind = function (decorated, container, $container) {[m
[32m+[m[32m    decorated.call(this, container, $container);[m
[32m+[m
[32m+[m[32m    var stoppedEvents = [[m
[32m+[m[32m    'blur',[m
[32m+[m[32m    'change',[m
[32m+[m[32m    'click',[m
[32m+[m[32m    'dblclick',[m
[32m+[m[32m    'focus',[m
[32m+[m[32m    'focusin',[m
[32m+[m[32m    'focusout',[m
[32m+[m[32m    'input',[m
[32m+[m[32m    'keydown',[m
[32m+[m[32m    'keyup',[m
[32m+[m[32m    'keypress',[m
[32m+[m[32m    'mousedown',[m
[32m+[m[32m    'mouseenter',[m
[32m+[m[32m    'mouseleave',[m
[32m+[m[32m    'mousemove',[m
[32m+[m[32m    'mouseover',[m
[32m+[m[32m    'mouseup',[m
[32m+[m[32m    'search',[m
[32m+[m[32m    'touchend',[m
[32m+[m[32m    'touchstart'[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    this.$dropdown.on(stoppedEvents.join(' '), function (evt) {[m
[32m+[m[32m      evt.stopPropagation();[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return StopPropagation;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mS2.define('select2/selection/stopPropagation',[[m
[32m+[m
[32m+[m[32m], function () {[m
[32m+[m[32m  function StopPropagation () { }[m
[32m+[m
[32m+[m[32m  StopPropagation.prototype.bind = function (decorated, container, $container) {[m
[32m+[m[32m    decorated.call(this, container, $container);[m
[32m+[m
[32m+[m[32m    var stoppedEvents = [[m
[32m+[m[32m      'blur',[m
[32m+[m[32m      'change',[m
[32m+[m[32m      'click',[m
[32m+[m[32m      'dblclick',[m
[32m+[m[32m      'focus',[m
[32m+[m[32m      'focusin',[m
[32m+[m[32m      'focusout',[m
[32m+[m[32m      'input',[m
[32m+[m[32m      'keydown',[m
[32m+[m[32m      'keyup',[m
[32m+[m[32m      'keypress',[m
[32m+[m[32m      'mousedown',[m
[32m+[m[32m      'mouseenter',[m
[32m+[m[32m      'mouseleave',[m
[32m+[m[32m      'mousemove',[m
[32m+[m[32m      'mouseover',[m
[32m+[m[32m      'mouseup',[m
[32m+[m[32m      'search',[m
[32m+[m[32m      'touchend',[m
[32m+[m[32m      'touchstart'[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    this.$selection.on(stoppedEvents.join(' '), function (evt) {[m
[32m+[m[32m      evt.stopPropagation();[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return StopPropagation;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/*![m
[32m+[m[32m * jQuery Mousewheel 3.1.13[m
[32m+[m[32m *[m
[32m+[m[32m * Copyright jQuery Foundation and other contributors[m
[32m+[m[32m * Released under the MIT license[m
[32m+[m[32m * http://jquery.org/license[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m(function (factory) {[m
[32m+[m[32m    if ( typeof S2.define === 'function' && S2.define.amd ) {[m
[32m+[m[32m        // AMD. Register as an anonymous module.[m
[32m+[m[32m        S2.define('jquery-mousewheel',['jquery'], factory);[m
[32m+[m[32m    } else if (typeof exports === 'object') {[m
[32m+[m[32m        // Node/CommonJS style for Browserify[m
[32m+[m[32m        module.exports = factory;[m
[32m+[m[32m    } else {[m
[32m+[m[32m        // Browser globals[m
[32m+[m[32m        factory(jQuery);[m
[32m+[m[32m    }[m
[32m+[m[32m}(function ($) {[m
[32m+[m
[32m+[m[32m    var toFix  = ['wheel', 'mousewheel', 'DOMMouseScroll', 'MozMousePixelScroll'],[m
[32m+[m[32m        toBind = ( 'onwheel' in document || document.documentMode >= 9 ) ?[m
[32m+[m[32m                    ['wheel'] : ['mousewheel', 'DomMouseScroll', 'MozMousePixelScroll'],[m
[32m+[m[32m        slice  = Array.prototype.slice,[m
[32m+[m[32m        nullLowestDeltaTimeout, lowestDelta;[m
[32m+[m
[32m+[m[32m    if ( $.event.fixHooks ) {[m
[32m+[m[32m        for ( var i = toFix.length; i; ) {[m
[32m+[m[32m            $.event.fixHooks[ toFix[--i] ] = $.event.mouseHooks;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var special = $.event.special.mousewheel = {[m
[32m+[m[32m        version: '3.1.12',[m
[32m+[m
[32m+[m[32m        setup: function() {[m
[32m+[m[32m            if ( this.addEventListener ) {[m
[32m+[m[32m                for ( var i = toBind.length; i; ) {[m
[32m+[m[32m                    this.addEventListener( toBind[--i], handler, false );[m
[32m+[m[32m                }[m
[32m+[m[32m            } else {[m
[32m+[m[32m                this.onmousewheel = handler;[m
[32m+[m[32m            }[m
[32m+[m[32m            // Store the line height and page height for this particular element[m
[32m+[m[32m            $.data(this, 'mousewheel-line-height', special.getLineHeight(this));[m
[32m+[m[32m            $.data(this, 'mousewheel-page-height', special.getPageHeight(this));[m
[32m+[m[32m        },[m
[32m+[m
[32m+[m[32m        teardown: function() {[m
[32m+[m[32m            if ( this.removeEventListener ) {[m
[32m+[m[32m                for ( var i = toBind.length; i; ) {[m
[32m+[m[32m                    this.removeEventListener( toBind[--i], handler, false );[m
[32m+[m[32m                }[m
[32m+[m[32m            } else {[m
[32m+[m[32m                this.onmousewheel = null;[m
[32m+[m[32m            }[m
[32m+[m[32m            // Clean up the data we added to the element[m
[32m+[m[32m            $.removeData(this, 'mousewheel-line-height');[m
[32m+[m[32m            $.removeData(this, 'mousewheel-page-height');[m
[32m+[m[32m        },[m
[32m+[m
[32m+[m[32m        getLineHeight: function(elem) {[m
[32m+[m[32m            var $elem = $(elem),[m
[32m+[m[32m                $parent = $elem['offsetParent' in $.fn ? 'offsetParent' : 'parent']();[m
[32m+[m[32m            if (!$parent.length) {[m
[32m+[m[32m                $parent = $('body');[m
[32m+[m[32m            }[m
[32m+[m[32m            return parseInt($parent.css('fontSize'), 10) || parseInt($elem.css('fontSize'), 10) || 16;[m
[32m+[m[32m        },[m
[32m+[m
[32m+[m[32m        getPageHeight: function(elem) {[m
[32m+[m[32m            return $(elem).height();[m
[32m+[m[32m        },[m
[32m+[m
[32m+[m[32m        settings: {[m
[32m+[m[32m            adjustOldDeltas: true, // see shouldAdjustOldDeltas() below[m
[32m+[m[32m            normalizeOffset: true  // calls getBoundingClientRect for each event[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    $.fn.extend({[m
[32m+[m[32m        mousewheel: function(fn) {[m
[32m+[m[32m            return fn ? this.bind('mousewheel', fn) : this.trigger('mousewheel');[m
[32m+[m[32m        },[m
[32m+[m
[32m+[m[32m        unmousewheel: function(fn) {[m
[32m+[m[32m            return this.unbind('mousewheel', fn);[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m
[32m+[m[32m    function handler(event) {[m
[32m+[m[32m        var orgEvent   = event || window.event,[m
[32m+[m[32m            args       = slice.call(arguments, 1),[m
[32m+[m[32m            delta      = 0,[m
[32m+[m[32m            deltaX     = 0,[m
[32m+[m[32m            deltaY     = 0,[m
[32m+[m[32m            absDelta   = 0,[m
[32m+[m[32m            offsetX    = 0,[m
[32m+[m[32m            offsetY    = 0;[m
[32m+[m[32m        event = $.event.fix(orgEvent);[m
[32m+[m[32m        event.type = 'mousewheel';[m
[32m+[m
[32m+[m[32m        // Old school scrollwheel delta[m
[32m+[m[32m        if ( 'detail'      in orgEvent ) { deltaY = orgEvent.detail * -1;      }[m
[32m+[m[32m        if ( 'wheelDelta'  in orgEvent ) { deltaY = orgEvent.wheelDelta;       }[m
[32m+[m[32m        if ( 'wheelDeltaY' in orgEvent ) { deltaY = orgEvent.wheelDeltaY;      }[m
[32m+[m[32m        if ( 'wheelDeltaX' in orgEvent ) { deltaX = orgEvent.wheelDeltaX * -1; }[m
[32m+[m
[32m+[m[32m        // Firefox < 17 horizontal scrolling related to DOMMouseScroll event[m
[32m+[m[32m        if ( 'axis' in orgEvent && orgEvent.axis === orgEvent.HORIZONTAL_AXIS ) {[m
[32m+[m[32m            deltaX = deltaY * -1;[m
[32m+[m[32m            deltaY = 0;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Set delta to be deltaY or deltaX if deltaY is 0 for backwards compatabilitiy[m
[32m+[m[32m        delta = deltaY === 0 ? deltaX : deltaY;[m
[32m+[m
[32m+[m[32m        // New school wheel delta (wheel event)[m
[32m+[m[32m        if ( 'deltaY' in orgEvent ) {[m
[32m+[m[32m            deltaY = orgEvent.deltaY * -1;[m
[32m+[m[32m            delta  = deltaY;[m
[32m+[m[32m        }[m
[32m+[m[32m        if ( 'deltaX' in orgEvent ) {[m
[32m+[m[32m            deltaX = orgEvent.deltaX;[m
[32m+[m[32m            if ( deltaY === 0 ) { delta  = deltaX * -1; }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // No change actually happened, no reason to go any further[m
[32m+[m[32m        if ( deltaY === 0 && deltaX === 0 ) { return; }[m
[32m+[m
[32m+[m[32m        // Need to convert lines and pages to pixels if we aren't already in pixels[m
[32m+[m[32m        // There are three delta modes:[m
[32m+[m[32m        //   * deltaMode 0 is by pixels, nothing to do[m
[32m+[m[32m        //   * deltaMode 1 is by lines[m
[32m+[m[32m        //   * deltaMode 2 is by pages[m
[32m+[m[32m        if ( orgEvent.deltaMode === 1 ) {[m
[32m+[m[32m            var lineHeight = $.data(this, 'mousewheel-line-height');[m
[32m+[m[32m            delta  *= lineHeight;[m
[32m+[m[32m            deltaY *= lineHeight;[m
[32m+[m[32m            deltaX *= lineHeight;[m
[32m+[m[32m        } else if ( orgEvent.deltaMode === 2 ) {[m
[32m+[m[32m            var pageHeight = $.data(this, 'mousewheel-page-height');[m
[32m+[m[32m            delta  *= pageHeight;[m
[32m+[m[32m            deltaY *= pageHeight;[m
[32m+[m[32m            deltaX *= pageHeight;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Store lowest absolute delta to normalize the delta values[m
[32m+[m[32m        absDelta = Math.max( Math.abs(deltaY), Math.abs(deltaX) );[m
[32m+[m
[32m+[m[32m        if ( !lowestDelta || absDelta < lowestDelta ) {[m
[32m+[m[32m            lowestDelta = absDelta;[m
[32m+[m
[32m+[m[32m            // Adjust older deltas if necessary[m
[32m+[m[32m            if ( shouldAdjustOldDeltas(orgEvent, absDelta) ) {[m
[32m+[m[32m                lowestDelta /= 40;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Adjust older deltas if necessary[m
[32m+[m[32m        if ( shouldAdjustOldDeltas(orgEvent, absDelta) ) {[m
[32m+[m[32m            // Divide all the things by 40![m
[32m+[m[32m            delta  /= 40;[m
[32m+[m[32m            deltaX /= 40;[m
[32m+[m[32m            deltaY /= 40;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Get a whole, normalized value for the deltas[m
[32m+[m[32m        delta  = Math[ delta  >= 1 ? 'floor' : 'ceil' ](delta  / lowestDelta);[m
[32m+[m[32m        deltaX = Math[ deltaX >= 1 ? 'floor' : 'ceil' ](deltaX / lowestDelta);[m
[32m+[m[32m        deltaY = Math[ deltaY >= 1 ? 'floor' : 'ceil' ](deltaY / lowestDelta);[m
[32m+[m
[32m+[m[32m        // Normalise offsetX and offsetY properties[m
[32m+[m[32m        if ( special.settings.normalizeOffset && this.getBoundingClientRect ) {[m
[32m+[m[32m            var boundingRect = this.getBoundingClientRect();[m
[32m+[m[32m            offsetX = event.clientX - boundingRect.left;[m
[32m+[m[32m            offsetY = event.clientY - boundingRect.top;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Add information to the event object[m
[32m+[m[32m        event.deltaX = deltaX;[m
[32m+[m[32m        event.deltaY = deltaY;[m
[32m+[m[32m        event.deltaFactor = lowestDelta;[m
[32m+[m[32m        event.offsetX = offsetX;[m
[32m+[m[32m        event.offsetY = offsetY;[m
[32m+[m[32m        // Go ahead and set deltaMode to 0 since we converted to pixels[m
[32m+[m[32m        // Although this is a little odd since we overwrite the deltaX/Y[m
[32m+[m[32m        // properties with normalized deltas.[m
[32m+[m[32m        event.deltaMode = 0;[m
[32m+[m
[32m+[m[32m        // Add event and delta to the front of the arguments[m
[32m+[m[32m        args.unshift(event, delta, deltaX, deltaY);[m
[32m+[m
[32m+[m[32m        // Clearout lowestDelta after sometime to better[m
[32m+[m[32m        // handle multiple device types that give different[m
[32m+[m[32m        // a different lowestDelta[m
[32m+[m[32m        // Ex: trackpad = 3 and mouse wheel = 120[m
[32m+[m[32m        if (nullLowestDeltaTimeout) { clearTimeout(nullLowestDeltaTimeout); }[m
[32m+[m[32m        nullLowestDeltaTimeout = setTimeout(nullLowestDelta, 200);[m
[32m+[m
[32m+[m[32m        return ($.event.dispatch || $.event.handle).apply(this, args);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function nullLowestDelta() {[m
[32m+[m[32m        lowestDelta = null;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function shouldAdjustOldDeltas(orgEvent, absDelta) {[m
[32m+[m[32m        // If this is an older event and the delta is divisable by 120,[m
[32m+[m[32m        // then we are assuming that the browser is treating this as an[m
[32m+[m[32m        // older mouse wheel event and that we should divide the deltas[m
[32m+[m[32m        // by 40 to try and get a more usable deltaFactor.[m
[32m+[m[32m        // Side note, this actually impacts the reported scroll distance[m
[32m+[m[32m        // in older browsers and can cause scrolling to be slower than native.[m
[32m+[m[32m        // Turn this off by setting $.event.special.mousewheel.settings.adjustOldDeltas to false.[m
[32m+[m[32m        return special.settings.adjustOldDeltas && orgEvent.type === 'mousewheel' && absDelta % 120 === 0;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m}));[m
[32m+[m
[32m+[m[32mS2.define('jquery.select2',[[m
[32m+[m[32m  'jquery',[m
[32m+[m[32m  'jquery-mousewheel',[m
[32m+[m
[32m+[m[32m  './select2/core',[m
[32m+[m[32m  './select2/defaults',[m
[32m+[m[32m  './select2/utils'[m
[32m+[m[32m], function ($, _, Select2, Defaults, Utils) {[m
[32m+[m[32m  if ($.fn.select2 == null) {[m
[32m+[m[32m    // All methods that should return the element[m
[32m+[m[32m    var thisMethods = ['open', 'close', 'destroy'];[m
[32m+[m
[32m+[m[32m    $.fn.select2 = function (options) {[m
[32m+[m[32m      options = options || {};[m
[32m+[m
[32m+[m[32m      if (typeof options === 'object') {[m
[32m+[m[32m        this.each(function () {[m
[32m+[m[32m          var instanceOptions = $.extend(true, {}, options);[m
[32m+[m
[32m+[m[32m          var instance = new Select2($(this), instanceOptions);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        return this;[m
[32m+[m[32m      } else if (typeof options === 'string') {[m
[32m+[m[32m        var ret;[m
[32m+[m[32m        var args = Array.prototype.slice.call(arguments, 1);[m
[32m+[m
[32m+[m[32m        this.each(function () {[m
[32m+[m[32m          var instance = Utils.GetData(this, 'select2');[m
[32m+[m
[32m+[m[32m          if (instance == null && window.console && console.error) {[m
[32m+[m[32m            console.error([m
[32m+[m[32m              'The select2(\'' + options + '\') method was called on an ' +[m
[32m+[m[32m              'element that is not using Select2.'[m
[32m+[m[32m            );[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          ret = instance[options].apply(instance, args);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        // Check if we should be returning `this`[m
[32m+[m[32m        if ($.inArray(options, thisMethods) > -1) {[m
[32m+[m[32m          return this;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return ret;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        throw new Error('Invalid arguments for Select2: ' + options);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if ($.fn.select2.defaults == null) {[m
[32m+[m[32m    $.fn.select2.defaults = Defaults;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return Select2;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m  // Return the AMD loader configuration so it can be used outside of this file[m
[32m+[m[32m  return {[m
[32m+[m[32m    define: S2.define,[m
[32m+[m[32m    require: S2.require[m
[32m+[m[32m  };[m
[32m+[m[32m}());[m
[32m+[m
[32m+[m[32m  // Autoload the jQuery bindings[m
[32m+[m[32m  // We know that all of the modules exist above this, so we're safe[m
[32m+[m[32m  var select2 = S2.require('jquery.select2');[m
[32m+[m
[32m+[m[32m  // Hold the AMD module references on the jQuery function that was just loaded[m
[32m+[m[32m  // This allows Select2 to use the internal loader outside of this file, such[m
[32m+[m[32m  // as in the language files.[m
[32m+[m[32m  jQuery.fn.select2.amd = S2;[m
[32m+[m
[32m+[m[32m  // Return the Select2 instance for anyone who is importing it.[m
[32m+[m[32m  return select2;[m
[32m+[m[32m}));[m
