[32m+[m[32m},{"../internals/length-of-array-like":123,"../internals/to-absolute-index":153,"../internals/to-indexed-object":154}],64:[function(require,module,exports){[m
[32m+[m[32mvar bind = require('../internals/function-bind-context');[m
[32m+[m[32mvar uncurryThis = require('../internals/function-uncurry-this');[m
[32m+[m[32mvar IndexedObject = require('../internals/indexed-object');[m
[32m+[m[32mvar toObject = require('../internals/to-object');[m
[32m+[m[32mvar lengthOfArrayLike = require('../internals/length-of-array-like');[m
[32m+[m[32mvar arraySpeciesCreate = require('../internals/array-species-create');[m
[32m+[m
[32m+[m[32mvar push = uncurryThis([].push);[m
[32m+[m
[32m+[m[32m// `Array.prototype.{ forEach, map, filter, some, every, find, findIndex, filterReject }` methods implementation[m
[32m+[m[32mvar createMethod = function (TYPE) {[m
[32m+[m[32m  var IS_MAP = TYPE == 1;[m
[32m+[m[32m  var IS_FILTER = TYPE == 2;[m
[32m+[m[32m  var IS_SOME = TYPE == 3;[m
[32m+[m[32m  var IS_EVERY = TYPE == 4;[m
[32m+[m[32m  var IS_FIND_INDEX = TYPE == 6;[m
[32m+[m[32m  var IS_FILTER_REJECT = TYPE == 7;[m
[32m+[m[32m  var NO_HOLES = TYPE == 5 || IS_FIND_INDEX;[m
[32m+[m[32m  return function ($this, callbackfn, that, specificCreate) {[m
[32m+[m[32m    var O = toObject($this);[m
[32m+[m[32m    var self = IndexedObject(O);[m
[32m+[m[32m    var boundFunction = bind(callbackfn, that);[m
[32m+[m[32m    var length = lengthOfArrayLike(self);[m
[32m+[m[32m    var index = 0;[m
[32m+[m[32m    var create = specificCreate || arraySpeciesCreate;[m
[32m+[m[32m    var target = IS_MAP ? create($this, length) : IS_FILTER || IS_FILTER_REJECT ? create($this, 0) : undefined;[m
[32m+[m[32m    var value, result;[m
[32m+[m[32m    for (;length > index; index++) if (NO_HOLES || index in self) {[m
[32m+[m[32m      value = self[index];[m
[32m+[m[32m      result = boundFunction(value, index, O);[m
[32m+[m[32m      if (TYPE) {[m
[32m+[m[32m        if (IS_MAP) target[index] = result; // map[m
[32m+[m[32m        else if (result) switch (TYPE) {[m
[32m+[m[32m          case 3: return true;              // some[m
[32m+[m[32m          case 5: return value;             // find[m
[32m+[m[32m          case 6: return index;             // findIndex[m
[32m+[m[32m          case 2: push(target, value);      // filter[m
[32m+[m[32m        } else switch (TYPE) {[m
[32m+[m[32m          case 4: return false;             // every[m
[32m+[m[32m          case 7: push(target, value);      // filterReject[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    return IS_FIND_INDEX ? -1 : IS_SOME || IS_EVERY ? IS_EVERY : target;[m
[32m+[m[32m  };[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m  // `Array.prototype.forEach` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-array.prototype.foreach[m
[32m+[m[32m  forEach: createMethod(0),[m
[32m+[m[32m  // `Array.prototype.map` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-array.prototype.map[m
[32m+[m[32m  map: createMethod(1),[m
[32m+[m[32m  // `Array.prototype.filter` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-array.prototype.filter[m
[32m+[m[32m  filter: createMethod(2),[m
[32m+[m[32m  // `Array.prototype.some` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-array.prototype.some[m
[32m+[m[32m  some: createMethod(3),[m
[32m+[m[32m  // `Array.prototype.every` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-array.prototype.every[m
[32m+[m[32m  every: createMethod(4),[m
[32m+[m[32m  // `Array.prototype.find` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-array.prototype.find[m
[32m+[m[32m  find: createMethod(5),[m
[32m+[m[32m  // `Array.prototype.findIndex` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-array.prototype.findIndex[m
[32m+[m[32m  findIndex: createMethod(6),[m
[32m+[m[32m  // `Array.prototype.filterReject` method[m
[32m+[m[32m  // https://github.com/tc39/proposal-array-filtering[m
[32m+[m[32m  filterReject: createMethod(7)[m
[32m+[m[32m};[m
