[32m+[m
[32m+[m[32m/*![m
[32m+[m[32m * XRegExp Unicode Base 5.1.1[m
[32m+[m[32m * <xregexp.com>[m
[32m+[m[32m * Steven Levithan (c) 2008-present MIT License[m
[32m+[m[32m */[m
[32m+[m[32mvar _default = function _default(XRegExp) {[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Adds base support for Unicode matching:[m
[32m+[m[32m   * - Adds syntax `\p{..}` for matching Unicode tokens. Tokens can be inverted using `\P{..}` or[m
[32m+[m[32m   *   `\p{^..}`. Token names ignore case, spaces, hyphens, and underscores. You can omit the[m
[32m+[m[32m   *   braces for token names that are a single letter (e.g. `\pL` or `PL`).[m
[32m+[m[32m   * - Adds flag A (astral), which enables 21-bit Unicode support.[m
[32m+[m[32m   * - Adds the `XRegExp.addUnicodeData` method used by other addons to provide character data.[m
[32m+[m[32m   *[m
[32m+[m[32m   * Unicode Base relies on externally provided Unicode character data. Official addons are[m
[32m+[m[32m   * available to provide data for Unicode categories, scripts, and properties.[m
[32m+[m[32m   *[m
[32m+[m[32m   * @requires XRegExp[m
[32m+[m[32m   */[m
[32m+[m[32m  // ==--------------------------==[m
[32m+[m[32m  // Private stuff[m
[32m+[m[32m  // ==--------------------------==[m
[32m+[m[32m  // Storage for Unicode data[m
[32m+[m[32m  var unicode = {};[m
[32m+[m[32m  var unicodeTypes = {}; // Reuse utils[m
[32m+[m
[32m+[m[32m  var dec = XRegExp._dec;[m
[32m+[m[32m  var hex = XRegExp._hex;[m
[32m+[m[32m  var pad4 = XRegExp._pad4; // Generates a token lookup name: lowercase, with hyphens, spaces, and underscores removed[m
[32m+[m
[32m+[m[32m  function normalize(name) {[m
[32m+[m[32m    return name.replace(/[- _]+/g, '').toLowerCase();[m
[32m+[m[32m  } // Gets the decimal code of a literal code unit, \xHH, \uHHHH, or a backslash-escaped literal[m
[32m+[m
[32m+[m
[32m+[m[32m  function charCode(chr) {[m
[32m+[m[32m    var esc = /^\\[xu](.+)/.exec(chr);[m
[32m+[m[32m    return esc ? dec(esc[1]) : chr.charCodeAt(chr[0] === '\\' ? 1 : 0);[m
[32m+[m[32m  } // Inverts a list of ordered BMP characters and ranges[m
[32m+[m
[32m+[m
[32m+[m[32m  function invertBmp(range) {[m
[32m+[m[32m    var output = '';[m
[32m+[m[32m    var lastEnd = -1;[m
[32m+[m[32m    (0, _forEach["default"])(XRegExp).call(XRegExp, range, /(\\x..|\\u....|\\?[\s\S])(?:-(\\x..|\\u....|\\?[\s\S]))?/, function (m) {[m
[32m+[m[32m      var start = charCode(m[1]);[m
[32m+[m
[32m+[m[32m      if (start > lastEnd + 1) {[m
[32m+[m[32m        output += "\\u".concat(pad4(hex(lastEnd + 1)));[m
[32m+[m
[32m+[m[32m        if (start > lastEnd + 2) {[m
[32m+[m[32m          output += "-\\u".concat(pad4(hex(start - 1)));[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      lastEnd = charCode(m[2] || m[1]);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    if (lastEnd < 0xFFFF) {[m
[32m+[m[32m      output += "\\u".concat(pad4(hex(lastEnd + 1)));[m
[32m+[m
[32m+[m[32m      if (lastEnd < 0xFFFE) {[m
[32m+[m[32m        output += '-\\uFFFF';[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return output;[m
[32m+[m[32m  } // Generates an inverted BMP range on first use[m
[32m+[m
[32m+[m
[32m+[m[32m  function cacheInvertedBmp(slug) {[m
[32m+[m[32m    var prop = 'b!';[m
[32m+[m[32m    return unicode[slug][prop] || (unicode[slug][prop] = invertBmp(unicode[slug].bmp));[m
[32m+[m[32m  } // Combines and optionally negates BMP and astral data[m
[32m+[m
[32m+[m
[32m+[m[32m  function buildAstral(slug, isNegated) {[m
[32m+[m[32m    var item = unicode[slug];[m
[32m+[m[32m    var combined = '';[m
[32m+[m
[32m+[m[32m    if (item.bmp && !item.isBmpLast) {[m
[32m+[m[32m      var _context;[m
[32m+[m
[32m+[m[32m      combined = (0, _concat["default"])(_context = "[".concat(item.bmp, "]")).call(_context, item.astral ? '|' : '');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (item.astral) {[m
[32m+[m[32m      combined += item.astral;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (item.isBmpLast && item.bmp) {[m
[32m+[m[32m      var _context2;[m
[32m+[m
[32m+[m[32m      combined += (0, _concat["default"])(_context2 = "".concat(item.astral ? '|' : '', "[")).call(_context2, item.bmp, "]");[m
[32m+[m[32m    } // Astral Unicode tokens always match a code point, never a code unit[m
[32m+[m
[32m+[m
[32m+[m[32m    return isNegated ? "(?:(?!".concat(combined, ")(?:[\uD800-\uDBFF][\uDC00-\uDFFF]|[\0-\uFFFF]))") : "(?:".concat(combined, ")");[m
[32m+[m[32m  } // Builds a complete astral pattern on first use[m
[32m+[m
[32m+[m
[32m+[m[32m  function cacheAstral(slug, isNegated) {[m
[32m+[m[32m    var prop = isNegated ? 'a!' : 'a=';[m
[32m+[m[32m    return unicode[slug][prop] || (unicode[slug][prop] = buildAstral(slug, isNegated));[m
[32m+[m[32m  } // ==--------------------------==[m
[32m+[m[32m  // Core functionality[m
[32m+[m[32m  // ==--------------------------==[m
[32m+[m
[32m+[m[32m  /*[m
[32m+[m[32m   * Add astral mode (flag A) and Unicode token syntax: `\p{..}`, `\P{..}`, `\p{^..}`, `\pC`.[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m
[32m+[m[32m  XRegExp.addToken( // Use `*` instead of `+` to avoid capturing `^` as the token name in `\p{^}`[m
[32m+[m[32m  /\\([pP])(?:{(\^?)(?:(\w+)=)?([^}]*)}|([A-Za-z]))/, function (match, scope, flags) {[m
[32m+[m[32m    var ERR_DOUBLE_NEG = 'Invalid double negation ';[m
[32m+[m[32m    var ERR_UNKNOWN_NAME = 'Unknown Unicode token ';[m
[32m+[m[32m    var ERR_UNKNOWN_REF = 'Unicode token missing data ';[m
[32m+[m[32m    var ERR_ASTRAL_ONLY = 'Astral mode required for Unicode token ';[m
[32m+[m[32m    var ERR_ASTRAL_IN_CLASS = 'Astral mode does not support Unicode tokens within character classes';[m
[32m+[m
[32m+[m[32m    var _match = (0, _slicedToArray2["default"])(match, 6),[m
[32m+[m[32m        fullToken = _match[0],[m
[32m+[m[32m        pPrefix = _match[1],[m
[32m+[m[32m        caretNegation = _match[2],[m
[32m+[m[32m        typePrefix = _match[3],[m
[32m+[m[32m        tokenName = _match[4],[m
[32m+[m[32m        tokenSingleCharName = _match[5]; // Negated via \P{..} or \p{^..}[m
[32m+[m
[32m+[m
[32m+[m[32m    var isNegated = pPrefix === 'P' || !!caretNegation; // Switch from BMP (0-FFFF) to astral (0-10FFFF) mode via flag A[m
[32m+[m
[32m+[m[32m    var isAstralMode = (0, _indexOf["default"])(flags).call(flags, 'A') !== -1; // Token lookup name. Check `tokenSingleCharName` first to avoid passing `undefined`[m
[32m+[m[32m    // via `\p{}`[m
[32m+[m
[32m+[m[32m    var slug = normalize(tokenSingleCharName || tokenName); // Token data object[m
[32m+[m
[32m+[m[32m    var item = unicode[slug];[m
[32m+[m
[32m+[m[32m    if (pPrefix === 'P' && caretNegation) {[m
[32m+[m[32m      throw new SyntaxError(ERR_DOUBLE_NEG + fullToken);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!unicode.hasOwnProperty(slug)) {[m
[32m+[m[32m      throw new SyntaxError(ERR_UNKNOWN_NAME + fullToken);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typePrefix) {[m
[32m+[m[32m      if (!(unicodeTypes[typePrefix] && unicodeTypes[typePrefix][slug])) {[m
[32m+[m[32m        throw new SyntaxError(ERR_UNKNOWN_NAME + fullToken);[m
[32m+[m[32m      }[m
[32m+[m[32m    } // Switch to the negated form of the referenced Unicode token[m
[32m+[m
[32m+[m
[32m+[m[32m    if (item.inverseOf) {[m
[32m+[m[32m      slug = normalize(item.inverseOf);[m
[32m+[m
[32m+[m[32m      if (!unicode.hasOwnProperty(slug)) {[m
[32m+[m[32m        var _context3;[m
[32m+[m
[32m+[m[32m        throw new ReferenceError((0, _concat["default"])(_context3 = "".concat(ERR_UNKNOWN_REF + fullToken, " -> ")).call(_context3, item.inverseOf));[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      item = unicode[slug];[m
[32m+[m[32m      isNegated = !isNegated;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!(item.bmp || isAstralMode)) {[m
[32m+[m[32m      throw new SyntaxError(ERR_ASTRAL_ONLY + fullToken);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (isAstralMode) {[m
[32m+[m[32m      if (scope === 'class') {[m
[32m+[m[32m        throw new SyntaxError(ERR_ASTRAL_IN_CLASS);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return cacheAstral(slug, isNegated);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return scope === 'class' ? isNegated ? cacheInvertedBmp(slug) : item.bmp : "".concat((isNegated ? '[^' : '[') + item.bmp, "]");[m
[32m+[m[32m  }, {[m
[32m+[m[32m    scope: 'all',[m
[32m+[m[32m    optionalFlags: 'A',[m
[32m+[m[32m    leadChar: '\\'[m
[32m+[m[32m  });[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Adds to the list of Unicode tokens that XRegExp regexes can match via `\p` or `\P`.[m
[32m+[m[32m   *[m
[32m+[m[32m   * @memberOf XRegExp[m
[32m+[m[32m   * @param {Array} data Objects with named character ranges. Each object may have properties[m
[32m+[m[32m   *   `name`, `alias`, `isBmpLast`, `inverseOf`, `bmp`, and `astral`. All but `name` are[m
[32m+[m[32m   *   optional, although one of `bmp` or `astral` is required (unless `inverseOf` is set). If[m
[32m+[m[32m   *   `astral` is absent, the `bmp` data is used for BMP and astral modes. If `bmp` is absent,[m
[32m+[m[32m   *   the name errors in BMP mode but works in astral mode. If both `bmp` and `astral` are[m
[32m+[m[32m   *   provided, the `bmp` data only is used in BMP mode, and the combination of `bmp` and[m
[32m+[m[32m   *   `astral` data is used in astral mode. `isBmpLast` is needed when a token matches orphan[m
[32m+[m[32m   *   high surrogates *and* uses surrogate pairs to match astral code points. The `bmp` and[m
[32m+[m[32m   *   `astral` data should be a combination of literal characters and `\xHH` or `\uHHHH` escape[m
[32m+[m[32m   *   sequences, with hyphens to create ranges. Any regex metacharacters in the data should be[m
[32m+[m[32m   *   escaped, apart from range-creating hyphens. The `astral` data can additionally use[m
[32m+[m[32m   *   character classes and alternation, and should use surrogate pairs to represent astral code[m
[32m+[m[32m   *   points. `inverseOf` can be used to avoid duplicating character data if a Unicode token is[m
[32m+[m[32m   *   defined as the exact inverse of another token.[m
[32m+[m[32m   * @param {String} [typePrefix] Enables optionally using this type as a prefix for all of the[m
[32m+[m[32m   *   provided Unicode tokens, e.g. if given `'Type'`, then `\p{TokenName}` can also be written[m
[32m+[m[32m   *   as `\p{Type=TokenName}`.[m
[32m+[m[32m   * @example[m
[32m+[m[32m   *[m
[32m+[m[32m   * // Basic use[m
[32m+[m[32m   * XRegExp.addUnicodeData([{[m
[32m+[m[32m   *   name: 'XDigit',[m
[32m+[m[32m   *   alias: 'Hexadecimal',[m
[32m+[m[32m   *   bmp: '0-9A-Fa-f'[m
[32m+[m[32m   * }]);[m
[32m+[m[32m   * XRegExp('\\p{XDigit}:\\p{Hexadecimal}+').test('0:3D'); // -> true[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  XRegExp.addUnicodeData = function (data, typePrefix) {[m
[32m+[m[32m    var ERR_NO_NAME = 'Unicode token requires name';[m
[32m+[m[32m    var ERR_NO_DATA = 'Unicode token has no character data ';[m
[32m+[m
[32m+[m[32m    if (typePrefix) {[m
[32m+[m[32m      // Case sensitive to match ES2018[m
[32m+[m[32m      unicodeTypes[typePrefix] = {};[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var _iterator = _createForOfIteratorHelper(data),[m
[32m+[m[32m        _step;[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m      for (_iterator.s(); !(_step = _iterator.n()).done;) {[m
[32m+[m[32m        var item = _step.value;[m
[32m+[m
[32m+[m[32m        if (!item.name) {[m
[32m+[m[32m          throw new Error(ERR_NO_NAME);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!(item.inverseOf || item.bmp || item.astral)) {[m
[32m+[m[32m          throw new Error(ERR_NO_DATA + item.name);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var normalizedName = normalize(item.name);[m
[32m+[m[32m        unicode[normalizedName] = item;[m
[32m+[m
[32m+[m[32m        if (typePrefix) {[m
[32m+[m[32m          unicodeTypes[typePrefix][normalizedName] = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (item.alias) {[m
[32m+[m[32m          var normalizedAlias = normalize(item.alias);[m
[32m+[m[32m          unicode[normalizedAlias] = item;[m
[32m+[m
[32m+[m[32m          if (typePrefix) {[m
[32m+[m[32m            unicodeTypes[typePrefix][normalizedAlias] = true;[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      } // Reset the pattern cache used by the `XRegExp` constructor, since the same pattern and[m
[32m+[m[32m      // flags might now produce different results[m
[32m+[m
[32m+[m[32m    } catch (err) {[m
[32m+[m[32m      _iterator.e(err);[m
[32m+[m[32m    } finally {[m
[32m+[m[32m      _iterator.f();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    XRegExp.cache.flush('patterns');[m
[32m+[m[32m  };[m
[32m+[m[32m  /**[m
[32m+[m[32m   * @ignore[m
[32m+[m[32m   *[m
[32m+[m[32m   * Return a reference to the internal Unicode definition structure for the given Unicode[m
[32m+[m[32m   * Property if the given name is a legal Unicode Property for use in XRegExp `\p` or `\P` regex[m
[32m+[m[32m   * constructs.[m
[32m+[m[32m   *[m
[32m+[m[32m   * @memberOf XRegExp[m
[32m+[m[32m   * @param {String} name Name by which the Unicode Property may be recognized (case-insensitive),[m
[32m+[m[32m   *   e.g. `'N'` or `'Number'`. The given name is matched against all registered Unicode[m
[32m+[m[32m   *   Properties and Property Aliases.[m
[32m+[m[32m   * @returns {Object} Reference to definition structure when the name matches a Unicode Property.[m
[32m+[m[32m   *[m
[32m+[m[32m   * @note[m
[32m+[m[32m   * For more info on Unicode Properties, see also http://unicode.org/reports/tr18/#Categories.[m
[32m+[m[32m   *[m
[32m+[m[32m   * @note[m
[32m+[m[32m   * This method is *not* part of the officially documented API and may change or be removed in[m
[32m+[m[32m   * the future. It is meant for userland code that wishes to reuse the (large) internal Unicode[m
[32m+[m[32m   * structures set up by XRegExp.[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m
[32m+[m[32m  XRegExp._getUnicodeProperty = function (name) {[m
[32m+[m[32m    var slug = normalize(name);[m
[32m+[m[32m    return unicode[slug];[m
[32m+[m[32m  };[m
[32m+[m[32m};[m
[32m+[m
