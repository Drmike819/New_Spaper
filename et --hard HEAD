[36m@@ -0,0 +1,49 @@[m
[32m+[m[32mfrom django.shortcuts import render, redirect[m
[32m+[m[32mfrom django.urls import reverse_lazy[m
[32m+[m[32mfrom django.views.generic import CreateView[m
[32m+[m[32mfrom .forms import CustomUserCreationForm[m
[32m+[m[32mfrom django.contrib.auth.decorators import login_required[m
[32m+[m[32mfrom django.contrib.auth import logout[m
[32m+[m[32mfrom django.contrib import messages  # Para mostrar mensajes de error o √©xito[m
[32m+[m
[32m+[m[32mfrom .forms import UpdateProfileImageForm[m
[32m+[m
[32m+[m[32m# Create your views here.[m
[32m+[m
[32m+[m[32m#vista para poder registarse[m
[32m+[m[32mclass SignUpView(CreateView):[m
[32m+[m[32m    form_class = CustomUserCreationForm[m
[32m+[m[32m    success_url = reverse_lazy('login')[m
[32m+[m[32m    template_name = 'registration/signup.html'[m
[32m+[m
[32m+[m[32m    # Incluir request.FILES para manejar archivos de imagen[m
[32m+[m[32m    def form_valid(self, form):[m
[32m+[m[32m        # Asignar la imagen subida al formulario[m
[32m+[m[32m        form.instance.image = self.request.FILES.get('image')[m
[32m+[m[32m        # Llamar a la implementaci√≥n del m√©todo padre para devolver la respuesta[m
[32m+[m[32m        return super().form_valid(form)[m
[32m+[m[41m [m
[32m+[m[32m#vista para cerrar sesion[m[41m      [m
[32m+[m[32m@login_required[m
[32m+[m[32mdef user_logout(request):[m
[32m+[m[32m    logout(request)[m
[32m+[m[32m    return render(request, 'registration/logout.html')[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m@login_required[m
[32m+[m[32mdef update_profile_image(request):[m
[32m+[m[32m    user = request.user  # Obt√©n el usuario autenticado[m
[32m+[m[32m    if request.method == 'POST':[m
[32m+[m[32m        form = UpdateProfileImageForm(request.POST, request.FILES, instance=user)[m
[32m+[m[32m        if form.is_valid():[m
[32m+[m[32m            form.save()  # Guarda el formulario y actualiza la imagen[m
[32m+[m[32m            messages.success(request, "¬°Imagen de perfil actualizada con √©xito!")[m
[32m+[m[32m            return redirect('home')  # Redirige a la p√°gina de inicio o la que prefieras[m
[32m+[m[32m        else:[m
[32m+[m[32m            messages.error(request, "Hubo un error al actualizar tu imagen de perfil. Por favor, intenta nuevamente.")[m
[32m+[m[32m            print(form.errors)  # Esto imprime los errores en la consola para depuraci√≥n[m
[32m+[m[32m    else:[m
[32m+[m[32m        form = UpdateProfileImageForm(instance=user)  # Instancia del usuario autenticado[m
[32m+[m
[32m+[m[32m    return render(request, 'update_profile_image.html', {'form': form})[m
