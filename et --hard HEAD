[32m+[m[32m  var result = Array(max(fin - k, 0));[m
[32m+[m[32m  for (var n = 0; k < fin; k++, n++) createProperty(result, n, O[k]);[m
[32m+[m[32m  result.length = n;[m
[32m+[m[32m  return result;[m
[32m+[m[32m};[m
