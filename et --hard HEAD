[32m+[m[32m'use strict';[m
[32m+[m[32mvar global = require('../internals/global');[m
[32m+[m[32mvar bind = require('../internals/function-bind-context');[m
[32m+[m[32mvar call = require('../internals/function-call');[m
[32m+[m[32mvar toObject = require('../internals/to-object');[m
[32m+[m[32mvar callWithSafeIterationClosing = require('../internals/call-with-safe-iteration-closing');[m
[32m+[m[32mvar isArrayIteratorMethod = require('../internals/is-array-iterator-method');[m
[32m+[m[32mvar isConstructor = require('../internals/is-constructor');[m
[32m+[m[32mvar lengthOfArrayLike = require('../internals/length-of-array-like');[m
[32m+[m[32mvar createProperty = require('../internals/create-property');[m
[32m+[m[32mvar getIterator = require('../internals/get-iterator');[m
[32m+[m[32mvar getIteratorMethod = require('../internals/get-iterator-method');[m
[32m+[m
[32m+[m[32mvar Array = global.Array;[m
[32m+[m
[32m+[m[32m// `Array.from` method implementation[m
[32m+[m[32m// https://tc39.es/ecma262/#sec-array.from[m
[32m+[m[32mmodule.exports = function from(arrayLike /* , mapfn = undefined, thisArg = undefined */) {[m
[32m+[m[32m  var O = toObject(arrayLike);[m
[32m+[m[32m  var IS_CONSTRUCTOR = isConstructor(this);[m
[32m+[m[32m  var argumentsLength = arguments.length;[m
[32m+[m[32m  var mapfn = argumentsLength > 1 ? arguments[1] : undefined;[m
[32m+[m[32m  var mapping = mapfn !== undefined;[m
[32m+[m[32m  if (mapping) mapfn = bind(mapfn, argumentsLength > 2 ? arguments[2] : undefined);[m
[32m+[m[32m  var iteratorMethod = getIteratorMethod(O);[m
[32m+[m[32m  var index = 0;[m
[32m+[m[32m  var length, result, step, iterator, next, value;[m
[32m+[m[32m  // if the target is not iterable or it's an array with the default iterator - use a simple case[m
[32m+[m[32m  if (iteratorMethod && !(this == Array && isArrayIteratorMethod(iteratorMethod))) {[m
[32m+[m[32m    iterator = getIterator(O, iteratorMethod);[m
[32m+[m[32m    next = iterator.next;[m
[32m+[m[32m    result = IS_CONSTRUCTOR ? new this() : [];[m
[32m+[m[32m    for (;!(step = call(next, iterator)).done; index++) {[m
[32m+[m[32m      value = mapping ? callWithSafeIterationClosing(iterator, mapfn, [step.value, index], true) : step.value;[m
[32m+[m[32m      createProperty(result, index, value);[m
[32m+[m[32m    }[m
[32m+[m[32m  } else {[m
