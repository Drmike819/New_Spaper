[1mdiff --git a/accounts/forms.py b/accounts/forms.py[m
[1mnew file mode 100644[m
[1mindex 0000000..f5a09fb[m
[1m--- /dev/null[m
[1m+++ b/accounts/forms.py[m
[36m@@ -0,0 +1,37 @@[m
[32m+[m[32mfrom django.contrib.auth.forms import UserCreationForm, UserChangeForm[m
[32m+[m[32mfrom .models import CustomUser[m
[32m+[m
[32m+[m[32mfrom django import forms[m
[32m+[m[32m# UserCreationForm:[m
[32m+[m[32m# Sirve para manejar la creaci√≥n de nuevos usuarios.[m
[32m+[m[32m# Incluye campos como username, password1 y password2 (para verificar que las contrase√±as coinciden).[m
[32m+[m[32m# Se utiliza principalmente en vistas o formularios donde se crea un nuevo usuario, como en un formulario de registro.[m
[32m+[m
[32m+[m[32m# ampliar los formularios integrados UserCreationForm y UserChangeForm[m[41m [m
[32m+[m[32mclass CustomUserCreationForm(UserCreationForm):[m
[32m+[m[32m    class Meta(UserCreationForm):[m
[32m+[m[32m        model = CustomUser[m
[32m+[m[32m        fields = ([m
[32m+[m[32m            'username',[m
[32m+[m[32m            'email',[m
[32m+[m[32m            'age',[m
[32m+[m[32m            'image',[m
[32m+[m[32m        )[m
[32m+[m
[32m+[m[32m# UserChangeForm:[m
[32m+[m[32m# Sirve para manejar la modificaci√≥n de usuarios existentes.[m
[32m+[m[32m# Permite editar campos como el nombre de usuario y otros datos de perfil, pero sin mostrar los campos de la contrase√±a de manera insegura.[m
[32m+[m[32mclass CustomUserChangeForm(UserChangeForm):[m
[32m+[m[32m    class Meta:[m
[32m+[m[32m        model = CustomUser[m
[32m+[m[32m        fields = ([m
[32m+[m[32m            'username',[m
[32m+[m[32m            'email',[m
[32m+[m[32m            'age',[m
[32m+[m[32m            'image',[m
[32m+[m[32m        )[m
[32m+[m[41m        [m
[32m+[m[32mclass UpdateProfileImageForm(forms.ModelForm):[m
[32m+[m[32m    class Meta:[m
[32m+[m[32m        model = CustomUser[m
[32m+[m[32m        fields = ['image']  # Campo de imagen para actualizar el perfil[m
\ No newline at end of file[m
