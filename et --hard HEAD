[32m+[m[32mvar isArray = require('../internals/is-array');[m
[32m+[m[32mvar isObject = require('../internals/is-object');[m
[32m+[m[32mvar toObject = require('../internals/to-object');[m
[32m+[m[32mvar lengthOfArrayLike = require('../internals/length-of-array-like');[m
[32m+[m[32mvar createProperty = require('../internals/create-property');[m
[32m+[m[32mvar arraySpeciesCreate = require('../internals/array-species-create');[m
[32m+[m[32mvar arrayMethodHasSpeciesSupport = require('../internals/array-method-has-species-support');[m
[32m+[m[32mvar wellKnownSymbol = require('../internals/well-known-symbol');[m
[32m+[m[32mvar V8_VERSION = require('../internals/engine-v8-version');[m
[32m+[m
[32m+[m[32mvar IS_CONCAT_SPREADABLE = wellKnownSymbol('isConcatSpreadable');[m
[32m+[m[32mvar MAX_SAFE_INTEGER = 0x1FFFFFFFFFFFFF;[m
[32m+[m[32mvar MAXIMUM_ALLOWED_INDEX_EXCEEDED = 'Maximum allowed index exceeded';[m
[32m+[m[32mvar TypeError = global.TypeError;[m
[32m+[m
[32m+[m[32m// We can't use this feature detection in V8 since it causes[m
[32m+[m[32m// deoptimization and serious performance degradation[m
[32m+[m[32m// https://github.com/zloirock/core-js/issues/679[m
[32m+[m[32mvar IS_CONCAT_SPREADABLE_SUPPORT = V8_VERSION >= 51 || !fails(function () {[m
[32m+[m[32m  var array = [];[m
[32m+[m[32m  array[IS_CONCAT_SPREADABLE] = false;[m
[32m+[m[32m  return array.concat()[0] !== array;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mvar SPECIES_SUPPORT = arrayMethodHasSpeciesSupport('concat');[m
[32m+[m
[32m+[m[32mvar isConcatSpreadable = function (O) {[m
[32m+[m[32m  if (!isObject(O)) return false;[m
[32m+[m[32m  var spreadable = O[IS_CONCAT_SPREADABLE];[m
[32m+[m[32m  return spreadable !== undefined ? !!spreadable : isArray(O);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar FORCED = !IS_CONCAT_SPREADABLE_SUPPORT || !SPECIES_SUPPORT;[m
[32m+[m
[32m+[m[32m// `Array.prototype.concat` method[m
[32m+[m[32m// https://tc39.es/ecma262/#sec-array.prototype.concat[m
[32m+[m[32m// with adding support of @@isConcatSpreadable and @@species[m
[32m+[m[32m$({ target: 'Array', proto: true, forced: FORCED }, {[m
[32m+[m[32m  // eslint-disable-next-line no-unused-vars -- required for `.length`[m
[32m+[m[32m  concat: function concat(arg) {[m
[32m+[m[32m    var O = toObject(this);[m
[32m+[m[32m    var A = arraySpeciesCreate(O, 0);[m
[32m+[m[32m    var n = 0;[m
[32m+[m[32m    var i, k, length, len, E;[m
[32m+[m[32m    for (i = -1, length = arguments.length; i < length; i++) {[m
[32m+[m[32m      E = i === -1 ? O : arguments[i];[m
[32m+[m[32m      if (isConcatSpreadable(E)) {[m
[32m+[m[32m        len = lengthOfArrayLike(E);[m
[32m+[m[32m        if (n + len > MAX_SAFE_INTEGER) throw TypeError(MAXIMUM_ALLOWED_INDEX_EXCEEDED);[m
[32m+[m[32m        for (k = 0; k < len; k++, n++) if (k in E) createProperty(A, n, E[k]);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        if (n >= MAX_SAFE_INTEGER) throw TypeError(MAXIMUM_ALLOWED_INDEX_EXCEEDED);[m
[32m+[m[32m        createProperty(A, n++, E);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    A.length = n;[m
[32m+[m[32m    return A;[m
[32m+[m[32m  }[m
[32m+[m[32m});[m
