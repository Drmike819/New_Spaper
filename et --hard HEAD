[32m+[m[32mfrom django.shortcuts import render, redirect[m
[32m+[m[32mfrom articles.models import Article, Comment[m
[32m+[m[32mfrom articles.forms import CommentForm[m
[32m+[m[32mfrom django.contrib.auth.decorators import login_required[m
[32m+[m[32m# Create your views here.[m
[32m+[m
[32m+[m[32m#renderizacion de la pagina principal[m[41m [m
[32m+[m[32m@login_required[m
[32m+[m[32mdef HomePageView(request):[m
[32m+[m[32m    articles = Article.objects.all()[m
[32m+[m[32m    form = CommentForm()[m
[32m+[m
[32m+[m[32m    if request.method == "POST":[m
[32m+[m[32m        form = CommentForm(request.POST)[m
[32m+[m[32m        if form.is_valid():[m
[32m+[m[32m            # Guardar el comentario sin comprometerlo a la base de datos todav√≠a[m
[32m+[m[32m            comment = form.save(commit=False)[m
[32m+[m[32m            # Asignar el art√≠culo y el autor al comentario[m
[32m+[m[32m            article_id = request.POST.get("article_id")  # Capturar el ID del art√≠culo desde el formulario[m
[32m+[m[32m            comment.article = Article.objects.get(id=article_id)[m
[32m+[m[32m            comment.author = request.user[m
[32m+[m[32m            comment.save()[m
[32m+[m[32m            return redirect("home")  # Recargar la p√°gina principal[m
[32m+[m
[32m+[m[32m    return render(request, "home.html", {[m
[32m+[m[32m        'articles': articles,[m
[32m+[m[32m        'form': form[m
[32m+[m[32m    })[m
