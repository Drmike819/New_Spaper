[32m+[m[32m$({ target: 'Array', proto: true, forced: FORCED }, {[m
[32m+[m[32m  sort: function sort(comparefn) {[m
[32m+[m[32m    if (comparefn !== undefined) aCallable(comparefn);[m
[32m+[m
[32m+[m[32m    var array = toObject(this);[m
[32m+[m
[32m+[m[32m    if (STABLE_SORT) return comparefn === undefined ? un$Sort(array) : un$Sort(array, comparefn);[m
[32m+[m
[32m+[m[32m    var items = [];[m
[32m+[m[32m    var arrayLength = lengthOfArrayLike(array);[m
[32m+[m[32m    var itemsLength, index;[m
[32m+[m
[32m+[m[32m    for (index = 0; index < arrayLength; index++) {[m
[32m+[m[32m      if (index in array) push(items, array[index]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    internalSort(items, getSortCompare(comparefn));[m
[32m+[m
[32m+[m[32m    itemsLength = items.length;[m
[32m+[m[32m    index = 0;[m
[32m+[m
[32m+[m[32m    while (index < itemsLength) array[index] = items[index++];[m
[32m+[m[32m    while (index < arrayLength) delete array[index++];[m
[32m+[m
[32m+[m[32m    return array;[m
[32m+[m[32m  }[m
[32m+[m[32m});[m
