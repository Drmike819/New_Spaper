[1m--- /dev/null[m
[1m+++ b/accounts/admin.py[m
[36m@@ -0,0 +1,30 @@[m
[32m+[m[32mfrom django.contrib import admin#Se importa el m√≥dulo de administraci√≥n de Django, que permite registrar modelos en el panel de administraci√≥n.[m
[32m+[m[32mfrom django.contrib.auth.admin import UserAdmin #Es una clase predefinida en Django que proporciona una interfaz de administraci√≥n para el modelo User. Aqu√≠ la est√°s extendiendo para trabajar con tu modelo personalizado.[m
[32m+[m
[32m+[m[32m#Son formularios personalizados para crear y cambiar usuarios.[m[41m [m
[32m+[m[32m#Esto te permite gestionar c√≥mo se crean y editan los usuarios en el panel de administraci√≥n.[m
[32m+[m[32mfrom .forms import CustomUserCreationForm, CustomUserChangeForm[m
[32m+[m[32mfrom . models import CustomUser[m
[32m+[m
[32m+[m[32m# Register your models here.[m
[32m+[m[32mclass CustomUserAdmin(UserAdmin):[m
[32m+[m[32m    #definicion de atributos de definiran que agregara uy editara los usuarios[m[41m [m
[32m+[m[32m    add_form = CustomUserCreationForm#creacion[m
[32m+[m[32m    form = CustomUserChangeForm#edicion[m
[32m+[m[32m    #especifica el modela que la clase esta gestionando[m[41m [m
[32m+[m[32m    model = CustomUser[m
[32m+[m[32m    #Define los campos que se mostrar√°n en la lista de usuarios dentro del panel de administraci√≥n[m
[32m+[m[32m    list_display =[[m
[32m+[m[32m        'email',[m
[32m+[m[32m        'username',[m
[32m+[m[32m        'age',[m
[32m+[m[32m        'image',[m
[32m+[m[32m        'is_staff',[m
[32m+[m[32m    ][m
[32m+[m[32m    #Especifica los campos que se mostrar√°n al editar un usuario[m
[32m+[m[32m    fieldsets = UserAdmin.fieldsets + ((None,{"fields": ('age','image',)}),)[m
[32m+[m[32m    #Especifica los campos que se mostrar√°n al agregar un nuevo usuario[m
[32m+[m[32m    add_fieldsets = UserAdmin.add_fieldsets + ((None,{'fields':('age','image',)}),)[m
[32m+[m[41m    [m
[32m+[m[32m #registra el modelo al panel de django[m[41m    [m
[32m+[m[32madmin.site.register(CustomUser, CustomUserAdmin)[m
\ No newline at end of file[m
