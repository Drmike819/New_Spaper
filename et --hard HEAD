[32m+[m[32mfunction addImageForm() {[m
[32m+[m[32m    var newForm = document.querySelector('.image-form').cloneNode(true);[m
[32m+[m[32m    newForm.querySelector('input[type="file"]').value = '';[m
[32m+[m[32m    newForm.querySelector('input[type="checkbox"]').checked = false;[m
[32m+[m[32m    document.querySelector('form').insertBefore(newForm, document.querySelector('button[onclick="addImageForm()"]'));[m
[32m+[m[32m}[m
