[32m+[m[32m      if (!hasOwn(O, HIDDEN)) nativeDefineProperty(O, HIDDEN, createPropertyDescriptor(1, {}));[m
[32m+[m[32m      O[HIDDEN][key] = true;[m
[32m+[m[32m    } else {[m
[32m+[m[32m      if (hasOwn(O, HIDDEN) && O[HIDDEN][key]) O[HIDDEN][key] = false;[m
[32m+[m[32m      Attributes = nativeObjectCreate(Attributes, { enumerable: createPropertyDescriptor(0, false) });[m
[32m+[m[32m    } return setSymbolDescriptor(O, key, Attributes);[m
[32m+[m[32m  } return nativeDefineProperty(O, key, Attributes);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar $defineProperties = function defineProperties(O, Properties) {[m
[32m+[m[32m  anObject(O);[m
[32m+[m[32m  var properties = toIndexedObject(Properties);[m
[32m+[m[32m  var keys = objectKeys(properties).concat($getOwnPropertySymbols(properties));[m
[32m+[m[32m  $forEach(keys, function (key) {[m
[32m+[m[32m    if (!DESCRIPTORS || call($propertyIsEnumerable, properties, key)) $defineProperty(O, key, properties[key]);[m
[32m+[m[32m  });[m
[32m+[m[32m  return O;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar $create = function create(O, Properties) {[m
[32m+[m[32m  return Properties === undefined ? nativeObjectCreate(O) : $defineProperties(nativeObjectCreate(O), Properties);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar $propertyIsEnumerable = function propertyIsEnumerable(V) {[m
[32m+[m[32m  var P = toPropertyKey(V);[m
[32m+[m[32m  var enumerable = call(nativePropertyIsEnumerable, this, P);[m
[32m+[m[32m  if (this === ObjectPrototype && hasOwn(AllSymbols, P) && !hasOwn(ObjectPrototypeSymbols, P)) return false;[m
[32m+[m[32m  return enumerable || !hasOwn(this, P) || !hasOwn(AllSymbols, P) || hasOwn(this, HIDDEN) && this[HIDDEN][P][m
[32m+[m[32m    ? enumerable : true;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar $getOwnPropertyDescriptor = function getOwnPropertyDescriptor(O, P) {[m
[32m+[m[32m  var it = toIndexedObject(O);[m
[32m+[m[32m  var key = toPropertyKey(P);[m
[32m+[m[32m  if (it === ObjectPrototype && hasOwn(AllSymbols, key) && !hasOwn(ObjectPrototypeSymbols, key)) return;[m
[32m+[m[32m  var descriptor = nativeGetOwnPropertyDescriptor(it, key);[m
[32m+[m[32m  if (descriptor && hasOwn(AllSymbols, key) && !(hasOwn(it, HIDDEN) && it[HIDDEN][key])) {[m
[32m+[m[32m    descriptor.enumerable = true;[m
[32m+[m[32m  }[m
[32m+[m[32m  return descriptor;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar $getOwnPropertyNames = function getOwnPropertyNames(O) {[m
[32m+[m[32m  var names = nativeGetOwnPropertyNames(toIndexedObject(O));[m
[32m+[m[32m  var result = [];[m
[32m+[m[32m  $forEach(names, function (key) {[m
[32m+[m[32m    if (!hasOwn(AllSymbols, key) && !hasOwn(hiddenKeys, key)) push(result, key);[m
[32m+[m[32m  });[m
[32m+[m[32m  return result;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar $getOwnPropertySymbols = function getOwnPropertySymbols(O) {[m
[32m+[m[32m  var IS_OBJECT_PROTOTYPE = O === ObjectPrototype;[m
[32m+[m[32m  var names = nativeGetOwnPropertyNames(IS_OBJECT_PROTOTYPE ? ObjectPrototypeSymbols : toIndexedObject(O));[m
[32m+[m[32m  var result = [];[m
[32m+[m[32m  $forEach(names, function (key) {[m
[32m+[m[32m    if (hasOwn(AllSymbols, key) && (!IS_OBJECT_PROTOTYPE || hasOwn(ObjectPrototype, key))) {[m
[32m+[m[32m      push(result, AllSymbols[key]);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m  return result;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// `Symbol` constructor[m
[32m+[m[32m// https://tc39.es/ecma262/#sec-symbol-constructor[m
[32m+[m[32mif (!NATIVE_SYMBOL) {[m
[32m+[m[32m  $Symbol = function Symbol() {[m
[32m+[m[32m    if (isPrototypeOf(SymbolPrototype, this)) throw TypeError('Symbol is not a constructor');[m
[32m+[m[32m    var description = !arguments.length || arguments[0] === undefined ? undefined : $toString(arguments[0]);[m
[32m+[m[32m    var tag = uid(description);[m
[32m+[m[32m    var setter = function (value) {[m
[32m+[m[32m      if (this === ObjectPrototype) call(setter, ObjectPrototypeSymbols, value);[m
[32m+[m[32m      if (hasOwn(this, HIDDEN) && hasOwn(this[HIDDEN], tag)) this[HIDDEN][tag] = false;[m
[32m+[m[32m      setSymbolDescriptor(this, tag, createPropertyDescriptor(1, value));[m
[32m+[m[32m    };[m
[32m+[m[32m    if (DESCRIPTORS && USE_SETTER) setSymbolDescriptor(ObjectPrototype, tag, { configurable: true, set: setter });[m
[32m+[m[32m    return wrap(tag, description);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  SymbolPrototype = $Symbol[PROTOTYPE];[m
[32m+[m
[32m+[m[32m  redefine(SymbolPrototype, 'toString', function toString() {[m
[32m+[m[32m    return getInternalState(this).tag;[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  redefine($Symbol, 'withoutSetter', function (description) {[m
[32m+[m[32m    return wrap(uid(description), description);[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  propertyIsEnumerableModule.f = $propertyIsEnumerable;[m
[32m+[m[32m  definePropertyModule.f = $defineProperty;[m
[32m+[m[32m  getOwnPropertyDescriptorModule.f = $getOwnPropertyDescriptor;[m
[32m+[m[32m  getOwnPropertyNamesModule.f = getOwnPropertyNamesExternal.f = $getOwnPropertyNames;[m
[32m+[m[32m  getOwnPropertySymbolsModule.f = $getOwnPropertySymbols;[m
[32m+[m
[32m+[m[32m  wrappedWellKnownSymbolModule.f = function (name) {[m
[32m+[m[32m    return wrap(wellKnownSymbol(name), name);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  if (DESCRIPTORS) {[m
[32m+[m[32m    // https://github.com/tc39/proposal-Symbol-description[m
[32m+[m[32m    nativeDefineProperty(SymbolPrototype, 'description', {[m
[32m+[m[32m      configurable: true,[m
[32m+[m[32m      get: function description() {[m
[32m+[m[32m        return getInternalState(this).description;[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m    if (!IS_PURE) {[m
[32m+[m[32m      redefine(ObjectPrototype, 'propertyIsEnumerable', $propertyIsEnumerable, { unsafe: true });[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m$({ global: true, wrap: true, forced: !NATIVE_SYMBOL, sham: !NATIVE_SYMBOL }, {[m
[32m+[m[32m  Symbol: $Symbol[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m$forEach(objectKeys(WellKnownSymbolsStore), function (name) {[m
[32m+[m[32m  defineWellKnownSymbol(name);[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m$({ target: SYMBOL, stat: true, forced: !NATIVE_SYMBOL }, {[m
[32m+[m[32m  // `Symbol.for` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-symbol.for[m
[32m+[m[32m  'for': function (key) {[m
[32m+[m[32m    var string = $toString(key);[m
[32m+[m[32m    if (hasOwn(StringToSymbolRegistry, string)) return StringToSymbolRegistry[string];[m
[32m+[m[32m    var symbol = $Symbol(string);[m
[32m+[m[32m    StringToSymbolRegistry[string] = symbol;[m
[32m+[m[32m    SymbolToStringRegistry[symbol] = string;[m
[32m+[m[32m    return symbol;[m
[32m+[m[32m  },[m
[32m+[m[32m  // `Symbol.keyFor` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-symbol.keyfor[m
[32m+[m[32m  keyFor: function keyFor(sym) {[m
[32m+[m[32m    if (!isSymbol(sym)) throw TypeError(sym + ' is not a symbol');[m
[32m+[m[32m    if (hasOwn(SymbolToStringRegistry, sym)) return SymbolToStringRegistry[sym];[m
[32m+[m[32m  },[m
[32m+[m[32m  useSetter: function () { USE_SETTER = true; },[m
[32m+[m[32m  useSimple: function () { USE_SETTER = false; }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m$({ target: 'Object', stat: true, forced: !NATIVE_SYMBOL, sham: !DESCRIPTORS }, {[m
[32m+[m[32m  // `Object.create` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-object.create[m
[32m+[m[32m  create: $create,[m
[32m+[m[32m  // `Object.defineProperty` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-object.defineproperty[m
[32m+[m[32m  defineProperty: $defineProperty,[m
[32m+[m[32m  // `Object.defineProperties` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-object.defineproperties[m
[32m+[m[32m  defineProperties: $defineProperties,[m
[32m+[m[32m  // `Object.getOwnPropertyDescriptor` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-object.getownpropertydescriptors[m
[32m+[m[32m  getOwnPropertyDescriptor: $getOwnPropertyDescriptor[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m$({ target: 'Object', stat: true, forced: !NATIVE_SYMBOL }, {[m
[32m+[m[32m  // `Object.getOwnPropertyNames` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-object.getownpropertynames[m
[32m+[m[32m  getOwnPropertyNames: $getOwnPropertyNames,[m
[32m+[m[32m  // `Object.getOwnPropertySymbols` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-object.getownpropertysymbols[m
[32m+[m[32m  getOwnPropertySymbols: $getOwnPropertySymbols[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Chrome 38 and 39 `Object.getOwnPropertySymbols` fails on primitives[m
[32m+[m[32m// https://bugs.chromium.org/p/v8/issues/detail?id=3443[m
[32m+[m[32m$({ target: 'Object', stat: true, forced: fails(function () { getOwnPropertySymbolsModule.f(1); }) }, {[m
[32m+[m[32m  getOwnPropertySymbols: function getOwnPropertySymbols(it) {[m
[32m+[m[32m    return getOwnPropertySymbolsModule.f(toObject(it));[m
[32m+[m[32m  }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// `JSON.stringify` method behavior with symbols[m
[32m+[m[32m// https://tc39.es/ecma262/#sec-json.stringify[m
[32m+[m[32mif ($stringify) {[m
[32m+[m[32m  var FORCED_JSON_STRINGIFY = !NATIVE_SYMBOL || fails(function () {[m
[32m+[m[32m    var symbol = $Symbol();[m
[32m+[m[32m    // MS Edge converts symbol values to JSON as {}[m
[32m+[m[32m    return $stringify([symbol]) != '[null]'[m
[32m+[m[32m      // WebKit converts symbol values to JSON as null[m
[32m+[m[32m      || $stringify({ a: symbol }) != '{}'[m
[32m+[m[32m      // V8 throws on boxed symbols[m
[32m+[m[32m      || $stringify(Object(symbol)) != '{}';[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  $({ target: 'JSON', stat: true, forced: FORCED_JSON_STRINGIFY }, {[m
[32m+[m[32m    // eslint-disable-next-line no-unused-vars -- required for `.length`[m
[32m+[m[32m    stringify: function stringify(it, replacer, space) {[m
[32m+[m[32m      var args = arraySlice(arguments);[m
[32m+[m[32m      var $replacer = replacer;[m
[32m+[m[32m      if (!isObject(replacer) && it === undefined || isSymbol(it)) return; // IE8 returns string on undefined[m
[32m+[m[32m      if (!isArray(replacer)) replacer = function (key, value) {[m
[32m+[m[32m        if (isCallable($replacer)) value = call($replacer, this, key, value);[m
[32m+[m[32m        if (!isSymbol(value)) return value;[m
[32m+[m[32m      };[m
[32m+[m[32m      args[1] = replacer;[m
[32m+[m[32m      return apply($stringify, null, args);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// `Symbol.prototype[@@toPrimitive]` method[m
[32m+[m[32m// https://tc39.es/ecma262/#sec-symbol.prototype-@@toprimitive[m
[32m+[m[32mif (!SymbolPrototype[TO_PRIMITIVE]) {[m
[32m+[m[32m  var valueOf = SymbolPrototype.valueOf;[m
[32m+[m[32m  // eslint-disable-next-line no-unused-vars -- required for .length[m
[32m+[m[32m  redefine(SymbolPrototype, TO_PRIMITIVE, function (hint) {[m
[32m+[m[32m    // TODO: improve hint logic[m
[32m+[m[32m    return call(valueOf, this);[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m[32m// `Symbol.prototype[@@toStringTag]` property[m
[32m+[m[32m// https://tc39.es/ecma262/#sec-symbol.prototype-@@tostringtag[m
[32m+[m[32msetToStringTag($Symbol, SYMBOL);[m
[32m+[m
[32m+[m[32mhiddenKeys[HIDDEN] = true;[m
