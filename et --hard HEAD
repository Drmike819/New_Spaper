[1mdiff --git a/staticfiles/admin/js/SelectBox.js b/staticfiles/admin/js/SelectBox.js[m
[1mnew file mode 100644[m
[1mindex 0000000..3db4ec7[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/SelectBox.js[m
[36m@@ -0,0 +1,116 @@[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    const SelectBox = {[m
[32m+[m[32m        cache: {},[m
[32m+[m[32m        init: function(id) {[m
[32m+[m[32m            const box = document.getElementById(id);[m
[32m+[m[32m            SelectBox.cache[id] = [];[m
[32m+[m[32m            const cache = SelectBox.cache[id];[m
[32m+[m[32m            for (const node of box.options) {[m
[32m+[m[32m                cache.push({value: node.value, text: node.text, displayed: 1});[m
[32m+[m[32m            }[m
[32m+[m[32m        },[m
[32m+[m[32m        redisplay: function(id) {[m
[32m+[m[32m            // Repopulate HTML select box from cache[m
[32m+[m[32m            const box = document.getElementById(id);[m
[32m+[m[32m            const scroll_value_from_top = box.scrollTop;[m
[32m+[m[32m            box.innerHTML = '';[m
[32m+[m[32m            for (const node of SelectBox.cache[id]) {[m
[32m+[m[32m                if (node.displayed) {[m
[32m+[m[32m                    const new_option = new Option(node.text, node.value, false, false);[m
[32m+[m[32m                    // Shows a tooltip when hovering over the option[m
[32m+[m[32m                    new_option.title = node.text;[m
[32m+[m[32m                    box.appendChild(new_option);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            box.scrollTop = scroll_value_from_top;[m
[32m+[m[32m        },[m
[32m+[m[32m        filter: function(id, text) {[m
[32m+[m[32m            // Redisplay the HTML select box, displaying only the choices containing ALL[m
[32m+[m[32m            // the words in text. (It's an AND search.)[m
[32m+[m[32m            const tokens = text.toLowerCase().split(/\s+/);[m
[32m+[m[32m            for (const node of SelectBox.cache[id]) {[m
[32m+[m[32m                node.displayed = 1;[m
[32m+[m[32m                const node_text = node.text.toLowerCase();[m
[32m+[m[32m                for (const token of tokens) {[m
[32m+[m[32m                    if (!node_text.includes(token)) {[m
[32m+[m[32m                        node.displayed = 0;[m
[32m+[m[32m                        break; // Once the first token isn't found we're done[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            SelectBox.redisplay(id);[m
[32m+[m[32m        },[m
[32m+[m[32m        get_hidden_node_count(id) {[m
[32m+[m[32m            const cache = SelectBox.cache[id] || [];[m
[32m+[m[32m            return cache.filter(node => node.displayed === 0).length;[m
[32m+[m[32m        },[m
[32m+[m[32m        delete_from_cache: function(id, value) {[m
[32m+[m[32m            let delete_index = null;[m
[32m+[m[32m            const cache = SelectBox.cache[id];[m
[32m+[m[32m            for (const [i, node] of cache.entries()) {[m
[32m+[m[32m                if (node.value === value) {[m
[32m+[m[32m                    delete_index = i;[m
[32m+[m[32m                    break;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            cache.splice(delete_index, 1);[m
[32m+[m[32m        },[m
[32m+[m[32m        add_to_cache: function(id, option) {[m
[32m+[m[32m            SelectBox.cache[id].push({value: option.value, text: option.text, displayed: 1});[m
[32m+[m[32m        },[m
[32m+[m[32m        cache_contains: function(id, value) {[m
[32m+[m[32m            // Check if an item is contained in the cache[m
[32m+[m[32m            for (const node of SelectBox.cache[id]) {[m
[32m+[m[32m                if (node.value === value) {[m
[32m+[m[32m                    return true;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            return false;[m
[32m+[m[32m        },[m
[32m+[m[32m        move: function(from, to) {[m
[32m+[m[32m            const from_box = document.getElementById(from);[m
[32m+[m[32m            for (const option of from_box.options) {[m
[32m+[m[32m                const option_value = option.value;[m
[32m+[m[32m                if (option.selected && SelectBox.cache_contains(from, option_value)) {[m
[32m+[m[32m                    SelectBox.add_to_cache(to, {value: option_value, text: option.text, displayed: 1});[m
[32m+[m[32m                    SelectBox.delete_from_cache(from, option_value);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            SelectBox.redisplay(from);[m
[32m+[m[32m            SelectBox.redisplay(to);[m
[32m+[m[32m        },[m
[32m+[m[32m        move_all: function(from, to) {[m
[32m+[m[32m            const from_box = document.getElementById(from);[m
[32m+[m[32m            for (const option of from_box.options) {[m
[32m+[m[32m                const option_value = option.value;[m
[32m+[m[32m                if (SelectBox.cache_contains(from, option_value)) {[m
[32m+[m[32m                    SelectBox.add_to_cache(to, {value: option_value, text: option.text, displayed: 1});[m
[32m+[m[32m                    SelectBox.delete_from_cache(from, option_value);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            SelectBox.redisplay(from);[m
[32m+[m[32m            SelectBox.redisplay(to);[m
[32m+[m[32m        },[m
[32m+[m[32m        sort: function(id) {[m
[32m+[m[32m            SelectBox.cache[id].sort(function(a, b) {[m
[32m+[m[32m                a = a.text.toLowerCase();[m
[32m+[m[32m                b = b.text.toLowerCase();[m
[32m+[m[32m                if (a > b) {[m
[32m+[m[32m                    return 1;[m
[32m+[m[32m                }[m
[32m+[m[32m                if (a < b) {[m
[32m+[m[32m                    return -1;[m
[32m+[m[32m                }[m
[32m+[m[32m                return 0;[m
[32m+[m[32m            } );[m
[32m+[m[32m        },[m
[32m+[m[32m        select_all: function(id) {[m
[32m+[m[32m            const box = document.getElementById(id);[m
[32m+[m[32m            for (const option of box.options) {[m
[32m+[m[32m                option.selected = true;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m[32m    window.SelectBox = SelectBox;[m
[32m+[m[32m}[m
[1mdiff --git a/staticfiles/admin/js/SelectFilter2.js b/staticfiles/admin/js/SelectFilter2.js[m
[1mnew file mode 100644[m
[1mindex 0000000..fc59eba[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/SelectFilter2.js[m
[36m@@ -0,0 +1,286 @@[m
[32m+[m[32m/*global SelectBox, gettext, interpolate, quickElement, SelectFilter*/[m
[32m+[m[32m/*[m
[32m+[m[32mSelectFilter2 - Turns a multiple-select box into a filter interface.[m
[32m+[m
[32m+[m[32mRequires core.js and SelectBox.js.[m
[32m+[m[32m*/[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    window.SelectFilter = {[m
[32m+[m[32m        init: function(field_id, field_name, is_stacked) {[m
[32m+[m[32m            if (field_id.match(/__prefix__/)) {[m
[32m+[m[32m                // Don't initialize on empty forms.[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m            const from_box = document.getElementById(field_id);[m
[32m+[m[32m            from_box.id += '_from'; // change its ID[m
[32m+[m[32m            from_box.className = 'filtered';[m
[32m+[m
[32m+[m[32m            for (const p of from_box.parentNode.getElementsByTagName('p')) {[m
[32m+[m[32m                if (p.classList.contains("info")) {[m
[32m+[m[32m                    // Remove <p class="info">, because it just gets in the way.[m
[32m+[m[32m                    from_box.parentNode.removeChild(p);[m
[32m+[m[32m                } else if (p.classList.contains("help")) {[m
[32m+[m[32m                    // Move help text up to the top so it isn't below the select[m
[32m+[m[32m                    // boxes or wrapped off on the side to the right of the add[m
[32m+[m[32m                    // button:[m
[32m+[m[32m                    from_box.parentNode.insertBefore(p, from_box.parentNode.firstChild);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // <div class="selector"> or <div class="selector stacked">[m
[32m+[m[32m            const selector_div = quickElement('div', from_box.parentNode);[m
[32m+[m[32m            // Make sure the selector div is at the beginning so that the[m
[32m+[m[32m            // add link would be displayed to the right of the widget.[m
[32m+[m[32m            from_box.parentNode.prepend(selector_div);[m
[32m+[m[32m            selector_div.className = is_stacked ? 'selector stacked' : 'selector';[m
[32m+[m
[32m+[m[32m            // <div class="selector-available">[m
[32m+[m[32m            const selector_available = quickElement('div', selector_div);[m
[32m+[m[32m            selector_available.className = 'selector-available';[m
[32m+[m[32m            const title_available = quickElement('h2', selector_available, interpolate(gettext('Available %s') + ' ', [field_name]));[m
[32m+[m[32m            quickElement([m
[32m+[m[32m                'span', title_available, '',[m
[32m+[m[32m                'class', 'help help-tooltip help-icon',[m
[32m+[m[32m                'title', interpolate([m
[32m+[m[32m                    gettext([m
[32m+[m[32m                        'This is the list of available %s. You may choose some by ' +[m
[32m+[m[32m                        'selecting them in the box below and then clicking the ' +[m
[32m+[m[32m                        '"Choose" arrow between the two boxes.'[m
[32m+[m[32m                    ),[m
[32m+[m[32m                    [field_name][m
[32m+[m[32m                )[m
[32m+[m[32m            );[m
[32m+[m
[32m+[m[32m            const filter_p = quickElement('p', selector_available, '', 'id', field_id + '_filter');[m
[32m+[m[32m            filter_p.className = 'selector-filter';[m
[32m+[m
[32m+[m[32m            const search_filter_label = quickElement('label', filter_p, '', 'for', field_id + '_input');[m
[32m+[m
[32m+[m[32m            quickElement([m
[32m+[m[32m                'span', search_filter_label, '',[m
[32m+[m[32m                'class', 'help-tooltip search-label-icon',[m
[32m+[m[32m                'title', interpolate(gettext("Type into this box to filter down the list of available %s."), [field_name])[m
[32m+[m[32m            );[m
[32m+[m
[32m+[m[32m            filter_p.appendChild(document.createTextNode(' '));[m
[32m+[m
[32m+[m[32m            const filter_input = quickElement('input', filter_p, '', 'type', 'text', 'placeholder', gettext("Filter"));[m
[32m+[m[32m            filter_input.id = field_id + '_input';[m
[32m+[m
[32m+[m[32m            selector_available.appendChild(from_box);[m
[32m+[m[32m            const choose_all = quickElement('a', selector_available, gettext('Choose all'), 'title', interpolate(gettext('Click to choose all %s at once.'), [field_name]), 'href', '#', 'id', field_id + '_add_all_link');[m
[32m+[m[32m            choose_all.className = 'selector-chooseall';[m
[32m+[m
[32m+[m[32m            // <ul class="selector-chooser">[m
[32m+[m[32m            const selector_chooser = quickElement('ul', selector_div);[m
[32m+[m[32m            selector_chooser.className = 'selector-chooser';[m
[32m+[m[32m            const add_link = quickElement('a', quickElement('li', selector_chooser), gettext('Choose'), 'title', gettext('Choose'), 'href', '#', 'id', field_id + '_add_link');[m
[32m+[m[32m            add_link.className = 'selector-add';[m
[32m+[m[32m            const remove_link = quickElement('a', quickElement('li', selector_chooser), gettext('Remove'), 'title', gettext('Remove'), 'href', '#', 'id', field_id + '_remove_link');[m
[32m+[m[32m            remove_link.className = 'selector-remove';[m
[32m+[m
[32m+[m[32m            // <div class="selector-chosen">[m
[32m+[m[32m            const selector_chosen = quickElement('div', selector_div, '', 'id', field_id + '_selector_chosen');[m
[32m+[m[32m            selector_chosen.className = 'selector-chosen';[m
[32m+[m[32m            const title_chosen = quickElement('h2', selector_chosen, interpolate(gettext('Chosen %s') + ' ', [field_name]));[m
[32m+[m[32m            quickElement([m
[32m+[m[32m                'span', title_chosen, '',[m
[32m+[m[32m                'class', 'help help-tooltip help-icon',[m
[32m+[m[32m                'title', interpolate([m
[32m+[m[32m                    gettext([m
[32m+[m[32m                        'This is the list of chosen %s. You may remove some by ' +[m
[32m+[m[32m                        'selecting them in the box below and then clicking the ' +[m
[32m+[m[32m                        '"Remove" arrow between the two boxes.'[m
[32m+[m[32m                    ),[m
[32m+[m[32m                    [field_name][m
[32m+[m[32m                )[m
[32m+[m[32m            );[m
[32m+[m[41m            [m
[32m+[m[32m            const filter_selected_p = quickElement('p', selector_chosen, '', 'id', field_id + '_filter_selected');[m
[32m+[m[32m            filter_selected_p.className = 'selector-filter';[m
[32m+[m
[32m+[m[32m            const search_filter_selected_label = quickElement('label', filter_selected_p, '', 'for', field_id + '_selected_input');[m
[32m+[m
[32m+[m[32m            quickElement([m
[32m+[m[32m                'span', search_filter_selected_label, '',[m
[32m+[m[32m                'class', 'help-tooltip search-label-icon',[m
[32m+[m[32m                'title', interpolate(gettext("Type into this box to filter down the list of selected %s."), [field_name])[m
[32m+[m[32m            );[m
[32m+[m
[32m+[m[32m            filter_selected_p.appendChild(document.createTextNode(' '));[m
[32m+[m
[32m+[m[32m            const filter_selected_input = quickElement('input', filter_selected_p, '', 'type', 'text', 'placeholder', gettext("Filter"));[m
[32m+[m[32m            filter_selected_input.id = field_id + '_selected_input';[m
[32m+[m
[32m+[m[32m            const to_box = quickElement('select', selector_chosen, '', 'id', field_id + '_to', 'multiple', '', 'size', from_box.size, 'name', from_box.name);[m
[32m+[m[32m            to_box.className = 'filtered';[m
[32m+[m[41m            [m
[32m+[m[32m            const warning_footer = quickElement('div', selector_chosen, '', 'class', 'list-footer-display');[m
[32m+[m[32m            quickElement('span', warning_footer, '', 'id', field_id + '_list-footer-display-text');[m
[32m+[m[32m            quickElement('span', warning_footer, ' (click to clear)', 'class', 'list-footer-display__clear');[m
[32m+[m[41m            [m
[32m+[m[32m            const clear_all = quickElement('a', selector_chosen, gettext('Remove all'), 'title', interpolate(gettext('Click to remove all chosen %s at once.'), [field_name]), 'href', '#', 'id', field_id + '_remove_all_link');[m
[32m+[m[32m            clear_all.className = 'selector-clearall';[m
[32m+[m
[32m+[m[32m            from_box.name = from_box.name + '_old';[m
[32m+[m
[32m+[m[32m            // Set up the JavaScript event handlers for the select box filter interface[m
[32m+[m[32m            const move_selection = function(e, elem, move_func, from, to) {[m
[32m+[m[32m                if (elem.classList.contains('active')) {[m
[32m+[m[32m                    move_func(from, to);[m
[32m+[m[32m                    SelectFilter.refresh_icons(field_id);[m
[32m+[m[32m                    SelectFilter.refresh_filtered_selects(field_id);[m
[32m+[m[32m                    SelectFilter.refresh_filtered_warning(field_id);[m
[32m+[m[32m                }[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[32m            };[m
[32m+[m[32m            choose_all.addEventListener('click', function(e) {[m
[32m+[m[32m                move_selection(e, this, SelectBox.move_all, field_id + '_from', field_id + '_to');[m
[32m+[m[32m            });[m
[32m+[m[32m            add_link.addEventListener('click', function(e) {[m
[32m+[m[32m                move_selection(e, this, SelectBox.move, field_id + '_from', field_id + '_to');[m
[32m+[m[32m            });[m
[32m+[m[32m            remove_link.addEventListener('click', function(e) {[m
[32m+[m[32m                move_selection(e, this, SelectBox.move, field_id + '_to', field_id + '_from');[m
[32m+[m[32m            });[m
[32m+[m[32m            clear_all.addEventListener('click', function(e) {[m
[32m+[m[32m                move_selection(e, this, SelectBox.move_all, field_id + '_to', field_id + '_from');[m
[32m+[m[32m            });[m
[32m+[m[32m            warning_footer.addEventListener('click', function(e) {[m
[32m+[m[32m                filter_selected_input.value = '';[m
[32m+[m[32m                SelectBox.filter(field_id + '_to', '');[m
[32m+[m[32m                SelectFilter.refresh_filtered_warning(field_id);[m
[32m+[m[32m                SelectFilter.refresh_icons(field_id);[m
[32m+[m[32m            });[m
[32m+[m[32m            filter_input.addEventListener('keypress', function(e) {[m
[32m+[m[32m                SelectFilter.filter_key_press(e, field_id, '_from', '_to');[m
[32m+[m[32m            });[m
[32m+[m[32m            filter_input.addEventListener('keyup', function(e) {[m
[32m+[m[32m                SelectFilter.filter_key_up(e, field_id, '_from');[m
[32m+[m[32m            });[m
[32m+[m[32m            filter_input.addEventListener('keydown', function(e) {[m
[32m+[m[32m                SelectFilter.filter_key_down(e, field_id, '_from', '_to');[m
[32m+[m[32m            });[m
[32m+[m[32m            filter_selected_input.addEventListener('keypress', function(e) {[m
[32m+[m[32m                SelectFilter.filter_key_press(e, field_id, '_to', '_from');[m
[32m+[m[32m            });[m
[32m+[m[32m            filter_selected_input.addEventListener('keyup', function(e) {[m
[32m+[m[32m                SelectFilter.filter_key_up(e, field_id, '_to', '_selected_input');[m
[32m+[m[32m            });[m
[32m+[m[32m            filter_selected_input.addEventListener('keydown', function(e) {[m
[32m+[m[32m                SelectFilter.filter_key_down(e, field_id, '_to', '_from');[m
[32m+[m[32m            });[m
[32m+[m[32m            selector_div.addEventListener('change', function(e) {[m
[32m+[m[32m                if (e.target.tagName === 'SELECT') {[m
[32m+[m[32m                    SelectFilter.refresh_icons(field_id);[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m            selector_div.addEventListener('dblclick', function(e) {[m
[32m+[m[32m                if (e.target.tagName === 'OPTION') {[m
[32m+[m[32m                    if (e.target.closest('select').id === field_id + '_to') {[m
[32m+[m[32m                        SelectBox.move(field_id + '_to', field_id + '_from');[m
[32m+[m[32m                    } else {[m
[32m+[m[32m                        SelectBox.move(field_id + '_from', field_id + '_to');[m
[32m+[m[32m                    }[m
[32m+[m[32m                    SelectFilter.refresh_icons(field_id);[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m            from_box.closest('form').addEventListener('submit', function() {[m
[32m+[m[32m                SelectBox.filter(field_id + '_to', '');[m
[32m+[m[32m                SelectBox.select_all(field_id + '_to');[m
[32m+[m[32m            });[m
[32m+[m[32m            SelectBox.init(field_id + '_from');[m
[32m+[m[32m            SelectBox.init(field_id + '_to');[m
[32m+[m[32m            // Move selected from_box options to to_box[m
[32m+[m[32m            SelectBox.move(field_id + '_from', field_id + '_to');[m
[32m+[m
[32m+[m[32m            // Initial icon refresh[m
[32m+[m[32m            SelectFilter.refresh_icons(field_id);[m
[32m+[m[32m        },[m
[32m+[m[32m        any_selected: function(field) {[m
[32m+[m[32m            // Temporarily add the required attribute and check validity.[m
[32m+[m[32m            field.required = true;[m
[32m+[m[32m            const any_selected = field.checkValidity();[m
[32m+[m[32m            field.required = false;[m
[32m+[m[32m            return any_selected;[m
[32m+[m[32m        },[m
[32m+[m[32m        refresh_filtered_warning: function(field_id) {[m
[32m+[m[32m            const count = SelectBox.get_hidden_node_count(field_id + '_to');[m
[32m+[m[32m            const selector = document.getElementById(field_id + '_selector_chosen');[m
[32m+[m[32m            const warning = document.getElementById(field_id + '_list-footer-display-text');[m
[32m+[m[32m            selector.className = selector.className.replace('selector-chosen--with-filtered', '');[m
[32m+[m[32m            warning.textContent = interpolate(ngettext([m
[32m+[m[32m                '%s selected option not visible',[m
[32m+[m[32m                '%s selected options not visible',[m
[32m+[m[32m                count[m
[32m+[m[32m            ), [count]);[m
[32m+[m[32m            if(count > 0) {[m
[32m+[m[32m                selector.className += ' selector-chosen--with-filtered';[m
[32m+[m[32m            }[m
[32m+[m[32m        },[m
[32m+[m[32m        refresh_filtered_selects: function(field_id) {[m
[32m+[m[32m            SelectBox.filter(field_id + '_from', document.getElementById(field_id + "_input").value);[m
[32m+[m[32m            SelectBox.filter(field_id + '_to', document.getElementById(field_id + "_selected_input").value);[m
[32m+[m[32m        },[m
[32m+[m[32m        refresh_icons: function(field_id) {[m
[32m+[m[32m            const from = document.getElementById(field_id + '_from');[m
[32m+[m[32m            const to = document.getElementById(field_id + '_to');[m
[32m+[m[32m            // Active if at least one item is selected[m
[32m+[m[32m            document.getElementById(field_id + '_add_link').classList.toggle('active', SelectFilter.any_selected(from));[m
[32m+[m[32m            document.getElementById(field_id + '_remove_link').classList.toggle('active', SelectFilter.any_selected(to));[m
[32m+[m[32m            // Active if the corresponding box isn't empty[m
[32m+[m[32m            document.getElementById(field_id + '_add_all_link').classList.toggle('active', from.querySelector('option'));[m
[32m+[m[32m            document.getElementById(field_id + '_remove_all_link').classList.toggle('active', to.querySelector('option'));[m
[32m+[m[32m            SelectFilter.refresh_filtered_warning(field_id);[m
[32m+[m[32m        },[m
[32m+[m[32m        filter_key_press: function(event, field_id, source, target) {[m
[32m+[m[32m            const source_box = document.getElementById(field_id + source);[m
[32m+[m[32m            // don't submit form if user pressed Enter[m
[32m+[m[32m            if ((event.which && event.which === 13) || (event.keyCode && event.keyCode === 13)) {[m
[32m+[m[32m                source_box.selectedIndex = 0;[m
[32m+[m[32m                SelectBox.move(field_id + source, field_id + target);[m
[32m+[m[32m                source_box.selectedIndex = 0;[m
[32m+[m[32m                event.preventDefault();[m
[32m+[m[32m            }[m
[32m+[m[32m        },[m
[32m+[m[32m        filter_key_up: function(event, field_id, source, filter_input) {[m
[32m+[m[32m            const input = filter_input || '_input';[m
[32m+[m[32m            const source_box = document.getElementById(field_id + source);[m
[32m+[m[32m            const temp = source_box.selectedIndex;[m
[32m+[m[32m            SelectBox.filter(field_id + source, document.getElementById(field_id + input).value);[m
[32m+[m[32m            source_box.selectedIndex = temp;[m
[32m+[m[32m            SelectFilter.refresh_filtered_warning(field_id);[m
[32m+[m[32m            SelectFilter.refresh_icons(field_id);[m
[32m+[m[32m        },[m
[32m+[m[32m        filter_key_down: function(event, field_id, source, target) {[m
[32m+[m[32m            const source_box = document.getElementById(field_id + source);[m
[32m+[m[32m            // right key (39) or left key (37)[m
[32m+[m[32m            const direction = source === '_from' ? 39 : 37;[m
[32m+[m[32m            // right arrow -- move across[m
[32m+[m[32m            if ((event.which && event.which === direction) || (event.keyCode && event.keyCode === direction)) {[m
[32m+[m[32m                const old_index = source_box.selectedIndex;[m
[32m+[m[32m                SelectBox.move(field_id + source, field_id + target);[m
[32m+[m[32m                SelectFilter.refresh_filtered_selects(field_id);[m
[32m+[m[32m                SelectFilter.refresh_filtered_warning(field_id);[m
[32m+[m[32m                source_box.selectedIndex = (old_index === source_box.length) ? source_box.length - 1 : old_index;[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m            // down arrow -- wrap around[m
[32m+[m[32m            if ((event.which && event.which === 40) || (event.keyCode && event.keyCode === 40)) {[m
[32m+[m[32m                source_box.selectedIndex = (source_box.length === source_box.selectedIndex + 1) ? 0 : source_box.selectedIndex + 1;[m
[32m+[m[32m            }[m
[32m+[m[32m            // up arrow -- wrap around[m
[32m+[m[32m            if ((event.which && event.which === 38) || (event.keyCode && event.keyCode === 38)) {[m
[32m+[m[32m                source_box.selectedIndex = (source_box.selectedIndex === 0) ? source_box.length - 1 : source_box.selectedIndex - 1;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    window.addEventListener('load', function(e) {[m
[32m+[m[32m        document.querySelectorAll('select.selectfilter, select.selectfilterstacked').forEach(function(el) {[m
[32m+[m[32m            const data = el.dataset;[m
[32m+[m[32m            SelectFilter.init(el.id, data.fieldName, parseInt(data.isStacked, 10));[m
[32m+[m[32m        });[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
