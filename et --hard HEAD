[32m+[m[32m    result = IS_CONSTRUCTOR ? new this(length) : Array(length);[m
[32m+[m[32m    for (;length > index; index++) {[m
[32m+[m[32m      value = mapping ? mapfn(O[index], index) : O[index];[m
[32m+[m[32m      createProperty(result, index, value);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  result.length = index;[m
[32m+[m[32m  return result;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m},{"../internals/call-with-safe-iteration-closing":72,"../internals/create-property":80,"../internals/function-bind-context":96,"../internals/function-call":97,"../internals/get-iterator":102,"../internals/get-iterator-method":101,"../internals/global":104,"../internals/is-array-iterator-method":112,"../internals/is-constructor":115,"../internals/length-of-array-like":123,"../internals/to-object":157}],63:[function(require,module,exports){[m
[32m+[m[32mvar toIndexedObject = require('../internals/to-indexed-object');[m
[32m+[m[32mvar toAbsoluteIndex = require('../internals/to-absolute-index');[m
[32m+[m[32mvar lengthOfArrayLike = require('../internals/length-of-array-like');[m
[32m+[m
[32m+[m[32m// `Array.prototype.{ indexOf, includes }` methods implementation[m
[32m+[m[32mvar createMethod = function (IS_INCLUDES) {[m
[32m+[m[32m  return function ($this, el, fromIndex) {[m
[32m+[m[32m    var O = toIndexedObject($this);[m
[32m+[m[32m    var length = lengthOfArrayLike(O);[m
[32m+[m[32m    var index = toAbsoluteIndex(fromIndex, length);[m
[32m+[m[32m    var value;[m
[32m+[m[32m    // Array#includes uses SameValueZero equality algorithm[m
[32m+[m[32m    // eslint-disable-next-line no-self-compare -- NaN check[m
[32m+[m[32m    if (IS_INCLUDES && el != el) while (length > index) {[m
[32m+[m[32m      value = O[index++];[m
[32m+[m[32m      // eslint-disable-next-line no-self-compare -- NaN check[m
[32m+[m[32m      if (value != value) return true;[m
[32m+[m[32m    // Array#indexOf ignores holes, Array#includes - not[m
[32m+[m[32m    } else for (;length > index; index++) {[m
[32m+[m[32m      if ((IS_INCLUDES || index in O) && O[index] === el) return IS_INCLUDES || index || 0;[m
[32m+[m[32m    } return !IS_INCLUDES && -1;[m
[32m+[m[32m  };[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mmodule.exports = {[m
[32m+[m[32m  // `Array.prototype.includes` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-array.prototype.includes[m
[32m+[m[32m  includes: createMethod(true),[m
[32m+[m[32m  // `Array.prototype.indexOf` method[m
[32m+[m[32m  // https://tc39.es/ecma262/#sec-array.prototype.indexof[m
[32m+[m[32m  indexOf: createMethod(false)[m
[32m+[m[32m};[m
