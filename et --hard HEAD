[1mdiff --git a/staticfiles/admin/js/prepopulate.js b/staticfiles/admin/js/prepopulate.js[m
[1mnew file mode 100644[m
[1mindex 0000000..89e95ab[m
[1m--- /dev/null[m
[1m+++ b/staticfiles/admin/js/prepopulate.js[m
[36m@@ -0,0 +1,43 @@[m
[32m+[m[32m/*global URLify*/[m
[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    const $ = django.jQuery;[m
[32m+[m[32m    $.fn.prepopulate = function(dependencies, maxLength, allowUnicode) {[m
[32m+[m[32m        /*[m
[32m+[m[32m            Depends on urlify.js[m
[32m+[m[32m            Populates a selected field with the values of the dependent fields,[m
[32m+[m[32m            URLifies and shortens the string.[m
[32m+[m[32m            dependencies - array of dependent fields ids[m
[32m+[m[32m            maxLength - maximum length of the URLify'd string[m
[32m+[m[32m            allowUnicode - Unicode support of the URLify'd string[m
[32m+[m[32m        */[m
[32m+[m[32m        return this.each(function() {[m
[32m+[m[32m            const prepopulatedField = $(this);[m
[32m+[m
[32m+[m[32m            const populate = function() {[m
[32m+[m[32m                // Bail if the field's value has been changed by the user[m
[32m+[m[32m                if (prepopulatedField.data('_changed')) {[m
[32m+[m[32m                    return;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                const values = [];[m
[32m+[m[32m                $.each(dependencies, function(i, field) {[m
[32m+[m[32m                    field = $(field);[m
[32m+[m[32m                    if (field.val().length > 0) {[m
[32m+[m[32m                        values.push(field.val());[m
[32m+[m[32m                    }[m
[32m+[m[32m                });[m
[32m+[m[32m                prepopulatedField.val(URLify(values.join(' '), maxLength, allowUnicode));[m
[32m+[m[32m            };[m
[32m+[m
[32m+[m[32m            prepopulatedField.data('_changed', false);[m
[32m+[m[32m            prepopulatedField.on('change', function() {[m
[32m+[m[32m                prepopulatedField.data('_changed', true);[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            if (!prepopulatedField.val()) {[m
[32m+[m[32m                $(dependencies.join(',')).on('keyup change focus', populate);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    };[m
[32m+[m[32m}[m
