[1m+++ b/static/js/script.js[m
[36m@@ -0,0 +1,24 @@[m
[32m+[m[32mdocument.addEventListener('DOMContentLoaded', function() {[m
[32m+[m[32m    // Comprobar si el modo oscuro est√° guardado en las cookies[m
[32m+[m[32m    const cookies = document.cookie.split('; ');[m
[32m+[m[32m    const darkModeCookie = cookies.find(cookie => cookie.startsWith('dark_mode='));[m
[32m+[m
[32m+[m[32m    if (darkModeCookie) {[m
[32m+[m[32m        const isDarkMode = darkModeCookie.split('=')[1] === 'true';[m
[32m+[m[32m        if (isDarkMode) {[m
[32m+[m[32m            document.body.classList.add('dark-mode');[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Alternar el modo oscuro cuando el bot√≥n es clickeado[m
[32m+[m[32m    const toggleButton = document.getElementById('modoJH');[m
[32m+[m[32m    if (toggleButton) {[m
[32m+[m[32m        toggleButton.addEventListener('click', () => {[m
[32m+[m[32m            document.body.classList.toggle('dark-mode');[m
[32m+[m
[32m+[m[32m            // Guardar la preferencia en las cookies[m
[32m+[m[32m            const isDarkMode = document.body.classList.contains('dark-mode');[m
[32m+[m[32m            document.cookie = `dark_mode=${isDarkMode}; path=/; max-age=31536000`; // Expira en 1 a√±o[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
