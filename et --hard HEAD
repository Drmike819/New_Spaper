[1mdiff --git a/articles/views.py b/articles/views.py[m
[1mnew file mode 100644[m
[1mindex 0000000..9bc40d5[m
[1m--- /dev/null[m
[1m+++ b/articles/views.py[m
[36m@@ -0,0 +1,187 @@[m
[32m+[m[32mfrom django.contrib.auth.mixins import LoginRequiredMixin, UserPassesTestMixin[m
[32m+[m[32mfrom django.views import View[m
[32m+[m[32mfrom django.shortcuts import render, redirect, get_object_or_404[m
[32m+[m[32mfrom django.views.generic import ListView, DetailView, FormView[m
[32m+[m[32mfrom django.views.generic.detail import SingleObjectMixin[m
[32m+[m[32mfrom django.views.generic.edit import UpdateView, DeleteView, CreateView[m
[32m+[m[32mfrom django.urls import reverse_lazy, reverse[m
[32m+[m[32mfrom .models import Article, Comment[m
[32m+[m[32mfrom .forms import CommentForm, ArticleImageFormSet, ArticleForm[m
[32m+[m
[32m+[m[32m# Create your views here.[m
[32m+[m
[32m+[m[32m# Vista para manejar la obtenci√≥n de un art√≠culo con el formulario de comentarios[m
[32m+[m[32mclass CommentGet(LoginRequiredMixin, DeleteView):[m
[32m+[m[32m    model = Article[m
[32m+[m[32m    template_name ="home.html"[m
[32m+[m[32m    # Se pasa el formulario de comentarios al contexto[m
[32m+[m[32m    def get_context_data(self, **Kwargs):[m
[32m+[m[32m        context = super().get_context_data(**Kwargs)[m
[32m+[m[32m        context['form'] = CommentForm()[m
[32m+[m[32m        return context[m
[32m+[m
[32m+[m[32m# Vista para manejar el env√≠o del formulario de comentarios[m
[32m+[m[32m# Corregido: Se hereda de `FormView` y `SingleObjectMixin` para tener acceso a `get_object()`[m[41m       [m
[32m+[m[32mclass CommentPost(LoginRequiredMixin, SingleObjectMixin, FormView):[m
[32m+[m[32m    model = Article[m
[32m+[m[32m    form_class = CommentForm[m
[32m+[m[32m    template_name ="home.html"[m
[32m+[m
[32m+[m[32m    def post(self, request, *args, **kwargs):[m
[32m+[m[32m        # Corregido: Usamos `self.get_object()` para obtener el art√≠culo relacionado[m
[32m+[m[32m        self.object = self.get_object(queryset=Article.objects.all())[m
[32m+[m[32m        return super().post(request, *args, **kwargs)[m
[32m+[m[41m    [m
[32m+[m[32m    def form_valid(self, form):[m
[32m+[m[32m        # Guardamos el comentario sin comprometerlo a la base de datos a√∫n[m
[32m+[m[32m        comment = form.save(commit=False)[m
[32m+[m[32m        # Asignamos el art√≠culo al comentario[m
[32m+[m[32m        comment.article = self.object[m
[32m+[m[32m        comment.author = self.request.user[m
[32m+[m[32m        # Guardamos el comentario[m
[32m+[m[32m        comment.save()[m
[32m+[m[32m        return super().form_valid(form)[m
[32m+[m[41m    [m
[32m+[m[32m    def get_success_url(self):[m
[32m+[m[32m        # Redirige de nuevo al detalle del art√≠culo despu√©s de guardar el comentario[m
[32m+[m[32m        # return reverse('article_detail', kwargs={'pk': self.object.pk})[m
[32m+[m[32m        return reverse('home')[m
[32m+[m
[32m+[m[32m# Vista que combina las operaciones GET y POST para el detalle de un art√≠culo y la creaci√≥n de comentarios[m[41m      [m
[32m+[m[32mclass ArticleDetailView(LoginRequiredMixin, View):[m
[32m+[m[32m    def get(self, request, *args, **kwargs):[m
[32m+[m[32m        # Llamamos a la vista CommentGet para manejar la vista de detalles del art√≠culo[m
[32m+[m[32m        view = CommentGet.as_view()[m
[32m+[m[32m        return view(request, *args, **kwargs)[m
[32m+[m[32m     # Llamamos a la vista CommentPost para manejar la creaci√≥n de un comentario[m
[32m+[m[32m    def post(self, request, *args, **kwargs):[m
[32m+[m[32m        view = CommentPost.as_view()[m
[32m+[m[32m        return view(request, *args, **kwargs)[m
[32m+[m
[32m+[m[32m# Vista para actualizar un art√≠culo[m[41m        [m
[32m+[m[32mclass ArticleUpdateView(LoginRequiredMixin, UpdateView):[m
[32m+[m[32m    # indica el modelo[m
[32m+[m[32m    model = Article[m
[32m+[m[32m    # formulario principal[m
[32m+[m[32m    form_class = ArticleForm[m
[32m+[m[32m    # template que se utilizara para esta vista[m
[32m+[m[32m    template_name = "article_edit.html"[m
[32m+[m
[32m+[m[32m    # funcion que define el contexto(crear o editar)[m
[32m+[m[32m    def get_context_data(self, **kwargs):[m
[32m+[m[32m        # llama a la clase padre para obtner el contexto (ArticleUpdateView ArticleCreateView)[m
[32m+[m[32m        data = super().get_context_data(**kwargs)[m
[32m+[m[32m        # verifica si el metodo es HTTP POST lo cual asegura si el usuario esta enviando datos[m
[32m+[m[32m        if self.request.POST:[m
[32m+[m[32m            # crea un formulario de las imagenes y le paso datos envias como archivos enviados[m
[32m+[m[32m            data['formset'] = ArticleImageFormSet(self.request.POST, self.request.FILES, instance=self.object)[m
[32m+[m[32m        else:[m
[32m+[m[32m            # si no es POST crea un formulario vacio para que el usuario suba sus imagenes[m
[32m+[m[32m            data['formset'] = ArticleImageFormSet(instance=self.object)[m
[32m+[m[32m        # devuelve el contexto y el formulario de las imagenes[m
[32m+[m[32m        return data[m
[32m+[m[32m    # metodo llamado cuando el formulario es valido[m
[32m+[m[32m    def form_valid(self, form):[m
[32m+[m[32m        # llamamos al contexto actual[m
[32m+[m[32m        context = self.get_context_data()[m
[32m+[m[32m        # se extrae el formulario del contexto[m
[32m+[m[32m        formset = context['formset'][m
[32m+[m[32m        # se verifica que si los formularios son validos[m
[32m+[m[32m        if form.is_valid() and formset.is_valid():[m
[32m+[m[32m            # Guarda el formulario principal en la base de datos[m
[32m+[m[32m            self.object = form.save()[m
[32m+[m[32m            # se asigna el formulario a la instanci[m
[32m+[m[32m            formset.instance = self.object[m
[32m+[m[32m            # se guarda el formulario[m
[32m+[m[32m            formset.save()[m
[32m+[m[32m            return redirect('home')  # Redirige al home despu√©s de una actualizaci√≥n exitosa[m
[32m+[m[32m        else:[m
[32m+[m[32m            # si no es valido se llama al meto el cual puede manejar errores[m
[32m+[m[32m            return self.form_invalid(form)[m
[32m+[m
[32m+[m[32m    def get_success_url(self):[m
[32m+[m[32m        # No es necesario definir un m√©todo get_success_url si est√°s usando 'redirect' en form_valid[m
[32m+[m[32m        return reverse_lazy('home')  # Esto redirige al home si se usara el m√©todo 'get_success_url'[m
[32m+[m
[32m+[m[32m# Vista para eliminar un art√≠culo[m
[32m+[m[32mclass ArticleDeleteView(LoginRequiredMixin, DeleteView):[m
[32m+[m[32m    model = Article[m
[32m+[m[32m    template_name = "article_delete.html"[m
[32m+[m[32m    success_url = reverse_lazy("home")[m
[32m+[m[32m    # Verificaci√≥n para asegurarse de que solo el autor pueda eliminar[m
[32m+[m[32m    def test_func(self):[m
[32m+[m[32m        obj = self.get_object()[m
[32m+[m[32m        return obj.author == self.request.user[m
[32m+[m
[32m+[m[32m# Vista para crear un nuevo art√≠culo[m
[32m+[m[32mclass ArticleCreateView(LoginRequiredMixin, CreateView):[m
[32m+[m[32m    # indicamos el modelo que se utilizara[m
[32m+[m[32m    model = Article[m
[32m+[m[32m    # indicamos el template el cual se utilizara[m
[32m+[m[32m    template_name = 'article_new.html'[m
[32m+[m[32m    # indicamos el formulario que vamos a utilizar[m
[32m+[m[32m    form_class = ArticleForm[m
[32m+[m
[32m+[m[32m    # funcion que define el contexto(crear o editar)[m
[32m+[m[32m    def get_context_data(self, **kwargs):[m
[32m+[m[32m        # llama a la clase padre para obtner el contexto (ArticleUpdateView ArticleCreateView)[m
[32m+[m[32m        data = super().get_context_data(**kwargs)[m
[32m+[m[32m        # verifica que el metodo sea POST[m
[32m+[m[32m        if self.request.POST:[m
[32m+[m[32m            # si es POST Crea un formulario de las imagenes y le paso datos envias como archivos enviados[m
[32m+[m[32m            data['formset'] = ArticleImageFormSet(self.request.POST, self.request.FILES)[m
[32m+[m[32m        else:[m
[32m+[m[32m            # si no es POST crea un formulario vacio para que el usuario suba sus imagenes[m
[32m+[m[32m            data['formset'] = ArticleImageFormSet()[m
[32m+[m[32m        return data[m
[32m+[m
[32m+[m[32m    # se llama la funcion cuando el formulario es valido[m
[32m+[m[32m    def form_valid(self, form):[m
[32m+[m[32m        # se asigna el author al formulario[m
[32m+[m[32m        form.instance.author = self.request.user[m
[32m+[m[32m        # llama a el contexto[m[41m [m
[32m+[m[32m        context = self.get_context_data()[m
[32m+[m[32m        #Extrae el formset del contexto[m
[32m+[m[32m        formset = context['formset'][m
[32m+[m[32m        # verifica si el formulario es valido[m
[32m+[m[32m        if form.is_valid() and formset.is_valid():[m
[32m+[m[32m            # Guarda el formulario principal en la base de datos[m
[32m+[m[32m            self.object = form.save()[m
[32m+[m[32m            #Asigna el objeto guardado (self.object) como la instancia relacionada para cada formulario[m
[32m+[m[32m            formset.instance = self.object[m
[32m+[m[32m            # Guarda el formset en la base de datos[m
[32m+[m[32m            formset.save()[m
[32m+[m[32m            return redirect(self.get_success_url())[m
[32m+[m[32m        else:[m
[32m+[m[32m            return self.form_invalid(form)[m
[32m+[m[41m    [m
[32m+[m[32mclass CommentUpdateView(LoginRequiredMixin, UserPassesTestMixin, UpdateView):[m
[32m+[m[32m    model = Comment[m
[32m+[m[32m    form_class = CommentForm[m
[32m+[m[32m    template_name = 'comment_edit.html'[m
[32m+[m[41m    [m
[32m+[m[32m    def test_func(self):[m
[32m+[m[32m        comment = self.get_object()[m
[32m+[m[32m        return comment.author == self.request.user[m
[32m+[m[32m    # redirige al template home[m
[32m+[m[32m    def get_success_url(self):[m
[32m+[m[32m        return reverse_lazy('home', kwargs={'pk':self.object.article.pk})[m
[32m+[m
[32m+[m
[32m+[m[32mclass CommentDeleteView(LoginRequiredMixin, UserPassesTestMixin, DeleteView):[m
[32m+[m[32m    model = Comment[m
[32m+[m[32m    template_name = 'comment_delete.html'[m
[32m+[m[41m    [m
[32m+[m[32m    # Este m√©todo asegura que solo el autor puede eliminar el comentario[m
[32m+[m[32m    def test_func(self):[m
[32m+[m[32m        comment = self.get_object()[m
[32m+[m[32m        return comment.author == self.request.user[m
[32m+[m[41m    [m
[32m+[m[32m    # redirige al template home[m
[32m+[m[32m    def get_success_url(self):[m
[32m+[m[32m        # Redirigir a la p√°gina del art√≠culo despu√©s de eliminar el comentario[m
[32m+[m[32m        return reverse_lazy('home', kwargs={'pk': self.object.article.pk})[m
[32m+[m
