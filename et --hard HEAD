[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    const ALL_DOWNCODE_MAPS = [[m
[32m+[m[32m        LATIN_MAP,[m
[32m+[m[32m        LATIN_SYMBOLS_MAP,[m
[32m+[m[32m        GREEK_MAP,[m
[32m+[m[32m        TURKISH_MAP,[m
[32m+[m[32m        ROMANIAN_MAP,[m
[32m+[m[32m        RUSSIAN_MAP,[m
[32m+[m[32m        UKRAINIAN_MAP,[m
[32m+[m[32m        CZECH_MAP,[m
[32m+[m[32m        SLOVAK_MAP,[m
[32m+[m[32m        POLISH_MAP,[m
[32m+[m[32m        LATVIAN_MAP,[m
[32m+[m[32m        ARABIC_MAP,[m
[32m+[m[32m        LITHUANIAN_MAP,[m
[32m+[m[32m        SERBIAN_MAP,[m
[32m+[m[32m        AZERBAIJANI_MAP,[m
[32m+[m[32m        GEORGIAN_MAP[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    const Downcoder = {[m
[32m+[m[32m        'Initialize': function() {[m
[32m+[m[32m            if (Downcoder.map) { // already made[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m            Downcoder.map = {};[m
[32m+[m[32m            for (const lookup of ALL_DOWNCODE_MAPS) {[m
[32m+[m[32m                Object.assign(Downcoder.map, lookup);[m
[32m+[m[32m            }[m
[32m+[m[32m            Downcoder.regex = new RegExp(Object.keys(Downcoder.map).join('|'), 'g');[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    function downcode(slug) {[m
[32m+[m[32m        Downcoder.Initialize();[m
[32m+[m[32m        return slug.replace(Downcoder.regex, function(m) {[m
[32m+[m[32m            return Downcoder.map[m];[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m    function URLify(s, num_chars, allowUnicode) {[m
[32m+[m[32m        // changes, e.g., "Petty theft" to "petty-theft"[m
[32m+[m[32m        if (!allowUnicode) {[m
[32m+[m[32m            s = downcode(s);[m
[32m+[m[32m        }[m
[32m+[m[32m        s = s.toLowerCase(); // convert to lowercase[m
[32m+[m[32m        // if downcode doesn't hit, the char will be stripped here[m
[32m+[m[32m        if (allowUnicode) {[m
[32m+[m[32m            // Keep Unicode letters including both lowercase and uppercase[m
[32m+[m[32m            // characters, whitespace, and dash; remove other characters.[m
[32m+[m[32m            s = XRegExp.replace(s, XRegExp('[^-_\\p{L}\\p{N}\\s]', 'g'), '');[m
[32m+[m[32m        } else {[m
[32m+[m[32m            s = s.replace(/[^-\w\s]/g, ''); // remove unneeded chars[m
[32m+[m[32m        }[m
[32m+[m[32m        s = s.replace(/^\s+|\s+$/g, ''); // trim leading/trailing spaces[m
[32m+[m[32m        s = s.replace(/[-\s]+/g, '-'); // convert spaces to hyphens[m
[32m+[m[32m        s = s.substring(0, num_chars); // trim to first num_chars chars[m
[32m+[m[32m        return s.replace(/-+$/g, ''); // trim any trailing hyphens[m
[32m+[m[32m    }[m
[32m+[m[32m    window.URLify = URLify;[m
[32m+[m[32m}[m
