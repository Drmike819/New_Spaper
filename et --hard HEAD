[32m+[m
[32m+[m[32m},{"../internals/add-to-unscopables":59,"../internals/define-iterator":81,"../internals/descriptors":83,"../internals/internal-state":111,"../internals/is-pure":118,"../internals/iterators":122,"../internals/object-define-property":129,"../internals/to-indexed-object":154}],174:[function(require,module,exports){[m
[32m+[m[32m'use strict';[m
[32m+[m[32mvar $ = require('../internals/export');[m
[32m+[m[32mvar global = require('../internals/global');[m
[32m+[m[32mvar isArray = require('../internals/is-array');[m
[32m+[m[32mvar isConstructor = require('../internals/is-constructor');[m
[32m+[m[32mvar isObject = require('../internals/is-object');[m
[32m+[m[32mvar toAbsoluteIndex = require('../internals/to-absolute-index');[m
[32m+[m[32mvar lengthOfArrayLike = require('../internals/length-of-array-like');[m
[32m+[m[32mvar toIndexedObject = require('../internals/to-indexed-object');[m
[32m+[m[32mvar createProperty = require('../internals/create-property');[m
[32m+[m[32mvar wellKnownSymbol = require('../internals/well-known-symbol');[m
[32m+[m[32mvar arrayMethodHasSpeciesSupport = require('../internals/array-method-has-species-support');[m
[32m+[m[32mvar un$Slice = require('../internals/array-slice');[m
[32m+[m
[32m+[m[32mvar HAS_SPECIES_SUPPORT = arrayMethodHasSpeciesSupport('slice');[m
[32m+[m
[32m+[m[32mvar SPECIES = wellKnownSymbol('species');[m
[32m+[m[32mvar Array = global.Array;[m
[32m+[m[32mvar max = Math.max;[m
[32m+[m
[32m+[m[32m// `Array.prototype.slice` method[m
[32m+[m[32m// https://tc39.es/ecma262/#sec-array.prototype.slice[m
[32m+[m[32m// fallback for not array-like ES3 strings and DOM objects[m
[32m+[m[32m$({ target: 'Array', proto: true, forced: !HAS_SPECIES_SUPPORT }, {[m
[32m+[m[32m  slice: function slice(start, end) {[m
[32m+[m[32m    var O = toIndexedObject(this);[m
[32m+[m[32m    var length = lengthOfArrayLike(O);[m
[32m+[m[32m    var k = toAbsoluteIndex(start, length);[m
[32m+[m[32m    var fin = toAbsoluteIndex(end === undefined ? length : end, length);[m
[32m+[m[32m    // inline `ArraySpeciesCreate` for usage native `Array#slice` where it's possible[m
[32m+[m[32m    var Constructor, result, n;[m
[32m+[m[32m    if (isArray(O)) {[m
[32m+[m[32m      Constructor = O.constructor;[m
[32m+[m[32m      // cross-realm fallback[m
[32m+[m[32m      if (isConstructor(Constructor) && (Constructor === Array || isArray(Constructor.prototype))) {[m
[32m+[m[32m        Constructor = undefined;[m
[32m+[m[32m      } else if (isObject(Constructor)) {[m
[32m+[m[32m        Constructor = Constructor[SPECIES];[m
[32m+[m[32m        if (Constructor === null) Constructor = undefined;[m
[32m+[m[32m      }[m
[32m+[m[32m      if (Constructor === Array || Constructor === undefined) {[m
[32m+[m[32m        return un$Slice(O, k, fin);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    result = new (Constructor === undefined ? Array : Constructor)(max(fin - k, 0));[m
