[32m+[m[32m'use strict';[m
[32m+[m[32m{[m
[32m+[m[32m    const toggleNavSidebar = document.getElementById('toggle-nav-sidebar');[m
[32m+[m[32m    if (toggleNavSidebar !== null) {[m
[32m+[m[32m        const navSidebar = document.getElementById('nav-sidebar');[m
[32m+[m[32m        const main = document.getElementById('main');[m
[32m+[m[32m        let navSidebarIsOpen = localStorage.getItem('django.admin.navSidebarIsOpen');[m
[32m+[m[32m        if (navSidebarIsOpen === null) {[m
[32m+[m[32m            navSidebarIsOpen = 'true';[m
[32m+[m[32m        }[m
[32m+[m[32m        main.classList.toggle('shifted', navSidebarIsOpen === 'true');[m
[32m+[m[32m        navSidebar.setAttribute('aria-expanded', navSidebarIsOpen);[m
[32m+[m
[32m+[m[32m        toggleNavSidebar.addEventListener('click', function() {[m
[32m+[m[32m            if (navSidebarIsOpen === 'true') {[m
[32m+[m[32m                navSidebarIsOpen = 'false';[m
[32m+[m[32m            } else {[m
[32m+[m[32m                navSidebarIsOpen = 'true';[m
[32m+[m[32m            }[m
[32m+[m[32m            localStorage.setItem('django.admin.navSidebarIsOpen', navSidebarIsOpen);[m
[32m+[m[32m            main.classList.toggle('shifted');[m
[32m+[m[32m            navSidebar.setAttribute('aria-expanded', navSidebarIsOpen);[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function initSidebarQuickFilter() {[m
[32m+[m[32m        const options = [];[m
[32m+[m[32m        const navSidebar = document.getElementById('nav-sidebar');[m
[32m+[m[32m        if (!navSidebar) {[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        navSidebar.querySelectorAll('th[scope=row] a').forEach((container) => {[m
[32m+[m[32m            options.push({title: container.innerHTML, node: container});[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        function checkValue(event) {[m
[32m+[m[32m            let filterValue = event.target.value;[m
[32m+[m[32m            if (filterValue) {[m
[32m+[m[32m                filterValue = filterValue.toLowerCase();[m
[32m+[m[32m            }[m
[32m+[m[32m            if (event.key === 'Escape') {[m
[32m+[m[32m                filterValue = '';[m
[32m+[m[32m                event.target.value = ''; // clear input[m
[32m+[m[32m            }[m
[32m+[m[32m            let matches = false;[m
[32m+[m[32m            for (const o of options) {[m
[32m+[m[32m                let displayValue = '';[m
[32m+[m[32m                if (filterValue) {[m
[32m+[m[32m                    if (o.title.toLowerCase().indexOf(filterValue) === -1) {[m
[32m+[m[32m                        displayValue = 'none';[m
[32m+[m[32m                    } else {[m
[32m+[m[32m                        matches = true;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                // show/hide parent <TR>[m
[32m+[m[32m                o.node.parentNode.parentNode.style.display = displayValue;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (!filterValue || matches) {[m
[32m+[m[32m                event.target.classList.remove('no-results');[m
[32m+[m[32m            } else {[m
[32m+[m[32m                event.target.classList.add('no-results');[m
[32m+[m[32m            }[m
[32m+[m[32m            sessionStorage.setItem('django.admin.navSidebarFilterValue', filterValue);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        const nav = document.getElementById('nav-filter');[m
[32m+[m[32m        nav.addEventListener('change', checkValue, false);[m
[32m+[m[32m        nav.addEventListener('input', checkValue, false);[m
[32m+[m[32m        nav.addEventListener('keyup', checkValue, false);[m
[32m+[m
[32m+[m[32m        const storedValue = sessionStorage.getItem('django.admin.navSidebarFilterValue');[m
[32m+[m[32m        if (storedValue) {[m
[32m+[m[32m            nav.value = storedValue;[m
[32m+[m[32m            checkValue({target: nav, key: ''});[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    window.initSidebarQuickFilter = initSidebarQuickFilter;[m
[32m+[m[32m    initSidebarQuickFilter();[m
[32m+[m[32m}[m
