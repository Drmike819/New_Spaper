[32m+[m[32mfrom django.db import models[m
[32m+[m[32mfrom django.conf import settings[m
[32m+[m[32mfrom django.urls import reverse[m
[32m+[m
[32m+[m[32m# Create your models here.[m
[32m+[m
[32m+[m[32mclass Article(models.Model):[m
[32m+[m[32m    title = models.CharField(max_length=255)[m
[32m+[m[32m    body = models.TextField()[m
[32m+[m[32m    date = models.DateTimeField(auto_now_add=True)[m
[32m+[m[32m    # Relaci√≥n en donde un articulo puede tener varias imagenes[m
[32m+[m[32m    article_images = models.ManyToManyField('ArticleImage', related_name='articles_related', blank=True)  # Cambio a 'articles_related'[m
[32m+[m[32m    # Relaci√≥n con el autor[m
[32m+[m[32m    author = models.ForeignKey(settings.AUTH_USER_MODEL, on_delete=models.CASCADE)[m
[32m+[m
[32m+[m[32m    def __str__(self):[m
[32m+[m[32m        return self.title[m
[32m+[m
[32m+[m[32m    # Funci√≥n que crea una URL en base al nombre de la vista[m
[32m+[m[32m    def get_absolute_url(self):[m
[32m+[m[32m        return reverse("home")[m
[32m+[m
[32m+[m
[32m+[m[32m# modelo que para las imagenes de los articulos[m
[32m+[m[32mclass ArticleImage(models.Model):[m
[32m+[m[32m    #conexion de muchos a uno (muchas imagenes pueden estar en un articulo)[m
[32m+[m[32m    article = models.ForeignKey(Article, on_delete=models.CASCADE, related_name='article_images_related')  # Modificar a 'article_images_related'[m
[32m+[m[32m    image = models.ImageField(upload_to='article_images/')[m
[32m+[m[32m    #almacena una fecha de creacion[m
[32m+[m[32m    created_at = models.DateTimeField(auto_now_add=True)[m
[32m+[m
[32m+[m[32m    def __str__(self):[m
[32m+[m[32m        return f"Image for {self.article.title}"[m
[32m+[m
[32m+[m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[32m#creamos una base para lo comentarios[m
[32m+[m[32mclass Comment(models.Model):[m
[32m+[m[32m    # indicamos que a la hora de eliminar el articulo este comentario se eliminara[m
[32m+[m[32m    article = models.ForeignKey(Article, on_delete=models.CASCADE)[m
[32m+[m[32m    comment = models.CharField(max_length=140)[m
[32m+[m[32m    # indiacamos que el autor del articulo es el usuario personalizado[m
[32m+[m[32m    author = models.ForeignKey(settings.AUTH_USER_MODEL, on_delete=models.CASCADE)[m
[32m+[m[41m    [m
[32m+[m[32m    def __str__(self):[m
[32m+[m[32m        return self.comment[m
[32m+[m[41m    [m
[32m+[m[32m    def get_absolute_url(self):[m
[32m+[m[32m        return reverse('article_list')[m
